DEBUGLOG(("CreateNewDepositTxn txn_date [%s]\n", csTxnDateTime));
                PutField_CString(hHeader, "transmission_datetime", csTxnDateTime);
	}


        if (GetField_CString(hMyHash, "post_datetime", &csTxnDateTime)) {
