	}

        /*
	DBObjPtr = CreateObj(DBPtr,"DBTransaction","Add");
	iRet = (unsigned long)(*DBObjPtr)(hHeader);
	*/

	iRet = Txn_Header_Add(hHeader);

	if (iRet == SUCCESS) {
		/*
		DBObjPtr = CreateObj(DBPtr,"DBTransaction","AddDetail");
		iRet = (unsigned long)(*DBObjPtr)(hDetail);
		*/
