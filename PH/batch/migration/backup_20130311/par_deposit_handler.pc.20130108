	if (iRet == SUCCESS) {
		/*
		DBObjPtr = CreateObj(DBPtr,"DBTransaction","UpdateDetail");
		iRet = (unsigned long)(*DBObjPtr)(hDetail);
		*/
		iRet = Txn_Detail_UpdateDetail(hDetail);
