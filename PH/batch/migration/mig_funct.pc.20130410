//////////////////Migration Period////////////////////////
int     mf_ChkValidMigPeriod(const char *csMerchNmb, const char *csTxnDate)
{
        int     iRet = PD_NOT_FOUND;

        EXEC SQL WHENEVER SQLERROR GOTO chkvalidmigperiod_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;
                varchar         hv_merch_nmb[MIG_PD_MERCH_ACCOUNT_NMB_LEN];
                varchar         hv_txn_date [PD_DATE_LEN];

                int             v_no_of_record;
                short           ind_no_of_record = -1;

        EXEC SQL END DECLARE SECTION;

        hv_merch_nmb.len = strlen(csMerchNmb);
        memcpy(hv_merch_nmb.arr, csMerchNmb, hv_merch_nmb.len);
DEBUGLOG(("ChkValidMigPeriod csMerchNmb = [%.*s]\n",hv_merch_nmb.len, hv_merch_nmb.arr));

        hv_txn_date.len = strlen(csTxnDate);
        memcpy(hv_txn_date.arr, csTxnDate, hv_txn_date.len);
DEBUGLOG(("ChkValidMigPeriod csTxnDate= [%.*s]\n",hv_txn_date.len, hv_txn_date.arr));

        EXEC SQL
                SELECT count(1)
                   INTO :v_no_of_record:ind_no_of_record
                   FROM par_def_merch_mig_period, par_def_merch_nmb_map
                  WHERE mn_merchant_acct_nmb = :hv_merch_nmb
                    and mn_mig_group = mp_mig_group
                    and :hv_txn_date between mp_start_date and mp_end_date;

        if (ind_no_of_record >= 0) {
                if (v_no_of_record > 0) {
DEBUGLOG(("ChkValidMigPeriod FOUND\n"));
                        iRet = PD_FOUND;
                }
        }

        if (iRet!= PD_FOUND) {
DEBUGLOG(("ChkValidMigPeriod NOT FOUND\n"));
        }

        return iRet;

chkvalidmigperiod_error:
DEBUGLOG(("chkvalidmigperiod_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
        EXEC SQL WHENEVER SQLERROR CONTINUE;
        return PD_ERR;

}
