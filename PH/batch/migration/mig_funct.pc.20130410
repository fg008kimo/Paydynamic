int mf_merchant_getmerchanttxninfo(hash_t *hContext, const hash_t *hRequest)
{
        int     iRet = PD_OK;

        char*   csServiceCode;
        char*   csMerchantId;
        char*   csTmp;
        char*   csTxnCcy;
        char*   csTxnCountry;
        char*   csMerchantClientId;
        int     iTmp;
        double  dTmp;
        hash_t  *hClient;
        char*   csPayMethod;

        hash_t  *hRec;
        recordset_t     *rRecordSet;
        rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rRecordSet,0);

        if (GetField_CString(hRequest,"service_code",&csServiceCode)) {
DEBUGLOG(("mf_merchant_getmerchanttxninfo() service_code = [%s]\n",csServiceCode));
        }
        else  {
DEBUGLOG(("mf_merchant_getmerchanttxninfo() service_code is missing!!!\n"));
ERRLOG("mig_funct::mf_merchant_getmerchanttxninfo() service_code is missing!!!\n");
                iRet = INT_ERR;
        }

        if (GetField_CString(hRequest,"merchant_id",&csMerchantId)) {
DEBUGLOG(("mf_merchant_getmerchanttxninfo() merchant_id = [%s]\n",csMerchantId));
        }
        else {
                iRet = INT_MERCHANT_ID_NOT_FOUND;
	}

        if (iRet == PD_OK ) {
		if (mf_MerchDetail_GetMerchant(csMerchantId, rRecordSet) != PD_OK) {
                        iRet = INT_MERCHANT_ID_NOT_FOUND;
ERRLOG("mig_funct::mf_merchant_getmerchanttxninfo() Merchant Account [%s] not found\n",csMerchantId);
DEBUGLOG(("mf_merchant_getmerchanttxninfo() Merchant Account [%s] not found\n",csMerchantId));
                }
                else {
                        iRet = INT_MERCHANT_ID_NOT_FOUND;
                        hRec = RecordSet_GetFirst(rRecordSet);
                        while (hRec) {
                                if (GetField_Int(hRec,"disabled",&iTmp)) {
DEBUGLOG(("mf_merchant_getmerchanttxninfo() GetMerchantDetail - disabled = [%d]\n",iTmp));
                                }
                                if(GetField_CString(hRec,"status",&csTmp)){
DEBUGLOG(("mf_merchant_getmerchanttxninfo() GetMerchantDetail - status = [%s]\n",csTmp));
                                }

                                if (iTmp != PD_DISABLED) {
                                        if(!strcmp(csTmp,PD_ACC_OPEN)){
                                                iRet = PD_OK;
                                                if (GetField_CString(hRec,"merchant_name",&csTmp)) {
DEBUGLOG(("mf_merchant_getmerchanttxninfo() GetMerchantDetail - merchant_name = [%s]\n",csTmp));
                                                }
                                                if (GetField_CString(hRec,"merchant_client_id",&csMerchantClientId)) {
DEBUGLOG(("mf_merchant_getmerchanttxninfo() GetMerchantTxnInfo() GetMerchantDetail - merchant_client_id = [%s]\n",csMerchantClientId));
                                                        PutField_CString(hContext,"merchant_client_id",csMerchantClientId);
                                                }
                                                if (GetField_Double(hRec,"approximate_fee_rate",&dTmp)) {
//DEBUGLOG(("mf_merchant_getmerchanttxninfo() GetMerchantTxnInfo() GetMerchantDetail - approximate_fee_rate = [%lf]\n",dTmp));
                                                        PutField_Double(hContext,"approximate_fee_rate",dTmp);
                                                }
                                        }
                                        else{
ERRLOG("mig_funct::mf_merchant_getmerchanttxninfo() GetMerchantTxnInfo() Merchant Account [%s] not opened\n",csMerchantId);
                                                iRet = INT_ACCOUNT_DISABLED;
                                        }
                                }
                                else {
ERRLOG("mig_funct:: mf_merchant_getmerchanttxninfo GetMerchantTxnInfo() Merchant Account [%s] disabled\n",csMerchantId);
                                        iRet = INT_ACCOUNT_DISABLED;
                                }


                                hRec = RecordSet_GetNext(rRecordSet);
                        }
                }
        }
        //RecordSet_Destroy(rRecordSet);


        if(iRet == PD_OK){
                hClient = (hash_t*) malloc (sizeof(hash_t));
                hash_init(hClient,0);
		if (mf_Clients_GetClients(csMerchantClientId, hClient) != PD_OK) {
                        iRet = INT_ERR;
ERRLOG("mig_funct:: mf_merchant_getmerchanttxninfo GetMerchantTxnInfo() Client ID for Account[%s] not found\n",csMerchantId);
DEBUGLOG(("mf_merchant_getmerchanttxninfo GetMerchantTxnInfo() Client ID Account[%s] not found\n",csMerchantId));
                }
                else{
                        if (GetField_CString(hClient,"status",&csTmp)) {
DEBUGLOG(("mf_merchant_getmerchanttxninfo ::GetMerchantTxnInfo() GetClients - status = [%s]\n",csTmp));
                                        if(strcmp(csTmp,PD_ACC_OPEN)){
ERRLOG("mig_funct::mf_merchant_getmerchanttxninfo() Client Account [%s] not opened\n",csMerchantClientId);
                                                iRet = INT_ACCOUNT_DISABLED;
                                        }
                        }
                }
                FREE_ME(hClient);
        }
        if(iRet == PD_OK){
                if (!GetField_CString(hRequest,"txn_ccy",&csTxnCcy)) {
                        iRet = INT_ERR;
ERRLOG("mig_funct::mf_merchant_getmerchanttxnInfo() Currency for Account is missing\n");
DEBUGLOG(("mig_funct::mf_merchant_getmerchanttxninfo()  Currency for Account is missing\n"));
                }
                else {
DEBUGLOG(("mig_funct::mf_merchant_getmerchanttxninfo() txn ccy = [%s]\n",csTxnCcy));
                }
                if (!GetField_CString(hRequest,"txn_country",&csTxnCountry)) {
                        iRet = INT_ERR;
ERRLOG("mig_funct::mf_merchant_getmerchanttxninfo() Txn Country for Account is missing\n");
DEBUGLOG(("mig_funct::getmerchanttxninfo() Txn Country for Account is missing\n"));
                }
                else {
DEBUGLOG(("mig_funct::getmerchanttxninfo() txn country = [%s]\n",csTxnCountry));
                }
                if (iRet == PD_OK) {
                        if (mf_MerchBalAcct_CheckMerchantBalAcct(csMerchantId,
                                                        csTxnCountry,
                                                        csTxnCcy,
                                                        csServiceCode,
                                                        hContext) != PD_FOUND) {
                                iRet = INT_INVALID_TXN;
ERRLOG("mig_funct::getmerchanttxninfo() Invalid txn acct [%d] [%s] [%s] [%s]\n",iRet,csMerchantId,csTxnCountry,csTxnCcy);
DEBUGLOG(("mig_funct::getmerchanttxninfo () txn acct  [%d] [%s] [%s] [%s]\n",iRet,csMerchantId,csTxnCountry,csTxnCcy));
                        }
                }
        }

        if (iRet == PD_OK) {
                if (GetField_CString(hRequest, "pay_method", &csPayMethod)) {
DEBUGLOG(("mig_funct::getmerchanttxninfo() pay_method = [%s]\n",csPayMethod));
                        int iMaxField = (int)(PD_PAY_METHOD_LIST_LEN/PD_PAY_METHOD_LEN);
                        char csList[iMaxField][PD_PAY_METHOD_LEN];
                        char *p;
                        int iFieldCnt = 0;
                        int iValidCnt = 0;
                        int i;

                        p = strtok (csPayMethod,",");
                        while (p != NULL)
                        {
                                strcpy(csList[iFieldCnt],p);
                                csList[iFieldCnt][strlen(p)]='\0';
                                p = strtok (NULL,",");
                                iFieldCnt++;
                        }

                        for(i=0;i<iFieldCnt;i++){

DEBUGLOG(("mig_funct::getmerchanttxninfo() call DBMerchantPayMethod:FindMerchantPayMethod [%s] [%s]\n", csMerchantId, csList[i]));
                                if (mf_MerchantPayMethod_FindMerchantPayMethod(csMerchantId, csList[i]) != PD_OK) {
DEBUGLOG(("mig_funct::getmerchanttxninfo() Unsupport Pay Method [%d] [%s] [%s]\n",iRet,csMerchantId,csList[i]));
                                }
                                else {
                                        iValidCnt ++;
DEBUGLOG(("mig_funct::getmerchanttxninfo() Support Pay Method [%d] [%s] [%s]\n",iRet,csMerchantId,csList[i]));
                                }
                        }
                        if(iValidCnt==0){
                                        iRet = INT_UNSUPPORTED_PAY_METHOD;
ERRLOG("mig_funct:: getmerchanttxninfo() All Pay Method UnSupported[%d] [%s]\n",iRet,csMerchantId);
                        }
                }
        }


        RecordSet_Destroy(rRecordSet);
        FREE_ME(rRecordSet);

DEBUGLOG(("iRet = [%d]\n",iRet));
        return iRet;
}


