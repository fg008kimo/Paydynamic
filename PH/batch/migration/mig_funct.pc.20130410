                recordset_t* myRec)
{
        int iRet = 0;

        hash_t *myHash;
        EXEC SQL WHENEVER SQLERROR GOTO mf_getmerchant_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;



        EXEC SQL BEGIN DECLARE SECTION;
                varchar         hv_merchant_id[PD_MERCHANT_ID_LEN];

