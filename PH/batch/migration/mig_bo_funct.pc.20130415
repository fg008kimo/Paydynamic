char* migbo_GetNextMgtTxnSeq()
{
        EXEC SQL WHENEVER SQLERROR GOTO get_mgttxnseq_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;
                varchar hv_txn_seq_no[PD_TXN_SEQ_LEN + 1];
