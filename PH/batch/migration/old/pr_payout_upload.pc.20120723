				else if(iCount==IDX_DETAIL_MEMBER_FEE_CCY_CD)
					sprintf(csTag,"member_fee_ccy_%d",iRec);
				else if(iCount==IDX_DETAIL_MERCHANT_FEE_CCY_CD)
					sprintf(csTag,"merchant_fee_ccy_%d",iRec);
