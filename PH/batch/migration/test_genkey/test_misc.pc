int ma_num_of_decimal_places(const char *data)
{
        int i = 0, j = 0;
        int go = 0;

        while (*data) {

printf("[%s] [%c] [%d]\n", data, *data, go);

                if (*data >= '0' && *data <= '9' && go == 1) {
                        if (*data == '0') {
                                j++;
                        } else {
                                i += j + 1;
                                j = 0;
                        }
                } else if (go == 0) {
                        if (*data == '.') {
                                go = 1;
                        }
                } else {
                        return i;
                }
                data++;
        }

        return i;
}


