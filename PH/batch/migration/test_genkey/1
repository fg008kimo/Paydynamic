int batch_proc(int argc, char* argv[])
{
	int iRet = PD_TRUE;
	hash_t *hTemp;
	int	iCheck = PD_FALSE;
	
	char	*csTmp;
	double	dTmp = 0.0;
	long	lTmp;

        hTemp= (hash_t*)malloc(sizeof(hash_t));
        hash_init(hTemp, 0);

	dTmp = 1234.456789012345;
printf("dTmp [%lf]\n", dTmp);

	dTmp = newround(dTmp, 2);
printf("dTmp(newround) [%lf]\n", dTmp);




        hash_destroy(hTemp);
        FREE_ME(hTemp);	



	return iRet;
}


