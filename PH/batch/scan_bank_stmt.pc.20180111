int process_file(char *csProviderId,char *csAcctType,FILE *fin,FILE *fout,recordset_t *rRawBankStmt,recordset_t *rErrorBankStmt)
{
	int     iRet = SUCCESS;
	int	i = 0;
        int     iFieldCnt = 0;
        int     iStmtCnt = 0;
	int	iStmtFileValid = PD_TRUE;
        char    csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN];
        char    cs_input_buf[PD_MAX_BUFFER+1];
        char    cs_bank_stmt_file[PD_MAX_BUFFER+1];
        char    cs_file_ext[PD_MAX_BUFFER+1];
        char    *p;

	memset(cs_input_buf, 0, sizeof(cs_input_buf));
	memset(cs_bank_stmt_file, 0, sizeof(cs_bank_stmt_file));
	memset(cs_file_ext, 0, sizeof(cs_file_ext));

        while (fgets(cs_input_buf,PD_MAX_BUFFER, fin) != NULL) {

                iFieldCnt = 0;

		// Check 0x0A
                if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A)
                        cs_input_buf[strlen(cs_input_buf) - 1] = '\0';

		// Trim OxOD
		strcpy(cs_input_buf,TrimAllChar((const unsigned char*)cs_input_buf,strlen(cs_input_buf),PD_CHAR));
DEBUGLOG(("process_file: [%d][ORG_INPUT_FILE]=[%s]\n",iStmtCnt,cs_input_buf));

		strcpy(cs_bank_stmt_file, cs_input_buf);

		// Trim Extension
                p = mystrtok(cs_input_buf,PD_EXT_DELIMITOR);
                if (p == NULL) {
                        iRet = FAILURE;
		} else {
                      	while ((p = mystrtok(NULL,PD_EXT_DELIMITOR)) != NULL) {
				strcpy(cs_file_ext,p);
                      	}
		}
	
DEBUGLOG(("process_file: [%d][INPUT_FILE]=[%s][%s]\n",iStmtCnt,cs_input_buf,cs_file_ext));

		// Breakdown Raw Stmt File Name
		if (iRet == SUCCESS) {
                	p = mystrtok(cs_input_buf,PD_MY_DELIMITOR);
                	if (p == NULL) {
                        	iRet = FAILURE;
			} else {
                		strcpy(csList[iFieldCnt],p);
                		iFieldCnt++;

                		while ((p = mystrtok(NULL,PD_MY_DELIMITOR)) != NULL) {
                		        strcpy(csList[iFieldCnt],p);
                		        iFieldCnt++;
                		}
			}

			for (i=0;i<IMPORT_MAX_FIELD;i++) {
DEBUGLOG(("process_file: [%d][%s]=[%s](%d)\n",iStmtCnt,CS_FIELD_NAME[i],csList[i],(int)strlen(csList[i])));
			}
		}

		// Verify Raw Stmt File Name
		if (iRet == SUCCESS) {

			iStmtFileValid = check_detail(csList);

			if (iStmtFileValid == PD_TRUE) {

				/* File Name Valid */

				hash_t  *hRawStmtRec = NULL;
                        	hRawStmtRec = (hash_t*) malloc (sizeof(hash_t));
                        	hash_init(hRawStmtRec,0);

                		PutField_CString(hRawStmtRec, "acct_type", csAcctType);
                		PutField_CString(hRawStmtRec, "client_id", csProviderId);
                		PutField_CString(hRawStmtRec, "bank_init", csList[0]);
                		PutField_CString(hRawStmtRec, "bank_acct_name", csList[1]);
                		PutField_CString(hRawStmtRec, "last_6_bank_acct_num", csList[2]);
                		PutField_CString(hRawStmtRec, "datetime", csList[3]);
                		PutField_CString(hRawStmtRec, "file_ext", cs_file_ext);
                		PutField_CString(hRawStmtRec, "bank_stmt_file", cs_bank_stmt_file);
	                	
				RecordSet_Add(rRawBankStmt, hRawStmtRec);

				//hash_destroy(hRawStmtRec);
        			//FREE_ME(hRawStmtRec);
			} else {
					
				/* File Name Invalid */
					
				hash_t  *hErrorStmtRec = NULL;
                                hErrorStmtRec = (hash_t*) malloc (sizeof(hash_t));
                                hash_init(hErrorStmtRec,0);

                                PutField_CString(hErrorStmtRec, "acct_type", csAcctType);
                                PutField_CString(hErrorStmtRec, "client_id", csProviderId);
                                PutField_CString(hErrorStmtRec, "bank_init", csList[0]);
                                PutField_CString(hErrorStmtRec, "bank_acct_name", csList[1]);
                                PutField_CString(hErrorStmtRec, "last_6_bank_acct_num", csList[2]);
                                PutField_CString(hErrorStmtRec, "datetime", csList[3]);
                                PutField_CString(hErrorStmtRec, "file_ext", cs_file_ext);
                                PutField_CString(hErrorStmtRec, "bank_stmt_file", cs_bank_stmt_file);

                                RecordSet_Add(rErrorBankStmt, hErrorStmtRec);

                                //hash_destroy(hErrorStmtRec);
                                //FREE_ME(hErrorStmtRec);
			}
                }	

		// Clear input file buffer
		memset(cs_input_buf,0,sizeof(cs_input_buf));
		memset(cs_bank_stmt_file,0,sizeof(cs_bank_stmt_file));
		memset(cs_file_ext,0,sizeof(cs_file_ext));
		
                iStmtCnt++;
        }
	
	return iRet;
}


int prcoess_match(recordset_t *rBankAcctDetail,recordset_t *rRawBankStmt,recordset_t *rProcessBankStmt,recordset_t *rMissingBankStmt)
{
	int iRet = SUCCESS;

	int iMissingCnt = 0;

	char *csTmp = NULL;

        hash_t *hBankAcctRec = NULL;

        hBankAcctRec = RecordSet_GetFirst(rBankAcctDetail);
        while (hBankAcctRec) {

		int iMatchCnt = 0;

		char csBankAcctNum[PD_BANK_CODE_LEN + 1];

		hash_t *hRawStmtRec = NULL;

		memset(csBankAcctNum, 0, sizeof(csBankAcctNum));

/* bank_acct_num */
	        if (GetField_CString(hBankAcctRec,"bank_acct_num",&csTmp)) {
	                sprintf(csBankAcctNum, "%s", csTmp);
//DEBUGLOG(("prcoess_match: bank_acct_num = [%s]\n",csBankAcctNum));
	        } else {
DEBUGLOG(("prcoess_match: bank_acct_num is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_num is missing!!\n");
	        }

        	hRawStmtRec = RecordSet_GetFirst(rRawBankStmt);
        	while (hRawStmtRec) {
	
			char csRawShortBankAcctNum[PD_TMP_BUF_LEN + 1];

			memset(csRawShortBankAcctNum, 0, sizeof(csRawShortBankAcctNum));

/* last_6_bank_acct_num */
                	if (GetField_CString(hRawStmtRec,"last_6_bank_acct_num",&csTmp)) {
	                        sprintf(csRawShortBankAcctNum, "%s", csTmp);
//DEBUGLOG(("prcoess_match: last_6_bank_acct_num = [%s]\n",csRawShortBankAcctNum));
	                } else {
DEBUGLOG(("prcoess_match: last_6_bank_acct_num is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: last_6_bank_acct_num is missing!!\n");
	                }

			/* matching */
			csTmp = strstr(csBankAcctNum, csRawShortBankAcctNum);
                        if (csTmp != NULL) {	
	
				hash_t *hProcessStmtRec = NULL;
        			hProcessStmtRec = (hash_t*) malloc (sizeof(hash_t));
        			hash_init(hProcessStmtRec,0);

/* acct_type */
				if (GetField_CString(hRawStmtRec,"acct_type",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][acct_type] = [%s]\n",iMatchCnt,csTmp));
                                        PutField_CString(hProcessStmtRec, "acct_type", csTmp);
                                } else {
DEBUGLOG(("prcoess_match: acct_type is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: acct_type is missing!!\n");
                                }	

/* client_id */		
				if (GetField_CString(hRawStmtRec,"client_id",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][client_id] = [%s]\n",iMatchCnt,csTmp));
                                        PutField_CString(hProcessStmtRec, "client_id", csTmp);
                                } else {
DEBUGLOG(("prcoess_match: client_id is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: client_id is missing!!\n");
                                }
	
/* bank_init */
                        	if (GetField_CString(hRawStmtRec,"bank_init",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][bank_init] = [%s]\n",iMatchCnt,csTmp));
					PutField_CString(hProcessStmtRec, "bank_init", csTmp);
                        	} else {
DEBUGLOG(("prcoess_match: bank_init is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_init is missing!!\n");
                        	}

/* bank_acct_name */
				if (GetField_CString(hRawStmtRec,"bank_acct_name",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][bank_acct_name] = [%s]\n",iMatchCnt,csTmp));
                                        PutField_CString(hProcessStmtRec, "bank_acct_name", csTmp);
                                } else {
DEBUGLOG(("prcoess_match: bank_acct_name is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_name is missing!!\n");
                                }

/* last_6_bank_acct_num */
				PutField_CString(hProcessStmtRec, "last_6_bank_acct_num", csRawShortBankAcctNum);
DEBUGLOG(("prcoess_match: [Matched][%d][last_6_bank_acct_num] = [%s]\n",iMatchCnt,csRawShortBankAcctNum));

/* datetime */
				if (GetField_CString(hRawStmtRec,"datetime",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][datetime] = [%s]\n",iMatchCnt,csTmp));
                                        PutField_CString(hProcessStmtRec, "datetime", csTmp);
                                } else {
DEBUGLOG(("prcoess_match: datetime is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: datetime is missing!!\n");
                                }

/* file_ext */
                                if (GetField_CString(hRawStmtRec,"file_ext",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][file_ext] = [%s]\n",iMatchCnt,csTmp));
                                        PutField_CString(hProcessStmtRec, "file_ext", csTmp);
                                } else {
DEBUGLOG(("prcoess_match: file_ext is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: file_ext is missing!!\n");
                                }

/* bank_stmt_file */
                                if (GetField_CString(hRawStmtRec,"bank_stmt_file",&csTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][bank_stmt_file] = [%s]\n",iMatchCnt,csTmp));
                                        PutField_CString(hProcessStmtRec, "bank_stmt_file", csTmp);
                                } else {
DEBUGLOG(("prcoess_match: bank_stmt_file is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_stmt_file is missing!!\n");
                                }

				iMatchCnt++;

	                        RecordSet_Add(rProcessBankStmt, hProcessStmtRec);

				//hash_destroy(hProcessStmtRec);
                                //FREE_ME(hProcessStmtRec);
			}

                	hRawStmtRec = RecordSet_GetNext(rRawBankStmt);
        	}

		if (iMatchCnt < 1) {

