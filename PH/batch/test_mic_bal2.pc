int isNumericV2(const char *str) 
{
    while(*str != '.')
    {
        printf("checkV2:%d\r\n", *str);
        if(*str < '0' || *str > '9')
            return 0;
        str++;
    }
    return 1;
}

int isNumeric(const char *str) 
{
    while(*str != '\0')
    {
        printf("check:%d\r\n", *str);
        if(*str < '0' || *str > '9')
            return 0;
        str++;
    }
    return 1;
}


