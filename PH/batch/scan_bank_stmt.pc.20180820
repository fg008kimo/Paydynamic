        } else {
DEBUGLOG(("convert_stmt_file: file_ext is missing!!\n"));
ERRLOG("scan_bank_stmt: convert_stmt_file: file_ext is missing!!\n");
                iRet = PD_ERR;
        }

/* raw_file */
	sprintf(csRawFile, "%s/%s", cs_raw_path, csBankStmtFile);
