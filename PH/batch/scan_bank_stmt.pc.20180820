                                        	        //iDtlRet = PD_ERR;
                                        	}
			
					} else {

						/*
                                                 *
                                                 * If raw bank statement filename invalid,
                                                 * - Put status and err_code into Upload Recordset.
                                                 *
                                                 */

/* status */
                                                if (GetField_Char(hRawStmtRec,"status",&cTmp)) {
DEBUGLOG(("process_match: [Match][%d][status] = [%c]\n",iMatchCnt,cTmp));
                                                        PutField_Char(hProcessStmtRec, "status", cTmp);
                                                } else {
