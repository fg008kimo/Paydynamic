		strncpy(csFromTxnDate, csTxnDateRange, PD_YYMMDD_LEN);
DEBUGLOG(("convert_stmt_file: [from_txn_date]=[%s]\n", csFromTxnDate));

                strncpy(csToTxnDate, &csTxnDateRange[PD_YYMMDD_LEN+1], PD_YYMMDD_LEN);
