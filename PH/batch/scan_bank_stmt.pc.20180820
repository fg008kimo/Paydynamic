int batch_init(int argc, char* argv[])
{
    if (argc < 17) {
    	printf("usage: -a raw_path -r raw_file -o process_file -s job_seq -n nature_path -p provider_path -b process_bank -d bank_exclude\n");
        return FAILURE;
    }
    else
        return SUCCESS;
}


int batch_proc(int argc, char* argv[])
{
	int	iRet = SUCCESS;
	int	iDtlRet = PD_FOUND;
	//int     iTmpRet = PD_OK;

	int     iRecExist = PD_FALSE;
	
	char	csAcctType[PD_ACCT_TYPE_LEN + 1];
	char 	csProviderName[PD_CLIENT_NAME_LEN + 1];
	char 	csProviderId[PD_CLIENT_ID_LEN + 1];
	char	csBankCode[PD_BANK_CODE_LEN + 1];

	DIR *draw;
	FILE	*fraw,*fproc;	

	hash_t  *hData = NULL;
        hData = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hData,0);

        recordset_t *rBankAcctDetail;
        rBankAcctDetail = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rBankAcctDetail, 0);

	recordset_t *rRawBankStmt;
        rRawBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rRawBankStmt, 0);
	
      	recordset_t *rProcessBankStmt;
        rProcessBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rProcessBankStmt, 0);

	recordset_t *rConvertBankStmt;
        rConvertBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rConvertBankStmt, 0);

DEBUGLOG(("batch_proc: start\n"));

	//memset(cs_raw_path, 0, sizeof(cs_raw_path));
	//memset(cs_raw_file, 0, sizeof(cs_raw_file));
	//memset(cs_process_file, 0, sizeof(cs_process_file));
	//memset(cs_nature_path, 0, sizeof(cs_nature_path));
	//memset(cs_provider_path, 0, sizeof(cs_provider_path));
	//memset(cs_process_bank, 0, sizeof(cs_process_bank));

	memset(csAcctType, 0, sizeof(csAcctType));
	memset(csProviderName, 0, sizeof(csProviderName));
	memset(csProviderId, 0, sizeof(csProviderId));
	memset(csBankCode, 0, sizeof(csBankCode));

	iRet = parse_arg(argc,argv);

	if (iRet != SUCCESS) {
    		printf("*usage: -a raw_path -r raw_file -o process_file -s job_seq -n nature_path -p provider_path -b process_bank -d bank_exclude\n");
		return (iRet);
	}

DEBUGLOG(("batch_proc: job_seq = [%d]\n",int_job_seq));
DEBUGLOG(("batch_proc: nature_path = [%s]\n",cs_nature_path));
DEBUGLOG(("batch_proc: provider_path = [%s]\n",cs_provider_path));
DEBUGLOG(("batch_proc: process_bank = [%s]\n",cs_process_bank));
DEBUGLOG(("batch_proc: bank_exclude = [%d]\n",int_bank_exclude));

DEBUGLOG(("batch_proc: raw_path = [%s]\n",cs_raw_path));
DEBUGLOG(("batch_proc: raw_file = [%s]\n",cs_raw_file));
DEBUGLOG(("batch_proc: process_file = [%s]\n",cs_process_file));

	// Check Raw Path Exists
     	draw = opendir(cs_raw_path);
       	if (draw == NULL) {
DEBUGLOG(("batch_proc: Raw Path Not Exists\n"));
ERRLOG("scan_bank_stmt: batch_proc: Raw Path Not Exists\n");
              	return FAILURE;
    	} else {
              	closedir(draw);
      	}

	// Open Raw File
	fraw = fopen(cs_raw_file,"r");
	if (fraw == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_raw_file));
ERRLOG("scan_bank_stmt: batch_proc: Error Opening file!!\n");
		return FAILURE;
	}

	// Open Process File
	fproc = fopen(cs_process_file,"w");
	if (fproc == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_process_file));
ERRLOG("scan_bank_stmt: batch_proc: Error Opening file!!\n");
		fclose(fraw);
		return FAILURE;
	}

	// Update Auto Upload Job Status (Pending)
	if (iRet == SUCCESS) {
/*	
		iTmpRet = update_job_status(PD_AUTO_UPL_JOB_STATUS_PENDING);
                if (iTmpRet != PD_OK) {
                   	iRet = FAILURE;
                }	
*/
	}

	// Get Acct Type, Provider Id and Provider Name
	if (iRet == SUCCESS) {
		DBObjPtr = CreateObj(DBPtr,"DBOLAutoUploadStmtSetting","GetDetailByPathName");
                if ((unsigned long)(DBObjPtr)(cs_provider_path,cs_nature_path,csProviderName,csProviderId,csAcctType) == PD_FOUND) {
DEBUGLOG(("batch_proc: DBOLAutoUploadStmtSetting: GetDetailByPathName: acct_type = [%s], client_id = [%s], client_name = [%s]\n", csAcctType, csProviderId, csProviderName));
                        PutField_CString(hData, "acct_type", csAcctType);	
			PutField_CString(hData, "client_id", csProviderId);
                        PutField_CString(hData, "client_name", csProviderName);
                } else {
DEBUGLOG(("batch_proc: DBOLAutoUploadStmtSetting: GetDetailByPathName Failed!!\n"));
ERRLOG("scan_bank_stmt: batch_proc: DBOLAutoUploadStmtSetting: GetDetailByPathName Failed!!\n");
                        iRet = FAILURE;
                }
	}

	// Get Bank Code
	if (iRet == SUCCESS) {

		// If Process Bank is equal to ALL, skip!!
		if (strcmp(cs_process_bank, PD_BANK_NAME_ALL_BANK)) {

			int i = 0;
			int iCnt = 0;
                        char *csTmp;
                        char csTmpBankList[PD_TMP_BUF_LEN + 1];
                        char csTmpBankName[PD_TMP_BUF_LEN][PD_TMP_BUF_LEN];
			char csTag[PD_TMP_BUF_LEN];

			/*
                         * Breakdown the Process Bank Name
                         *
                         * E.g. [ICBC_ABC_CCB]
                         *      -> [ICBC][ABC][CCB]
                         *
                         */

                       	strcpy(csTmpBankList, cs_process_bank);

                     	csTmp = mystrtok(csTmpBankList,PD_BANK_DELIMITOR);
                     	if (csTmp == NULL) {
                            	iRet = FAILURE;
                      	} else {
                           	strcpy(csTmpBankName[iCnt],csTmp);
				iCnt++;
                            	while ((csTmp = mystrtok(NULL,PD_BANK_DELIMITOR)) != NULL) {
                                     	strcpy(csTmpBankName[iCnt],csTmp);
					iCnt++;
                               	}
                      	}

			for (i=0;i<iCnt;i++) {

				// Find Internal Bank Code by Bank Abbrev Name
				DBObjPtr = CreateObj(DBPtr,"DBBankDesc","FindOfflineBankByBankAbbrevName");
                        	if ((unsigned long)(DBObjPtr)(csTmpBankName[i],csBankCode) == FOUND) {
DEBUGLOG(("batch_proc: DBBankDesc: FindOfflineBankByBankAbbrevName: int_bank_code = [%s]\n", csBankCode)); 	
				} else {
DEBUGLOG(("batch_proc: DBBankDesc: FindOfflineBankByBankAbbrevName Failed!!\n"));
ERRLOG("scan_bank_stmt: batch_proc: DBBankDesc: FindOfflineBankByBankAbbrevName Failed!!\n");
                                	iRet = FAILURE;
                        	}

DEBUGLOG(("batch_proc: bank_name[%d] = [%s]\n",i,csTmpBankName[i]));
				sprintf(csTag, "%d_bank_short_name", i);
				PutField_CString(hData, csTag, csTmpBankName[i]);

DEBUGLOG(("batch_proc: bank_code[%d] = [%s]\n",i,csBankCode));
                                sprintf(csTag, "%d_int_bank_code", i);
                                PutField_CString(hData, csTag, csBankCode);

			}

			PutField_Int(hData, "total_bank_cnt", iCnt);
DEBUGLOG(("batch_proc: total_bank_cnt = [%d]\n",iCnt));

                   	PutField_Int(hData, "int_bank_code_exclude", int_bank_exclude);
		}
	}
	
	// Get Current PH Date
	if (iRet == SUCCESS) {
		char    *csCode = strdup("");
               	char    *csValue = strdup("");

              	hash_t  *hPHDate;
             	recordset_t     *rPHDate;
              	rPHDate = (recordset_t*) malloc (sizeof(recordset_t));
              	recordset_init(rPHDate,0);

             	// Get Current PH Date
              	DBObjPtr = CreateObj(DBPtr,"DBSystemControl","GetAllCodes");
              	if ((*DBObjPtr)(rPHDate) == PD_OK) {
                   	hPHDate = RecordSet_GetFirst(rPHDate);
                       	while (hPHDate) {
                              	if (GetField_CString(hPHDate,"code",&csCode)) {
                                     	if (GetField_CString(hPHDate,"value",&csValue)) {
                                             	if (!strcmp(csCode,"CTPHDATE")) {
//DEBUGLOG(("batch_proc: Call DBSystemControl: Current Processor Hub Date = [%s]\n",csValue));
							 PutField_CString(hData, "PHDATE", csValue);
DEBUGLOG(("batch_proc: PHDATE = [%s][%d]\n",csValue,strlen(csValue)));
                                                   	break;
                                             	}
                                     	}
                             	}
                            	hPHDate = RecordSet_GetNext(rPHDate);
                   	}
          	} else {
DEBUGLOG(("batch_proc: Call DBSystemControl: Record Not Found\n"));
ERRLOG("scan_bank_stmt: batch_proc: Call DBSystemControl: Record Not Found\n");
                     	iRet = FAILURE;
            	}

		FREE_ME(csCode);
		FREE_ME(csValue);

		RecordSet_Destroy(rPHDate);
           	FREE_ME(rPHDate);
	}

	// Process Bank Acct
	if (iRet == SUCCESS) {

		iDtlRet = process_bank_acct(hData,rBankAcctDetail);
		if (iDtlRet == PD_FOUND) {
DEBUGLOG(("batch_proc: process_bank_acct: Record Found!!\n"));
		} else if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("batch_proc: process_bank_acct: No Record Found!!\n"));
			iRet = FAILURE;
		} else {
DEBUGLOG(("batch_proc: process_bank_acct: Failed!!\n"));
                        iRet = FAILURE;
		}
	}

	// Process File
	if (iRet == SUCCESS) {
			
       		iDtlRet = process_file(hData,fraw,rRawBankStmt);
		if (iDtlRet == PD_FOUND) {
DEBUGLOG(("batch_proc: process_file: Record Found!!\n"));
		} else if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("batch_proc: process_file: No Record Found!!\n"));
			iRet = FAILURE;
		} else {
DEBUGLOG(("batch_proc: process_file: Failed!!\n"));
                      	iRet = FAILURE;
      		}
	}

	// Process Matching
	if (iRet == SUCCESS) {

		iDtlRet = process_match(rBankAcctDetail,rRawBankStmt,rProcessBankStmt);
		if (iDtlRet == PD_FOUND) {
DEBUGLOG(("batch_proc: process_match: Match Record Found!!\n"));
			iRecExist = PD_TRUE;
                } else if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("batch_proc: process_match: No Match Record Found!!\n"));
                        iRet = FAILURE;
                } else {
DEBUGLOG(("batch_proc: process_match: Failed!!\n"));
                        iRet = FAILURE;
                }
	}

	// Process Convert
	if (iRet == SUCCESS) {
		
		iDtlRet = process_convert(rProcessBankStmt, rConvertBankStmt);
		if (iDtlRet == PD_FOUND) {
DEBUGLOG(("batch_proc: process_convert: Convert Record Found!!\n"));
		} else if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("batch_proc: process_convert: No Convert Record Found!!\n"));		
                } else {
DEBUGLOG(("batch_proc: process_convert: Failed!!\n"));
			iRet = FAILURE;
		}
	}

	// Process Results
	if (iRet == SUCCESS) {
	
		iRet = process_output(rConvertBankStmt,fproc);
		if (iRet == SUCCESS) {
DEBUGLOG(("batch_proc: process_output: Success!!\n"));
		} else {
DEBUGLOG(("batch_proc: process_output: Failed!!\n"));
		}
	}

	// Update Auto Upload Job Status (Completed)
       	if (iRecExist == PD_FALSE) {
/*
            	iTmpRet = update_job_status(PD_AUTO_UPL_JOB_STATUS_COMPLETE);
*/
  	}

	hash_destroy(hData);
        FREE_ME(hData);	
	
	RecordSet_Destroy(rBankAcctDetail);
        FREE_ME(rBankAcctDetail);

	RecordSet_Destroy(rRawBankStmt);
        FREE_ME(rRawBankStmt);

	RecordSet_Destroy(rProcessBankStmt);
	FREE_ME(rProcessBankStmt);

	RecordSet_Destroy(rConvertBankStmt);
        FREE_ME(rConvertBankStmt);

	fclose(fraw);
	fclose(fproc);

DEBUGLOG(("batch_proc: iRet = [%d]\n", iRet));
	return iRet;
}


int batch_terminate(int argc, char* argv[])
{
	return SUCCESS;
}

                        
int parse_arg(int argc,char **argv)
{               
        char    c;
	strcpy(cs_raw_path,"");
	strcpy(cs_raw_file,"");
        strcpy(cs_process_file,"");
        strcpy(cs_nature_path,"");
	strcpy(cs_provider_path,"");
        strcpy(cs_process_bank,"");
	int_job_seq = 0;                        
	int_bank_exclude = 0;                        

        while ((c = getopt(argc,argv,"a:r:o:s:n:p:b:d:")) != EOF) {
                switch (c) {
			case 'a':
                                strcpy(cs_raw_path, optarg);
                                break;
			case 'r':
                                strcpy(cs_raw_file, optarg);
                                break;
                        case 'o':
                                strcpy(cs_process_file, optarg);
                                break;
			case 's':
                                int_job_seq = atoi(optarg);
                                break;
			case 'n':
                                strcpy(cs_nature_path, optarg);
                                break;
			case 'p':
                                strcpy(cs_provider_path, optarg);
                                break;
			case 'b':
                                strcpy(cs_process_bank, optarg);
                                break;
			case 'd':
                                int_bank_exclude = atoi(optarg);
                                break;
                        default:
                                return FAILURE;
                }
        }       
        
        if (!strcmp(cs_raw_path,"") || !strcmp(cs_raw_file,"") || !strcmp(cs_process_file,"") || !strcmp(cs_nature_path,"") || !strcmp(cs_provider_path,"") || !strcmp(cs_process_bank,""))
                return FAILURE;
                
        return SUCCESS; 
}               


int update_job_status(char cStatus)
{
        int iRet = PD_OK;

        hash_t  *hUpdateJob = NULL;
        hUpdateJob = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hUpdateJob,0);

        PutField_Int(hUpdateJob, "job_seq", int_job_seq);
        PutField_CString(hUpdateJob, "nature_path", cs_nature_path);
        PutField_CString(hUpdateJob, "provider_path", cs_provider_path);
        PutField_CString(hUpdateJob, "process_bank", cs_process_bank);
        PutField_Int(hUpdateJob, "exclude_mode", int_bank_exclude);
        PutField_Char(hUpdateJob, "status", cStatus);
        PutField_CString(hUpdateJob, "update_user", PD_UPDATE_USER);

        DBObjPtr = CreateObj(DBPtr,"DBOLAutoUploadJobStatus","UpdateStatus");
        if ((unsigned long)(DBObjPtr)(hUpdateJob) == PD_OK) {
DEBUGLOG(("update_job_status: DBOLAutoUploadJobStatus: UpdateStatus Success!!\n"));
DEBUGLOG(("update_job_status: DBOLAutoUploadJobStatus: UpdateStatus TxnCommit()\n"));
                TxnCommit();
        } else {
DEBUGLOG(("update_job_status: DBOLAutoUploadJobStatus: UpdateStatus Failed!!\n"));
ERRLOG("scan_bank_stmt: update_job_status: DBOLAutoUploadJobStatus: UpdateStatus Failed!!\n");
                iRet = PD_ERR;
        }

        hash_destroy(hUpdateJob);
        FREE_ME(hUpdateJob);

        return iRet;
}


int check_is_digit(const char *csValue)
{
        int iRet = PD_FALSE;

        if (is_numeric((char*)csValue) != PD_FALSE) {
                iRet = PD_TRUE;
        }

        return iRet;
}


int check_is_month(const char *csValue)
{
        int iRet = PD_FALSE;
	int iValue = 0;

	iValue = atoi(csValue);
        if (iValue >=0 && iValue <= 12) {
                iRet = PD_TRUE;
        }

        return iRet;
}


int check_filename(char *cs_bank_stmt_file)
{
        int iRet = PD_TRUE;

        char *csTmp = NULL;

//DEBUGLOG(("process_file: check_filename: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

       	csTmp = cs_bank_stmt_file;
        while (*csTmp) {
    		if (!(*csTmp >= 'a' && *csTmp <= 'z') &&
        	    !(*csTmp >= 'A' && *csTmp <= 'Z') &&
        	    !(*csTmp >= '0' && *csTmp <= '9') &&
        	    !(*csTmp == '-' || *csTmp == '_')) {
DEBUGLOG(("process_file: check_filename: NOT Match [a-zA-Z0-9-_]\n", *csTmp));
        	      	iRet = PD_FALSE;
			break;
      		}
      		csTmp++;
      	}	

//DEBUGLOG(("process_file: check_filename: iRet = [%d]\n", iRet));

//DEBUGLOG(("process_file: check_filename: end: ----------------------------------------------------------------------------------------------\n"));

        return iRet;
}


int check_detail(int iAcctTypeIdx, int iNumOfField, char csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN])
{
	int iRet = PD_TRUE;
	int i,j = 0;

//DEBUGLOG(("process_file: check_detail: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

	if (iNumOfField != IMPORT_MAX_FIELD) {
DEBUGLOG(("process_file: check_detail: Number of Field = [%d] not equal to Max Field Number [%d]\n",iNumOfField,IMPORT_MAX_FIELD));
             	iRet = PD_FALSE;
	} else {
		for (i=0;i<iNumOfField;i++) {

//DEBUGLOG(("process_file: check_detail: %s=[%s](%d)\n", CS_SCAN_FIELD_NAME[i], csList[i], (int)strlen(csList[i])));

			// Check Field Not Empty
			if (strlen(csList[i]) <= 0) {
DEBUGLOG(("process_file: check_detail: Empty Field [%d]\n", (int)strlen(csList[i])));
                             	iRet = PD_FALSE;
				break;
			}

			// Check Length
			if (INT_SCAN_DETAIL_CHECK_LEN[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i] != -1) {
				if ((strlen(csList[i]) < INT_SCAN_DETAIL_MIN_LEN[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i]) 
				    || (strlen(csList[i]) > INT_SCAN_DETAIL_MAX_LEN[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i])
				)
				{
DEBUGLOG(("process_file: check_detail: Length [%d] NOT IN THE RANGE [%d] - [%d]\n", (int)strlen(csList[i]), INT_SCAN_DETAIL_MIN_LEN[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i], INT_SCAN_DETAIL_MAX_LEN[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i]));
					iRet = PD_FALSE;
					break;
				}
			} else {
//DEBUGLOG(("process_file: check_detail: SKIP Check Length \n"));	
			}

			// Check Content
			if (INT_SCAN_DETAIL_CHECK_TYPE[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i] != -1) {
				if (!strcmp(CS_SCAN_DETAIL_TYPE[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i],"ALL_CHAR")) {

					for (j=0;j<strlen(csList[i]);j++) {

                        			if (!(csList[i][j] >= 'a' && csList[i][j] <= 'z') &&
						    !(csList[i][j] >= 'A' && csList[i][j] <= 'Z') &&
						    !(csList[i][j] >= '0' && csList[i][j] <= '9') &&
						    !(csList[i][j] >= '-' || csList[i][j] <= '_')
						) {
DEBUGLOG(("process_file: check_detail: [%c] NOT Match [a-zA-Z0-9-_]\n", csList[i][j]));
							iRet = PD_FALSE;
                                			break;
                        			}
                			}

				} else if (!strcmp(CS_SCAN_DETAIL_TYPE[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i],"ENGLISH_CHAR")) {

                                        for (j=0;j<strlen(csList[i]);j++) {

						if (!(csList[i][j] >= 'a' && csList[i][j] <= 'z') &&
                                                    !(csList[i][j] >= 'A' && csList[i][j] <= 'Z')
						) {
DEBUGLOG(("process_file: check_detail: [%c] NOT Match [a-zA-Z]\n", csList[i][j]));
                                                        iRet = PD_FALSE;
                                                        break;
                                                }
                                        }

				} else if (!strcmp(CS_SCAN_DETAIL_TYPE[(iAcctTypeIdx*IMPORT_MAX_FIELD)+i],"DIGIT")) {
	
                        		for (j=0;j<strlen(csList[i]);j++) {

                        		        if (!(csList[i][j] >= '0' && csList[i][j] <= '9')) {
DEBUGLOG(("process_file: check_detail: [%c] NOT Match [0-9]\n", csList[i][j]));
							iRet = PD_FALSE;	
                        		                break;
                        		        }
                       	 		}
				}

				if (iRet == PD_FALSE) {
					break;
				}
			} else {
//DEBUGLOG(("process_file: check_detail: SKIP Check Content \n"));
			}
		}
	}

//DEBUGLOG(("process_file: check_detail: iRet = [%d]\n", iRet));

//DEBUGLOG(("process_file: check_detail: end: ----------------------------------------------------------------------------------------------\n"));

	return iRet;
}


int split_line_by_format(const char *csLine, const char *csFormatId, const hash_t *hFormat, hash_t *hRls)
{
	int iRet = PD_OK;

        int iField = 0;

	char *csDesc = NULL, *csTemplate = NULL;
        char *csLineField, *csRemainField;
        char *csTmp;
        char *csTag = (char*) malloc (64);
        char csDelimiter[2];
        char csNewField[PD_TMP_MSG_BUF_LEN], csNewField2[PD_TMP_MSG_BUF_LEN], csNewLine[PD_TMP_MSG_BUF_LEN];

        /* delimiter */
        sprintf(csTag, "delimiter_%s", csFormatId);
        if (!GetField_CString(hFormat, csTag, &csTmp)) {
DEBUGLOG(("split_line_by_format: [%s] not found\n", csTag));
ERRLOG("scan_bank_stmt: split_line_by_format: [%s] not found\n", csTag);
                iRet = PD_ERR;
        } else {
                sprintf(csDelimiter, "%c", atoi(csTmp));
        }

        strcpy(csNewLine, csLine);

        csLineField = mystrtok_r(csNewLine, csDelimiter, &csRemainField);
        while (csLineField != NULL && iRet == PD_OK) {
                strcpy(csNewField, TrimAll((const unsigned char*)csLineField, strlen(csLineField)));

                iField++;

                if (*csNewField == '\0') {
                        csLineField = mystrtok_r(NULL, csDelimiter, &csRemainField);
                        continue;
                }

                csDesc = NULL;

		strcpy(csNewField2, csNewField);

                /* desc - CONTENT */
                sprintf(csTag, "content_desc_%s_%d", csFormatId, iField);
                if (GetField_CString(hFormat, csTag, &csDesc)) {
//DEBUGLOG(("debug desc %s (%s)[%s]\n", csTag, csDesc, csNewField));
                        PutField_CString(hRls, csTag, csDesc);
                        PutField_CString(hRls, csDesc, csNewField);
                } else {
//DEBUGLOG(("debug desc %s [%s]\n", csTag, csNewField));
                        PutField_CString(hRls, csTag, csNewField);
                }

		/* desc_2 - CONTENT */
                sprintf(csTag, "content_desc_2_%s_%d", csFormatId, iField);
                if (GetField_CString(hFormat, csTag, &csDesc)) {
//DEBUGLOG(("debug desc_2 %s (%s)[%s]\n", csTag, csDesc, csNewField2));
                        PutField_CString(hRls, csTag, csDesc);
                        PutField_CString(hRls, csDesc, csNewField2);
                } else {
//DEBUGLOG(("debug desc_2 %s [%s]\n", csTag, csNewField2));
                        PutField_CString(hRls, csTag, csNewField2);
                }

		/* template - CONTENT */
                sprintf(csTag, "content_temp_%s_%d", csFormatId, iField);
                if (GetField_CString(hFormat, csTag, &csTemplate)) {
//DEBUGLOG(("debug temp %s [%s]\n", csTag, csTemplate));
                        PutField_CString(hRls, "tmp_content_temp", csTemplate);
                        iRet = split_line_by_format(csNewField, csTemplate, hFormat, hRls);
                }

                csLineField = mystrtok_r(NULL, csDelimiter, &csRemainField);
//DEBUGLOG(("debug remain %s [%s]\n", csTag, csRemainField));
        }

        FREE_ME(csTag);

        return iRet;
}


