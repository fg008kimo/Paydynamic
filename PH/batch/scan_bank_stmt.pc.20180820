							if ((!strcmp(csBankStmtFile, csMatchStmtFile))
							    && (iBankDisabled == PD_FALSE)
							    && (iBankAcctDisabled == PD_FALSE)
							
							) 
							{
						
								/* Match */
								iMatch = PD_TRUE;
								break;
							}
						}

						hMatchRec = RecordSet_GetNext(rMatch);
					}
				} else {

					/*
                                 	 *
                                 	 * If raw bank statement filename invalid,
                                 	 * - do nothing.
                                 	 *
                                 	 */
				}	
			}

			// Check Matching Results
			if (iDtlRet == PD_OK) {

				if (iStmtFileValid == PD_TRUE) {

					if (iMatch == PD_TRUE) {

						/* 
						 *	Raw bank statement filename:	Valid and able to match to the input provider bank account
						 *	AND Bank Level: 		Enabled
						 *	AND Bank Account Level:		Enabled
						 *	
						 *	Action:				Put into Process Recordset
						 *
						 */

						iSkip = PD_FALSE;						
					} else {

						/* 
                                                 *      Raw bank statement filename:  	Valid but cannot match to the input provider bank account
                                                 *      OR Bank Level:                  Disabled
                                                 *      OR Bank Account Level:          Disabled
                                                 *
						 *
						 *	Action:				Skip the Record
                                                 */

						iSkip = PD_TRUE;
					}
				} else {

					if (iMatch == PD_TRUE) {
			
						/*
						 *	Invalid Case
						 *
						 *	Action:				Return Error
						 */

						iSkip = PD_TRUE;
						iDtlRet = PD_ERR;
					} else {

						/* 
                                                 *      Raw bank statement filename:    Invalid 
                                                 *      AND Bank Level:                 N/A 
                                                 *      AND Bank Account Level:         N/A
                                                 *
						 *
						 *	Action:                         Put into Process Recordset
                                                 */	
			
						iSkip = PD_FALSE;
					}
				}
			}

			// Put into Process Recordset or Skip
