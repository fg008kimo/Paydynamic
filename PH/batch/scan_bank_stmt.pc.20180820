DEBUGLOG(("convert_stmt_file: [converted_file]=[%s]\n", csConvertedFile));

	// Convert Raw Stmt File
        if (iRet == PD_OK) {

                hash_t  *hStmtFile = NULL;
                hStmtFile = (hash_t*) malloc (sizeof(hash_t));
                hash_init(hStmtFile,0);

                //bank_acct_type
                PutField_CString(hStmtFile, "in_file_name", csBankStmtFile);
                PutField_CString(hStmtFile, "new_file", csRawFile);
                PutField_CString(hStmtFile, "converted_file", csConvertedFile);
                PutField_CString(hStmtFile, "int_bank_code", csBankCode);

                BOObjPtr = CreateObj(BOPtr, "BOOLBankStmt", "ConvertStmtFile");
                if ((unsigned long)(*BOObjPtr)(hStmtFile) != PD_OK) {
                        //iErrorCode = INT_INVALID_FILE_FORMAT;
			iRet = PD_ERR;
DEBUGLOG(("convert_stmt_file: Call BOOLBankStmt: ConvertStmtFile() Failure!!!\n"));
ERRLOG("scan_bank_stmt: convert_stmt_file: Call BOOLBankStmt: ConvertStmtFile() Failure!!!\n");
                } else {
DEBUGLOG(("convert_stmt_file: Call BOOLBankStmt: ConvertStmtFile() Success!!\n"));
                }

                hash_destroy(hStmtFile);
                FREE_ME(hStmtFile);
        }

	// Open Converted Stmt File
        if (iRet == PD_OK) {

                fin = fopen(csConvertedFile, "r");
                if (fin == NULL) {
DEBUGLOG(("convert_stmt_file: Error opening file = [%s]\n", csConvertedFile));
ERRLOG("scan_bank_stmt: convert_stmt_file: Error opening file = [%s]\n", csConvertedFile);
                        return PD_ERR;
                } else {
                        while (fgets(cs_input_buf, sizeof(cs_input_buf), fin) != NULL) {
                                if (strlen_content(cs_input_buf) == 0) {
                                        iSBlankLine++;
                                } else {
                                        break;
                                }
                        }
                        rewind(fin);
                        while (fgets(cs_input_buf, sizeof(cs_input_buf), fin) != NULL) {
                                if (strlen_content(cs_input_buf) == 0) {
                                        iEBlankLine++;
                                } else {
                                        iEBlankLine = 0;
                                }
                                iTotalLine++;
                        }
DEBUGLOG(("convert_stmt_file: Summary: Total line = [%d] Starting/Ending Blank Line = [%d]/[%d]\n", iTotalLine, iSBlankLine, iEBlankLine));
                }
        }

        // Get Stmt File Format
        if (iRet == PD_OK) {
DEBUGLOG(("convert_stmt_file: Call DBOLStmtFormat: GetFormat()\n"));
                DBObjPtr = CreateObj(DBPtr, "DBOLStmtFormat", "GetFormat");
                if ((unsigned long)(*DBObjPtr)(csBankCode, rFormat) != PD_OK) {
DEBUGLOG(("convert_stmt_file: Call DBOLStmtFormat: GetFormat() fail\n"));
ERRLOG("scan_bank_stmt: convert_stmt_file: Call DBOLStmtFormat :GetFormat() fail\n");
                        //iErrorCode = INT_FORMAT_TEMPLATE_ERROR;
                        iRet = PD_ERR;
                }
        }

	// Match Stmt File Format
        if (iRet == PD_OK) {
                hFormat = RecordSet_GetFirst(rFormat);
                while (hFormat) {

                        if (GetField_CString(hFormat, "format_id", &csFormatId)) {
DEBUGLOG(("convert_stmt_file: [%d][format_id] = [%s]\n", iFormatCnt, csFormatId));

                                // delimiter
                                sprintf(csTag, "delimiter_%s", csFormatId);
                                if (GetField_CString(hFormat, csTag, &csTmp)) {
                                        sprintf(csDelimiter, "%c", atoi(csTmp));
DEBUGLOG(("convert_stmt_file: [%d][delimiter] = [%s]\n", iFormatCnt, csDelimiter));
                                } else {
DEBUGLOG(("convert_stmt_file: [%d]delimiter not found\n", iFormatCnt));
ERRLOG("scan_bank_stmt: convert_stmt_file: delimiter not found\n");
                                        //iErrorCode = INT_FORMAT_TEMPLATE_ERROR;
