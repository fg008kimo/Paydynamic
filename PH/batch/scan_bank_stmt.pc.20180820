        }

	// Get Txn Date List
	if (iRet == PD_OK) {
	
                int iTxnDateMatched = PD_FALSE;

                int iYear = 2000, iMonth = 0, iDay = 0;

        	char    csTmpTxnDate[PD_YYYYMMDD_LEN+1];

                time_t  tNow = time(NULL);
                struct  tm *tStruct = localtime(&tNow);

                memset(csTmpTxnDate,0,sizeof(csTmpTxnDate));

                strncpy(csTmpTxnDate, csFromTxnDate, PD_YY_LEN);
