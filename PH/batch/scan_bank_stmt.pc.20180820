ERRLOG("scan_bank_stmt: process_match: client_id is missing!!\n");
                                      		iDtlRet = PD_ERR;
                           		}

/* client_name */
                            		if (GetField_CString(hRawStmtRec,"client_name",&csTmp)) {
//DEBUGLOG(("process_match: [Match][%d][client_name] = [%s]\n",iMatchCnt,csTmp));
                                	    	PutField_CString(hProcessStmtRec, "client_name", csTmp);
                            		} else {
DEBUGLOG(("process_match: client_name is missing!!\n"));
ERRLOG("scan_bank_stmt: process_match: client_name is missing!!\n");
                                      		iDtlRet = PD_ERR;
                              		}

/* bank_init */
                             		if (GetField_CString(hRawStmtRec,"bank_init",&csTmp)) {
//DEBUGLOG(("process_match: [Match][%d][bank_init] = [%s]\n",iMatchCnt,csTmp));
                                    		PutField_CString(hProcessStmtRec, "bank_init", csTmp);
                             		} else {
//DEBUGLOG(("process_match: [Match][%d]bank_init is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: process_match: bank_init is missing!!\n");
                                       		//iDtlRet = PD_ERR;
                           		}

/* bank_acct_init */
                             		if (GetField_CString(hRawStmtRec,"bank_acct_init",&csTmp)) {
//DEBUGLOG(("process_match: [Match][%d][bank_acct_init] = [%s]\n",iMatchCnt,csTmp));
                                	    	PutField_CString(hProcessStmtRec, "bank_acct_init", csTmp);
                             		} else {
//DEBUGLOG(("process_match: [Match][%d]bank_acct_init is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: process_match: bank_acct_init is missing!!\n");
                                	     	//iDtlRet = PD_ERR;
                             		}

/* last_6_bank_acct_num */
                           		if (GetField_CString(hRawStmtRec,"last_6_bank_acct_num",&csTmp)) {
//DEBUGLOG(("process_match: [Match][%d][last_6_bank_acct_num] = [%s]\n",iMatchCnt,csTmp));
