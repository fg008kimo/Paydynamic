                                iTotalField = 0;
                        } else {
                                iTotalField = atoi(csTmp);
                        }

DEBUGLOG(("convert_stmt_file: [%d]Summary: Total Field = [%d] Starting/Ending Row = [%d]/[%d]\n", iFormatCnt, iTotalField, iStartRow, iEndRow));

			// check input file total field
                        iLineCnt = 0;
                        iFieldCnt = 0;
                        rewind(fin);
                        while (fgets(cs_input_buf, sizeof(cs_input_buf), fin) != NULL) {
