                strncpy(csTmpTxnDate, &csFromTxnDate[PD_YY_LEN+PD_MM_LEN], PD_DD_LEN);
                iDay = atoi(csTmpTxnDate);
//DEBUGLOG(("convert_stmt_file: [iDay]=[%d]\n", iDay));

                while (iTxnDateMatched == PD_FALSE) {

			if (iTxnDateCnt >= MAX_NUM_OF_TXN_DATE) {
DEBUGLOG(("convert_stmt_file: Txn Date Range is too large!!\n"));
                                iRet = PD_ERR;
                                break;
                        }

                        tStruct->tm_year = iYear - 1900;
                        tStruct->tm_mon = iMonth - 1;
                        tStruct->tm_mday = iDay;

                        mktime(tStruct);

                        strftime(csTmpTxnDate, sizeof(csTmpTxnDate), "%Y%m%d", tStruct);
//DEBUGLOG(("convert_stmt_file: [tmp_txn_date]=[%s]\n", csTmpTxnDate));

                        strncpy(csTxnDateList[iTxnDateCnt], &csTmpTxnDate[2], PD_YYMMDD_LEN);
                        csTxnDateList[iTxnDateCnt][PD_YYMMDD_LEN] = '\0';

                        if (!strcmp(&csTmpTxnDate[2], csToTxnDate)) {
                                iTxnDateMatched = PD_TRUE;
                        } else {
                                iDay++;
                                memset(csTmpTxnDate,0,sizeof(csTmpTxnDate));
                        }

                        iTxnDateCnt++;
                }

DEBUGLOG(("convert_stmt_file: [txn_date_cnt]=[%d]\n", iTxnDateCnt));

	}

	// Handle Split Stmt File List
        if (iRet == PD_OK) {
		
		int iTotalSplitCnt = 0;

		for (i=0;i<iTxnDateCnt;i++) {

			FILE *fout;

			// Get Split Stmt File
			sprintf(csSplitFileList[i], "%s_%s_%s_%s.%s", csBankInit, csBankAcctInit, csShortBankAcctNum, csTxnDateList[i], csFileExt);		
	
			// Get Split Stmt File (Full Path)
			sprintf(csSplitFileFullList[i], "%s/%s", cs_raw_path, csSplitFileList[i]);

			if (strncmp(csTxnDateList[i], &csPHDate[2], PD_YYMMDD_LEN) < 0) {
DEBUGLOG(("convert_stmt_file: [%d][split_file_list]=[%s][upload]\n", i, csSplitFileList[i]));
//DEBUGLOG(("convert_stmt_file: [%d][split_file_full_list]=[%s][upload]\n", i, csSplitFileFullList[i]));
                        } else {
DEBUGLOG(("convert_stmt_file: [%d][split_file_list]=[%s][not upload]\n", i, csSplitFileList[i]));
//DEBUGLOG(("convert_stmt_file: [%d][split_file_full_list]=[%s][not upload]\n", i, csSplitFileFullList[i]));
                        }

			// Create Split Stmt File
        		if (iRet == PD_OK) {

				// Check Txn Date with PH DATE
				if (strncmp(csTxnDateList[i], &csPHDate[2], PD_YYMMDD_LEN) < 0) {

					// Open Split Stmt File
					fout = fopen(csSplitFileFullList[i], "w");
                                	if (fout == NULL) {
DEBUGLOG(("convert_stmt_file: [%d]Error opening file = [%s]\n", i, csSplitFileFullList[i]));
ERRLOG("scan_bank_stmt: convert_stmt_file: Error opening file = [%s]\n", csSplitFileFullList[i]);
                                	        fclose(fin);
                                	        return PD_ERR;
                                	}
	
					// Loop All Line of Stmt File
					iLineCnt = 0;
                                        rewind(fin);
        		        	while (fgets(cs_input_buf, sizeof(cs_input_buf), fin) != NULL) {

        		                	iLineCnt++;

						hash_t *hRec;
						hRec = (hash_t*) malloc (sizeof(hash_t));
                        			hash_init(hRec, 0);

        		                	if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A) cs_input_buf[strlen(cs_input_buf) - 1] = '\0';
        		                	if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0D) cs_input_buf[strlen(cs_input_buf) - 1] = '\0';

						// output lines before and after detail records
                        			if (iLineCnt < iStartRow || iLineCnt > iTotalLine - iEndRow + 1) {
                                			fprintf(fout, "%.*s\n", (int)strlen(cs_input_buf), cs_input_buf);
                                			continue;
                        			}

						// split line by format template
						if (split_line_by_format(cs_input_buf, csFormatId, hFormat, hRec) != PD_OK) {
DEBUGLOG(("convert_stmt_file: [%d]Call split_line_by_format() fail\n",i));
ERRLOG("scan_bank_stmt: convert_stmt_file: Call split_line_by_format() fail\n");
                     	                	   	//iErrorCode = INT_FORMAT_TEMPLATE_ERROR;
                        	        	        iRet = PD_ERR;
                        	        	        break;
                        	        	}

/* statement_date */					
                        			if (GetField_CString(hRec,"statement_date",&csTmp)) {

							struct tm tm;

							char csStmtDate[50];
							char csStmtDateConv[50];

                                        		memset(csStmtDate,0,sizeof(csStmtDate));
                                        		memset(csStmtDateConv,0,sizeof(csStmtDateConv));

							strcpy(csStmtDate, csTmp);
//DEBUGLOG(("convert_stmt_file: [%d]Call split_line_by_format() [Line_%d][statement_date] = [%s]\n", i, iLineCnt, csStmtDate));

							strptime((const char*)csStmtDate, csDateFormat, &tm);
                                        		strftime(csStmtDateConv, sizeof(csStmtDateConv), PD_DEFAULT_DATE_FORMAT, &tm);
                					csStmtDateConv[strlen(csStmtDateConv)]='\0';
//DEBUGLOG(("convert_stmt_file: [%d]Call split_line_by_format() [Line_%d][statement_date_conv] = [%s]\n", i, iLineCnt, csStmtDateConv));
	
							if (!strncmp(&csStmtDateConv[2], csTxnDateList[i], PD_YYMMDD_LEN)) {
DEBUGLOG(("convert_stmt_file: [%d]Call split_line_by_format() [Line_%d][statement_date_conv] = [%s]\n", i, iLineCnt, csStmtDateConv));
								fprintf(fout, "%.*s\n", (int)strlen(cs_input_buf), cs_input_buf);	     
							}
						} 

						hash_destroy(hRec);
                                	        FREE_ME(hRec);
        		        	}
	
                          		// Put Split Stmt File into Hash
                            		sprintf(csTag, "%d_split_stmt_file", i);
                             		PutField_CString(hProcessStmtRec, csTag, csSplitFileList[i]);

                              		iTotalSplitCnt++;
			
					fclose(fout);
				}
                      	}
		}

		// Put Total Split Cnt into Hash
		PutField_Int(hProcessStmtRec, "total_split_cnt", iTotalSplitCnt);
	}

	// Rename Stmt File
	if (iRet == PD_OK) {

                FILE *fbankstmt;

                fbankstmt = fopen(csRawFile,"r");
                if (fbankstmt != NULL) {
//DEBUGLOG(("convert_stmt_file: Raw File Exists\n"));
                        fclose(fbankstmt);

                        // Rename file
                        rename(csRawFile, csRenamedFile);
DEBUGLOG(("convert_stmt_file: Rename Stmt File: [%s]\n", csRenamedFile));
                } else {
DEBUGLOG(("convert_stmt_file: Raw File Not Exists\n"));
                }
        }

	// Remove Converted Stmt File
	if (iRet == PD_OK) {

            	FILE *fbankstmt;

            	fbankstmt = fopen(csConvertedFile,"r");
             	if (fbankstmt != NULL) {
//DEBUGLOG(("convert_stmt_file: Converted File Exists\n"));
                    	fclose(fbankstmt);

                     	// Remove file
                      	remove(csConvertedFile);
DEBUGLOG(("convert_stmt_file: Remove Converted Stmt File: [%s]\n", csConvertedFile));
                } else {
DEBUGLOG(("convert_stmt_file: Converted File Not Exists\n"));
		}
	}

        fclose(fin);

	FREE_ME(csTag);

        RecordSet_Destroy(rFormat);
        FREE_ME(rFormat);

        return iRet;
}


int process_bank_acct(hash_t *hData,recordset_t *rBankAcctDetail)
{
	int     iRet = PD_NOT_FOUND;

	int	iTotalBankCnt = 0;

	char 	csBankCodeList[PD_TMP_BUF_LEN + 1];

DEBUGLOG(("process_bank_acct: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

/* total_bank_cnt */
	if (GetField_Int(hData,"total_bank_cnt",&iTotalBankCnt)) {
DEBUGLOG(("process_bank_acct: total_bank_cnt = [%d]\n",iTotalBankCnt));
    	} else {
//DEBUGLOG(("process_bank_acct: total_bank_cnt is missing!!\n"));
//ERRLOG("scan_bank_stmt: process_bank_acct: total_bank_cnt is missing!!\n");
		//iRet = PD_ERR;
       	}

	// If Process Bank is equal to ALL, skip!!
	if (iRet != PD_ERR) {

		int 	i = 0;
		int 	iBankExclude = 0;

		for (i=0;i<iTotalBankCnt;i++)  {

			/*
                         * Fornmat the Process Bank List String
                         *
                         * E.g. [ICBC][ABC][CCB]
                         *      -> [ICBC,ABC,CCB]
                         *
			 */

			char *csBankCode = NULL;
			char csTag[PD_TMP_BUF_LEN];
			char csTmpBankCodeList[PD_TMP_BUF_LEN];

			sprintf(csTag, "%d_int_bank_code", i);
			if (GetField_CString(hData,csTag,&csBankCode)) {
				
				if (i == 0) {
					sprintf(csBankCodeList, csBankCode);
				} else {
					sprintf(csTmpBankCodeList, csBankCodeList);
					sprintf(csBankCodeList, "%s%s%s", csTmpBankCodeList, PD_SQL_DELIMITOR, csBankCode);
				}
			} else {
DEBUGLOG(("process_bank_acct: int_bank_code is missing!!\n"));
ERRLOG("scan_bank_stmt: process_bank_acct: int_bank_code is missing!!\n");
                		iRet = PD_ERR;
				break;
			}
		}

		if ((iRet != PD_ERR) && (iTotalBankCnt > 0)) {
                
                        if (!GetField_Int(hData,"int_bank_code_exclude",&iBankExclude)) {
DEBUGLOG(("process_bank_acct: bank_exclude is missing!!\n"));
ERRLOG("scan_bank_stmt: process_bank_acct: bank_exclude is missing!!\n");
                                iRet = PD_ERR;				
			}
                }
	}
	
	// Put into the rBankAcctDetail
	if (iRet != PD_ERR) {
	
		// Loop All PIDs for input Provider
		// OL_PSP_MASTER (Provider ID, Provider Name, Status), 
		// OL_PSP_DETAIL (Provider ID, PSP ID, PSP Name, Bank Acct Type, Status),
		// OL_BANK_ACCT_ID (PSP ID, BAID, BAID Name, Internal Bank Acct Code, Bank Acct Number, Status, Category),
		// OL_BANK_ACCTS (Internal Bank Acct Code, Bank Acct Number, Bank Acct Name, Acct Type, Status Type)
		// OL_STMT_FORMAT_KEYWORDS (Internal Bank Code)	
	
		// Check PID (Status)
		// Check BAID (Status)
		// Check Bank Code (Exists)
		// Check Bank Acct (Status, Nature, System Switch Enabled)
		// Check Auto-Upload Option (Bank Level and Bank Acct Level)

		if (iTotalBankCnt > 0) {
			PutField_CString(hData, "int_bank_code_list", csBankCodeList);		
DEBUGLOG(("process_bank_acct: csBankCodeList = [%s]\n", csBankCodeList));
		}

       		DBObjPtr = CreateObj(DBPtr,"DBOLAutoUploadBankDetail","GetAutoUploadBankAccts");
        	iRet = (unsigned long)(DBObjPtr)(hData,rBankAcctDetail);
		if (iRet == PD_FOUND) {
DEBUGLOG(("process_bank_acct: DBOLAutoUploadBankDetail: GetAutoUploadBankAccts Found\n"));
	
			int	iBankAcctCnt = 0;
	
			hash_t 	*hBankAcctRec;

			hBankAcctRec = RecordSet_GetFirst(rBankAcctDetail);
        		while (hBankAcctRec) {

				int     iBankDisabled = PD_TRUE;
				int     iBankAcctDisabled = PD_TRUE;

	                        char    *csAcctType = NULL;
	                        char    *csProviderId = NULL;
	                        char    *csProviderName = NULL;
	                        char    *csBankCode = NULL;
	                        char    *csBankAbbrevName = NULL;
	                        char    *csBankAcctNum = NULL;

        			if (GetField_CString(hBankAcctRec,"acct_type",&csAcctType)) {

                                	GetField_CString(hBankAcctRec,"client_id",&csProviderId);
                                	GetField_CString(hBankAcctRec,"client_name",&csProviderName);
					GetField_CString(hBankAcctRec,"int_bank_code",&csBankCode);
					GetField_CString(hBankAcctRec,"bank_abbrev_name",&csBankAbbrevName);
					GetField_CString(hBankAcctRec,"bank_acct_num",&csBankAcctNum);
                                	GetField_Int(hBankAcctRec,"bank_disabled",&iBankDisabled);
                                	GetField_Int(hBankAcctRec,"bank_acct_disabled",&iBankAcctDisabled);

DEBUGLOG(("process_bank_acct: [%d]bank_acct_detail = [%s,%s,%s,%s,%s,%s,%d,%d]\n",iBankAcctCnt,csAcctType,csProviderId,csProviderName,csBankCode,csBankAbbrevName,csBankAcctNum,iBankDisabled,iBankAcctDisabled));
				}

				iBankAcctCnt++;

                        	hBankAcctRec = RecordSet_GetNext(rBankAcctDetail);
			}

DEBUGLOG(("process_bank_acct: bank_acct_cnt = [%d]\n",iBankAcctCnt));

        	} else if (iRet == PD_NOT_FOUND) {
DEBUGLOG(("process_bank_acct: DBOLAutoUploadBankDetail: GetAutoUploadBankAccts Not Found\n"));
DEBUGLOG(("process_bank_acct: bank_acct_cnt = [0]\n"));
		} else {
DEBUGLOG(("process_bank_acct: DBOLAutoUploadBankDetail: GetAutoUploadBankAccts Failed!!\n"));
ERRLOG("scan_bank_stmt: process_bank_acct: DBOLAutoUploadBankDetail: GetAutoUploadBankAccts Failed!!\n");
			iRet = PD_ERR;
        	}
	}

DEBUGLOG(("process_bank_acct: end: ----------------------------------------------------------------------------------------------\n"));

	return iRet;
}


int process_file(hash_t *hData,FILE *fraw,recordset_t *rRawBankStmt)
{
	int     iRet = PD_NOT_FOUND;
	int     iDtlRet = PD_OK;
        
	int	iAcctTypeIdx = IDX_ACCT_TYPE_DEPOSIT;
	int     iFieldCnt = 0;
        int     iStmtCnt = 0;
     	int     iStmtConvertCnt = 0;
        int     iStmtValidCnt = 0;
        int     iStmtSkipCnt = 0;
        int     iStmtAcceptCnt = 0;
	char 	*csAcctType = NULL;
       	char 	*csProviderId = NULL;
       	char 	*csProviderName = NULL;
	char    *csTmpPHDATE = NULL;
	char    *p;
	char    *csBankCode = strdup("");
        char    *csBankAcctNum = strdup("");
        char    csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN];
	char    csPHDate[PD_YYYYMMDD_LEN+1];
        char    cs_input_buf[PD_MAX_BUFFER+1];
        char    cs_bank_stmt_file[PD_MAX_BUFFER+1];
        char    cs_file_ext[IMPORT_MAX_FILE_LEN+1];

DEBUGLOG(("process_file: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

        memset(csPHDate,0,sizeof(csPHDate));
	memset(cs_input_buf, 0, sizeof(cs_input_buf));
	memset(cs_bank_stmt_file, 0, sizeof(cs_bank_stmt_file));
	memset(cs_file_ext, 0, sizeof(cs_file_ext));

/* acct_type */
       	if (GetField_CString(hData,"acct_type",&csAcctType)) {

		// Acct Type Index
                if (!strcmp(csAcctType, PD_NATURE_DEPOSIT)) {
                        iAcctTypeIdx = IDX_ACCT_TYPE_DEPOSIT;
                } else if (!strcmp(csAcctType, PD_NATURE_PAYOUT)) {
                        iAcctTypeIdx = IDX_ACCT_TYPE_PAYOUT;
                } else if (!strcmp(csAcctType, PD_NATURE_INTERMEDIATE)) {
                        iAcctTypeIdx = IDX_ACCT_TYPE_INTERMEDIATE;
                }	
	} else {
DEBUGLOG(("process_file: [%d]acct_type is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_file: acct_type is missing!!\n");
		iDtlRet = PD_ERR;
     	}

/* provider_id */
    	if (!GetField_CString(hData,"client_id",&csProviderId)) {
DEBUGLOG(("process_file: [%d]provider_id is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_file: provider_id is missing!!\n");
              	iDtlRet = PD_ERR;
    	}

/* provider_name */
        if (!GetField_CString(hData,"client_name",&csProviderName)) {
DEBUGLOG(("process_file: [%d]provider_name is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_file: provider_name is missing!!\n");
                iDtlRet = PD_ERR;
        }

/* PHDATE */
	if (GetField_CString(hData,"PHDATE",&csTmpPHDATE)) {		

		strncpy(csPHDate, csTmpPHDATE, PD_YYYYMMDD_LEN);
	} else {
DEBUGLOG(("process_file: [%d]PHDATE is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_file: PHDATE is missing!!\n");
         
	       iDtlRet = PD_ERR;
        }

        while ((iDtlRet == PD_OK) && (fgets(cs_input_buf,PD_MAX_BUFFER, fraw) != NULL)) {
		
		int     iStmtFileConvert = PD_FALSE;
		int     iStmtFileValid = PD_TRUE;
		int 	iStmtFileSkip = PD_FALSE;
		int     iStmtFileAccept = PD_FALSE;
		int 	iMsgCode = INT_NO_ERR; 

		char 	cStatus;

                iFieldCnt = 0;

		cStatus = PD_STMT_APPROVED;

		// Check 0x0A
                if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A) {
                        cs_input_buf[strlen(cs_input_buf) - 1] = '\0';
		}

		// Trim 0x0D
		strcpy(cs_input_buf,TrimAllChar((const unsigned char*)cs_input_buf,strlen(cs_input_buf),PD_CHAR));
DEBUGLOG(("process_file: [%d][ORG_INPUT_FILE]=[%s]\n",iStmtCnt,cs_input_buf));

		// Copy to cs_bank_stmt_file
		strcpy(cs_bank_stmt_file, cs_input_buf);

		// Verify Raw Stmt File Name Length
		if (iDtlRet == PD_OK) {

                        if (strlen(cs_bank_stmt_file) > IMPORT_MAX_FILE_LEN) {
				iStmtFileValid = PD_FALSE;
                                iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d][ORG_INPUT_FILE_LENGTH][%d] > Max Length [%d]\n",iStmtCnt,strlen(cs_bank_stmt_file),IMPORT_MAX_FILE_LEN));
                        }
		}

		// Trim File Extension
		if ((iDtlRet == PD_OK)
 		    && (iStmtFileSkip == PD_FALSE)
		) {
               		p = mystrtok(cs_input_buf,PD_EXT_DELIMITOR);
            		if (p == NULL) {
               			iDtlRet = PD_ERR;
              		} else {
               			while ((p = mystrtok(NULL,PD_EXT_DELIMITOR)) != NULL) {
                		    	strcpy(cs_file_ext,p);
               			}
DEBUGLOG(("process_file: [%d][INPUT_FILE]=[%s][%s]\n",iStmtCnt,cs_input_buf,cs_file_ext));
			}
		}

		// Verify Raw Stmt File Name
		if ((iDtlRet == PD_OK) 
		    && (iStmtFileSkip == PD_FALSE)
		) {
                	iStmtFileValid = check_filename(cs_input_buf);
			if (iStmtFileValid == PD_FALSE) {
				iMsgCode = INT_INVALID_FILE_NAME;
				cStatus = PD_STMT_DECLINED;		
			}
		}

		// Verify Raw Stmt File Name Field - General Details
		if ((iDtlRet == PD_OK) 
		    && (iStmtFileSkip == PD_FALSE)
		    && (iStmtFileValid == PD_TRUE)
		) {	
			// Breakdown Raw Stmt File Name
                       	p = mystrtok(cs_input_buf,PD_RAW_DELIMITOR);
                    	if (p == NULL) {
                             	iDtlRet = PD_ERR;
                    	} else {
                               	strcpy(csList[iFieldCnt],p);
                               	iFieldCnt++;

                  		while ((p = mystrtok(NULL,PD_RAW_DELIMITOR)) != NULL) {
					if (iFieldCnt < IMPORT_MAX_FIELD) {
                              			strcpy(csList[iFieldCnt],p);
					}
                               		iFieldCnt++;
                             	}
                      	}

                        // Check Number of Field Valid
                      	if (iDtlRet == PD_OK) {
				iStmtFileValid = check_detail(iAcctTypeIdx, iFieldCnt, csList);
				if (iStmtFileValid == PD_FALSE) {
                                	iMsgCode = INT_INVALID_FILE_NAME;
                                	cStatus = PD_STMT_DECLINED;
                        	}
			}
		}

		// Verify Raw Stmt File Name Field - [Bank Init][Bank Acct Num]
		if ((iDtlRet == PD_OK) 
		    && (iStmtFileSkip == PD_FALSE)
		    && (iStmtFileValid == PD_TRUE)
		) {
			int iTmpRet = FOUND;
	
			// Get Bank Code, Bank Acct Num and Provider
			if (iDtlRet == PD_OK) {

				hash_t  *hRec = NULL;
                        	hRec = (hash_t*) malloc (sizeof(hash_t));
                        	hash_init(hRec,0);

                                DBObjPtr = CreateObj(DBPtr,"DBOLBankAccts","GetBankAcctsDetailByShort");
                                iTmpRet = (unsigned long)(DBObjPtr)(csList[IDX_BANK_INIT],csList[IDX_BANK_ACCT_NUM],csAcctType,hRec);
                                if (iTmpRet == FOUND) {
//DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort Found\n", iStmtCnt));

					char *csTmp = NULL;
					char *csTmpBankCode = NULL;

					if ((iStmtFileSkip == PD_FALSE)
                                            && (iStmtFileValid == PD_TRUE)
                                        ) {
/* int_bank_code */
						if (GetField_CString(hRec,"int_bank_code",&csTmpBankCode)) {
//DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort int_bank_code = [%s]\n", iStmtCnt, csTmpBankCode));
                                        	} else {
							iStmtFileValid = PD_FALSE;
                                                	iMsgCode = INT_BANK_ACCT_NOT_FOUND;
                                               	 	cStatus = PD_STMT_DECLINED;
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort int_bank_code not found\n", iStmtCnt));
                                        	}
					}

					if ((iStmtFileSkip == PD_FALSE)
                    		 	    && (iStmtFileValid == PD_TRUE)
					) {
/* support_bank_code */
                                        	if (GetField_CString(hRec,"support_bank_code",&csTmp)) {
//DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort support_bank_code = [%s]\n", iStmtCnt, csTmp));

							if (strcmp(csTmp, csTmpBankCode)) {
                                                             	iStmtFileValid = PD_FALSE;
                                                              	iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Raw Bank Stmt File Bank [%s] is NOT equal to Support Bank [%s]\n",iStmtCnt, csTmpBankCode, csTmp));
                                                    	}
                                        	} else {
                                                	iStmtFileValid = PD_FALSE;
                                                	iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort support_bank_code not found\n", iStmtCnt));
                                        	}
					}

					if ((iStmtFileSkip == PD_FALSE)
                                            && (iStmtFileValid == PD_TRUE)
                                        ) {	
/* bank_acct_num */
	                                        if (GetField_CString(hRec,"bank_acct_num",&csBankAcctNum)) {
//DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort bank_acct_num = [%s]\n", iStmtCnt, csBankAcctNum));
        	                                } else {
                	                                iStmtFileValid = PD_FALSE;
                        	                        iMsgCode = INT_BANK_ACCT_NOT_FOUND;
                                	                cStatus = PD_STMT_DECLINED;
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort bank_acct_num not found\n", iStmtCnt));
                                        	}
					}

					if ((iStmtFileSkip == PD_FALSE)
                                            && (iStmtFileValid == PD_TRUE)
                                        ) {
/* provider_id */
                                        	if (GetField_CString(hRec,"client_id",&csTmp)) {
//DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort provider_id = [%s]\n", iStmtCnt, csTmp));

							if (strcmp(csTmp, csProviderId)) {
                	                                        iStmtFileValid = PD_FALSE;
                	                                        iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Raw Bank Stmt File Provider [%s] is NOT equal to Job Provider [%s]\n",iStmtCnt, csTmp, csProviderId));
                        	                        }
                                        	} else {
							iStmtFileValid = PD_FALSE;
							iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort client_id not found\n", iStmtCnt));
						}
					}		

					if ((iStmtFileSkip == PD_FALSE)
                                            && (iStmtFileValid == PD_TRUE)
                                        ) {
/* provider_name */
                                        	if (GetField_CString(hRec,"client_name",&csTmp)) {
//DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort provider_name = [%s]\n", iStmtCnt, csTmp));
                                        	} else {
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort client_name is missing\n", iStmtCnt));
						}
					}

                                } else if (iTmpRet == NOT_FOUND) {
					iStmtFileValid = PD_FALSE;
                                       	iMsgCode = INT_BANK_ACCT_NOT_FOUND;
                                       	cStatus = PD_STMT_DECLINED;
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort Not Found\n", iStmtCnt));
                                } else {
DEBUGLOG(("process_file: [%d]DBOLBankAccts: GetBankAcctsDetailByShort Failed!!\n", iStmtCnt));
ERRLOG("scan_bank_stmt: process_file: DBOLBankAccts: GetBankAcctsDetailByShort Failed!!\n");
                                        iDtlRet = PD_ERR;
                                }

				hash_destroy(hRec);
                        	FREE_ME(hRec);
                        }

			// Check Process Bank Name
			if ((iDtlRet == PD_OK) 
			    && (iStmtFileSkip == PD_FALSE)
			    && (iStmtFileValid == PD_TRUE)
			) 
			{
        			int     iTotalBankCnt = 0;

/* total_bank_cnt */
        			if (GetField_Int(hData,"total_bank_cnt",&iTotalBankCnt)) {
DEBUGLOG(("process_file: total_bank_cnt = [%d]\n",iTotalBankCnt));
        			} else {
//DEBUGLOG(("process_file: total_bank_cnt is missing!!\n"));
//ERRLOG("scan_bank_stmt: process_file: total_bank_cnt is missing!!\n");
                			//iDtlRet = PD_ERR;
        			}

        			if (iDtlRet == PD_OK) {

					int     i = 0;
					int     iBankExclude = 0;

					// If Process Bank is equal to ALL, skip!!
					if (iTotalBankCnt > 0) {
						
						if (!GetField_Int(hData,"int_bank_code_exclude",&iBankExclude)) {
DEBUGLOG(("process_file: [%d]bank_exclude is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_file: bank_exclude is missing!!\n");
                                        		iDtlRet = PD_ERR;
						}

						if (iDtlRet == PD_OK) {
                			
							for (i=0;i<iTotalBankCnt;i++)  {

                        					char *csBankShortName = NULL;
                        					char csTag[PD_TMP_BUF_LEN];

                        					sprintf(csTag, "%d_bank_short_name", i);
                        					if (GetField_CString(hData,csTag,&csBankShortName)) {
									
DEBUGLOG(("process_file: [%d]bank_short_name = [%s]\n",iStmtCnt,csBankShortName));
DEBUGLOG(("process_file: [%d]bank_exclude = [%d]\n",iStmtCnt,iBankExclude));

									/*
                                                                         * Check Prcoess Bank if Exclude
                                                                         * - Loop all process bannk
                                                                         *
                                                                         * E.g. 
                                                                         *
                                                                         *      Loop process bank list [ICBC_ABC_CCB], [ICBC]->[ABC]->[CCB]
                                                                         *
                                                                         *      Case 1:
                                                                         *      Process Bank            Exclude Mode    Bank in Statement File  Result
                                                                         *      ABC                     0               ABC                     match, not skip, break
                                                                         *      ICBC                    0               ABC                     not match, skip, continue
                                                                         *      CCB                     0               ABC                     not match, skip, continue
                                                                         *      ICBC_ABC_CCB            1               ABC                     match, skip, break
                                                                         *
                                                                         *      Case 2:
                                                                         *      Process Bank            Exclude Mode    Bank in Statement File  Result
                                                                         *      ABC                     0               CTB                     not match, skip, continue
                                                                         *      ICBC                    0               CTB                     not match, skip, continue
                                                                         *      CCB                     0               CTB                     not match, skip, continue
                                                                         *      ICBC_ABC_CCB            1               CTB                     not match, not skip, continue
                                                                         *
                                                                         */

									if (!strcmp(csBankShortName, csList[IDX_BANK_INIT])) {

                                                                                if (iBankExclude == PD_FALSE) {

                                                                                        // bank match, not skip, break the loop
                                                                                        iStmtFileSkip = PD_FALSE;
                                                                                } else {

                                                                                        // bank match, skip, break the loop
                                                                                        iStmtFileSkip = PD_TRUE;
                                                                                }

                                                                                break;

                                                                        } else if (strcmp(csBankShortName, csList[IDX_BANK_INIT])) {

                                                                                if (iBankExclude == PD_FALSE) {

                                                                                        // bank not match, skip, continue the loop
                                                                                        iStmtFileSkip = PD_TRUE;
                                                                                } else {

                                                                                        // bank not match, not Skip, continue the loop
                                                                                        iStmtFileSkip = PD_FALSE;
                                                                                }
                                                                        }
                        					} else {
DEBUGLOG(("process_file: bank_short_name is missing!!\n"));
ERRLOG("scan_bank_stmt: process_file: bank_short_name is missing!!\n");
                                					iDtlRet = PD_ERR;
                                					break;
                        					}
							}
						
							if (iStmtFileSkip == PD_TRUE) {
DEBUGLOG(("process_file: Process Bank is Excluded!!\n", iStmtCnt));
								iStmtFileValid = PD_FALSE;
							}							
						}
                			}
        			}
			}
		}
	
                // Verify Raw Stmt File Name Field - [Txn Date]
                if ((iDtlRet == PD_OK)
		    && (iStmtFileSkip == PD_FALSE)
                    && (iStmtFileValid == PD_TRUE)
		    && ((!strcmp(csAcctType, PD_NATURE_PAYOUT))
                        || (!strcmp(csAcctType, PD_NATURE_INTERMEDIATE)))
                ) {

			char    csTxnDate[PD_YYYYMMDD_LEN+1];

			memset(csTxnDate,0,sizeof(csTxnDate));

			// Txn Date
                     	if ((strlen(csList[IDX_TXN_DATE]) == PD_STMT_FILENAME_LEN_6_DIGIT)
			    || (strlen(csList[IDX_TXN_DATE]) == PD_STMT_FILENAME_LEN_10_DIGIT) 
			    || (strlen(csList[IDX_TXN_DATE]) == PD_STMT_FILENAME_LEN_11_DIGIT) 
			)
			{

                          	strncpy(csTxnDate, csList[IDX_TXN_DATE], PD_YYMMDD_LEN);
//DEBUGLOG(("process_file: [%d]PH Date = [%s], Txn Date = [%s]\n",iStmtCnt, csPHDate, csTxnDate));

				iStmtFileValid = is_numeric(csTxnDate);
				if (iStmtFileValid == PD_TRUE) {

	                            	// Skip Bank Statement File if Txn Date is greater than or equal to PH Date
	                             	if (strncmp(csTxnDate, &csPHDate[2], PD_YYMMDD_LEN) >= 0) {
	                                     	iStmtFileValid = PD_FALSE;
	                                      	iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Txn Date >= Current PH Date\n",iStmtCnt));
	                             	}
				} else {
                                      	iMsgCode = INT_INVALID_FILE_NAME;
                                     	cStatus = PD_STMT_DECLINED;
				}

			} else if (strlen(csList[IDX_TXN_DATE]) == PD_STMT_FILENAME_LEN_8_DIGIT) {

				char    csFirst2Digit[PD_MM_LEN+1];		

				memset(csFirst2Digit,0,sizeof(csFirst2Digit));

				strncpy(csFirst2Digit, csList[IDX_TXN_DATE], PD_MM_LEN);	
//DEBUGLOG(("process_file: [%d]Txn Date (first 2 digit) = [%s]\n",iStmtCnt, csFirst2Digit));

				if (check_is_month(csFirst2Digit) == PD_TRUE) {

					strncpy(csTxnDate, csList[IDX_TXN_DATE], PD_MMDD_LEN);
//DEBUGLOG(("process_file: [%d]PH Date = [%s], Txn Date = [%s]\n",iStmtCnt, csPHDate, csTxnDate));
				
					iStmtFileValid = is_numeric(csTxnDate);
                                        if (iStmtFileValid == PD_TRUE) {	

                                                // Skip Bank Statement File if Txn Date is greater than or equal to PH Date
                                                if (strncmp(csTxnDate, &csPHDate[4], PD_MMDD_LEN) >= 0) {
                                                        iStmtFileValid = PD_FALSE;
                                                        iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Txn Date >= Current PH Date\n",iStmtCnt));
                                                }
                                        } else {
                                                iMsgCode = INT_INVALID_FILE_NAME;
                                                cStatus = PD_STMT_DECLINED;
                                        }

				} else {

					strncpy(csTxnDate, csList[IDX_TXN_DATE], PD_YYMMDD_LEN);
//DEBUGLOG(("process_file: [%d]PH Date = [%s], Txn Date = [%s]\n",iStmtCnt, csPHDate, csTxnDate));

					iStmtFileValid = is_numeric(csTxnDate);
                                        if (iStmtFileValid == PD_TRUE) {

                                           	// Skip Bank Statement File if Txn Date is greater than or equal to PH Date
                                             	if (strncmp(csTxnDate, &csPHDate[2], PD_YYMMDD_LEN) >= 0) {
                                                      	iStmtFileValid = PD_FALSE;
                                                     	iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Txn Date >= Current PH Date\n",iStmtCnt));
                                             	}
                                     	} else {
                                             	iMsgCode = INT_INVALID_FILE_NAME;
                                              	cStatus = PD_STMT_DECLINED;
                                    	}
				}				
	
			} else if (strlen(csList[IDX_TXN_DATE]) == PD_STMT_FILENAME_LEN_12_DIGIT) {

				char    *csTmp = NULL;

				csTmp = strstr(csList[IDX_TXN_DATE], PD_DATE_RANGE_DELIMITOR);
                              	if (csTmp != NULL) {
		
					strncpy(csTxnDate, csList[IDX_TXN_DATE], PD_YYMMDD_LEN);
//DEBUGLOG(("process_file: [%d]PH Date = [%s], Txn Date = [%s]\n",iStmtCnt, csPHDate, csTxnDate));

					iStmtFileValid = is_numeric(csTxnDate);
                                        if (iStmtFileValid == PD_TRUE) {

						// Skip Bank Statement File if Txn Date is greater than or equal to PH Date
						if (strncmp(csTxnDate, &csPHDate[2], PD_YYMMDD_LEN) >= 0) {
	                                               	iStmtFileValid = PD_FALSE;
	                                                iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Txn Date >= Current PH Date\n",iStmtCnt));
	                                        }
					} else {
                                                iMsgCode = INT_INVALID_FILE_NAME;
                                               	cStatus = PD_STMT_DECLINED;				
					}
				} else {

					strncpy(csTxnDate, csList[IDX_TXN_DATE], PD_YYYYMMDD_LEN);
//DEBUGLOG(("process_file: [%d]PH Date = [%s], Txn Date = [%s]\n",iStmtCnt, csPHDate, csTxnDate));

					iStmtFileValid = is_numeric(csTxnDate);
	                            	if (iStmtFileValid == PD_TRUE) {

						// Skip Bank Statement File if Txn Date is greater than or equal to PH Date
						if (strncmp(csTxnDate, csPHDate, PD_YYYYMMDD_LEN) >= 0) {
	                                               	iStmtFileValid = PD_FALSE;
	                                             	iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]Txn Date >= Current PH Date\n",iStmtCnt));
        	                             	}
					} else {
                                                iMsgCode = INT_INVALID_FILE_NAME;
                                                cStatus = PD_STMT_DECLINED;
					}
				}

			} else if (strlen(csList[IDX_TXN_DATE]) == PD_STMT_FILENAME_LEN_13_DIGIT) {

				char    csFromTxnDate[PD_YYMMDD_LEN+1];
				char    csToTxnDate[PD_YYMMDD_LEN+1];

                        	memset(csFromTxnDate,0,sizeof(csFromTxnDate));
                        	memset(csToTxnDate,0,sizeof(csToTxnDate));

				strncpy(csFromTxnDate, csList[IDX_TXN_DATE], PD_YYMMDD_LEN);
				strncpy(csToTxnDate, &csList[IDX_TXN_DATE][PD_YYMMDD_LEN+1], PD_YYMMDD_LEN);

				// Check From Txn Date is digit or not
				if (iStmtFileValid == PD_TRUE) {
				
					iStmtFileValid = is_numeric(csFromTxnDate);
					if (iStmtFileValid == PD_FALSE) {
						iMsgCode = INT_INVALID_FILE_NAME;
                                	        cStatus = PD_STMT_DECLINED;
					}
				}

				// Check To Txn Date is digit or not
				if (iStmtFileValid == PD_TRUE) {

					iStmtFileValid = is_numeric(csToTxnDate);
					if (iStmtFileValid == PD_FALSE) {
                                 	       iMsgCode = INT_INVALID_FILE_NAME;
                                 	       cStatus = PD_STMT_DECLINED;
                                	}
				}

//DEBUGLOG(("process_file: [%d]PH Date = [%s], From Txn Date = [%s], To Txn Date = [%s]\n",iStmtCnt, csPHDate, csFromTxnDate, csToTxnDate));

				// Check From Txn Date and To Txn Date are valid or not
				if (iStmtFileValid == PD_TRUE) {

					if (strncmp(csFromTxnDate, csToTxnDate, PD_YYMMDD_LEN) > 0) {
						iStmtFileValid = PD_FALSE;
						iMsgCode = INT_INVALID_FILE_NAME;
                                                cStatus = PD_STMT_DECLINED;
					}
				}

				// Compare Current PH Date, From Txn Date and To Txn Date
				if (iStmtFileValid == PD_TRUE) {
 
					if ((strncmp(csFromTxnDate, csToTxnDate, PD_YYMMDD_LEN) == 0)
					   && (strncmp(&csPHDate[2], csFromTxnDate, PD_YYMMDD_LEN) > 0)
					) {
DEBUGLOG(("process_file: [%d]Current PH Date > (From Txn Date == To Txn Date)\n",iStmtCnt));		
					} else {

						// Skip Bank Statement File if From Txn Date is greater than or equal to PH Date
						if (iStmtFileValid == PD_TRUE) {
				
	               		               		if (strncmp(csFromTxnDate, &csPHDate[2], PD_YYMMDD_LEN) >= 0) {
								iStmtFileValid = PD_FALSE;
	                	                       		iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_file: [%d]From Txn Date >= Current PH Date\n",iStmtCnt));
	                	             		}
						}
		
						// Set Bank Statement File Convert
                                               	if (iStmtFileValid == PD_TRUE) {

							 if (strncmp(csToTxnDate, &csPHDate[2], PD_YYMMDD_LEN) >= 0) {
                                                	        iStmtFileConvert = PD_TRUE;
DEBUGLOG(("process_file: [%d]To Txn Date >= Current PH Date\n",iStmtCnt));
                                               	 	}
						}
					}
				}
                        } else {

				iStmtFileValid = PD_FALSE;
				iMsgCode = INT_INVALID_FILE_NAME;
                               	cStatus = PD_STMT_DECLINED;			
DEBUGLOG(("process_file: [%d]No Txn Date Format Matched\n",iStmtCnt));
			}
                }

		// Put into Raw Statement Recordset
		if ((iDtlRet == PD_OK) 
		    && (iStmtFileSkip == PD_FALSE)
		) {
			hash_t  *hRawStmtRec = NULL;
	             	hRawStmtRec = (hash_t*) malloc (sizeof(hash_t));
	             	hash_init(hRawStmtRec,0);

/* acct_type */
	              	PutField_CString(hRawStmtRec, "acct_type", csAcctType);
//DEBUGLOG(("process_file: [%d]acct_type = [%s]\n",iStmtCnt,csAcctType));

/* provider_id */
	            	PutField_CString(hRawStmtRec, "client_id", csProviderId);
//DEBUGLOG(("process_file: [%d]provider_id = [%s]\n",iStmtCnt,csProviderId));

/* provider_name */
	           	PutField_CString(hRawStmtRec, "client_name", csProviderName);
//DEBUGLOG(("process_file: [%d]provider_name = [%s]\n",iStmtCnt,csProviderName));

			if (iStmtFileValid == PD_TRUE) {	
				PutField_CString(hRawStmtRec, "bank_init", csList[IDX_BANK_INIT]);
                               	PutField_CString(hRawStmtRec, "bank_acct_init", csList[IDX_BANK_ACCT_INIT]);
                              	PutField_CString(hRawStmtRec, "last_6_bank_acct_num", csList[IDX_BANK_ACCT_NUM]);
                              	PutField_CString(hRawStmtRec, "datetime", csList[IDX_TXN_DATE]);
			}
		
			if (iStmtFileConvert == PD_TRUE) {
				PutField_CString(hRawStmtRec, "int_bank_code", csBankCode);
				PutField_CString(hRawStmtRec, "PHDATE", csPHDate);
			}					

                       	PutField_CString(hRawStmtRec, "file_ext", cs_file_ext);
                       	PutField_CString(hRawStmtRec, "bank_stmt_file", cs_bank_stmt_file);
			PutField_Char(hRawStmtRec, "status", cStatus);
                       	PutField_Int(hRawStmtRec, "error_code", iMsgCode);
			PutField_Int(hRawStmtRec, "stmt_file_valid", iStmtFileValid); 
			PutField_Int(hRawStmtRec, "stmt_file_convert", iStmtFileConvert); 
	
			iStmtFileAccept = PD_TRUE;	

			RecordSet_Add(rRawBankStmt, hRawStmtRec);
	
			//hash_destroy(hRawStmtRec);
        	       	//FREE_ME(hRawStmtRec);
                }	

                if (iDtlRet == PD_OK) {

                   	if (iStmtFileSkip == PD_TRUE) {
				iStmtSkipCnt++;
			}
			
                        if (iStmtFileAccept == PD_TRUE) {
                                iStmtAcceptCnt++;
                        }

			if (iStmtFileValid == PD_TRUE) {
                                iStmtValidCnt++;
                        }

			if (iStmtFileConvert == PD_TRUE) {
                                iStmtConvertCnt++;
                        }
                } 

DEBUGLOG(("process_file: [%d][stmt_file_skip][stmt_file_accept][stmt_file_valid][stmt_file_convert] = [%d][%d][%d][%d]\n",iStmtCnt,iStmtFileSkip,iStmtFileAccept,iStmtFileValid,iStmtFileConvert));

		// Clear input file buffer
		memset(cs_input_buf,0,sizeof(cs_input_buf));
		memset(cs_bank_stmt_file,0,sizeof(cs_bank_stmt_file));
		memset(cs_file_ext,0,sizeof(cs_file_ext));
		
                iStmtCnt++;
        }
	
	if (iDtlRet == PD_OK) {

		if (iStmtSkipCnt > 0) {
DEBUGLOG(("process_file: stmt_skip_cnt = [%d]\n",iStmtSkipCnt));
                } else {
DEBUGLOG(("process_file: stmt_skip_cnt = [0]\n"));
                }

		if (iStmtAcceptCnt > 0) {
			
			if (iStmtValidCnt > 0) {

				if (iStmtConvertCnt > 0) {
DEBUGLOG(("process_file: stmt_accept_cnt = [%d], stmt_valid_cnt = [%d], stmt_convert_cnt = [%d]\n",iStmtAcceptCnt, iStmtValidCnt, iStmtConvertCnt));
				} else {
DEBUGLOG(("process_file: stmt_accept_cnt = [%d], stmt_valid_cnt = [%d], stmt_convert_cnt = [0]\n",iStmtAcceptCnt, iStmtValidCnt));
				}
				
                	} else {
DEBUGLOG(("process_file: stmt_accept_cnt = [%d], stmt_valid_cnt = [0], stmt_convert_cnt = [0]\n",iStmtAcceptCnt));
                	}
			iRet = PD_FOUND;
		} else {
DEBUGLOG(("process_file: stmt_accept_cnt = [0]\n"));
			iRet = PD_NOT_FOUND;
		}

	} else {
		iRet = PD_ERR;
	}

DEBUGLOG(("process_file: end: ----------------------------------------------------------------------------------------------\n"));

	FREE_ME(csBankCode);
	FREE_ME(csBankAcctNum);

	return iRet;
}


int process_match(recordset_t *rBankAcctDetail,recordset_t *rRawBankStmt,recordset_t *rProcessBankStmt)
{	
	int iRet = PD_NOT_FOUND;
	int iDtlRet = PD_OK;

	int iBankAcctCnt = 0;
	int iMatchingCnt = 0;
	int iMatchCnt = 0;
	int iSkipCnt = 0;
	int iTmp = 0;

	char cTmp;

	char *csTmp = NULL;

        hash_t *hBankAcctRec = NULL;
	hash_t *hRawStmtRec = NULL;

	cTmp = ' ';

	recordset_t *rMatch;
        rMatch = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rMatch, 0);

DEBUGLOG(("process_match: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

	// Loop Bank Account Records
        hBankAcctRec = RecordSet_GetFirst(rBankAcctDetail);
        while ((iDtlRet == PD_OK) && (hBankAcctRec)) {

		int iRawCnt = 0;

		char csBankAbbrevName[PD_EXT_BANK_CODE_LEN + 1];
		char csBankAcctNum[PD_BANK_ACCT_NUM_LEN + 1];

		memset(csBankAbbrevName, 0, sizeof(csBankAbbrevName));
		memset(csBankAcctNum, 0, sizeof(csBankAcctNum));

/* bank_disabled */
		if (GetField_Int(hBankAcctRec,"bank_disabled",&iTmp)) {
//DEBUGLOG(("process_match: [bank_acct][%d]bank_disabled = [%d]\n",iBankAcctCnt,iTmp));
		} else {
//DEBUGLOG(("process_match: [bank_acct][%d]bank_disabled is missing!!\n",iBankAcctCnt));
		}                

/* bank_acct_disabled */
                if (GetField_Int(hBankAcctRec,"bank_acct_disabled",&iTmp)) {
//DEBUGLOG(("process_match: [bank_acct][%d]bank_acct_disabled = [%d]\n",iBankAcctCnt,iTmp));
                } else {
//DEBUGLOG(("process_match: [bank_acct][%d]bank_acct_disabled is missing!!\n",iBankAcctCnt));
                }

/* bank_abbrev_name */
		if (GetField_CString(hBankAcctRec,"bank_abbrev_name",&csTmp)) {
                        sprintf(csBankAbbrevName, "%s", csTmp);
//DEBUGLOG(("process_match: [bank_acct][%d]bank_abbrev_name = [%s]\n",iBankAcctCnt,csBankAbbrevName));
                } else {
DEBUGLOG(("process_match: [bank_acct][%d]bank_abbrev_name is missing!!\n",iBankAcctCnt));
ERRLOG("scan_bank_stmt: process_match: bank_abbrev_name is missing!!\n");
                        iDtlRet = PD_ERR;
                }

/* bank_acct_num */
                if (GetField_CString(hBankAcctRec,"bank_acct_num",&csTmp)) {
                        sprintf(csBankAcctNum, "%s", csTmp);
//DEBUGLOG(("process_match: [bank_acct][%d]bank_acct_num = [%s]\n",iBankAcctCnt,csBankAcctNum));
                } else {
DEBUGLOG(("process_match: [bank_acct][%d]bank_acct_num is missing!!\n",iBankAcctCnt));
ERRLOG("scan_bank_stmt: process_match: bank_acct_num is missing!!\n");
                        iDtlRet = PD_ERR;
                }

		// Loop Raw Bank Statement File Records
        	hRawStmtRec = RecordSet_GetFirst(rRawBankStmt);
        	while ((iDtlRet == PD_OK) && (hRawStmtRec)) {

			int iStmtFileValid = PD_FALSE;
	
			char csRawBankInit[IMPORT_MAX_FILE_LEN + 1];
			char csRawShortBankAcctNum[IMPORT_MAX_FILE_LEN + 1];

			memset(csRawBankInit, 0, sizeof(csRawBankInit));
			memset(csRawShortBankAcctNum, 0, sizeof(csRawShortBankAcctNum));

/* stmt_file_valid */
			if (GetField_Int(hRawStmtRec,"stmt_file_valid",&iStmtFileValid)) {
//DEBUGLOG(("process_match: [raw][%d]stmt_file_valid = [%d]\n",iRawCnt,iStmtFileValid));
                        } else {
DEBUGLOG(("process_match: [raw][%d]stmt_file_valid is missing!!\n",iRawCnt));
ERRLOG("scan_bank_stmt: process_match: stmt_file_valid is missing!!\n");
                                iDtlRet = PD_ERR;
                        }

                        if (iStmtFileValid == PD_TRUE) {

				/*
				 *
				 * If raw bank statement filename valid, 
				 * - compare bank account number between bank account records and raw bank statement file records.
				 * - put the matched records into Match Recordset.
				 *
				 */

/* bank_init */
				if (GetField_CString(hRawStmtRec,"bank_init",&csTmp)) {
        	                        sprintf(csRawBankInit, "%s", csTmp);
//DEBUGLOG(("process_match: [raw][%d]bank_init = [%s]\n",iRawCnt,csRawBankInit));
        	                } else {
DEBUGLOG(("process_match: [raw][%d]bank_init is missing!!\n",iRawCnt));
ERRLOG("scan_bank_stmt: process_match: bank_init is missing!!\n");
        	                        iDtlRet = PD_ERR;
        	                }

/* last_6_bank_acct_num */
        	        	if (GetField_CString(hRawStmtRec,"last_6_bank_acct_num",&csTmp)) {
		                        sprintf(csRawShortBankAcctNum, "%s", csTmp);
//DEBUGLOG(("process_match: [raw][%d]last_6_bank_acct_num = [%s]\n",iRawCnt,csRawShortBankAcctNum));
	        	        } else {
DEBUGLOG(("process_match: [raw][%d]last_6_bank_acct_num is missing!!\n",iRawCnt));
ERRLOG("scan_bank_stmt: process_match: last_6_bank_acct_num is missing!!\n");
					iDtlRet = PD_ERR;
	        	        }

				if (!strcmp(csBankAbbrevName, csRawBankInit)) {
					csTmp = strstr(csBankAcctNum, csRawShortBankAcctNum);
                        		if (csTmp != NULL) {	
	
						hash_t *hMatchRec = NULL;
        					hMatchRec = (hash_t*) malloc (sizeof(hash_t));
        					hash_init(hMatchRec,0);

/* bank_disabled */
                                                if (GetField_Int(hBankAcctRec,"bank_disabled",&iTmp)) {
                                                        PutField_Int(hMatchRec, "bank_disabled", iTmp);
                                                } else {
							PutField_Int(hMatchRec, "bank_disabled", PD_TRUE);
						}

/* bank_acct_disabled */
                                                if (GetField_Int(hBankAcctRec,"bank_acct_disabled",&iTmp)) {
                                                        PutField_Int(hMatchRec, "bank_acct_disabled", iTmp);
						} else {
							PutField_Int(hMatchRec, "bank_acct_disabled", PD_TRUE);
						}

/* bank_name */
                                                if (GetField_CString(hBankAcctRec,"bank_name",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_name", csTmp);
                                                }

/* bank_acct_num */
                                                if (GetField_CString(hBankAcctRec,"bank_acct_num",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_acct_num", csTmp);
                                                }

/* bank_acct_name */
                                                if (GetField_CString(hBankAcctRec,"bank_acct_name",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_acct_name", csTmp);
                                                }

/* bank_acct_short_name */
                                                if (GetField_CString(hBankAcctRec,"bank_acct_short_name",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_acct_short_name", csTmp);
                                                }

/* bank_stmt_file */
						if (GetField_CString(hRawStmtRec,"bank_stmt_file",&csTmp)) {
							PutField_CString(hMatchRec, "match_stmt_file", csTmp);
						}

	                	        	RecordSet_Add(rMatch, hMatchRec);

						//hash_destroy(hMatchRec);
                        	        	//FREE_ME(hMatchRec);
					}
				}
			} else {

				/*
                                 *
                                 * If raw bank statement filename invalid,
                                 * do nothing.
                                 *
                                 */
			}

			iRawCnt++;

                	hRawStmtRec = RecordSet_GetNext(rRawBankStmt);
        	}

		iBankAcctCnt++;

                hBankAcctRec = RecordSet_GetNext(rBankAcctDetail);
        }

	// Compare Raw Bank Statement File Records and Match Recordset Records 
	if (iDtlRet == PD_OK) {

		hash_t *hMatchRec = NULL;
