/* renamed_file */
	sprintf(csRenamedFile, "%s/%s_%s_%s_%s%s%s.%s", cs_raw_path, csBankInit, csBankAcctInit, csShortBankAcctNum, &csPHDate[2], PD_DATE_RANGE_DELIMITOR, csToTxnDate, csFileExt);
