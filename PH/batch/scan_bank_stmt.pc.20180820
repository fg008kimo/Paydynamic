                             		} else {
//DEBUGLOG(("process_match: [Match][%d]last_6_bank_acct_num is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: process_match: last_6_bank_acct_num is missing!!\n");
                                	     	//iDtlRet = PD_ERR;
                            		}

/* datetime */
                            		if (GetField_CString(hRawStmtRec,"datetime",&csTmp)) {
//DEBUGLOG(("process_match: [Match][%d][datetime] = [%s]\n",iMatchCnt,csTmp));
                                	       	PutField_CString(hProcessStmtRec, "datetime", csTmp);
                             		} else {
//DEBUGLOG(("process_match: [Match][%d]datetime is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: process_match: datetime is missing!!\n");
                                     		//iDtlRet = PD_ERR;
                              		}

/* file_ext */
                            		if (GetField_CString(hRawStmtRec,"file_ext",&csTmp)) {
//DEBUGLOG(("process_match: [Match][%d][file_ext] = [%s]\n",iMatchCnt,csTmp));
                                	    	PutField_CString(hProcessStmtRec, "file_ext", csTmp);
                             		} else {
//DEBUGLOG(("process_match: [Match][%d]file_ext is missing!!\n",iMatchCnt));
