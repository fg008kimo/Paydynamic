			iRet = PD_FOUND;
                } else {
DEBUGLOG(("process_match: match_cnt = [0]\n"));
			iRet = PD_NOT_FOUND;
                }
        
	} else {
		iRet = PD_ERR;
	}

DEBUGLOG(("process_match: end: ----------------------------------------------------------------------------------------------\n"));

	return iRet;
}


int process_convert(recordset_t *rProcessBankStmt, recordset_t *rConvertBankStmt)
{
        int iRet = PD_NOT_FOUND;
	int iDtlRet = PD_OK;

	int iProcessCnt = 0;
        int iConvertCnt = 0;
	int iStmtFileConvert = PD_FALSE;

	char *csBankStmtFile = NULL;

        hash_t *hProcessStmtRec = NULL;

DEBUGLOG(("process_convert: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

        // Loop Process Stmt Records
        hProcessStmtRec = RecordSet_GetFirst(rProcessBankStmt);
        while ((iDtlRet == PD_OK) && (hProcessStmtRec)) {

		hash_t  *hConvertStmtRec = NULL;
        	hConvertStmtRec = (hash_t*) malloc (sizeof(hash_t));
        	hash_init(hConvertStmtRec,0);

DEBUGLOG(("process_convert: [%d]: ====================================================\n", iProcessCnt));

/* bank_stmt_file */
                if (GetField_CString(hProcessStmtRec,"bank_stmt_file",&csBankStmtFile)) {
DEBUGLOG(("process_convert: [%d][bank_stmt_file]=[%s]\n", iProcessCnt, csBankStmtFile));
			PutField_CString(hConvertStmtRec, "bank_stmt_file", csBankStmtFile);
                } else {
DEBUGLOG(("process_convert: bank_stmt_file is missing!!\n"));
ERRLOG("scan_bank_stmt: process_convert: bank_stmt_file is missing!!\n");
                        iDtlRet = PD_ERR;
                }

/* stmt_file_convert */
                if (GetField_Int(hProcessStmtRec,"stmt_file_convert",&iStmtFileConvert)) {
DEBUGLOG(("process_convert: [%d][stmt_file_convert]=[%d]\n", iProcessCnt, iStmtFileConvert));
			PutField_Int(hConvertStmtRec, "stmt_file_convert", iStmtFileConvert);
                } else {
DEBUGLOG(("process_convert: stmt_file_convert is missing!!\n"));
ERRLOG("scan_bank_stmt: process_convert: stmt_file_convert is missing!!\n");
                        iDtlRet = PD_ERR;
                }		

		if (iDtlRet == PD_OK) {

			if (iStmtFileConvert == PD_TRUE) {

				int i = 0;
				int iTotalSplitCnt = 0;

				iDtlRet = convert_stmt_file(hProcessStmtRec);				
				if (iDtlRet == PD_OK) {

/* total_split_cnt */
	                		if (GetField_Int(hProcessStmtRec,"total_split_cnt",&iTotalSplitCnt)) {
DEBUGLOG(("process_convert: [%d][total_split_cnt]=[%d]\n", iProcessCnt, iTotalSplitCnt));

						PutField_Int(hConvertStmtRec, "total_split_cnt", iTotalSplitCnt);

						for (i=0;i<iTotalSplitCnt;i++)  {

							char *csTmp = NULL;
							char csTag[PD_TMP_BUF_LEN];

/* split_stmt_file */
							sprintf(csTag, "%d_split_stmt_file", i);
			                                if (GetField_CString(hProcessStmtRec,csTag,&csTmp)) {
DEBUGLOG(("process_convert: [%d][%s]=[%s]\n", iProcessCnt, csTag, csTmp));
	
								PutField_CString(hConvertStmtRec, csTag, csTmp);
						
							} else {
DEBUGLOG(("process_convert: split_stmt_file is missing!!\n"));
ERRLOG("scan_bank_stmt: process_convert: split_stmt_file is missing!!\n");
                	                        		iDtlRet = PD_ERR;
							}
						}

						RecordSet_Add(rConvertBankStmt, hConvertStmtRec);
                			} else {
DEBUGLOG(("process_convert: total_split_cnt is missing!!\n"));
ERRLOG("scan_bank_stmt: process_convert: total_split_cnt is missing!!\n");
                        			iDtlRet = PD_ERR;
                			}				

					iConvertCnt++;	
				}
			} else {

				RecordSet_Add(rConvertBankStmt, hConvertStmtRec);	
			}
		}

		iProcessCnt++;

        	//hash_destroy(hConvertStmtRec);
        	//FREE_ME(hConvertStmtRec);

                hProcessStmtRec = RecordSet_GetNext(rProcessBankStmt);
        }

	if (iDtlRet == PD_OK) {

		if (iProcessCnt > 0) {		
	
                	if (iConvertCnt > 0) {
DEBUGLOG(("process_convert: process_cnt = [%d], convert_cnt = [%d]\n", iProcessCnt, iConvertCnt));
                	        iRet = PD_FOUND;
                	} else {
DEBUGLOG(("process_convert: process_cnt = [%d], convert_cnt = [0]\n", iProcessCnt));
                	        iRet = PD_NOT_FOUND;;
                	}
		} else {
DEBUGLOG(("process_convert: process_cnt = [0], convert_cnt = [0]\n"));
                        iRet = PD_ERR;
		}

        } else {
                iRet = PD_ERR;
        }	

DEBUGLOG(("process_convert: end: ----------------------------------------------------------------------------------------------\n"));

        return iRet;
}


