<!-- @@TITLE@@ -->
<h1>@@tpl_Title@@</h1>
<!-- @@TITLE@@ -->

<!-- @@TABLE_START@@ -->
<table class="@@TPL_NUM@@">
<!-- @@TABLE_START@@ -->

        <!-- @@TIMESTAMP@@ -->
        <tr>
                <th>Alert Time:</th>
                <td colspan="@@TIMESTAMP_NUM_COLUMNS@@">@@tpl_Timestamp@@</td>
        </tr>
        <!-- @@TIMESTAMP@@ -->

        <!-- @@DOMAIN@@ -->
        <tr>
                <th>Domain:</th>
                <td colspan="@@DOMAIN_NUM_COLUMNS@@">@@tpl_Domain@@</td>
        </tr>
        <!-- @@DOMAIN@@ -->

        <!-- @@RATE@@ -->
        <tr>
                <th>Rate:</th>
                <td colspan="@@RATE_NUM_COLUMNS@@" rowspan"1" font color="@@RATE_FONT_COLOR@@">@@tpl_Rate_Type@@</td>
                <td>@@tpl_Rate@@</td>
        </tr>
        <!-- @@RATE@@ -->

        <!-- @@THRESHOLD_RATE@@ -->
        <tr>
                <th></th>
                <td colspan="@@THRESHOLD_RATE_NUM_COLUMNS@@" font color="@@THRESHOLD_RATE_FONT_COLOR@@">@@tpl_Threshold_Rate_Type@@</td>
                <td>@@tpl_Threshold_Rate@@</td>
        </tr>
        <!-- @@THRESHOLD_RATE@@ -->

        <!-- @@STATUS_ROW@@ -->
        <tr>
                <th>Status[@@tpl_Status_Num@@]:</th>
                <td>@@tpl_Status@@</td>
                <td>@@tpl_Sub_Status@@</td>
                <td>@@tpl_Txn_Cnt@@</td>
        </tr>
        <!-- @@STATUS_ROW@@ -->

        <!-- @@TOTAL_TXN_CNT@@ -->
        <tr>
                <th colspan="@@TOTAL_TXN_CNT_NUM_COLUMNS@@">Total Transaction Count:</th>
                <td>@@tpl_Total_Txn_Cnt@@</td>
        </tr>
        <!-- @@TOTAL_TXN_CNT@@ -->

        <!-- @@DURATION@@ -->
        <tr>
                <th>Duration:</th>
                <td colspan="@@DURATION_NUM_COLUMNS@@">@@tpl_Duration@@</td>
        </tr>
        <!-- @@DURATION@@ -->

        <!-- @@ALERT@@ -->
        <tr>
                <th>@@tpl_Alert@@</th>
                <td colspan="@@ALERT_NUM_COLUMNS@@">@@tpl_Alert_Domain@@</td>
        </tr>
        <!-- @@ALERT@@ -->

<!-- @@TABLE_END@@ -->
</table>
<!-- @@TABLE_END@@ -->
