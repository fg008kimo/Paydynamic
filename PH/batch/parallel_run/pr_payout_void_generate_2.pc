//DEBUGLOG(("batch_proc: [%d] merch_ref = [%.*s]\n",iCnt, v_merch_ref.len, v_merch_ref.arr));
                                PutField_CString(myHash, "merch_ref", (const char *) v_merch_ref.arr);
                        }

                        if (ind_txn_type >=0) {
