ERRLOG("upload_bank_stmt: prcoess_upload: Successful Path Not Exists\n");
						iRet = FAILURE;
                               		} else {
                                	 	closedir(dbankstmt);
                               		}
			
					// Check file exists
					if (iRet == SUCCESS) {
                         		       sprintf(csNewFullName, "%s/%s", csNewFullPath, csFileName);
DEBUGLOG(("prcoess_upload: [%d]Destination: csNewFullName = [%s]\n", iRecCnt, csNewFullName));

/*
                                		fbankstmt = fopen(csNewFullName,"r");
                                		if (fbankstmt != NULL) {
DEBUGLOG(("prcoess_upload: [%d]Destination: File Exists\n", iRecCnt));
                                		        fclose(fbankstmt);
							iMsgCode = INT_FILE_NAME_ALREADY_EXIST;
							cStatus = PD_STMT_DECLINED;
                                		}
*/
					}
				} else {
	
					// Check directory exists
        	                        sprintf(csNewFullPath, "%s/%s", cs_stmt_path, PD_PATH_UNSUCCESSFUL);
DEBUGLOG(("prcoess_upload: [%d]Destination: csNewFullPath = [%s]\n", iRecCnt, csNewFullPath));

        	                        dbankstmt = opendir(csNewFullPath);
        	                        if (dbankstmt == NULL) {
DEBUGLOG(("prcoess_upload: [%d]Destination: Unsuccessful Path Not Exists\n", iRecCnt));
ERRLOG("upload_bank_stmt: prcoess_upload: Unsuccessful Path Not Exists\n");
        	                                iRet = FAILURE;
        	                        } else {
        	                                closedir(dbankstmt);
        	                        }

                   	             	// Change file name
                   	             	if (iRet == SUCCESS) {

                   	     			char csDateTime[PD_DATETIME_LEN + 1] = "";
                        			time_t  tNow = time(NULL);
                        			struct  tm tStruct = *localtime(&tNow);

                        			strftime(csDateTime, sizeof(csDateTime), PD_DEFAULT_DATETIME_FORMAT, &tStruct);
                			
						if (iNoFileExt == PD_TRUE) {
                                       			sprintf(csNewFileName, "%s_%s", csBankStmtFile, csDateTime);
						} else {
							sprintf(csNewFileName, "%s_%s.%s", csBankStmtFile, csDateTime, csFileExt);
						}
                                       		sprintf(csNewFullName, "%s/%s", csNewFullPath, csNewFileName);
DEBUGLOG(("prcoess_upload: [%d]Destination: csNewFullName = [%s]\n", iRecCnt, csNewFullName));
                                	}
				}
			}

			// Move file from Source Path to Destination Path
			if (iRet == SUCCESS) {

				rename(csInFullName, csNewFullName);	
DEBUGLOG(("prcoess_upload: [%d]Move Statement File: From[%s] To[%s]\n", iRecCnt, csInFullName, csNewFullName));
			}
		}
	
		// Update OL_Statement_Header (Error Cases) 
		if ((iRet == SUCCESS) && (iMsgCode != INT_NO_ERR) && (iBankStmtFileRenamed == PD_TRUE)) {

			PutField_CString(hContext, "renamed_filename", csNewFileName);

			DBObjPtr = CreateObj(DBPtr, "DBOLStatement", "UpdateHeaderRenamedFilename");
                     	if ((unsigned long)(*DBObjPtr)(hContext) == PD_OK) {
DEBUGLOG(("process_upload: [%d]Call DBOLStatement: UpdateHeaderRenamedFilename() Success!!!\n", iRecCnt));
//DEBUGLOG(("process_upload: [%d]Call DBOLStatement: UpdateHeaderRenamedFilename(): TxnCommit()\n", iRecCnt));
                               	//TxnCommit();	
			} else {
DEBUGLOG(("process_upload: [%d]Call DBOLStatement: UpdateHeaderRenamedFilename() Failure!!!\n", iRecCnt));
ERRLOG("upload_bank_stmt: process_upload: Call DBOLStatement: UpdateHeaderRenamedFilename Failure!!!\n");
                             	iRet = FAILURE;
                       	}
		}

		// Add to Auto Upload Err Log 
                if ((iRet == SUCCESS) && (iMsgCode != INT_NO_ERR)) {

			hash_t *hStmtRec = NULL;
                        hStmtRec = (hash_t*) malloc (sizeof(hash_t));
                        hash_init(hStmtRec,0);

/*
                        PutField_CString(hStmtRec, "acct_type", csAcctType);
                        PutField_CString(hStmtRec, "client_id", csClientId);
			PutField_CString(hStmtRec, "bank_stmt_file", csBankStmtFile);

			if (GetField_CString(hUploadStmtRec,"int_bank_code",&csTmp)) {
                                PutField_CString(hStmtRec, "int_bank_code", csTmp);
                        }
			if (GetField_CString(hUploadStmtRec,"bank_init",&csTmp)) {
                                PutField_CString(hStmtRec, "bank_init", csTmp);
                        }
                        if (GetField_CString(hUploadStmtRec,"bank_acct_init",&csTmp)) {
                                PutField_CString(hStmtRec, "bank_acct_init", csTmp);
                        }
                        if (GetField_CString(hUploadStmtRec,"last_6_bank_acct_num",&csTmp)) {
                                PutField_CString(hStmtRec, "last_6_bank_acct_num", csTmp);
                        }	
			if (GetField_CString(hUploadStmtRec,"datetime",&csTmp)) {
                                PutField_CString(hStmtRec, "datetime", csTmp);
                        }
                        if (iNoFileExt == PD_FALSE) {
                                PutField_CString(hStmtRec, "file_ext", csFileExt);
                        }
*/

			PutField_CString(hStmtRec, "create_user", PD_UPDATE_USER);
                        PutField_Int(hStmtRec, "job_seq", int_job_seq);
                        PutField_CString(hStmtRec, "provider_name", csClientName);
			PutField_CString(hStmtRec, "stmt_filename", csFileName);
			//PutField_CString(hStmtRec, "stmt_filename", csNewFileName);
                        PutField_Char(hStmtRec, "status", cStatus);
                        PutField_Int(hStmtRec, "err_code", iMsgCode);

			if (GetField_CString(hUploadStmtRec,"bank_acct_num",&csTmp)) {
	               		PutField_CString(hStmtRec, "bank_acct_num", csTmp);
			}
			if (GetField_CString(hUploadStmtRec,"bank_name",&csTmp)) {
                                PutField_CString(hStmtRec, "bank_name", csTmp);
                        }
                        if (GetField_CString(hUploadStmtRec,"bank_acct_name",&csTmp)) {
				PutField_CString(hStmtRec, "bank_acct_name", csTmp);
			}
                        if (GetField_CString(hUploadStmtRec,"bank_acct_short_name",&csTmp)) {
				PutField_CString(hStmtRec, "bank_acct_short_name", csTmp);
			}

                        if (iMsgCode != INT_NO_ERR) {

				DBObjPtr = CreateObj(DBPtr,"DBOLAutoUploadErrLog","Add");
                		if ((unsigned long)(DBObjPtr)(hStmtRec) == PD_OK) {
DEBUGLOG(("process_upload: [%d]DBOLAutoUploadErrLog: Add Success!!\n", iRecCnt));
//DEBUGLOG(("process_upload: [%d]DBOLAutoUploadErrLog: TxnCommit()\n", iRecCnt));
                        		//TxnCommit();
                		} else { 
DEBUGLOG(("process_upload: [%d]DBOLAutoUploadErrLog: Add Failed!!\n", iRecCnt));
ERRLOG("upload_bank_stmt: process_upload: DBOLAutoUploadErrLog: Add Failed!!\n");
                        		iRet = FAILURE;
                		}
                        }

                        hash_destroy(hStmtRec);
                        FREE_ME(hStmtRec);
		}

		// Call Txn Commit
           	if (iRet == SUCCESS) {
                    	TxnCommit();
DEBUGLOG(("prcoess_upload: [%d]TxnCommit()\n", iRecCnt));
              	} else {
			TxnAbort();
DEBUGLOG(("prcoess_upload: [%d]TxnAbort()\n", iRecCnt));
		}
	
		iRecCnt++;

                hUploadStmtRec = RecordSet_GetNext(rUploadBankStmt);

		FREE_ME(hRequest);
        	FREE_ME(hContext);
        	FREE_ME(hResponse);
	}	

	return iRet;
}

