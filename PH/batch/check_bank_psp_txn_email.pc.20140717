// <-- Elvis 2014071 --
int find_checking_parameters_with_bankcode(tpl_t* tpl)
{
	int iFailureCnt = 0;
	int iRet = SUCCESS;
	
	EXEC SQL WHENEVER SQLERROR GOTO getpsp_error;
	EXEC SQL WHENEVER NOTFOUND CONTINUE;

	EXEC SQL BEGIN DECLARE SECTION;
		varchar		v_bank_code[PD_BANK_CODE_LEN+1];

		short		ind_bank_code;
	EXEC SQL END DECLARE SECTION;

	EXEC SQL DECLARE c_cursor_getbank CURSOR FOR
		SELECT	BC1.BC_BANK_CODE
		FROM	BANK_TXN_CHECK BC1
		WHERE	BC1.BC_DISABLED = 0
		GROUP BY BC1.BC_BANK_CODE
		ORDER BY BC1.BC_BANK_CODE;

	EXEC SQL OPEN c_cursor_getbank;
	for(;;){
		EXEC SQL FETCH c_cursor_getbank
		INTO
			:v_bank_code:ind_bank_code;

		if (SQLCODE == SQL_NOT_FOUND) {
//printf("find_checking_para_with_bankcode: Error(SQL Not Found)!! \n");
			break;
		}

		if (ind_bank_code >= 0) {
			sprintf(cs_bank_code,"%.*s",v_bank_code.len,v_bank_code.arr);
		
			iFailureCnt += find_checking_parameters_with_psp_id(cs_bank_code, tpl);

//printf("find_checking_para_with_bankcode: bankcode = %s, iFailureCnt = % d \n", cs_bank_code, iFailureCnt);
		}
	}
	if (iFailureCnt > 0)
	{
		iRet = FAILURE;
	}
	EXEC SQL CLOSE c_cursor_getbank;

//printf("find_checking_para_with_bankcode: iRet = %d, SUCCESS = % d, FAILURE = % d \n", iRet, SUCCESS, FAILURE);
	return iRet;

getpsp_error:
DEBUGLOG(("getpsp_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
	EXEC SQL WHENEVER SQLERROR CONTINUE;
	EXEC SQL CLOSE c_cursor_getbank;
	return PD_ERR;	
}	
