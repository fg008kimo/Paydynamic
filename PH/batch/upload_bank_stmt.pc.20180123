                } else {
DEBUGLOG(("prcoess_upload: file_ext is missing!!\n"));
ERRLOG("upload_bank_stmt: prcoess_upload: file_ext is missing!!\n");
                        iRet = FAILURE;
                }

/* bank_country */
                if (GetField_CString(hUploadStmtRec,"bank_country",&csBankCountry)) {
