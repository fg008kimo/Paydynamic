int batch_init(int argc, char* argv[])
{
    if (argc < 8) {
    	printf("usage: -t process_path -l upload_path -o process_file -s succ_file -u unsucc_file -e error_file -a acct_type -p provider_name\n");
        return FAILURE;
    }
    else
        return SUCCESS;
}


int batch_proc(int argc, char* argv[])
{
	int	iRet = SUCCESS;

	char	csAcctType[PD_ACCT_TYPE_LEN + 1];
	char 	csProviderId[PD_CLIENT_ID_LEN + 1];
	char	csBankCode[PD_BANK_CODE_LEN + 1];

	FILE	*fproc,*fsucc,*funsucc,*ferr;	

	hash_t  *hData = NULL;
        hData = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hData,0);

        recordset_t *rBankAcctDetail;
        rBankAcctDetail = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rBankAcctDetail, 0);

      	recordset_t *rProcessBankStmt;
        rProcessBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rProcessBankStmt, 0);

	recordset_t *rUploadBankStmt;
        rUploadBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rUploadBankStmt, 0);

	recordset_t *rSuccBankStmt;
        rSuccBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rSuccBankStmt, 0);

	recordset_t *rUnsuccBankStmt;
        rUnsuccBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rUnsuccBankStmt, 0);

        recordset_t *rErrorBankStmt;
        rErrorBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rErrorBankStmt, 0);

DEBUGLOG(("batch_proc: start\n"));

	//memset(cs_process_path, 0, sizeof(cs_process_path));
	//memset(cs_upload_path, 0, sizeof(cs_upload_path));
	//memset(cs_process_file, 0, sizeof(cs_process_file));
	//memset(cs_succ_file, 0, sizeof(cs_succ_file));
	//memset(cs_unsucc_file, 0, sizeof(cs_unsucc_file));
	//memset(cs_error_file, 0, sizeof(cs_error_file));
	//memset(cs_acct_type_name, 0, sizeof(cs_acct_type_name));
	//memset(cs_provider_name, 0, sizeof(cs_provider_name));
	//memset(cs_bank_short_name, 0, sizeof(cs_bank_short_name));

	memset(csAcctType, 0, sizeof(csAcctType));
	memset(csProviderId, 0, sizeof(csProviderId));
	memset(csBankCode, 0, sizeof(csBankCode));

	iRet = parse_arg(argc,argv);

	if (iRet != SUCCESS) {
    		printf("*usage: -t process_path -l upload_path -o process_file -s succ_file -u unsucc_file -e error_file -a acct_type -p provider_name\n");
		return (iRet);
	}

DEBUGLOG(("batch_proc: process_path = [%s]\n",cs_process_path));
DEBUGLOG(("batch_proc: upload_path = [%s]\n",cs_upload_path));
DEBUGLOG(("batch_proc: process_file = [%s]\n",cs_process_file));
DEBUGLOG(("batch_proc: succ_file = [%s]\n",cs_succ_file));
DEBUGLOG(("batch_proc: unsucc_file = [%s]\n",cs_unsucc_file));
DEBUGLOG(("batch_proc: error_file = [%s]\n",cs_error_file));
DEBUGLOG(("batch_proc: acct_type_name = [%s]\n",cs_acct_type_name));
DEBUGLOG(("batch_proc: provider_name = [%s]\n",cs_provider_name));
DEBUGLOG(("batch_proc: bank_short_name = [%s]\n",cs_bank_short_name));

	fproc = fopen(cs_process_file,"r");
	if (fproc == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_process_file));
		return FAILURE;
	}

	fsucc = fopen(cs_succ_file,"w");
        if (fsucc == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_succ_file));
                fclose(fproc);
                return FAILURE;
        }

	funsucc = fopen(cs_unsucc_file,"w");
        if (funsucc == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_unsucc_file));
                fclose(fproc);
                fclose(fsucc);
                return FAILURE;
        }

	ferr = fopen(cs_error_file,"a");
        if (ferr == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_error_file));
                fclose(fproc);
                fclose(fsucc);
                fclose(funsucc);
                return FAILURE;
        }

	// Get Acct Type
	if (iRet == SUCCESS) {
		if (!strcmp(cs_acct_type_name, "Deposit_Statement")) {
			sprintf(csAcctType, "%s", PD_NATURE_DEPOSIT);	
			PutField_CString(hData, "acct_type", csAcctType);
DEBUGLOG(("batch_proc: acct_type = [%s]\n", csAcctType));
		} else if (!strcmp(cs_acct_type_name, "Payout_Statement")) {
			sprintf(csAcctType, "%s", PD_NATURE_PAYOUT);	
			PutField_CString(hData, "acct_type", csAcctType);
DEBUGLOG(("batch_proc: acct_type = [%s]\n", csAcctType));
		} else if (!strcmp(cs_acct_type_name, "Intermediate_Statement")) {
			sprintf(csAcctType, "%s", PD_NATURE_INTERMEDIATE);	
			PutField_CString(hData, "acct_type", csAcctType);
DEBUGLOG(("batch_proc: acct_type = [%s]\n", csAcctType));
		} else {
DEBUGLOG(("batch_proc: acct_type invalid\n"));
			iRet = FAILURE;
		}
	}

        // Get Provider Id
        if (iRet == SUCCESS) {
                DBObjPtr = CreateObj(DBPtr,"DBPspMaster","GetClientIdByClientName");
                if ((unsigned long)(DBObjPtr)(cs_provider_name,csProviderId) == PD_OK) {
DEBUGLOG(("batch_proc: DBPspMaster: GetClientIdByClientName: client_id = [%s]\n", csProviderId));
			PutField_CString(hData, "client_id", csProviderId);
			PutField_CString(hData, "client_name", cs_provider_name);
                } else {
DEBUGLOG(("batch_proc: DBPspMaster: GetClientIdByClientName Failed\n"));
ERRLOG("upload_bank_stmt: batch_proc: DBPspMaster: GetClientIdByClientName Failed\n");
                        iRet = FAILURE;
                }
        }

	// Get Bank Code
	if (iRet == SUCCESS) {
		if (strlen(cs_bank_short_name) > 0) {
			DBObjPtr = CreateObj(DBPtr,"DBBankDesc","FindBankCodeByBankAbbrevName");
                	if ((unsigned long)(DBObjPtr)(cs_bank_short_name,csBankCode) == FOUND) {
				PutField_CString(hData, "int_bank_code", csBankCode);
DEBUGLOG(("batch_proc: DBBankDesc: FindBankCodeByBankAbbrevName: int_bank_code = [%s]\n", csBankCode));
                	} else {
DEBUGLOG(("batch_proc: DBBankDesc: FindBankCodeByBankAbbrevName Failed\n"));
ERRLOG("upload_bank_stmt: batch_proc: DBBankDesc: FindBankCodeByBankAbbrevName Failed\n");
                	        iRet = FAILURE;
                	}
		}
	}
	
	// Process Bank Acct
	if (iRet == SUCCESS) {
		iRet = process_bank_acct(hData,rBankAcctDetail);
	}

	// Process File
        if (iRet == SUCCESS) {
                iRet = process_file(hData,fproc,rProcessBankStmt);
        }

	// Process Matching
	if (iRet == SUCCESS) {
		iRet = prcoess_match(rBankAcctDetail,rProcessBankStmt,rUploadBankStmt,rErrorBankStmt);
	}

	// Process Upload Stmt
	if (iRet == SUCCESS) {
		iRet = process_upload(rUploadBankStmt,rSuccBankStmt,rUnsuccBankStmt,rErrorBankStmt);
	}

	// Process Results
	if (iRet == SUCCESS) {

		// Success File
		iRet = process_success_file(rSuccBankStmt,fsucc);
		
		// Unsuccess File
		if (iRet == SUCCESS) {		
			iRet = process_unsuccess_file(rUnsuccBankStmt,funsucc);
		}
		
		// Error File
		if (iRet == SUCCESS) {
			iRet = process_error_file(rErrorBankStmt,ferr);
		}
	}

	hash_destroy(hData);
        FREE_ME(hData);	

	RecordSet_Destroy(rBankAcctDetail);
        FREE_ME(rBankAcctDetail);

	RecordSet_Destroy(rProcessBankStmt);
	FREE_ME(rProcessBankStmt);

	RecordSet_Destroy(rUploadBankStmt);
        FREE_ME(rUploadBankStmt);

	RecordSet_Destroy(rSuccBankStmt);
        FREE_ME(rSuccBankStmt);

	RecordSet_Destroy(rUnsuccBankStmt);
        FREE_ME(rUnsuccBankStmt);

	RecordSet_Destroy(rErrorBankStmt);
        FREE_ME(rErrorBankStmt);

	fclose(fproc);
	fclose(fsucc);
	fclose(funsucc);
	fclose(ferr);

DEBUGLOG(("batch_proc: iRet = [%d]\n", iRet));
	return iRet;
}


int batch_terminate(int argc, char* argv[])
{
	return SUCCESS;
}

                        
int parse_arg(int argc,char **argv)
{               
        char    c;
        strcpy(cs_process_path,"");
        strcpy(cs_upload_path,"");
        strcpy(cs_process_file,"");
	strcpy(cs_succ_file,"");
	strcpy(cs_unsucc_file,"");
        strcpy(cs_error_file,"");
        strcpy(cs_acct_type_name,"");
	strcpy(cs_provider_name,"");
        strcpy(cs_bank_short_name,"");
                        
        while ((c = getopt(argc,argv,"t:l:o:s:u:e:a:p:b:")) != EOF) {
                switch (c) {
			case 't':
                                strcpy(cs_process_path, optarg);
                                break;
			case 'l':
                                strcpy(cs_upload_path, optarg);
                                break;
                        case 'o':
                                strcpy(cs_process_file, optarg);
                                break;
			case 's':
                                strcpy(cs_succ_file, optarg);
                                break;
			case 'u':
                                strcpy(cs_unsucc_file, optarg);
                                break;
			case 'e':
                                strcpy(cs_error_file, optarg);
                                break;
			case 'a':
                                strcpy(cs_acct_type_name, optarg);
                                break;
			case 'p':
                                strcpy(cs_provider_name, optarg);
                                break;
			case 'b':
                                strcpy(cs_bank_short_name, optarg);
                                break;
                        default:
                                return FAILURE;
                }
        }       
        
        if (!strcmp(cs_process_path,"") || !strcmp(cs_upload_path,"") || !strcmp(cs_process_file,"") || !strcmp(cs_succ_file,"") || !strcmp(cs_unsucc_file,"") || !strcmp(cs_error_file,"") || !strcmp(cs_acct_type_name,"") || !strcmp(cs_provider_name,""))
                return FAILURE;
                
        return SUCCESS; 
}               


int check_filename(char *cs_bank_stmt_file)
{
	int iRet = PD_TRUE;
	
	char *csTmp = NULL;
	
//DEBUGLOG(("******************** check_filename::start ********************\n"));

	csTmp = cs_bank_stmt_file;
     	while (*csTmp) {
            	if (!(*csTmp >= 'a' && *csTmp <= 'z') &&
                    !(*csTmp >= 'A' && *csTmp <= 'Z') &&
                    !(*csTmp >= '0' && *csTmp <= '9') &&
                    !(*csTmp == '-' || *csTmp == '_')) {
DEBUGLOG(("check_filename:: NOT Match [a-zA-Z0-9-_]\n", *csTmp));
                       	iRet = PD_FALSE;
		}
		csTmp++;
	}

//DEBUGLOG(("******************** check_filename::end ********************\n"));

        return iRet;
}


int check_detail(char csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN])
{
        int iRet = PD_TRUE;
        int i,j = 0;

//DEBUGLOG(("******************** check_detail::start ********************\n"));

        for (i=0;i<IMPORT_MAX_FIELD;i++) {

DEBUGLOG(("check_detail:: %s=[%s](%d)\n", CS_FIELD_NAME[i], csList[i], (int)strlen(csList[i])));

                // Check Length
                if (INT_DETAIL_CHECK_LEN[i] != -1) {
                        if (strlen(csList[i]) != INT_DETAIL_MAX_LEN[i]) {
DEBUGLOG(("check_detail:: Length [%d] NOT Match [%d]\n", (int)strlen(csList[i]), INT_DETAIL_MAX_LEN[i]));
                                iRet = PD_FALSE;
                        } else {
                        }
                } else {
//DEBUGLOG(("check_detail:: SKIP Check Length \n"));
                }

                // Check Content
                if (INT_DETAIL_CHECK_TYPE[i] != -1) {
                        if (!strcmp(CS_DETAIL_CHECK_TYPE[i],"ENGLISH_CHAR")) {

                                for (j=0;j<strlen(csList[i]);j++) {

                                        if (!(csList[i][j] >= 'a' && csList[i][j] <= 'z') &&
                                            !(csList[i][j] >= 'A' && csList[i][j] <= 'Z')
                                        ) {
DEBUGLOG(("check_detail:: [%c] NOT Match [a-zA-Z]\n", csList[i][j]));
                                                iRet = PD_FALSE;
                                                break;
                                        } else {
                                        }
                                }

                        } else if (!strcmp(CS_DETAIL_CHECK_TYPE[i],"DIGIT")) {

                                for (j=0;j<strlen(csList[i]);j++) {

                                        if (!(csList[i][j] >= '0' && csList[i][j] <= '9')) {
DEBUGLOG(("check_detail:: [%c] NOT Match [0-9]\n", csList[i][j]));
                                                iRet = PD_FALSE;
                                                break;
                                        } else {
                                        }
                                }
			}
                } else {
//DEBUGLOG(("check_detail:: SKIP Check Content \n"));
                }
        }

//DEBUGLOG(("******************** check_detail::end ********************\n"));

        return iRet;
}


