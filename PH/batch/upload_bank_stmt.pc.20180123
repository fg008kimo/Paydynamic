ERRLOG("upload_bank_stmt: prcoess_upload: bank_country is missing!!\n");
                        iRet = FAILURE;
                }

/* int_bank_code */
                if (GetField_CString(hUploadStmtRec,"int_bank_code",&csBankCode)) {
DEBUGLOG(("prcoess_upload: [%d][int_bank_code] = [%s]\n",iCnt,csBankCode));
			PutField_CString(hRequest, "int_bank_code", csBankCode);
                } else {
DEBUGLOG(("prcoess_upload: int_bank_code is missing!!\n"));
ERRLOG("upload_bank_stmt: prcoess_upload: int_bank_code is missing!!\n");
                        iRet = FAILURE;
                }

/* bank_acct_num */
               	if (GetField_CString(hUploadStmtRec,"bank_acct_num",&csBankAcctNum)) {
DEBUGLOG(("prcoess_upload: [%d][bank_acct_num] = [%s]\n",iCnt,csBankAcctNum));
			PutField_CString(hRequest, "bank_acct_num", csBankAcctNum);
                } else {
DEBUGLOG(("prcoess_upload: bank_acct_num is missing!!\n"));
ERRLOG("upload_bank_stmt: prcoess_upload: bank_acct_num is missing!!\n");
                        iRet = FAILURE;
                }

		// Copy bank statement file to NFS server
		if (iRet == SUCCESS) {
	
			int iCnt = 0;
			char csInFullName[PD_TMP_BUF_LEN];
        		char csNewFullName[PD_TMP_BUF_LEN];
                      	char *command[64];
			FILE *fbankstmt;
			DIR *dbankstmt;

			// Check directory exists
			if (iRet == SUCCESS) {
				for (iCnt=0;iCnt<2;iCnt++) {
					if (iCnt == 0) {
						sprintf(csNewFullPath, "%s/%s/%s/%s", cs_upload_path, csBankCountry, csBankCode, PD_CONVERTED);
DEBUGLOG(("prcoess_upload: csNewFullPath = [%s]\n", csNewFullPath));
					} else if (iCnt == 1) {
						sprintf(csNewFullPath, "%s/%s/%s/%s", cs_upload_path, csBankCountry, csBankCode, PD_RAW);
DEBUGLOG(("prcoess_upload: csNewFullPath = [%s]\n", csNewFullPath));
					}
			
					dbankstmt = opendir(csNewFullPath);
					if(dbankstmt == NULL) {
						command[0] = "mkdir";
                        			command[1] = "-p";
                        			command[2] = csNewFullPath;
                        			command[3] = NULL;
                        			command[4] = NULL;

                        			execute(command);
DEBUGLOG(("prcoess_upload: Make Directory: Call execute() [%s %s %s %s %s]\n", command[0], command[1], command[2], command[3], command[4]));
					} else {
						closedir(dbankstmt);
					}
				}

				dbankstmt = opendir(cs_upload_path);
                               	if(dbankstmt != NULL) {
                                     	command[0] = "chmod";
                                    	command[1] = "-R";
                                      	command[2] = "777";
                                     	command[3] = cs_upload_path;
                                    	command[4] = NULL;

                                   	execute(command);
DEBUGLOG(("prcoess_upload: Change Mode: Call execute() [%s %s %s %s %s]\n", command[0], command[1], command[2], command[3], command[4]));

					closedir(dbankstmt);
                            	}
			}

			// Check file exists
			if (iRet == SUCCESS) {
				sprintf(csNewFullName, "%s/%s", csNewFullPath, csFileName);
DEBUGLOG(("prcoess_upload: csNewFullName = [%s]\n", csNewFullName));

				fbankstmt = fopen(csNewFullName,"r");
        			if (fbankstmt != NULL) {
DEBUGLOG(("prcoess_upload: File Exists\n"));
					fclose(fbankstmt);
					iErrRetCode = INT_FILE_NAME_ALREADY_EXIST;
        			} 
			}

			// Copy file
			if (iRet == SUCCESS) {
				sprintf(csInFullName, "%s/%s", cs_process_path, csFileName);
DEBUGLOG(("prcoess_upload: csInFullName = [%s]\n", csInFullName));

                      		command[0] = "cp";
                   		command[1] = "-p";
                    		command[2] = csInFullName;
                     		command[3] = csNewFullName;
                    		command[4] = NULL;

                     		execute(command);
DEBUGLOG(("prcoess_upload: Copy Statement File: Call execute() [%s %s %s %s %s]\n", command[0], command[1], command[2], command[3], command[4]));
			}
		}

		// Call TxnOmtByUsBSU
		if (iRet == SUCCESS) {

        		char    *p;
			char    csTmpUploadPath[PD_MAX_BUFFER+1];
        		char    csDatetime[PD_MAX_BUFFER+1];

			memset(csTmpUploadPath, 0, sizeof(csTmpUploadPath));
			memset(csDatetime, 0, sizeof(csDatetime));

			sprintf(csTmpUploadPath, cs_upload_path);

			p = mystrtok(csTmpUploadPath,PD_DIR_DELIMITOR);
                        if (p == NULL) {
                                iRet = FAILURE;
                        } else {
                                while ((p = mystrtok(NULL,PD_DIR_DELIMITOR)) != NULL) {
                                        strcpy(csDatetime,p);
                                }
                        }

			PutField_CString(hContext, "PHDATE", csDatetime);
DEBUGLOG(("prcoess_upload: [%d][PHDATE] = [%s]\n",iCnt,csDatetime));

			PutField_CString(hRequest, "in_file_path", csNewFullPath);
DEBUGLOG(("prcoess_upload: [%d][in_file_path] = [%s]\n",iCnt,csNewFullPath));

			PutField_CString(hRequest, "channel_code", PD_CHANNEL_OMT);
DEBUGLOG(("prcoess_upload: [%d][channel_code] = [%s]\n",iCnt,PD_CHANNEL_OMT));

			if (iRet == SUCCESS) {

DEBUGLOG(("prcoess_upload: Call TxnOmtByUsBSU: Authorize()\n"));
	                	TxnObjPtr = CreateObj(TxnPtr, "TxnOmtByUsBSU", "Authorize");
	                	iErrRetCode = (unsigned long)(*TxnObjPtr)(hContext, hRequest, hResponse);
				if (iErrRetCode == INT_NO_ERR) {
DEBUGLOG(("prcoess_upload: Call TxnOmtByUsBSU: Authorize() Success = [%d]\n", iErrRetCode));	
				} else {
DEBUGLOG(("prcoess_upload: Call TxnOmtByUsBSU: Authorize() Error = [%d]\n", iErrRetCode));			
				}	

				TxnCommit();
DEBUGLOG(("prcoess_upload: Call TxnOmtByUsBSU: Authorize() TxnCommit()\n"));
			}
		}

		// Put into Success/Unsuccess/Error Recordset
		if (iRet == SUCCESS) {	

			hash_t *hStmtRec = NULL;
                       	hStmtRec = (hash_t*) malloc (sizeof(hash_t));
                       	hash_init(hStmtRec,0);

			PutField_CString(hStmtRec, "acct_type", csAcctType);
                       	PutField_CString(hStmtRec, "client_id", csClientId);
                      	PutField_CString(hStmtRec, "client_name", csClientName);
                    	PutField_CString(hStmtRec, "int_bank_code", csBankCode);
                      	PutField_CString(hStmtRec, "bank_acct_num", csBankAcctNum);
                     	PutField_CString(hStmtRec, "bank_init", csBankInit);
                     	PutField_CString(hStmtRec, "bank_acct_name", csBankAcctName);
                    	PutField_CString(hStmtRec, "last_6_bank_acct_num", csShortBankAcctNum);
                       	PutField_CString(hStmtRec, "datetime", csDatetime);
                     	PutField_CString(hStmtRec, "file_ext", csFileExt);
                     	PutField_CString(hStmtRec, "bank_stmt_file", csFileName);
                     	PutField_Int(hStmtRec, "error_code", iErrRetCode);	

			if (iErrRetCode == INT_NO_ERR) {				
				PutField_Char(hStmtRec, "status", PD_STMT_APPROVED);
		
				RecordSet_Add(rSuccBankStmt, hStmtRec);
DEBUGLOG(("prcoess_upload: Add Record to Success Bank Statement Recordset\n"));
			} else {
				PutField_Char(hStmtRec, "status", PD_STMT_DECLINED);

				RecordSet_Add(rUnsuccBankStmt, hStmtRec);
				RecordSet_Add(rErrorBankStmt, hStmtRec);
DEBUGLOG(("prcoess_upload: Add Record to Unsuccess Bank Statement Recordset and Error Recordset\n"));
			}

			//hash_destroy(hStmtRec);
                       	//FREE_ME(hStmtRec);
		}

		iCnt++;

                hUploadStmtRec = RecordSet_GetNext(rUploadBankStmt);
	}	

	FREE_ME(hRequest);
        FREE_ME(hContext);
        FREE_ME(hResponse);

	return iRet;
}


