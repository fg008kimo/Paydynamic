int process_file(hash_t *hData,FILE *fproc,recordset_t *rProcessBankStmt)
{
	int     iRet = SUCCESS;
	int	i = 0;
        int     iFieldCnt = 0;
        int     iStmtCnt = 0;
	char    *p;
        char    csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN];
        char    cs_input_buf[PD_MAX_BUFFER+1];
        char    cs_bank_stmt_file[PD_MAX_BUFFER+1];
        char    cs_file_ext[PD_MAX_BUFFER+1];

	memset(cs_input_buf, 0, sizeof(cs_input_buf));
	memset(cs_bank_stmt_file, 0, sizeof(cs_bank_stmt_file));
	memset(cs_file_ext, 0, sizeof(cs_file_ext));

        while (fgets(cs_input_buf,PD_MAX_BUFFER, fproc) != NULL) {

		int	iStmtFileValid = PD_TRUE;
                
		iFieldCnt = 0;
		
		// Check 0x0A
                if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A) {
                        cs_input_buf[strlen(cs_input_buf) - 1] = '\0';
		}

		// Trim OxOD
		strcpy(cs_input_buf,TrimAllChar((const unsigned char*)cs_input_buf,strlen(cs_input_buf),PD_CHAR));
DEBUGLOG(("process_file: [%d][ORG_INPUT_FILE]=[%s]\n",iStmtCnt,cs_input_buf));

		strcpy(cs_bank_stmt_file, cs_input_buf);

		// Trim Extension
            	p = mystrtok(cs_input_buf,PD_EXT_DELIMITOR);
             	if (p == NULL) {
                    	iRet = FAILURE;
             	} else {
                    	while ((p = mystrtok(NULL,PD_EXT_DELIMITOR)) != NULL) {
                          	strcpy(cs_file_ext,p);
                     	}
		}

DEBUGLOG(("process_file: [%d][INPUT_FILE]=[%s][%s]\n",iStmtCnt,cs_input_buf,cs_file_ext));

		// Verify Process Stmt File Name
		if (iRet == SUCCESS) {

              		iStmtFileValid = check_filename(cs_input_buf);
			if (iStmtFileValid == PD_TRUE) {

				// Breakdown Raw Stmt File Name
				if (iRet == SUCCESS) {
                			p = mystrtok(cs_input_buf,PD_PROC_DELIMITOR);
                			if (p == NULL) {
                	        		iRet = FAILURE;
					} else {
                				strcpy(csList[iFieldCnt],p);
                				iFieldCnt++;

                				while ((p = mystrtok(NULL,PD_PROC_DELIMITOR)) != NULL) {
                				        strcpy(csList[iFieldCnt],p);
                				        iFieldCnt++;
                				}
					}

					// Check Number of Field Valid
					if (iFieldCnt > IMPORT_MAX_FIELD) {
DEBUGLOG(("process_file: [%d][NUM_OF_FIELD]=[%s]\n",iStmtCnt,iFieldCnt));
						iStmtFileValid = PD_FALSE;
					} else {
						for (i=0;i<IMPORT_MAX_FIELD;i++) {
DEBUGLOG(("process_file: [%d][%s]=[%s](%d)\n",iStmtCnt,CS_FIELD_NAME[i],csList[i],(int)strlen(csList[i])));
						}
					}
				}
			}
		}

		// Verify Process Stmt File Detail
		if (iRet == SUCCESS) {

			char    *csAcctType = NULL;
       	 		char    *csProviderId = NULL;
       	 		char    *csProviderName = NULL;
        		char    *csBankCode = NULL;

			hash_t  *hProcessStmtRec = NULL;
                        hProcessStmtRec = (hash_t*) malloc (sizeof(hash_t));
                       	hash_init(hProcessStmtRec,0);

/* acct_type */
                     	if (GetField_CString(hData,"acct_type",&csAcctType)) {
                       		PutField_CString(hProcessStmtRec, "acct_type", csAcctType);
DEBUGLOG(("process_file: acct_type = [%s]\n",csAcctType));
                    	} else {
DEBUGLOG(("process_file: acct_type is missing!!\n"));
ERRLOG("upload_bank_stmt: process_file: acct_type is missing!!\n");
				iRet = FAILURE;
                    	}

/* provider_id */
                  	if (GetField_CString(hData,"client_id",&csProviderId)) {
                          	PutField_CString(hProcessStmtRec, "client_id", csProviderId);
DEBUGLOG(("process_file: provider_id = [%s]\n",csProviderId));
                     	} else {
DEBUGLOG(("process_file: provider_id is missing!!\n"));
ERRLOG("upload_bank_stmt: process_file: provider_id is missing!!\n");
				iRet = FAILURE;
                   	}

/* provider_name */
                        if (GetField_CString(hData,"client_name",&csProviderName)) {
                                PutField_CString(hProcessStmtRec, "client_name", csProviderName);
DEBUGLOG(("process_file: provider_name = [%s]\n",csProviderName));
                        } else {
DEBUGLOG(("process_file: provider_name is missing!!\n"));
ERRLOG("upload_bank_stmt: process_file: provider_name is missing!!\n");
                                iRet = FAILURE;
                        }

/* int_bank_code */
                    	if (GetField_CString(hData,"int_bank_code",&csBankCode)) {
                              	PutField_CString(hProcessStmtRec, "int_bank_code", csBankCode);
DEBUGLOG(("process_file: int_bank_code = [%s]\n",csBankCode));
                    	} else {
DEBUGLOG(("process_file: int_bank_code is missing!!\n"));
//ERRLOG("upload_bank_stmt: process_file: int_bank_code is missing!!\n");
                     	}

			if (iRet == SUCCESS) { 
	
				if (iStmtFileValid == PD_TRUE) {	
					PutField_CString(hProcessStmtRec, "bank_init", csList[0]);
