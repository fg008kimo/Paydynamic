        }

	// Get Bank Code
	if (iRet == SUCCESS) {
		if (strlen(cs_bank_short_name) > 0) {
			DBObjPtr = CreateObj(DBPtr,"DBBankDesc","FindOfflineBankByBankAbbrevName");
                	if ((unsigned long)(DBObjPtr)(cs_bank_short_name,csBankCode) == FOUND) {
