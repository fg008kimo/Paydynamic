int parse_arg(int argc,char **argv)
{               
        char    c;
        strcpy(cs_stmt_path,"");
        strcpy(cs_upload_path,"");
        strcpy(cs_process_file,"");
	strcpy(cs_succ_file,"");
	strcpy(cs_unsucc_file,"");
        strcpy(cs_error_file,"");
        strcpy(cs_acct_type_name,"");
	strcpy(cs_provider_name,"");
        strcpy(cs_bank_short_name,"");
                        
        while ((c = getopt(argc,argv,"t:l:o:s:u:e:a:p:b:")) != EOF) {
                switch (c) {
			case 't':
                                strcpy(cs_stmt_path, optarg);
                                break;
			case 'l':
                                strcpy(cs_upload_path, optarg);
                                break;
                        case 'o':
                                strcpy(cs_process_file, optarg);
                                break;
			case 's':
                                strcpy(cs_succ_file, optarg);
                                break;
			case 'u':
                                strcpy(cs_unsucc_file, optarg);
                                break;
			case 'e':
                                strcpy(cs_error_file, optarg);
                                break;
			case 'a':
                                strcpy(cs_acct_type_name, optarg);
                                break;
			case 'p':
                                strcpy(cs_provider_name, optarg);
                                break;
			case 'b':
                                strcpy(cs_bank_short_name, optarg);
                                break;
                        default:
                                return FAILURE;
                }
        }       
        
        if (!strcmp(cs_stmt_path,"") || !strcmp(cs_upload_path,"") || !strcmp(cs_process_file,"") || !strcmp(cs_succ_file,"") || !strcmp(cs_unsucc_file,"") || !strcmp(cs_error_file,"") || !strcmp(cs_acct_type_name,"") || !strcmp(cs_provider_name,""))
                return FAILURE;
                
        return SUCCESS; 
}               


int check_filename(char *cs_bank_stmt_file_ext, char *cs_bank_stmt_file)
{
        int iRet = PD_TRUE;

        char *csTmp = NULL;

//DEBUGLOG(("******************** check_filename::start ********************\n"));

        if (strlen(cs_bank_stmt_file_ext) > IMPORT_MAX_FILE_LEN) {
DEBUGLOG(("check_filename:: Length [%d] > Max Length [%d]\n",strlen(cs_bank_stmt_file_ext),IMPORT_MAX_FILE_LEN));
                iRet = PD_FALSE;
        } else {
                csTmp = cs_bank_stmt_file;
                while (*csTmp) {
                        if (!(*csTmp >= 'a' && *csTmp <= 'z') &&
                            !(*csTmp >= 'A' && *csTmp <= 'Z') &&
                            !(*csTmp >= '0' && *csTmp <= '9') &&
                            !(*csTmp == '-' || *csTmp == '_')) {
DEBUGLOG(("check_filename:: NOT Match [a-zA-Z0-9-_]\n", *csTmp));
                                iRet = PD_FALSE;
                        }
                        csTmp++;
                }
        }

//DEBUGLOG(("******************** check_filename::end ********************\n"));

DEBUGLOG(("check_filename:: iRet = [%d]\n", iRet));
        return iRet;
}


int check_detail(int iNumOfField, char csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN])
{
        int iRet = PD_TRUE;
        int i,j = 0;

//DEBUGLOG(("******************** check_detail::start ********************\n"));

        if (iNumOfField != IMPORT_MAX_FIELD) {
DEBUGLOG(("check_detail:: Number of Field = [%d] not equal to Max Field Number [%d]\n",iNumOfField,IMPORT_MAX_FIELD));
                iRet = PD_FALSE;
        } else {
                for (i=0;i<iNumOfField;i++) {

DEBUGLOG(("check_detail:: %s=[%s](%d)\n", CS_UPLOAD_FIELD_NAME[i], csList[i], (int)strlen(csList[i])));

                        // Check Length
                        if (INT_UPLOAD_DETAIL_CHECK_LEN[i] != -1) {
                                if (strlen(csList[i]) != INT_UPLOAD_DETAIL_MAX_LEN[i]) {
DEBUGLOG(("check_detail:: Length [%d] NOT Match [%d]\n", (int)strlen(csList[i]), INT_UPLOAD_DETAIL_MAX_LEN[i]));
                                        iRet = PD_FALSE;
                                }
                        } else {
//DEBUGLOG(("check_detail:: SKIP Check Length \n"));
                        }

                        // Check Content
                        if (INT_UPLOAD_DETAIL_CHECK_TYPE[i] != -1) {
                                if (!strcmp(CS_UPLOAD_DETAIL_CHECK_TYPE[i],"ALL_CHAR")) {

                                        for (j=0;j<strlen(csList[i]);j++) {

                                                if (!(csList[i][j] >= 'a' && csList[i][j] <= 'z') &&
                                                    !(csList[i][j] >= 'A' && csList[i][j] <= 'Z') &&
                                                    !(csList[i][j] >= '0' && csList[i][j] <= '9') &&
                                                    !(csList[i][j] >= '-' || csList[i][j] <= '_')
                                                ) {
DEBUGLOG(("check_detail:: [%c] NOT Match [a-zA-Z0-9-_]\n", csList[i][j]));
                                                        iRet = PD_FALSE;
                                                        break;
                                                }
                                        }
				
				} else if (!strcmp(CS_UPLOAD_DETAIL_CHECK_TYPE[i],"ENGLISH_CHAR")) {

                                        for (j=0;j<strlen(csList[i]);j++) {

                                                if (!(csList[i][j] >= 'a' && csList[i][j] <= 'z') &&
                                                    !(csList[i][j] >= 'A' && csList[i][j] <= 'Z')
                                                ) {
DEBUGLOG(("check_detail:: [%c] NOT Match [a-zA-Z]\n", csList[i][j]));
                                                        iRet = PD_FALSE;
                                                        break;
                                                }
                                        }

                                } else if (!strcmp(CS_UPLOAD_DETAIL_CHECK_TYPE[i],"DIGIT")) {

                                        for (j=0;j<strlen(csList[i]);j++) {

                                                if (!(csList[i][j] >= '0' && csList[i][j] <= '9')) {
DEBUGLOG(("check_detail:: [%c] NOT Match [0-9]\n", csList[i][j]));
                                                        iRet = PD_FALSE;
                                                        break;
                                                }
                                        }
                                }
                        } else {
//DEBUGLOG(("check_detail:: SKIP Check Content \n"));
                        }
                }
        }

//DEBUGLOG(("******************** check_detail::end ********************\n"));

        return iRet;
}


