	// Process Matching
	if (iRet == SUCCESS) {
		iRet = prcoess_match(rBankAcctDetail,rProcessBankStmt,rUploadBankStmt,rErrorBankStmt,ferr);
	}

	// Process Upload Stmt, file handling and error handling
	if (iRet == SUCCESS) {
		iRet = process_upload(rUploadBankStmt,rSuccBankStmt,rUnsuccBankStmt,rErrorBankStmt,ferr);
	}

	// Process Results
	if (iRet == SUCCESS) {

		// Success File
		//iRet = process_success_file(rSuccBankStmt,fsucc);
		
		// Unsuccess File
		if (iRet == SUCCESS) {		
			//iRet = process_unsuccess_file(rUnsuccBankStmt,funsucc);
		}
		
		// Error File
		if (iRet == SUCCESS) {
			//iRet = process_error_file(rErrorBankStmt,ferr);
		}
	}

	hash_destroy(hData);
        FREE_ME(hData);	

	RecordSet_Destroy(rBankAcctDetail);
        FREE_ME(rBankAcctDetail);

	RecordSet_Destroy(rProcessBankStmt);
	FREE_ME(rProcessBankStmt);

	RecordSet_Destroy(rUploadBankStmt);
        FREE_ME(rUploadBankStmt);

	RecordSet_Destroy(rSuccBankStmt);
        FREE_ME(rSuccBankStmt);

	RecordSet_Destroy(rUnsuccBankStmt);
        FREE_ME(rUnsuccBankStmt);

	RecordSet_Destroy(rErrorBankStmt);
        FREE_ME(rErrorBankStmt);

	fclose(fproc);
	fclose(fsucc);
	fclose(funsucc);
	fclose(ferr);

DEBUGLOG(("batch_proc: iRet = [%d]\n", iRet));
	return iRet;
}


