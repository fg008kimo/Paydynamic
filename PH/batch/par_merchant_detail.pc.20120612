int batch_init(int argc, char* argv[])
{

    if (argc < 1) {
    	printf("usage: -f input file\n");
        return FAILURE;
    }
    else
        return SUCCESS;
}


int batch_proc(int argc, char* argv[])
{
	FILE	*fin;
	int	iRet;

        hash_t          *hContext, *hRequest, *hResponse;

        hContext = (hash_t *)malloc(sizeof(hash_t));
        hRequest = (hash_t *)malloc(sizeof(hash_t));
        hResponse= (hash_t *)malloc(sizeof(hash_t));

        hash_init(hContext,   0);
        hash_init(hRequest,  0);
        hash_init(hResponse, 0);

	
	iRet = parse_arg(argc,argv);

	if (iRet != SUCCESS) {
    		printf("*usage: -f input file\n");
		return (iRet);
	}

	fin = fopen(cs_inputfile,"r");
	if (fin == NULL) {
DEBUGLOG(("Error Opening file = [%s]\n",cs_inputfile));
		return FAILURE;
	}

	ChannelObjPtr = CreateObj(ChannelPtr, "MGTChannel","UpdateContext");
	iRet = (unsigned long)((*ChannelObjPtr)(hContext));

	if (iRet == PD_OK) {
		iRet = verify_file(fin, hContext, hRequest, hResponse);	
	} else {
DEBUGLOG(("Error UpdateContext\n"));
	}


	fclose(fin);

	hash_destroy(hContext);
	hash_destroy(hRequest);
	hash_destroy(hResponse);

	FREE_ME(hContext);
	FREE_ME(hRequest);
	FREE_ME(hResponse);

	return iRet;

}


int batch_terminate(int argc, char* argv[])
{
	return SUCCESS;
}


                        
int parse_arg(int argc,char **argv)
{               
        char    c;
        strcpy(cs_inputfile,"");
                        
        while ((c = getopt(argc,argv,"f:")) != EOF) {
                switch (c) {
                        case 'f':
                                strcpy(cs_inputfile, optarg);
                                break;
                        default:
                                return FAILURE;
                }
        }       
        
DEBUGLOG(("[%s]\n",cs_inputfile));
        if (!strcmp(cs_inputfile,""))
                return FAILURE;
                
        return SUCCESS; 
}               


int verify_file(FILE *fin, hash_t *hContext, hash_t *hRequest, hash_t *hResponse)
{
        int     iRet = FAILURE;

	int	iCount;
        char    cs_input_buf[PD_MAX_BUFFER +1]; /*read buffer*/
	char	*p;
	char    csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN];

	int	iRowCnt = 0;

/*
        char    csDate[3][PD_YYYY_LEN+1];
	char	csTmp[PD_MAX_BUFFER +1];

	double	dTmp;
*/

/* Header */    
        iCount = 0;
	iRowCnt = 0;

        fgets(cs_input_buf,PD_MAX_BUFFER,fin);
        if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A || cs_input_buf[strlen(cs_input_buf) - 1] == 0x10)
               cs_input_buf[strlen(cs_input_buf) - 1] = '\0';
	strcpy(cs_input_buf,TrimAllChar((const unsigned char*)cs_input_buf,strlen(cs_input_buf),PD_CHAR));
	iRowCnt++;
/* End of Header */

        while (fgets(cs_input_buf,PD_MAX_BUFFER, fin) != NULL) {
		iRowCnt++;

        	if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A)
                        cs_input_buf[strlen(cs_input_buf) - 1] = '\0';
		strcpy(cs_input_buf,TrimAllChar((const unsigned char*)cs_input_buf,strlen(cs_input_buf),PD_CHAR));
DEBUGLOG(("%s\n",cs_input_buf));

                iCount = 0;

                p = mystrtok(cs_input_buf,PD_MY_DELIMITOR);
                if (p == NULL)
                        return FAILURE;

		/*
                strcpy(csTmp,TrimAllChar((const unsigned char*)p,strlen(p),PD_MY_TOKEN));
                strcpy(csList[iCount],TrimAllChar((const unsigned char*)csTmp,strlen(csTmp),PD_MY_QE_TOEKN));
		*/
		strcpy(csList[iCount],TrimAllChar((const unsigned char*)p, strlen(p), PD_MY_TOKEN));
                iCount++;

                while ( (p = mystrtok(NULL,PD_MY_DELIMITOR)) != NULL) {
			/*
                        strcpy(csTmp,TrimAllChar((const unsigned char*)p,strlen(p),PD_MY_TOKEN));
                        strcpy(csList[iCount],TrimAllChar((const unsigned char*)csTmp,strlen(csTmp),PD_MY_QE_TOEKN));
			*/
			strcpy(csList[iCount],TrimAllChar((const unsigned char*)p, strlen(p), PD_MY_TOKEN));
                        iCount++;
                }

DEBUGLOG((">>>>>>>>>> File Line[%d] <<<<<<<<<< \n",iRowCnt));

		iRet=process_txn(csList, hContext, hRequest, hResponse);


        }


	return iRet;
}

int process_txn(char csInList[][IMPORT_FIELD_LEN], hash_t *hContext, hash_t *hRequest, hash_t *hResponse)
{
	int iRet = SUCCESS;

DEBUGLOG(("MERCHANT_ACCOUNT_NMB=[%s]\n",csInList[IDX_MERCHANT_ACCOUNT_NMB]));
/*
DEBUGLOG(("USERNAME[%s]\n",csInList[IDX_USERNAME]));
DEBUGLOG(("COMPANY_NAME=[%s]\n",csInList[IDX_COMPANY_NAME]));
DEBUGLOG(("API_MERCHANT_ID=[%s]\n",csInList[IDX_API_MERCHANT_ID]));
DEBUGLOG(("FILE_PREFIX=[%s]\n",csInList[IDX_FILE_PREFIX]));
DEBUGLOG(("IS_BACKEND_ACK=[%s]\n",csInList[IDX_IS_BACKEND_ACK]));
DEBUGLOG(("BACK_ACK_URL=[%s]\n",csInList[IDX_BACK_ACK_URL]));
DEBUGLOG(("MERCHANT_TYPE_ID=[%s]\n",csInList[IDX_MERCHANT_TYPE_ID]));
DEBUGLOG(("DEPOSIT_PSP_TYPE_ID=[%s]\n",csInList[IDX_DEPOSIT_PSP_TYPE_ID]));
DEBUGLOG(("PAYOUT_ENABLE=[%s]\n",csInList[IDX_PAYOUT_ENABLE]));
DEBUGLOG(("PAYOUT_FILE_UPLOAD_ENABLED=[%s]\n",csInList[IDX_PAYOUT_FILE_UPLOAD_ENABLED]));
DEBUGLOG(("PAYOUT_FILE_TYPE_ID=[%s]\n",csInList[IDX_PAYOUT_FILE_TYPE_ID]));
DEBUGLOG(("PSP_PROFILE_ID=[%s]\n",csInList[IDX_PSP_PROFILE_ID]));
DEBUGLOG(("APPLY_MERCHANT_RESERVE=[%s]\n",csInList[IDX_APPLY_MERCHANT_RESERVE]));
DEBUGLOG(("RESERVE_FREQUENCY=[%s]\n",csInList[IDX_RESERVE_FREQUENCY]));
DEBUGLOG(("POST_DATE=[%s]\n",csInList[IDX_POST_DATE]));
DEBUGLOG(("UPDATE_DATE=[%s]\n",csInList[IDX_UPDATE_DATE]));
*/
	recordset_t 	*rRecordSet;
	hash_t		*hRec;
	char		*csTmp;
	char		*csGenMerchID;
	int		iRecord = PD_NOT_FOUND;
	
	int		iTmpRet = PD_OK;

	int		iVNCMerchType;
	int		iPayoutEnable;
	int 		iIntError;

	hash_t          *hMerchProfile;
	hash_t          *hClient;

	char		*csClientID = strdup("");	
	char		csMerchName[PD_NAME_LEN + 1];

        hClient = (hash_t *)malloc(sizeof(hash_t));
        hash_init(hClient, 0);
	
        hMerchProfile = (hash_t *)malloc(sizeof(hash_t));
        hash_init(hMerchProfile, 0);



	// ******************** Client ************************ //
	rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));
	recordset_init(rRecordSet, 0);

DEBUGLOG(("DBParClients.GetClientID\n"));
	DBObjPtr = CreateObj(DBPtr, "DBParClients","GetClientID");
	iTmpRet = (unsigned long)(*DBObjPtr)(csInList[IDX_COMPANY_NAME], rRecordSet);

	if (iTmpRet != PD_OK  && iTmpRet != SQL_NOT_FOUND) {
DEBUGLOG(("FAIL on DBClients.GetClientID\n"));
		iRet = FAILURE;
	}

	if (iRet == SUCCESS) {
		if (iTmpRet == SQL_NOT_FOUND) {
DEBUGLOG(("DBParClients.GetClientID No Record FOUND Gen a New One call DBParClients.GenClientID!\n"));

			DBObjPtr = CreateObj(DBPtr, "DBParClients","GenClientID");
			iTmpRet = (unsigned long)(*DBObjPtr)(PD_CLI_PREFIX, "M", csClientID);

			if (iTmpRet != PD_OK) {
				iRet = FAILURE;
			} else {
DEBUGLOG(("DBParClients.GetClientID Gen New ClientID [%s]\n", csClientID));

				PutField_CString(hClient, "company", csInList[IDX_COMPANY_NAME]);
				PutField_CString(hClient, "client_id", csClientID);
				PutField_Char(hClient, "type", PD_TYPE_MERCHANT);
				PutField_CString(hClient, "status", PD_ACC_OPEN);
				PutField_CString(hClient, "create_user", PD_UPDATE_USER);

				DBObjPtr = CreateObj(DBPtr, "DBParClients", "Add");
				iTmpRet = (unsigned long)(*DBObjPtr)(hClient);

				if (iTmpRet != PD_OK) {
DEBUGLOG(("DBParClients.Add Client Failed!\n"));
					iRet = FAILURE;
				}

			}			
		}
		else {
DEBUGLOG(("Fetch Client Record\n"));
			hRec = RecordSet_GetFirst(rRecordSet);
			while (hRec) {
				if (GetField_CString(hRec, "client_id", &csTmp)) {
DEBUGLOG(("Record Found client_id = [%s]\n", csTmp));
					strcpy(csClientID, csTmp);
				}
				hRec = RecordSet_GetNext(rRecordSet);
			}
		}
	}

	RecordSet_Destroy(rRecordSet);


	// ******************** MID ************************ //

	rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));
	recordset_init(rRecordSet, 0);
	

	if (iRet == PD_OK) {
DEBUGLOG(("DBParMerchProfile.GetMerchant\n"));
		DBObjPtr = CreateObj(DBPtr,"DBParMerchProfile","GetMerchant");

		iTmpRet = (unsigned long)(*DBObjPtr)(csInList[IDX_MERCHANT_ACCOUNT_NMB], rRecordSet);

		if (iTmpRet != PD_OK && iTmpRet != SQL_NOT_FOUND) {
DEBUGLOG(("FAIL on DBParMerchProfile.GetMerchant\n"));
			iRet = FAILURE;
		}

		if (iTmpRet == SQL_NOT_FOUND) {
DEBUGLOG(("DBParMerchProfile.GetMerchant No Record FOUND!\n"));
		}

		if (iRet == PD_OK) {
			hRec = RecordSet_GetFirst(rRecordSet);
			while (hRec) {
				iRecord = PD_FOUND;

				if (GetField_CString(hRec, "username", &csTmp)) {
DEBUGLOG(("Record Found  username = [%s]\n", csTmp));
				}

				hRec = RecordSet_GetNext(rRecordSet);
			}
		}
		RecordSet_Destroy(rRecordSet);

		if (iRet == PD_OK) {
			if (iRecord == PD_NOT_FOUND) {

				// add record into merch_detail, merch_key (TxnMgtByUsMCR)
				memset(csMerchName, 0, sizeof(csMerchName));
				sprintf(csMerchName, "%.*s", PD_NAME_LEN, csInList[IDX_COMPANY_NAME]);
	
				//PutField_CString(hRequest, "name", csInList[IDX_COMPANY_NAME]);
				PutField_CString(hRequest, "name", csMerchName);


				if (!strcmp(csInList[IDX_MERCHANT_TYPE_ID], "")) {
DEBUGLOG(("VNC_MerchType_ID IS NULL"));
					iRet = FAILURE;
				}

				iVNCMerchType = atoi(csInList[IDX_MERCHANT_TYPE_ID]);
DEBUGLOG(("VNC_MerchType_ID = [%d]\n", iVNCMerchType));


				if (iVNCMerchType == 1 || iVNCMerchType == 3) {
					PutField_CString(hRequest, "type", "G");
				}	
				else if (iVNCMerchType == 2) {
					PutField_CString(hRequest, "type", "N");
				}
				else if (iVNCMerchType == 4) {
DEBUGLOG(("VNC_MerchType_ID Offline Ignore\n"));
				}
				else {
DEBUGLOG(("VNC_MerchType_ID = [%d] Invalid \n"));
					iRet = FAILURE;
				}

				iPayoutEnable = atoi(csInList[IDX_PAYOUT_ENABLE]);

				if (GetField_CString(hRequest, "type", &csTmp)) {

					//PutField_CString(hRequest, "client_id", "1000000001"); // need to modify!!
					PutField_CString(hRequest, "client_id", csClientID); 
					PutField_CString(hRequest, "fee_rate", "0");

					if (GetField_CString(hContext, "PHDATE", &csTmp)) {
						PutField_CString(hRequest, "effect_date", csTmp);
					} else {
DEBUGLOG(("PHDATE not found for effect_date\n"));
						iRet = FAILURE;
					}

					PutField_CString(hRequest, "key_type", PD_PTK_KEY_NAME);
					PutField_CString(hRequest, "add_user", PD_UPDATE_USER);

					sleep(1); // to prevent same key_value as time will affect the random_gen
DEBUGLOG(("Calling TxnMgtByUsMCR::Authorize\n"));
					TxnObjPtr = CreateObj(TxnPtr,"TxnMgtByUsMCR","Authorize");
					iRet = (unsigned long)(*TxnObjPtr)(hContext,hRequest,hResponse);
	
					if (iRet == PD_OK) {
						if (GetField_Int(hContext, "internal_error", &iIntError)) {
DEBUGLOG(("Calling TxnMgtByUsMCR::Authorize Return internal_error [%d]\n", iIntError));
							iRet = FAILURE;
						}
					}

					if (iRet == PD_OK) {
						if (GetField_CString(hResponse, "merchant_id", &csGenMerchID)) {
DEBUGLOG(("Calling TxnMgtByUsMCR::Authorize response merchant_id [%s]\n", csGenMerchID));
						}
						if (GetField_CString(hResponse, "key_value", &csTmp)) {
DEBUGLOG(("Calling TxnMgtByUsMCR::Authorize response key_value [%s]\n", csTmp));
						}
					}

					// add record into table:ParMerchProfile
					if (iRet == PD_OK) {
						// Check Service
						if (strcmp(csInList[IDX_USERNAME], PD_EC2_USERNAME) == 0) {
							PutField_CString(hMerchProfile, "service", PD_SERVICE_EC2);
						} else if (iVNCMerchType == 2) {
							PutField_CString(hMerchProfile, "service", PD_SERVICE_NBX);
						} else {
							PutField_CString(hMerchProfile, "service", PD_SERVICE_VNC);
						}
						PutField_Int(hMerchProfile, "payout_enable", iPayoutEnable);

						PutField_CString(hMerchProfile, "merch_account_nmb", csInList[IDX_MERCHANT_ACCOUNT_NMB]);
						PutField_CString(hMerchProfile, "company", csInList[IDX_COMPANY_NAME]);
						PutField_CString(hMerchProfile, "username", csInList[IDX_USERNAME]);
						PutField_CString(hMerchProfile, "api_merch_id", csInList[IDX_API_MERCHANT_ID]);
						PutField_CString(hMerchProfile, "ack_url", csInList[IDX_BACK_ACK_URL]);
						PutField_CString(hMerchProfile, "merchant_id", csGenMerchID);
						PutField_CString(hMerchProfile, "create_user", PD_UPDATE_USER);
		
						DBObjPtr = CreateObj(DBPtr,"DBParMerchProfile","Add");
						iRet = (unsigned long)(*DBObjPtr)(hMerchProfile);
	
						if (iRet == PD_OK) {
							if (GetField_Int(hContext, "internal_error", &iIntError)) {
DEBUGLOG(("Calling DBParMerchProfile::Add Return internal_error [%d]\n", iIntError));
								iRet = FAILURE;
							}
						}
					}
				}
			}
			else {
DEBUGLOG(("Mechant is already created, ignore is ok\n"));
			}
		}
	}

	RecordSet_Destroy(rRecordSet);
	FREE_ME(rRecordSet);

	hash_destroy(hClient);
	FREE_ME(hClient);

	hash_destroy(hMerchProfile);
	FREE_ME(hMerchProfile);

	FREE_ME(csClientID);	

DEBUGLOG(("par_merchant_detail iRet [%d]\n", iRet));

	return iRet;
}
