                                        	        PutField_Int(hProcessStmtRec, "bank_disabled", iTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Match][%d]bank_disabled is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_disabled is missing!!\n");
                                        	        //iDtlRet = PD_ERR;
                                        	}

/* bank_acct_disabled */
                                        	if (GetField_Int(hMatchRec,"bank_acct_disabled",&iTmp)) {
DEBUGLOG(("prcoess_match: [Match][%d][bank_acct_disabled] = [%d]\n",iMatchCnt,iTmp));
                                        	        PutField_Int(hProcessStmtRec, "bank_acct_disabled", iTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Match][%d]bank_acct_disabled is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_disabled is missing!!\n");
                                        	        //iDtlRet = PD_ERR;
                                        	}
					} else {

						/*
                                                 *
                                                 * If raw bank statement filename invalid,
                                                 * - Put status and err_code into Upload Recordset.
                                                 *
                                                 */

/* status */
                                                if (GetField_Char(hRawStmtRec,"status",&cTmp)) {
DEBUGLOG(("prcoess_match: [Match][%d][status] = [%c]\n",iMatchCnt,cTmp));
                                                        PutField_Char(hProcessStmtRec, "status", cTmp);
                                                } else {
DEBUGLOG(("prcoess_match: [Match][%d]status is missing!!\n",iMatchCnt));
//ERRLOG("upload_bank_stmt: prcoess_match: status is missing!!\n");
                                                        //iDtlRet = PD_ERR;
                                                }

/* error_code */
                                                if (GetField_Int(hRawStmtRec,"error_code",&iTmp)) {
DEBUGLOG(("prcoess_match: [Match][%d][error_code] = [%d]\n",iMatchCnt,iTmp));
                                                        PutField_Int(hProcessStmtRec, "error_code", iTmp);
                                                } else {
DEBUGLOG(("prcoess_match: [Match][%d]error_code is missing!!\n",iMatchCnt));
//ERRLOG("upload_bank_stmt: prcoess_match: error_code is missing!!\n");
                                                        //iDtlRet = PD_ERR;
                                                }
					}

					iMatchCnt++;

					RecordSet_Add(rProcessBankStmt, hProcessStmtRec);

                               		//hash_destroy(hProcessStmtRec);
                              		//FREE_ME(hProcessStmtRec);

				}
			}
			
			iMatchingCnt++;

              		hRawStmtRec = RecordSet_GetNext(rRawBankStmt);
		}
	}

	if (iDtlRet == PD_OK) {

                if (iMatchCnt > 0) {
DEBUGLOG(("process_match: match_cnt = [%d]\n",iMatchCnt));
			iRet = PD_FOUND;
                } else {
DEBUGLOG(("process_match: match_cnt = [0]\n"));
			iRet = PD_NOT_FOUND;;
                }
        
	} else {
		iRet = PD_ERR;
	}

DEBUGLOG(("process_match: end: ----------------------------------------------------------------------------------------------\n"));

	return iRet;
}


int process_output(recordset_t *rProcessBankStmt,FILE *fproc)
{
	int iRet = SUCCESS;

	int iOutputCnt = 0;
	
	char *csTmp = NULL;

	hash_t *hProcessStmtRec = NULL;

DEBUGLOG(("process_output: start: ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++\n"));

	hProcessStmtRec = RecordSet_GetFirst(rProcessBankStmt);
        while ((iRet == SUCCESS) && (hProcessStmtRec)) {

        	char csTmpBuf[PD_TMP_BUF_LEN + 1];

		memset(csTmpBuf, 0, sizeof(csTmpBuf));

/* bank_stmt_file */
		if (GetField_CString(hProcessStmtRec,"bank_stmt_file",&csTmp)) {
                        sprintf(csTmpBuf,"%s",csTmp);
DEBUGLOG(("process_output: [%d][bank_stmt_file]=[%s][%d]\n", iOutputCnt, csTmpBuf, (int)strlen(csTmpBuf)));
                        fprintf(fproc,"%.*s",(int)strlen(csTmpBuf),csTmpBuf);
                } else {
DEBUGLOG(("process_output: bank_stmt_file is missing!!\n"));
ERRLOG("scan_bank_stmt: process_output: bank_stmt_file is missing!!\n");
                        iRet = FAILURE;
                }

		fprintf(fproc,"\n");

		iOutputCnt++;

		hProcessStmtRec = RecordSet_GetNext(rProcessBankStmt);
	}

	if (iRet == SUCCESS) {

		if (iOutputCnt > 0) {
DEBUGLOG(("process_output: output_cnt = [%d]\n",iOutputCnt));
                } else {
DEBUGLOG(("process_output: output_cnt = [0]\n",iOutputCnt));
                }
	}

DEBUGLOG(("process_output: end: ----------------------------------------------------------------------------------------------\n"));

	return iRet;
}

