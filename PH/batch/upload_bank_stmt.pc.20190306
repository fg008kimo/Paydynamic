                        PutField_CString(hStmtRec, "provider_name", csClientName);
			PutField_CString(hStmtRec, "stmt_filename", csFileName);
			//PutField_CString(hStmtRec, "stmt_filename", csNewFileName);
                        PutField_Char(hStmtRec, "status", cStatus);
                        PutField_Int(hStmtRec, "err_code", iMsgCode);

			if (GetField_CString(hUploadStmtRec,"int_bank_code",&csTmp)) {
                                PutField_CString(hStmtRec, "int_bank_code", csTmp);
                        }	
			if (GetField_CString(hUploadStmtRec,"bank_name",&csTmp)) {
                                PutField_CString(hStmtRec, "bank_name", csTmp);
			}
			if (GetField_CString(hUploadStmtRec,"bank_acct_num",&csTmp)) {
	               		PutField_CString(hStmtRec, "bank_acct_num", csTmp);
			}
                        if (GetField_CString(hUploadStmtRec,"bank_acct_name",&csTmp)) {
				PutField_CString(hStmtRec, "bank_acct_name", csTmp);
			}
                        if (GetField_CString(hUploadStmtRec,"bank_acct_short_name",&csTmp)) {
				PutField_CString(hStmtRec, "bank_acct_short_name", csTmp);
			}

                        if (iMsgCode != INT_NO_ERR) {

				DBObjPtr = CreateObj(DBPtr,"DBOLAutoUploadErrLog","Add");
                		if ((unsigned long)(DBObjPtr)(hStmtRec) == PD_OK) {
DEBUGLOG(("process_upload: [%d]Call DBOLAutoUploadErrLog: Add Success!!\n", iRecCnt));
//DEBUGLOG(("process_upload: [%d]Call DBOLAutoUploadErrLog: TxnCommit()\n", iRecCnt));
                        		//TxnCommit();
                		} else { 
DEBUGLOG(("process_upload: [%d]Call DBOLAutoUploadErrLog: Add Failed!!\n", iRecCnt));
ERRLOG("upload_bank_stmt: process_upload: Call DBOLAutoUploadErrLog: Add Failed!!\n");
                        		iRet = FAILURE;
                		}
                        }

			iErrorCnt++;

                        hash_destroy(hStmtRec);
                        FREE_ME(hStmtRec);
		}

		if ((iRet == SUCCESS) && (iMsgCode == INT_NO_ERR)) {
			iSuccessCnt++;	
		}

		// Call Txn Commit
           	if (iRet == SUCCESS) {
                    	TxnCommit();
DEBUGLOG(("process_upload: [%d]TxnCommit()\n", iRecCnt));
              	} else {
			TxnAbort();
DEBUGLOG(("process_upload: [%d]TxnAbort()\n", iRecCnt));
		}
	
		iRecCnt++;

                hUploadStmtRec = RecordSet_GetNext(rUploadBankStmt);

		FREE_ME(hRequest);
        	FREE_ME(hContext);
        	FREE_ME(hResponse);
	}	

	if (iRet == SUCCESS) {

                if (iErrorCnt > 0) {
DEBUGLOG(("process_upload: error_cnt = [%d]\n",iErrorCnt));
                } else {
DEBUGLOG(("process_upload: error_cnt = [0]\n",iErrorCnt));
                }
	
		if (iSuccessCnt > 0) {
DEBUGLOG(("process_upload: success_cnt = [%d]\n",iSuccessCnt));
                } else {
DEBUGLOG(("process_upload: success_cnt = [0]\n",iSuccessCnt));
                }
        }

DEBUGLOG(("process_upload: end: ----------------------------------------------------------------------------------------------\n"));

	return iRet;
}

