		memset(csBankAbbrevName, 0, sizeof(csBankAbbrevName));
		memset(csBankAcctNum, 0, sizeof(csBankAcctNum));

/* bank_disabled */
		if (GetField_Int(hBankAcctRec,"bank_disabled",&iTmp)) {
//DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_disabled = [%d]\n",iBankAcctCnt,iTmp));
		} else {
//DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_disabled is missing!!\n",iBankAcctCnt));
		}                

/* bank_acct_disabled */
                if (GetField_Int(hBankAcctRec,"bank_acct_disabled",&iTmp)) {
//DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_acct_disabled = [%d]\n",iBankAcctCnt,iTmp));
                } else {
//DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_acct_disabled is missing!!\n",iBankAcctCnt));
                }

/* bank_abbrev_name */
		if (GetField_CString(hBankAcctRec,"bank_abbrev_name",&csTmp)) {
                        sprintf(csBankAbbrevName, "%s", csTmp);
//DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_abbrev_name = [%s]\n",iBankAcctCnt,csBankAbbrevName));
                } else {
DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_abbrev_name is missing!!\n",iBankAcctCnt));
ERRLOG("scan_bank_stmt: prcoess_match: bank_abbrev_name is missing!!\n");
                        iRet = FAILURE;
                }

/* bank_acct_num */
                if (GetField_CString(hBankAcctRec,"bank_acct_num",&csTmp)) {
                        sprintf(csBankAcctNum, "%s", csTmp);
//DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_acct_num = [%s]\n",iBankAcctCnt,csBankAcctNum));
                } else {
DEBUGLOG(("prcoess_match: [bank_acct][%d]bank_acct_num is missing!!\n",iBankAcctCnt));
ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_num is missing!!\n");
                        iRet = FAILURE;
                }

        	hRawStmtRec = RecordSet_GetFirst(rRawBankStmt);
        	while ((iRet == SUCCESS) && (hRawStmtRec)) {

			int iStmtFileValid = PD_FALSE;
	
			char csRawBankInit[IMPORT_MAX_FILE_LEN + 1];
			char csRawShortBankAcctNum[IMPORT_MAX_FILE_LEN + 1];

			memset(csRawBankInit, 0, sizeof(csRawBankInit));
			memset(csRawShortBankAcctNum, 0, sizeof(csRawShortBankAcctNum));

/* stmt_file_valid */
			if (GetField_Int(hRawStmtRec,"stmt_file_valid",&iStmtFileValid)) {
//DEBUGLOG(("prcoess_match: [raw][%d]stmt_file_valid = [%d]\n",iRawCnt,iStmtFileValid));
                        } else {
DEBUGLOG(("prcoess_match: [raw][%d]stmt_file_valid is missing!!\n",iRawCnt));
ERRLOG("scan_bank_stmt: prcoess_match: stmt_file_valid is missing!!\n");
                                iRet = FAILURE;
                        }

			/* matching */
                        if (iStmtFileValid == PD_TRUE) {

/* bank_init */
				if (GetField_CString(hRawStmtRec,"bank_init",&csTmp)) {
        	                        sprintf(csRawBankInit, "%s", csTmp);
//DEBUGLOG(("prcoess_match: [raw][%d]bank_init = [%s]\n",iRawCnt,csRawBankInit));
        	                } else {
DEBUGLOG(("prcoess_match: [raw][%d]bank_init is missing!!\n",iRawCnt));
ERRLOG("scan_bank_stmt: prcoess_match: bank_init is missing!!\n");
        	                        iRet = FAILURE;
        	                }

/* last_6_bank_acct_num */
        	        	if (GetField_CString(hRawStmtRec,"last_6_bank_acct_num",&csTmp)) {
		                        sprintf(csRawShortBankAcctNum, "%s", csTmp);
//DEBUGLOG(("prcoess_match: [raw][%d]last_6_bank_acct_num = [%s]\n",iRawCnt,csRawShortBankAcctNum));
	        	        } else {
DEBUGLOG(("prcoess_match: [raw][%d]last_6_bank_acct_num is missing!!\n",iRawCnt));
ERRLOG("scan_bank_stmt: prcoess_match: last_6_bank_acct_num is missing!!\n");
					iRet = FAILURE;
	        	        }

				if (!strcmp(csBankAbbrevName, csRawBankInit)) {
					csTmp = strstr(csBankAcctNum, csRawShortBankAcctNum);
                        		if (csTmp != NULL) {	
	
						hash_t *hMatchRec = NULL;
        					hMatchRec = (hash_t*) malloc (sizeof(hash_t));
        					hash_init(hMatchRec,0);

/* bank_disabled */
                                                if (GetField_Int(hBankAcctRec,"bank_disabled",&iTmp)) {
                                                        PutField_Int(hMatchRec, "bank_disabled", iTmp);
                                                } else {
							PutField_Int(hMatchRec, "bank_disabled", PD_TRUE);
						}

/* bank_acct_disabled */
                                                if (GetField_Int(hBankAcctRec,"bank_acct_disabled",&iTmp)) {
                                                        PutField_Int(hMatchRec, "bank_acct_disabled", iTmp);
						} else {
							PutField_Int(hMatchRec, "bank_acct_disabled", PD_TRUE);
						}

/* bank_name */
                                                if (GetField_CString(hBankAcctRec,"bank_name",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_name", csTmp);
                                                }

/* bank_acct_num */
                                                if (GetField_CString(hBankAcctRec,"bank_acct_num",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_acct_num", csTmp);
                                                }

/* bank_acct_name */
                                                if (GetField_CString(hBankAcctRec,"bank_acct_name",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_acct_name", csTmp);
                                                }

/* bank_acct_short_name */
                                                if (GetField_CString(hBankAcctRec,"bank_acct_short_name",&csTmp)) {
                                                        PutField_CString(hMatchRec, "bank_acct_short_name", csTmp);
                                                }

/* bank_stmt_file */
						if (GetField_CString(hRawStmtRec,"bank_stmt_file",&csTmp)) {
							PutField_CString(hMatchRec, "match_stmt_file", csTmp);
						}

	                	        	RecordSet_Add(rMatch, hMatchRec);

						//hash_destroy(hMatchRec);
                        	        	//FREE_ME(hMatchRec);
					}
				}
			}

			iRawCnt++;

                	hRawStmtRec = RecordSet_GetNext(rRawBankStmt);
        	}

		iBankAcctCnt++;

                hBankAcctRec = RecordSet_GetNext(rBankAcctDetail);
        }

	// Check Matching Results
	if (iRet == SUCCESS) {

		hash_t *hMatchRec = NULL;
            	hMatchRec = (hash_t*) malloc (sizeof(hash_t));
            	hash_init(hMatchRec,0);

		hRawStmtRec = RecordSet_GetFirst(rRawBankStmt);
                while ((iRet == SUCCESS) && (hRawStmtRec)) {

			int iStmtFileValid = PD_FALSE;
			int iMatch = PD_FALSE;

			char *csBankStmtFile = NULL;

/* stmt_file_valid */
                        if (!GetField_Int(hRawStmtRec,"stmt_file_valid",&iStmtFileValid)) {
DEBUGLOG(("prcoess_match: stmt_file_valid is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: stmt_file_valid is missing!!\n");
                                iRet = FAILURE;
                        }

/* bank_stmt_file */
                        if (!GetField_CString(hRawStmtRec,"bank_stmt_file",&csBankStmtFile)) {
DEBUGLOG(("prcoess_match: bank_stmt_file is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_stmt_file is missing!!\n");
                                iRet = FAILURE;
                        }

			// Get Matching Record
			if (iRet == SUCCESS) {
			
				if (iStmtFileValid == PD_TRUE) {
			
					hMatchRec = RecordSet_GetFirst(rMatch);
                			while ((iRet == SUCCESS) && (hMatchRec)) {		
			
						int iBankDisabled = PD_FALSE;	
						int iBankAcctDisabled = PD_FALSE;	
						char *csMatchStmtFile = NULL;

/* match_stmt_file */
              		      			if (!GetField_CString(hMatchRec,"match_stmt_file",&csMatchStmtFile)) {
DEBUGLOG(("prcoess_match: match_stmt_file is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: match_stmt_file is missing!!\n");
                        	        		iRet = FAILURE;
                        			}

/* bank_disabled */
						if (!GetField_Int(hMatchRec,"bank_disabled",&iBankDisabled)) {
DEBUGLOG(("prcoess_match: bank_disabled is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_disabled is missing!!\n");
                                                        iRet = FAILURE;
                                                }

/* bank_acct_disabled */
						if (!GetField_Int(hMatchRec,"bank_acct_disabled",&iBankAcctDisabled)) {
DEBUGLOG(("prcoess_match: bank_acct_disabled is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_disabled is missing!!\n");
                                                        iRet = FAILURE;
                                                }

						if (iRet == SUCCESS) {

							if ((!strcmp(csBankStmtFile, csMatchStmtFile))
							    && (iBankDisabled == PD_FALSE)
							    && (iBankAcctDisabled == PD_FALSE)
							
							) 
							{
						
								/* Matched */
								iMatch = PD_TRUE;
								break;
							}
						}

						hMatchRec = RecordSet_GetNext(rMatch);
					}
				}	
			}

			// Put into Process Recordset or Skip
			if (iRet == SUCCESS) {

				if ((iStmtFileValid == PD_TRUE)
				    && (iMatch == PD_FALSE)
				) 
				{			
					// Skip
DEBUGLOG(("prcoess_match: [Skip][%d][bank_stmt_file] = [%s]\n",iSkipCnt,csBankStmtFile));

					iSkipCnt++;

				} else {

					/* Put into Process Recordset */

                            		hash_t *hProcessStmtRec = NULL;
                           		hProcessStmtRec = (hash_t*) malloc (sizeof(hash_t));
                            		hash_init(hProcessStmtRec,0);

/* bank_stmt_file */
                                        PutField_CString(hProcessStmtRec, "bank_stmt_file", csBankStmtFile);
DEBUGLOG(("prcoess_match: [Matched][%d][bank_stmt_file] = [%s]\n",iMatchCnt,csBankStmtFile));

/* acct_type */
                            		if (GetField_CString(hRawStmtRec,"acct_type",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][acct_type] = [%s]\n",iMatchCnt,csTmp));
                                    		PutField_CString(hProcessStmtRec, "acct_type", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: acct_type is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: acct_type is missing!!\n");
                                     		iRet = FAILURE;
                             		}

/* client_id */
                             		if (GetField_CString(hRawStmtRec,"client_id",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][client_id] = [%s]\n",iMatchCnt,csTmp));
                                    		PutField_CString(hProcessStmtRec, "client_id", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: client_id is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: client_id is missing!!\n");
                                      		iRet = FAILURE;
                           		}

/* client_name */
                            		if (GetField_CString(hRawStmtRec,"client_name",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][client_name] = [%s]\n",iMatchCnt,csTmp));
                                	    	PutField_CString(hProcessStmtRec, "client_name", csTmp);
                            		} else {
DEBUGLOG(("prcoess_match: client_name is missing!!\n"));
ERRLOG("scan_bank_stmt: prcoess_match: client_name is missing!!\n");
                                      		iRet = FAILURE;
                              		}

/* bank_init */
                             		if (GetField_CString(hRawStmtRec,"bank_init",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][bank_init] = [%s]\n",iMatchCnt,csTmp));
                                    		PutField_CString(hProcessStmtRec, "bank_init", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: [Matched][%d]bank_init is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_init is missing!!\n");
                                       		//iRet = FAILURE;
                           		}

/* bank_acct_init */
                             		if (GetField_CString(hRawStmtRec,"bank_acct_init",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][bank_acct_init] = [%s]\n",iMatchCnt,csTmp));
                                	    	PutField_CString(hProcessStmtRec, "bank_acct_init", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: [Matched][%d]bank_acct_init is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_init is missing!!\n");
                                	     	//iRet = FAILURE;
                             		}

/* last_6_bank_acct_num */
                           		if (GetField_CString(hRawStmtRec,"last_6_bank_acct_num",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][last_6_bank_acct_num] = [%s]\n",iMatchCnt,csTmp));
                                	  	PutField_CString(hProcessStmtRec, "last_6_bank_acct_num", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: [Matched][%d]last_6_bank_acct_num is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: last_6_bank_acct_num is missing!!\n");
                                	     	//iRet = FAILURE;
                            		}

/* datetime */
                            		if (GetField_CString(hRawStmtRec,"datetime",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][datetime] = [%s]\n",iMatchCnt,csTmp));
                                	       	PutField_CString(hProcessStmtRec, "datetime", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: [Matched][%d]datetime is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: datetime is missing!!\n");
                                     		//iRet = FAILURE;
                              		}

/* file_ext */
                            		if (GetField_CString(hRawStmtRec,"file_ext",&csTmp)) {
//DEBUGLOG(("prcoess_match: [Matched][%d][file_ext] = [%s]\n",iMatchCnt,csTmp));
                                	    	PutField_CString(hProcessStmtRec, "file_ext", csTmp);
                             		} else {
DEBUGLOG(("prcoess_match: [Matched][%d]file_ext is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: file_ext is missing!!\n");
                                	      	//iRet = FAILURE;
                            		}

					if (iStmtFileValid == PD_TRUE) {
/* bank_disabled */
                                        	if (GetField_Int(hMatchRec,"bank_disabled",&iTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][bank_disabled] = [%d]\n",iMatchCnt,iTmp));
                                        	        PutField_Int(hProcessStmtRec, "bank_disabled", iTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Matched][%d]bank_disabled is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_disabled is missing!!\n");
                                        	        //iRet = FAILURE;
                                        	}

/* bank_acct_disabled */
                                        	if (GetField_Int(hMatchRec,"bank_acct_disabled",&iTmp)) {
DEBUGLOG(("prcoess_match: [Matched][%d][bank_acct_disabled] = [%d]\n",iMatchCnt,iTmp));
                                        	        PutField_Int(hProcessStmtRec, "bank_acct_disabled", iTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Matched][%d]bank_acct_disabled is missing!!\n",iMatchCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_disabled is missing!!\n");
                                        	        //iRet = FAILURE;
                                        	}
					}

					iMatchCnt++;

					RecordSet_Add(rProcessBankStmt, hProcessStmtRec);

                               		//hash_destroy(hProcessStmtRec);
                              		//FREE_ME(hProcessStmtRec);

				}
			}

              		hRawStmtRec = RecordSet_GetNext(rRawBankStmt);
		}
	}

	if (iRet == SUCCESS) {

                if (iMatchCnt > 0) {
DEBUGLOG(("process_match: match_cnt = [%d]\n",iMatchCnt));
                } else {
DEBUGLOG(("process_match: match_cnt = [0]\n"));

			iTmpRet = update_job(PD_AUTO_UPL_JOB_STATUS_COMPLETE);
			
			iRet = FAILURE;
                }
        
	}

	return iRet;
}


