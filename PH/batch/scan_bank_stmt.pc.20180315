char *CS_SCAN_DETAIL_CHECK_TYPE[] = {"ALL_CHAR", NULL, "DIGIT", "DIGIT"};
int INT_SCAN_DETAIL_CHECK_TYPE[IMPORT_MAX_FIELD] = {1, -1, 1, 1};
int INT_SCAN_DETAIL_MAX_LEN[IMPORT_MAX_FIELD] = {0, 0, 6, 10};
