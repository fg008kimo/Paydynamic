int process_raw(hash_t *hData,FILE *fraw,recordset_t *rRawBankStmt)
{
	int     iRet = PD_NOT_FOUND;
	int     iDtlRet = PD_OK;
        
	int     iFieldCnt = 0;
        int     iStmtCnt = 0;
        int     iStmtValidCnt = 0;
	char    *p;
	char    csTmp[PD_TMP_BUF_LEN + 1];
        char    csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN];
        char    cs_input_buf[PD_MAX_BUFFER+1];
        char    cs_bank_stmt_file[PD_MAX_BUFFER+1];
        char    cs_file_ext[IMPORT_MAX_FILE_LEN+1];

	memset(cs_input_buf, 0, sizeof(cs_input_buf));
	memset(cs_bank_stmt_file, 0, sizeof(cs_bank_stmt_file));
	memset(cs_file_ext, 0, sizeof(cs_file_ext));

        while ((iDtlRet == PD_OK) && (fgets(cs_input_buf,PD_MAX_BUFFER, fraw) != NULL)) {

		int 	iStmtFileSkip = PD_FALSE;
		int     iStmtFileValid = PD_TRUE;

                iFieldCnt = 0;

		// Check 0x0A
                if (cs_input_buf[strlen(cs_input_buf) - 1] == 0x0A) {
                        cs_input_buf[strlen(cs_input_buf) - 1] = '\0';
		}

		// Trim OxOD
		strcpy(cs_input_buf,TrimAllChar((const unsigned char*)cs_input_buf,strlen(cs_input_buf),PD_CHAR));
DEBUGLOG(("process_raw: [%d][ORG_INPUT_FILE]=[%s]\n",iStmtCnt,cs_input_buf));

		// Copy to cs_bank_stmt_file
		strcpy(cs_bank_stmt_file, cs_input_buf);

		// Trim Extension
               	p = mystrtok(cs_input_buf,PD_EXT_DELIMITOR);
            	if (p == NULL) {
               		iDtlRet = PD_ERR;
              	} else {
               		while ((p = mystrtok(NULL,PD_EXT_DELIMITOR)) != NULL) {
                	    	strcpy(cs_file_ext,p);
               		}
DEBUGLOG(("process_raw: [%d][INPUT_FILE]=[%s][%s]\n",iStmtCnt,cs_input_buf,cs_file_ext));
		}

		// Verify Raw Stmt File Name
		if (iDtlRet == PD_OK) {
                	iStmtFileValid = check_filename(cs_bank_stmt_file, cs_input_buf);
		}

		// Verify Raw Stmt File Detail
		if ((iDtlRet == PD_OK) && (iStmtFileValid == PD_TRUE)) {
			
			// Breakdown Raw Stmt File Name
                       	p = mystrtok(cs_input_buf,PD_RAW_DELIMITOR);
                    	if (p == NULL) {
                             	iDtlRet = PD_ERR;
                    	} else {
                               	strcpy(csList[iFieldCnt],p);
                               	iFieldCnt++;

                  		while ((p = mystrtok(NULL,PD_RAW_DELIMITOR)) != NULL) {
                              		strcpy(csList[iFieldCnt],p);
                               		iFieldCnt++;
                             	}
                      	}

                        // Check Number of Field Valid
                      	if (iDtlRet == PD_OK) {
				iStmtFileValid = check_detail(iFieldCnt, csList);
			}
		}

		// Verify Raw Stmt File Field - Bank Init
		if ((iDtlRet == PD_OK) && (iStmtFileValid == PD_TRUE)) {

			int iTmpRet = FOUND;

			// Check All Support Offline Bank Name
			DBObjPtr = CreateObj(DBPtr,"DBBankDesc","FindOfflineBankByBankAbbrevName");
                        iTmpRet = (unsigned long)(DBObjPtr)(csList[IDX_BANK_INIT],csTmp); 
			if (iTmpRet == FOUND) {
DEBUGLOG(("process_raw: [%d]DBBankDesc: FindOfflineBankByBankAbbrevName Found\n", iStmtCnt));
                        } else if (iTmpRet == NOT_FOUND) {
DEBUGLOG(("process_raw: [%d]DBBankDesc: FindOfflineBankByBankAbbrevName Not Found\n", iStmtCnt));
                        	iStmtFileValid = PD_FALSE;
			} else {
DEBUGLOG(("process_raw: [%d]DBBankDesc: FindOfflineBankByBankAbbrevName Failed!!\n", iStmtCnt));
ERRLOG("scan_bank_stmt: process_raw: DBBankDesc: FindOfflineBankByBankAbbrevName Failed!!\n");
                        	iDtlRet = PD_ERR;
			}

			// Check Process Bank Name
			if ((iDtlRet == PD_OK) && (iStmtFileValid == PD_TRUE)) {

				char *csBankShortName = NULL;

/* bank_short_name */
                                if (GetField_CString(hData,"bank_short_name",&csBankShortName)) {
DEBUGLOG(("process_raw: [%d]bank_short_name = [%s]\n",iStmtCnt,csBankShortName));

                                        if (strcmp(csBankShortName, csList[IDX_BANK_INIT])) {
                                                iStmtFileSkip = PD_TRUE;
DEBUGLOG(("process_raw: [%d]Bank Statement File Skip\n", iStmtCnt));
                                        }
                                } else {
DEBUGLOG(("process_raw: [%d]bank_short_name is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_raw: bank_short_name is missing!!\n");
                                        iDtlRet = PD_ERR;
                                }
			}
		}

		if ((iDtlRet == PD_OK) && (iStmtFileSkip == PD_FALSE)) {
			
			char    *csAcctType = NULL;
       	 		char    *csProviderId = NULL;
       	 		char    *csProviderName = NULL;
        		char    *csBankCode = NULL;

			hash_t  *hRawStmtRec = NULL;
	             	hRawStmtRec = (hash_t*) malloc (sizeof(hash_t));
	             	hash_init(hRawStmtRec,0);

/* acct_type */
	               	if (GetField_CString(hData,"acct_type",&csAcctType)) {
	                       	PutField_CString(hRawStmtRec, "acct_type", csAcctType);
DEBUGLOG(("process_raw: [%d]acct_type = [%s]\n",iStmtCnt,csAcctType));
	               	} else {
DEBUGLOG(("process_raw: [%d]acct_type is missing!!\n",iStmtCnt));
ERRLOG("scan_bank_stmt: process_raw: acct_type is missing!!\n");
				iDtlRet = PD_ERR;
	               	}

/* provider_id */
	               	if (GetField_CString(hData,"client_id",&csProviderId)) {
	                       	PutField_CString(hRawStmtRec, "client_id", csProviderId);
