/* error_code */
						PutField_Int(hErrorStmtRec, "error_code", INT_AUTO_UPLOAD_BANK_ACCT_DIS);
DEBUGLOG(("prcoess_match: [Error][%d][error_code] = [%d]\n",iErrorCnt,INT_AUTO_UPLOAD_BANK_ACCT_DIS));	

/* bank_name */
                                        	if (GetField_CString(hMatchRec,"bank_name",&csTmp)) {
DEBUGLOG(("prcoess_match: [Error][%d][bank_name] = [%s]\n",iErrorCnt,csTmp));
                                        	      PutField_CString(hErrorStmtRec, "bank_name", csTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Error][%d]bank_name is missing!!\n",iErrorCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_name is missing!!\n");
                                        	        //iRet = FAILURE;
                                        	}

/* bank_acct_num */
                                        	if (GetField_CString(hMatchRec,"bank_acct_num",&csTmp)) {
DEBUGLOG(("prcoess_match: [Error][%d][bank_acct_num] = [%s]\n",iErrorCnt,csTmp));
                                        	        PutField_CString(hErrorStmtRec, "bank_acct_num", csTmp);
                                        	} else {
