//ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_num is missing!!\n");
                                        	        //iRet = FAILURE;
                                        	}

/* bank_acct_name */
                                        	if (GetField_CString(hMatchRec,"bank_acct_name",&csTmp)) {
DEBUGLOG(("prcoess_match: [Error][%d][bank_acct_name] = [%s]\n",iErrorCnt,csTmp));
                                        	        PutField_CString(hErrorStmtRec, "bank_acct_name", csTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Error][%d]bank_acct_name is missing!!\n",iErrorCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_name is missing!!\n");
                                        	        //iRet = FAILURE;
                                        	}

/* bank_acct_short_name */
                                        	if (GetField_CString(hMatchRec,"bank_acct_short_name",&csTmp)) {
DEBUGLOG(("prcoess_match: [Error][%d][bank_acct_short_name] = [%s]\n",iErrorCnt,csTmp));
                                        	        PutField_CString(hErrorStmtRec, "bank_acct_short_name", csTmp);
                                        	} else {
DEBUGLOG(("prcoess_match: [Error][%d]bank_acct_short_name is missing!!\n",iErrorCnt));
//ERRLOG("scan_bank_stmt: prcoess_match: bank_acct_short_name is missing!!\n");
                                        	        //iRet = FAILURE;
                                        	}
					} else {
/* error_code */
						PutField_Int(hErrorStmtRec, "error_code", INT_INVALID_FILE_FORMAT);
DEBUGLOG(("prcoess_match: [Error][%d][error_code] = [%d]\n",iErrorCnt,INT_INVALID_FILE_FORMAT));
					}
		
					iErrorCnt++;

					RecordSet_Add(rErrorBankStmt, hErrorStmtRec);

                              		//hash_destroy(hErrorStmtRec);
                              		//FREE_ME(hErrorStmtRec);
				}
			}

              		hRawStmtRec = RecordSet_GetNext(rRawBankStmt);
		}
	}

	return iRet;
}


