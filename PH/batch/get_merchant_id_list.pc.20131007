int batch_terminate(int argc, char* argv[])
{
    return SUCCESS;
}


int batch_proc(int argc, char* argv[])
{
        EXEC SQL WHENEVER SQLERROR GOTO sql_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;
		char            hv_status;
		int             hv_disabled;

		varchar         v_merch_id[PD_MERCHANT_ID_LEN+1];

		short           ind_merch_id = -1;
        EXEC SQL END DECLARE SECTION;

        hv_status = 'O';
        hv_disabled = 0;

        EXEC SQL DECLARE c_cursor_getmerchantid CURSOR FOR
                select  b.merchant_id
                from    clients a,
                        merch_detail b
                where   a.status = :hv_status
		and 	b.status = :hv_status
                and     b.client_id = a.client_id
                and     b.disabled = :hv_disabled
		and	b.merchant_id not in ('M8000006','M8000007','M8000021','M8000032');

        EXEC SQL OPEN c_cursor_getmerchantid;
        do{
                EXEC SQL FETCH c_cursor_getmerchantid
                INTO    :v_merch_id:ind_merch_id;


                if (SQLCODE == SQL_NOT_FOUND) {
                        break;
                }
		
		printf("%.*s\n",v_merch_id.len,v_merch_id.arr);

	}while(PD_TRUE);


        EXEC SQL CLOSE c_cursor_getmerchantid;

        return SUCCESS;
sql_error:
    DEBUGLOG(("sql_error code %d\n", sqlca.sqlcode));
    DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
    EXEC SQL WHENEVER SQLERROR CONTINUE;
    EXEC SQL CLOSE c_cursor_getmerchantid;
    EXEC SQL ROLLBACK RELEASE;
    return FAILURE;
}
