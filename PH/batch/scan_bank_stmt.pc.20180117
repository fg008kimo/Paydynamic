int batch_init(int argc, char* argv[])
{
    if (argc < 5) {
    	printf("usage: -r raw_file -o process_file -e error_file -a acct_type -p provider_name\n");
        return FAILURE;
    }
    else
        return SUCCESS;
}


int batch_proc(int argc, char* argv[])
{
	int	iRet;

	char	csAcctType[PD_ACCT_TYPE_LEN + 1];
	char 	csProviderId[PD_CLIENT_ID_LEN + 1];
	char	csBankCode[PD_BANK_CODE_LEN + 1];

	FILE	*fraw,*fproc,*ferr;	

	hash_t  *hData = NULL;
        hData = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hData,0);

        recordset_t *rBankAcctDetail;
        rBankAcctDetail = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rBankAcctDetail, 0);

	recordset_t *rRawBankStmt;
        rRawBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rRawBankStmt, 0);

      	recordset_t *rProcessBankStmt;
        rProcessBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rProcessBankStmt, 0);

        recordset_t *rErrorBankStmt;
        rErrorBankStmt = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rErrorBankStmt, 0);

        recordset_t *rMissingBankAcctDetail;
        rMissingBankAcctDetail = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rMissingBankAcctDetail, 0);
	
DEBUGLOG(("batch_proc: start\n"));

	memset(csAcctType, 0, sizeof(csAcctType));
	memset(csProviderId, 0, sizeof(csProviderId));
	memset(csBankCode, 0, sizeof(csBankCode));

	iRet = parse_arg(argc,argv);

	if (iRet != SUCCESS) {
    		printf("*usage: -r raw_file -o process_file -e error_file -a acct_type -p provider_name\n");
		return (iRet);
	}

DEBUGLOG(("batch_proc: raw_file = [%s]\n",cs_raw_file));
DEBUGLOG(("batch_proc: process_file = [%s]\n",cs_process_file));
DEBUGLOG(("batch_proc: error_file = [%s]\n",cs_error_file));
DEBUGLOG(("batch_proc: acct_type_name = [%s]\n",cs_acct_type_name));
DEBUGLOG(("batch_proc: provider_name = [%s]\n",cs_provider_name));
DEBUGLOG(("batch_proc: bank_short_name = [%s]\n",cs_bank_short_name));

	fraw = fopen(cs_raw_file,"r");
	if (fraw == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_raw_file));
		return FAILURE;
	}

	fproc = fopen(cs_process_file,"w");
	if (fproc == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_process_file));
		fclose(fraw);
		return FAILURE;
	}

	ferr = fopen(cs_error_file,"w");
        if (ferr == NULL) {
DEBUGLOG(("batch_proc: Error Opening file = [%s]\n",cs_error_file));
                fclose(fraw);
                fclose(fproc);
                return FAILURE;
        }

	// Get Acct Type
	if (iRet == SUCCESS) {
		if (!strcmp(cs_acct_type_name, "Deposit_Statement")) {
			sprintf(csAcctType, "%s", PD_NATURE_DEPOSIT);	
			PutField_CString(hData, "acct_type", csAcctType);
DEBUGLOG(("batch_proc: acct_type = [%s]\n", csAcctType));
		} else if (!strcmp(cs_acct_type_name, "Payout_Statement")) {
			sprintf(csAcctType, "%s", PD_NATURE_PAYOUT);	
			PutField_CString(hData, "acct_type", csAcctType);
DEBUGLOG(("batch_proc: acct_type = [%s]\n", csAcctType));
		} else if (!strcmp(cs_acct_type_name, "Intermediate_Statement")) {
			sprintf(csAcctType, "%s", PD_NATURE_INTERMEDIATE);	
			PutField_CString(hData, "acct_type", csAcctType);
DEBUGLOG(("batch_proc: acct_type = [%s]\n", csAcctType));
		} else {
DEBUGLOG(("batch_proc: acct_type invalid\n"));
			iRet = FAILURE;
		}
	}

        // Get Provider Id
        if (iRet == SUCCESS) {
                DBObjPtr = CreateObj(DBPtr,"DBPspMaster","GetClientIdByClientName");
                if ((unsigned long)(DBObjPtr)(cs_provider_name,csProviderId) == PD_OK) {
DEBUGLOG(("batch_proc: DBPspMaster: GetClientIdByClientName: client_id = [%s]\n", csProviderId));
			PutField_CString(hData, "client_id", csProviderId);
			PutField_CString(hData, "client_name", cs_provider_name);
                } else {
DEBUGLOG(("batch_proc: DBPspMaster: GetClientIdByClientName Failed\n"));
ERRLOG("scan_bank_stmt: batch_proc: DBPspMaster: GetClientIdByClientName Failed\n");
                        iRet = FAILURE;
                }
        }

	// Get Bank Code
	if (iRet == SUCCESS) {
		//if (cs_bank_short_name == ) 
		{
			DBObjPtr = CreateObj(DBPtr,"DBBankDesc","FindBankCodeByBankAbbrevName");
                	if ((unsigned long)(DBObjPtr)(cs_bank_short_name,csBankCode) == PD_OK) {
				PutField_CString(hData, "int_bank_code", csBankCode);
DEBUGLOG(("batch_proc: DBBankDesc: FindBankCodeByBankAbbrevName: int_bank_code = [%s]\n", csBankCode));
                	} else {
DEBUGLOG(("batch_proc: DBBankDesc: FindBankCodeByBankAbbrevName Failed\n"));
ERRLOG("scan_bank_stmt: batch_proc: DBBankDesc: FindBankCodeByBankAbbrevName Failed\n");
                	        iRet = FAILURE;
                	}
		}
	}
	
	// Process Bank Acct
	if (iRet == SUCCESS) {
		iRet = process_bank_acct(hData,rBankAcctDetail);
	}

	// Process File
        if (iRet == SUCCESS) {
                iRet = process_raw(hData,fraw,rRawBankStmt,rErrorBankStmt);
        }

	// Process Matching
	if (iRet == SUCCESS) {
		iRet = prcoess_match(rBankAcctDetail,rRawBankStmt,rProcessBankStmt,rMissingBankAcctDetail);
	}

	// Process Results
	if (iRet == SUCCESS) {
		iRet = process_output(rProcessBankStmt,fproc);

		if (iRet == SUCCESS) {
			iRet = process_error_file(rErrorBankStmt,ferr);
		}
	}

	hash_destroy(hData);
        FREE_ME(hData);	
	
	RecordSet_Destroy(rBankAcctDetail);
        FREE_ME(rBankAcctDetail);

	RecordSet_Destroy(rRawBankStmt);
        FREE_ME(rRawBankStmt);

	RecordSet_Destroy(rProcessBankStmt);
	FREE_ME(rProcessBankStmt);

	RecordSet_Destroy(rErrorBankStmt);
        FREE_ME(rErrorBankStmt);

	RecordSet_Destroy(rMissingBankAcctDetail);
        FREE_ME(rMissingBankAcctDetail);

	fclose(fraw);
	fclose(fproc);
	fclose(ferr);

DEBUGLOG(("batch_proc: iRet = [%d]\n", iRet));
	return iRet;
}


int batch_terminate(int argc, char* argv[])
{
	return SUCCESS;
}

                        
int parse_arg(int argc,char **argv)
{               
        char    c;
	strcpy(cs_raw_file,"");
        strcpy(cs_process_file,"");
        strcpy(cs_error_file,"");
        strcpy(cs_acct_type_name,"");
	strcpy(cs_provider_name,"");
        strcpy(cs_bank_short_name,"");
                        
        while ((c = getopt(argc,argv,"r:o:e:a:p:b:")) != EOF) {
                switch (c) {
			case 'r':
                                strcpy(cs_raw_file, optarg);
                                break;
                        case 'o':
                                strcpy(cs_process_file, optarg);
                                break;
			case 'e':
                                strcpy(cs_error_file, optarg);
                                break;
			case 'a':
                                strcpy(cs_acct_type_name, optarg);
                                break;
			case 'p':
                                strcpy(cs_provider_name, optarg);
                                break;
			case 'b':
                                strcpy(cs_bank_short_name, optarg);
                                break;
                        default:
                                return FAILURE;
                }
        }       
        
        if (!strcmp(cs_raw_file,"") || !strcmp(cs_process_file,"") || !strcmp(cs_error_file,"") || !strcmp(cs_acct_type_name,"") || !strcmp(cs_provider_name,""))
                return FAILURE;
                
        return SUCCESS; 
}               


int check_detail(char csList[IMPORT_MAX_FIELD][IMPORT_FIELD_LEN])
{
	int iRet = PD_TRUE;
	int i,j = 0;

DEBUGLOG(("******************** check_detail::start ********************\n"));

	for (i=0;i<IMPORT_MAX_FIELD;i++) {

DEBUGLOG(("check_detail:: %s=[%s](%d)\n", CS_FIELD_NAME[i], csList[i], (int)strlen(csList[i])));

		// Check Length
		if (INT_DETAIL_CHECK_LEN[i] != -1) {
			if (strlen(csList[i]) != INT_DETAIL_MAX_LEN[i]) {
DEBUGLOG(("check_detail:: Length [%d] NOT Match [%d]\n", (int)strlen(csList[i]), INT_DETAIL_MAX_LEN[i]));
				iRet = PD_FALSE;
			} else {
			}
		} else {
//DEBUGLOG(("check_detail:: SKIP Check Length \n"));	
		}

		// Check Content
		if (INT_DETAIL_CHECK_TYPE[i] != -1) {
			if (!strcmp(CS_DETAIL_CHECK_TYPE[i],"ENGLISH_CHAR")) {

				for (j=0;j<strlen(csList[i]);j++) {

                        		if (!(csList[i][j] >= 'a' && csList[i][j] <= 'z') &&
					    !(csList[i][j] >= 'A' && csList[i][j] <= 'Z')
					) {
DEBUGLOG(("check_detail:: [%c] NOT Match [a-zA-Z]\n", csList[i][j]));
						iRet = PD_FALSE;
                                		break;
                        		} else {
                        		}
                		}

			} else if (!strcmp(CS_DETAIL_CHECK_TYPE[i],"DIGIT")) {
	
                        	for (j=0;j<strlen(csList[i]);j++) {

                        	        if (!(csList[i][j] >= '0' && csList[i][j] <= '9')) {
DEBUGLOG(("check_detail:: [%c] NOT Match [0-9]\n", csList[i][j]));
						iRet = PD_FALSE;	
                        	                break;
                        	        } else {
                        	        }
                       	 	}
			}
		} else {
//DEBUGLOG(("check_detail:: SKIP Check Content \n"));
		}
	}

DEBUGLOG(("******************** check_detail::end ********************\n"));

	return iRet;
}


