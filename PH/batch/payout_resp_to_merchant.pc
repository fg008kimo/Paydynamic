                        v_province.arr[0] = '\0';
                fprintf(fp,"%.*s%s",v_province.len,v_province.arr,PD_MY_DELIMITOR);

/* Field #11 city*/
                if (ind_city< 0 )
                        v_city.arr[0] = '\0';
