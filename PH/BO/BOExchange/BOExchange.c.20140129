                }
        }
        else {
                strcpy(csTag,"txn_country");
                if (GetField_CString(hRequest,csTag,&csTxnCountry)) {
DEBUGLOG(("BOExchange::GetExchangeInfo() %s = [%s]\n",csTag,csTxnCountry));
                }
                else {
DEBUGLOG(("BOExchange::GetExchangeInfo() %s is missing!!!\n",csTag));
