/*Dirk Wong 20150702*/
int GetNatureBalanceForUpdate(hash_t *hContext, const hash_t* hReq) 
{
	int	iRet = PD_OK;
	double	dTmp;
	char	*csPtr;

	hash_t	*hData,*hRsp;

DEBUGLOG(("GetNatureBalanceForUpdate()\n"));
	hData = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hData,0);
	hRsp = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hRsp,0);

/* entity id */
	if (GetField_CString(hContext,"entity_id",&csPtr)) {
DEBUGLOG(("GetNatureBalanceForUpdate() Entity ID = [%s]\n",csPtr));
		PutField_CString(hData,"entity_id",csPtr);
	}
	else {
DEBUGLOG(("GetNatureBalanceForUpdate() Entity ID not found\n"));
ERRLOG("BOMMSEntityBalance::GetNatureBalanceForUpdate() Entity ID not found\n");
		iRet = PD_ERR;
	}

/* country */
	if (GetField_CString(hReq,"country",&csPtr)) {
DEBUGLOG(("GetNatureBalanceForUpdate() country = [%s]\n",csPtr));
		PutField_CString(hData,"country",csPtr);
	}
	else {
DEBUGLOG(("GetNatureBalanceForUpdate() country not found\n"));
ERRLOG("BOMMSEntityBalance::GetNatureBalanceForUpdate() country ID not found\n");
		iRet = PD_ERR;
	}


/* ccy */
	if (iRet == PD_OK ) {
		if (GetField_CString(hReq,"ccy",&csPtr)) {
DEBUGLOG(("GetNatureBalanceForUpdate() ccy = [%s]\n",csPtr));
			PutField_CString(hData,"ccy",csPtr);
		}
		else {
DEBUGLOG(("GetNatureBalanceForUpdate() ccy to found\n"));
ERRLOG("BOMMSEntityBalance::GetNatureBalanceForUpdate() ccy not found\n");
			iRet = PD_ERR;
		}
	}

/* nature_id */
	if (iRet == PD_OK ) {
		if (GetField_CString(hReq,"nature_id",&csPtr)) {
DEBUGLOG(("GetNatureBalanceForUpdate() nature_id = [%s]\n",csPtr));
			PutField_CString(hData,"nature_id",csPtr);
		}
		else {
DEBUGLOG(("GetNatureBalanceForUpdate() nature_id to found\n"));
ERRLOG("BOMMSEntityBalance::GetNatureBalanceForUpdate() nature_id not found\n");
			iRet = PD_ERR;
		}
	}

/* create entity balance for opening balance */
	if (iRet == PD_OK) {
		DBObjPtr = CreateObj(DBPtr,"DBMmsEntityBalance","GetNatureBalanceForUpdate");
		iRet = (unsigned long)((DBObjPtr)(hData,hRsp));

		if (iRet == PD_OK) {
/*opening_acct_bal */
			if (GetField_Double(hRsp,"acct_bal",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_acct_bal = [%lf]\n",dTmp));
				PutField_Double(hContext,"open_acct_bal",dTmp);
			}

/*opening_prepaid */
			if (GetField_Double(hRsp,"prepaid",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_prepaid = [%lf]\n",dTmp));
				PutField_Double(hContext,"open_prepaid",dTmp);
			}

/*opening_intransit */
			if (GetField_Double(hRsp,"intransit",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_intransit = [%lf]\n",dTmp));
				PutField_Double(hContext,"open_intransit",dTmp);
			}
/*opening_lien */
			if (GetField_Double(hRsp,"lien",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_lien = [%lf]\n",dTmp));
				PutField_Double(hContext,"open_lien",dTmp);
			}


		}
	}

	FREE_ME(hData);
	FREE_ME(hRsp);
DEBUGLOG(("GetNatureBalanceForUpdate:: () Noraml Exit iRet = [%d]\n",iRet));
	return iRet;
}
int UpdateNatureAcctBal(hash_t* hContext, const hash_t* hReq)
{
	int	iRet = PD_OK;
	char	*csPtr;
	char	*csBalAmt;
	char	*csAmtType;
	char	*csTxnCcy;
	char	*csEntityId;
	char	*csNatureId;
	char	*csTxnCountry;
	char	csTag[PD_TAG_LEN + 1];
	double	dTxnAmt,dNatureTxnAmt;
	double	dTmp;
	int 	i,iBalCnt = 0;

	hash_t	*hData,*hRsp;

DEBUGLOG(("UpdateNatureAcctBal:: ()\n"));
	hData = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hData,0);
	hRsp = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hRsp,0);

/* entity id */
        if (GetField_CString(hContext,"entity_id",&csEntityId)) {
DEBUGLOG(("UpdateNatureAcctBal() Entity ID = [%s]\n",csEntityId));
      	}
       	else {
DEBUGLOG(("UpdateNatureAcctBal() Entity ID nto found\n"));
ERRLOG("BOMMSEntityAcctBal::UpdateNatureAcctBal() Entity ID not found\n");
       		iRet = PD_ERR;
       	}

/* nature id */
        if (GetField_CString(hReq,"nature_id",&csNatureId)) {
DEBUGLOG(("UpdateNatureAcctBal() Nature ID = [%s]\n",csNatureId));
      	}
       	else {
DEBUGLOG(("UpdateNatureAcctBal() Nature ID nto found\n"));
ERRLOG("BOMMSEntityBalance::UpdateNatureAcctBal() Nature ID not found\n");
       		iRet = PD_ERR;
       	}

/* txn_country */
       	if (GetField_CString(hReq,"country",&csTxnCountry)) {
DEBUGLOG(("UpdateNatureAcctBal() txn_country = [%s]\n",csTxnCountry));
               	PutField_CString(hData,"country",csTxnCountry);
       	}
       	else {
DEBUGLOG(("UpdateNatureAcctBal() txn_country not found\n"));
ERRLOG("BOMMSEntityBalance::UpdateNatureAcctBal() txn_country not found\n");
               	iRet = PD_ERR;
       	}


/* ccy */
       	if (iRet == PD_OK ) {
               	if (GetField_CString(hReq,"ccy",&csTxnCcy)) {
DEBUGLOG(("UpdateNatureAcctBal() ccy = [%s]\n",csTxnCcy));
                       	PutField_CString(hData,"ccy",csTxnCcy);
               	}
               	else {
DEBUGLOG(("UpdateNatureAcctBal() ccy to found\n"));
ERRLOG("BOMMSEntityBalance::UpdateNatureAcctBal() ccy not found\n");
                       	iRet = PD_ERR;
               	}
       	}

/* bal_cnt */
	if (GetField_Int(hContext,"bal_cnt",&iBalCnt)){
DEBUGLOG(("UpdateNatureAcctBal() bal_cnt = [%d]\n",iBalCnt));
	}


	if (iRet == PD_OK) {
		PutField_CString(hData,"entity_id",csEntityId);
		PutField_CString(hData,"nature_id",csNatureId);
           	PutField_CString(hData,"country",csTxnCountry);
                PutField_CString(hData,"ccy",csTxnCcy);
DEBUGLOG(("Call DBMmsEntityBalance::GetNatureBalanceForUpdate()\n"));
	 	DBObjPtr = CreateObj(DBPtr,"DBMmsEntityBalance","GetNatureBalanceForUpdate");
                iRet = (unsigned long)((DBObjPtr)(hData,hRsp));

                if (iRet == PD_OK) {
/*curr_acct_bal */
                        if (GetField_Double(hRsp,"acct_bal",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_acct_bal = [%lf]\n",dTmp));
                                PutField_Double(hContext,"open_acct_bal",dTmp);
                        }

/*curr_prepaid */
                        if (GetField_Double(hRsp,"prepaid",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_prepaid = [%lf]\n",dTmp));
                                PutField_Double(hContext,"open_prepaid",dTmp);
                        }

/*curr_intransit */
                        if (GetField_Double(hRsp,"intransit",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_intransit = [%lf]\n",dTmp));
                                PutField_Double(hContext,"open_intransit",dTmp);
                        }
/*curr_lien */
                        if (GetField_Double(hRsp,"lien",&dTmp)) {
DEBUGLOG(("GetNatureBalanceForUpdate() open_lien = [%lf]\n",dTmp));
                                PutField_Double(hContext,"open_lien",dTmp);
                        }
                }
		else {
			iRet = INT_MMS_NATURE_BAL_NOT_FOUND;
			PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("GetNatureBalanceForUpdate() Nature Bal not found\n"));
ERRLOG("BOMMSEntityBalance:GetNatureBalanceForUpdate() Nature Bal not found\n");
		}
	}
	if  (iRet == PD_OK) {
		for (i = 0 ; i < iBalCnt; i++) {
			hash_init(hData,0);

			PutField_CString(hData,"entity_id",csEntityId);
			PutField_CString(hData,"nature_id",csNatureId);
               		PutField_CString(hData,"country",csTxnCountry);
                	PutField_CString(hData,"ccy",csTxnCcy);
/* for nature detail log */
			sprintf(csTag,"bal.%d.nature_txn_ccy",i+1);
                	PutField_CString(hContext,csTag,csTxnCcy);

			sprintf(csTag,"bal.%d.nature_net_ccy",i+1);
                	PutField_CString(hContext,csTag,csTxnCcy);



			sprintf(csTag,"bal.%d.nature_id",i+1);
/* nature_id */
                	if (GetField_CString(hContext,csTag,&csPtr)) {
DEBUGLOG(("UpdateNatureAcctBal() [%s] = [%s]\n",csTag,csPtr));
                       		PutField_CString(hData,"nature_id",csPtr);
                	}
                	else {
DEBUGLOG(("UpdateNatureAcctBal() %s to found\n",csTag));
ERRLOG("BOMMSEntityBalance::UpdateNatureAcctBal() %s not found\n",csTag);
                       		iRet = PD_ERR;
                	}

        			if (iRet == PD_OK ) {
					sprintf(csTag,"bal.%d.amt",i+1);
					if (GetField_Double(hContext,csTag,&dNatureTxnAmt)) {
DEBUGLOG(("UpdateNatureAcctBal:: () %s = [%lf]\n",csTag,dNatureTxnAmt));
/* for nature detail log */
						sprintf(csTag,"bal.%d.net_amt",i+1);
                				PutField_Double(hContext,csTag,dNatureTxnAmt);

						GetField_CString(hReq,"amt_type",&csAmtType);
						GetField_CString(hReq,"bal_amt",&csBalAmt);
DEBUGLOG(("UpdateNatureAcctBal:: () amt type = [%s] bal_amt = [%s]\n",csAmtType,csBalAmt));
						if (!strcmp(csAmtType,PD_CR))
							PutField_Double(hData,csBalAmt,dNatureTxnAmt);
						else if (!strcmp(csAmtType,PD_DR))
							PutField_Double(hData,csBalAmt,dNatureTxnAmt *(-1));
						else {
DEBUGLOG(("UpdateNatureAcctBal() invalid amt type [%s]\n",csPtr));
ERRLOG("BOMMSEntityBalance::UpdateNatureAcctBal() invalid amt type [%s]\n",csPtr);
							iRet = PD_ERR;
						}
					}
                			else {
DEBUGLOG(("UpdateNatureBalance() [%s] to found\n",csTag));
ERRLOG("BOMMSEntityBalance::UpdateNatureAcctBal() %s not found\n",csTag);
                        			iRet = PD_ERR;
                			}
				}

/* create Nature balance if doesn't exit */
				if (iRet == PD_OK) {
					DBObjPtr = CreateObj(DBPtr,"DBMmsEntityBalance","Update");
					iRet = (unsigned long)((DBObjPtr)(hData));
DEBUGLOG(("BOMMSEntityBalance:UpdateNatureAcctBal iRet = [%d] from MmsEntityBalance:Update\n",iRet));
				}

		}
	}

/* update txn element for intransit */
	if (iRet == PD_OK) {
		if (!strcmp(csBalAmt,"intransit") ||
		    !strcmp(csBalAmt,"lien")	||
		    !strcmp(csBalAmt,"acct_bal")	
		   ) {
			if(GetField_CString(hContext,"txn_seq",&csPtr)){
                       		PutField_CString(hData,"txn_seq",csPtr);
                	}
			
/* element type */
	/* intransit */
			if (!strcmp(csBalAmt,"intransit")) {
				PutField_CString(hData,"txn_element_type",PD_ELEMENT_IN_TRANSIT);
			}
	/* lien */
			else if (!strcmp(csBalAmt,"lien")) {
				PutField_CString(hData,"txn_element_type",PD_ELEMENT_HOLD_AMT);
			}
	/* acct_bal */
			else if (!strcmp(csBalAmt,"acct_bal")) {
				if (GetField_CString(hContext,"override_element_type",&csPtr)) {
DEBUGLOG(("BOMMSEntityBalance:Override transaction element to [%s]\n",csPtr));
					PutField_CString(hData,"txn_element_type",csPtr);
				}
				else {
					PutField_CString(hData,"txn_element_type",PD_ELEMENT_TXN_AMT);
				}
			}
/* ccy */
			PutField_CString(hData,"ccy",csTxnCcy);

			if (GetField_Double(hContext,"txn_amt",&dTxnAmt)) {
DEBUGLOG(("UpdateNatureBalance() txn_amt = [%lf]\n",dTxnAmt));
			}
			if (!strcmp(csAmtType,PD_CR)) {
				PutField_Double(hData,"amount",dTxnAmt);
			}
			else
				PutField_Double(hData,"amount",dTxnAmt * (-1));

			BOObjPtr = CreateObj(BOPtr,"BOMMSTxnElement","AddTxnElement");
                	iRet = (unsigned long)((BOObjPtr)(hData));
		}
	}

/* closing balance */
	if (iRet == PD_OK) {
DEBUGLOG(("Call DBMmsEntityBalance::GetNatureBalance\n"));
                DBObjPtr = CreateObj(DBPtr,"DBMmsEntityBalance","GetNatureBalance");
                iRet = (unsigned long)((DBObjPtr)(hData,hRsp));

                if (iRet == PD_OK) {
/*curr_acct_bal */
                        if (GetField_Double(hRsp,"acct_bal",&dTmp)) {
DEBUGLOG(("GetNatureBalance() curr_acct_bal = [%lf]\n",dTmp));
                                PutField_Double(hContext,"curr_acct_bal",dTmp);
                        }

/*curr_prepaid */
                        if (GetField_Double(hRsp,"prepaid",&dTmp)) {
DEBUGLOG(("GetNatureBalance() curr_prepaid = [%lf]\n",dTmp));
                                PutField_Double(hContext,"curr_prepaid",dTmp);
                        }

/*curr_intransit */
                        if (GetField_Double(hRsp,"intransit",&dTmp)) {
DEBUGLOG(("GetNatureBalance() curr_intransit = [%lf]\n",dTmp));
                                PutField_Double(hContext,"curr_intransit",dTmp);
                        }
/*curr_lien */
                        if (GetField_Double(hRsp,"lien",&dTmp)) {
DEBUGLOG(("GetNatureBalance() curr_lien = [%lf]\n",dTmp));
                                PutField_Double(hContext,"curr_lien",dTmp);
                        }


                }
        }


	FREE_ME(hData);
	FREE_ME(hRsp);
DEBUGLOG(("UpdateNatureAcctBal:: () Noraml Exit iRet = [%d]\n",iRet));
	return iRet;
}
/*End Dirk 20150702*/


