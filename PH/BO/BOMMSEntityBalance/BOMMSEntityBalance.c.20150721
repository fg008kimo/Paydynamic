DEBUGLOG(("CreditIntransit:: ()\n"));
	PutField_CString(hContext,"amt_type",PD_CR);
	PutField_CString(hContext,"bal_amt","intransit");

	iRet = UpdateEntityNatureBalance(hContext,hReq);

