        }
        else                    
                iRet = PD_ERR;

        hash_destroy(hTxn);
        FREE_ME(hTxn);          

        hash_destroy(hPspDetail);
        FREE_ME(hPspDetail);          
                        
	FREE_ME(csStatus);
DEBUGLOG(("UpdateTWVRespTxnLog:: Ret [%d]\n",iRet));
