                        PutField_CString(hTxn,"txn_code",csPtr);
                }

/* update txn header */
		DBObjPtr = CreateObj(DBPtr,"DBTransaction","Update");
                iRet = (unsigned long)(*DBObjPtr)(hTxn);
	}
	else	 	
