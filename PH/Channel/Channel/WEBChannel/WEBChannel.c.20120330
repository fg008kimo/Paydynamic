	for (i = 0; i < iTotal; i++ ) {
/* merchant_id */
		sprintf(csTag,"merchant_id_%d",i);
		if (GetField_CString(Request,csTag,&csValue)) {
DEBUGLOG(("process_input_txn: %s = [%s]\n",csTag,csValue));
