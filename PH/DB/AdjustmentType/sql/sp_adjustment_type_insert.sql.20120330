CREATE OR REPLACE FUNCTION sp_adjustment_type_insert(
	in_party_type		adjustment_type.at_party_type%type,
	in_code			adjustment_type.at_code%type,
	in_code_in_num		adjustment_type.at_code_in_num%type,
	in_dc_ind		adjustment_type.at_dc_ind%type,
	in_desc			adjustment_type.at_desc%type,
	in_disabled		adjustment_type.at_disabled%type,
	in_create_user		adjustment_type.at_create_user%type)
  RETURN NUMBEr IS
BEGIN

	INSERT INTO adjustment_type (
		at_party_type,
		at_code,
		at_code_in_num,
		at_dc_ind,
		at_desc,
		at_disabled,
		at_create_timestamp,
		at_create_user,
		at_update_timestamp,
		at_update_user
		)
	VALUES (	
		in_party_type,
		in_code,
		in_code_in_num,
		in_dc_ind,
		in_desc,
		in_disabled,
		sysdate,
		in_create_user,
		sysdate,
		in_create_user
		);

	IF SQL%ROWCOUNT = 0 THEN
		RETURN 1;
	ELSE
		RETURN 0;
	END IF;

EXCEPTION
	WHEN OTHERS THEN	
		RETURN 9;

END sp_adjustment_type_insert;
/
