DROP TABLE OL_STATEMENT_STATUS_LOG CASCADE CONSTRAINTS;

CREATE TABLE OL_STATEMENT_STATUS_LOG
(
  SL_STATEMENT_REF     VARCHAR2(16 BYTE),
  SL_STATUS            CHAR(1 BYTE),
  SL_SUB_STATUS        CHAR(3 BYTE),
  SL_TYPE              VARCHAR2(3 BYTE),
  SL_UPLOAD_FILENAME   VARCHAR2(100 BYTE),
  SL_REMARK            VARCHAR2(255 BYTE),
  SL_CREATE_USER       VARCHAR2(20 BYTE),
  SL_CREATE_TIMESTAMP  DATE                     DEFAULT sysdate
)
TABLESPACE USER_DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

