int FindStatementRefHold(hash_t *hRls)
{
	char	*csTmp;
	int	iTmp;

	EXEC SQL WHENEVER SQLERROR GOTO findrefhold_error;
	EXEC SQL WHENEVER NOTFOUND CONTINUE;

	EXEC SQL BEGIN DECLARE SECTION;
		varchar	hv_filename[PD_UPLOAD_FILENAME_LEN];
		varchar	hv_int_bank_code[PD_BANK_CODE_LEN];
		varchar	hv_bank_acct_num[PD_BANK_ACCT_NUM_LEN];
		int	hv_statement_seq;
		varchar v_statement_ref[PD_STMT_REF_LEN + 1];

		short	ind_filename = -1;
		short	ind_int_bank_code = -1;
		short	ind_bank_acct_num = -1;
		short	ind_statement_seq = -1;
		short	ind_statement_ref = -1;

		short hv_return_value;
	EXEC SQL END DECLARE SECTION;

/* filename */
	if (GetField_CString(hRls, "filename", &csTmp)) {
		hv_filename.len = strlen(csTmp);
		strncpy((char*)hv_filename.arr, csTmp, hv_filename.len);
		ind_filename = 0;
DEBUGLOG(("FindStatementRefHold: filename = [%.*s]\n", hv_filename.len, hv_filename.arr));
	}

/* statement_seq */
	if (GetField_Int(hRls,"statement_seq",&iTmp)) {
		hv_statement_seq = iTmp;
		ind_statement_seq = 0;
DEBUGLOG(("FindStatementRefHold: statement_seq = [%d]\n",hv_statement_seq));
	}

/* int_bank_code */
	if (GetField_CString(hRls, "int_bank_code", &csTmp)) {
		hv_int_bank_code.len = strlen(csTmp);
		strncpy((char*)hv_int_bank_code.arr, csTmp, hv_int_bank_code.len);
		ind_int_bank_code = 0;
DEBUGLOG(("FindStatementRefHold: int_bank_code = [%.*s]\n", hv_int_bank_code.len, hv_int_bank_code.arr));
	}

/* bank_acct_num */
	if (GetField_CString(hRls, "bank_acct_num", &csTmp)) {
		hv_bank_acct_num.len = strlen(csTmp);
		strncpy((char*)hv_bank_acct_num.arr, csTmp, hv_bank_acct_num.len);
		ind_bank_acct_num = 0;
DEBUGLOG(("FindStatementRefHold: bank_acct_num = [%.*s]\n", hv_bank_acct_num.len, hv_bank_acct_num.arr));
	}

	EXEC SQL EXECUTE
		BEGIN
			:hv_return_value := sp_ol_statement_find_ref_hold(
						:hv_filename:ind_filename,
						:hv_statement_seq:ind_statement_seq,
						:hv_int_bank_code:ind_int_bank_code,
						:hv_bank_acct_num:ind_bank_acct_num,
						:v_statement_ref:ind_statement_ref);
		END;
	END-EXEC;

DEBUGLOG(("FindStatementRefHold return = [%d]\n",hv_return_value));

	if (hv_return_value == SP_OK) {
		if (ind_statement_ref >= 0) {
			v_statement_ref.arr[v_statement_ref.len] = '\0';
			PutField_CString(hRls,"statement_ref",(char*)v_statement_ref.arr);
DEBUGLOG(("FindStatementRefHold: statement_ref = [%s]\n", (char*)v_statement_ref.arr));
		}
		return PD_OK;
	}

	if (hv_return_value == SP_OTHER_ERR) {
ERRLOG("OLStatement_FindStatementRefHold: SP_OTHER_ERR TxnAbort\n");
DEBUGLOG(("FindStatementRefHold: SP_OTHER_ERR TxnAbort\n"));
		return PD_OTHER_ERR;
	}

	if (hv_return_value == SP_ERR) {
ERRLOG("OLStatement_FindStatementRefHold: SP_ERR TxnAbort\n");
DEBUGLOG(("FindStatementRefHold: SP_ERR TxnAbort\n"));
		return PD_ERR;
	}

findrefhold_error:
DEBUGLOG(("findrefhold_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
	EXEC SQL WHENEVER SQLERROR CONTINUE;
ERRLOG("OLStatement_FindStatementRefHold: SP_INTERNAL_ERR TxnAbort\n");
DEBUGLOG(("FindStatementRefHold: SP_INTERNAL_ERR TxnAbort\n"));
	TxnAbort();
	return PD_INTERNAL_ERR;
}


