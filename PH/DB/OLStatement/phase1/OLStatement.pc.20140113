ERRLOG("OLStatement_UpdateDepositMach: SP_INTERNAL_ERR TxnAbort\n");
DEBUGLOG(("UpdateDepositMatch: SP_INTERNAL_ERR TxnAbort\n"));
	TxnAbort();
	return PD_INTERNAL_ERR;
