void removeChar(char *str, char garbage) {

	char *src, *dst;
	for (src = dst = str; *src != '\0'; src++) {
		*dst = *src;
		if (*dst != garbage) dst++;
	}
	*dst = '\0';
}

void ChangeDateTimeFormat(const char* csDateTime, int iStartYear, const char* csTemplate, const char* csOutTemplate, char* csOutDateTime, int iSizeDateTime)
{
	int nDays[12] = { 31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 };
	struct  tm tStruct;
	tStruct.tm_year = 101;
	tStruct.tm_mon = 0;
	tStruct.tm_mday = 1;
	tStruct.tm_hour = 0;
	tStruct.tm_min = 0;
	tStruct.tm_sec = 0;

	strptime(csDateTime, csTemplate, &tStruct);

	if (iStartYear > 0) {
		tStruct.tm_year+=iStartYear;
	}

	if (tStruct.tm_mday > nDays[tStruct.tm_mon]) {
		tStruct.tm_mon++;
		tStruct.tm_mday-=nDays[tStruct.tm_mon];
	}

DEBUGLOG(("strptime: [%02d][%02d][%02d] [%02d][%02d][%02d]\n",tStruct.tm_year,tStruct.tm_mon,tStruct.tm_mday,tStruct.tm_hour,tStruct.tm_min,tStruct.tm_sec));

	strftime(csOutDateTime, iSizeDateTime, csOutTemplate, &tStruct);
DEBUGLOG(("ChangeDateTimeFormat: in[%s][%s] out[%s][%s] [%d]\n",csDateTime,csTemplate,csOutTemplate,csOutDateTime,iSizeDateTime));
}

int CheckAmount(const char* csAmount, int iSupportDecimal, int iBalance, int iDetailField)
{
iDetailField = 1;
	int	iRet = PD_OK;
	int	iCnt = -1, iDigit = 0, iDecimal = 0;
	char	csTmpNumber[PD_TMP_MSG_BUF_LEN], *csTmpField;
	long	lTmp;
	double	dAmount = 0.0, dMinAmount = 0.0;
	int	dMaxLen = 0;

	if (iBalance == 1) {
		dMinAmount = 0.00-1E-9;
		dMaxLen = PD_DIGIT_LEN;
	} else {
		dMinAmount = 0.01-1E-9;
		dMaxLen = PD_DIGIT_LEN - PD_DECIMAL_LEN;
	}

	if (iRet == PD_OK) {
		dAmount = atof(csAmount);
		if (dAmount < dMinAmount) {
			iRet = INT_INVALID_PAY_AMOUNT;
if (iDetailField == 1) DEBUGLOG(("CheckAmount() [%s] INVALID!!!\n",csAmount));
		}
	}

	if (iRet == PD_OK) {
		strcpy(csTmpNumber,csAmount);
		csTmpField = mystrtok(csTmpNumber, ".");
		while (csTmpField != NULL) {
			iCnt++;
			if (iCnt == 0) {
				if (csTmpField[0] == '0' && csTmpField[1] >= '0' && csTmpField[1] <= '9') {
					iRet = INT_INVALID_PAY_AMOUNT;
if (iDetailField == 1) DEBUGLOG(("CheckAmount() [%s] [0] FOUND!!!\n",csAmount));
					break;
				}
				iDigit = strlen(csTmpField);
			} else if (iCnt == 1) {
				while (strlen(csTmpField) > PD_DECIMAL_LEN && csTmpField[strlen(csTmpField)-1] == '0') {
					csTmpField[strlen(csTmpField)-1] = '\0';
				}
				iDecimal = strlen(csTmpField);
			} else {
				iRet = INT_INVALID_PAY_AMOUNT;
if (iDetailField == 1) DEBUGLOG(("CheckAmount() [%s] [.] FOUND!!!\n",csAmount));
				break;
			}

			if(is_numeric(csTmpField) == 0) {
				iRet = INT_INVALID_PAY_AMOUNT;
if (iDetailField == 1) DEBUGLOG(("CheckAmount() [%s] non-number FOUND!!!\n",csAmount));
			}
			csTmpField = mystrtok(NULL, ".");
		}

		if (iRet == PD_OK &&
		    (iDecimal > PD_DECIMAL_LEN || iDigit > dMaxLen || (iDecimal == 0 && iCnt==1))) {
			iRet = INT_INVALID_PAY_AMOUNT;
if (iDetailField == 1) DEBUGLOG(("CheckAmount() [%s] [%d][%d] LEN!!!\n",csAmount,iDigit,iDecimal));
		}
	}

	if (iRet == PD_OK && iSupportDecimal == 0) {
		lTmp = (long) dAmount;
		if (dAmount > lTmp) {
			iRet = INT_UNSUPPORTED_PAY_AMOUNT;
if (iDetailField == 1) DEBUGLOG(("CheckAmount() [%s] UNSUPPORTED!!!\n",csAmount));
		}
	}

	return iRet;
}

