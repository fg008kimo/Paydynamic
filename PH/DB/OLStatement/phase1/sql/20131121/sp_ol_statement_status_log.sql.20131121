CREATE OR REPLACE FUNCTION sp_ol_stmt_status_log_insert(
	in_filename		ol_statement_status_log.sl_filename%type,
	in_statement_seq	ol_statement_status_log.sl_statement_seq%type,
	in_status		ol_statement_status_log.sl_status%type,
	in_process_group	ol_statement_status_log.sl_process_group%type,
	in_upload_filename	ol_statement_status_log.sl_upload_filename%type,
	in_input_channel	ol_statement_status_log.sl_input_channel%type,
	in_create_user		ol_statement_status_log.sl_create_user%type)
  RETURN NUMBEr IS
BEGIN

	INSERT INTO ol_statement_status_log(
		sl_filename,
		sl_statement_seq,
		sl_status,
		sl_process_group,
		sl_input_channel,
		sl_upload_filename,
		sl_create_user)
	VALUES (in_filename,
		in_statement_seq,
		in_status,
		in_process_group,
		in_input_channel,
		in_upload_filename,
		in_create_user);

	IF SQL%ROWCOUNT = 0 THEN
		RETURN 1;
	ELSE
		RETURN 0;
	END IF;

EXCEPTION
	WHEN OTHERS THEN	
		RETURN 9;

END sp_ol_stmt_status_log_insert;
/
