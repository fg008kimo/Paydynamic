CREATE OR REPLACE FUNCTION sp_entity_fx_bal_insert (
	in_entity_id		mms_entity_fx_bal.mfb_entity_id%TYPE,
	in_org_ccy		mms_entity_fx_bal.mfb_org_ccy%TYPE,
	in_ccy			mms_entity_fx_bal.mfb_ccy%TYPE,
	in_acr_ind		mms_entity_fx_bal.mfb_acr_ind%TYPE,
	in_bal			mms_entity_fx_bal.mfb_bal%TYPE,
	in_rate			mms_entity_fx_bal.mfb_rate%TYPE,
	in_ratio		mms_entity_fx_bal.mfb_ratio%TYPE,
	in_user			mms_entity_fx_bal.mfb_create_user%TYPE
	)
  RETURN NUMBER IS

BEGIN
  INSERT INTO MMS_ENTITY_FX_BAL (
	mfb_entity_id,
	mfb_org_ccy,
	mfb_ccy,
	mfb_acr_ind,
	mfb_bal,
	mfb_rate,
	mfb_ratio,
     	mfb_create_timestamp,
     	mfb_create_user,
       	mfb_update_timestamp,
       	mfb_update_user
	)

  VALUES (
	in_entity_id,
	in_org_ccy,
	in_ccy,
	in_acr_ind,
	in_bal,
	in_rate,
	in_ratio,
       	SYSDATE,
       	in_user,
       	SYSDATE,
       	in_user
	);

  IF SQL%ROWCOUNT = 0
  THEN
     RETURN 1;
  ELSE
     RETURN 0;
  END IF;

EXCEPTION
   WHEN OTHERS
   THEN
      RETURN 9;

END sp_entity_fx_bal_insert;
/

