CREATE OR REPLACE FUNCTION sp_ol_merch_upload_dt_palist(
  in_batch_id			ol_merchant_upload_file_detail.oud_batch_id%type,
  in_pre_approve_id		ol_merchant_upload_file_detail.oud_preapprove_id%type,
  out_cursor		out	sys_refcursor)

RETURN NUMBER Is
	iCnt	integer := 0;

Begin
	select count(oud_txn_id)
	  into	iCnt
	  from	ol_merchant_upload_file_detail,
		ol_merchant_upload_file_header
	  where oud_batch_id = ouh_batch_id
	  and	oud_batch_id = in_batch_id
	  and	ouh_disabled=0
	  and	oud_disabled=0
	  and	oud_preapprove_id= in_pre_approve_id; 
	
	if iCnt > 0 THEN
		OPEN out_cursor for
		select	
                        oud_seq_num
		from	ol_merchant_upload_file_detail,ol_merchant_upload_file_header
		where oud_preapprove_id=in_pre_approve_id
		and   oud_batch_id = ouh_batch_id
		and   oud_batch_id = in_batch_id
		and   ouh_disabled=0
		and   oud_disabled=0
		ORDER BY oud_seq_num;

		return 1;
		
	end if;

	return 0;

exception
   WHEN OTHERS THEN
     RETURN 0;
END sp_ol_merch_upload_dt_palist;
/
