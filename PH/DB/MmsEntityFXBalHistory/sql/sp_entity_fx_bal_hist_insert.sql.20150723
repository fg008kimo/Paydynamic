CREATE OR REPLACE FUNCTION sp_entity_fx_bal_hist_insert (
	in_history_id		mms_entity_fx_bal_history.mbh_history_id%TYPE,
	in_entity_id		mms_entity_fx_bal_history.mbh_entity_id%TYPE,
	in_fx_bal_date		mms_entity_fx_bal_history.mbh_fx_bal_date%TYPE,
	in_org_ccy		mms_entity_fx_bal_history.mbh_org_ccy%TYPE,
	in_ccy			mms_entity_fx_bal_history.mbh_ccy%TYPE,
	in_acr_ind		mms_entity_fx_bal_history.mbh_acr_ind%TYPE,
	in_old_bal		mms_entity_fx_bal_history.mbh_old_bal%TYPE,
	in_new_bal		mms_entity_fx_bal_history.mbh_new_bal%TYPE,
	in_old_rate		mms_entity_fx_bal_history.mbh_old_rate%TYPE,
	in_new_rate		mms_entity_fx_bal_history.mbh_new_rate%TYPE,
	in_old_ratio		mms_entity_fx_bal_history.mbh_old_ratio%TYPE,
	in_new_ratio		mms_entity_fx_bal_history.mbh_new_ratio%TYPE,
	in_funds_direction	mms_entity_fx_bal_history.mbh_funds_direction%TYPE,
	in_input_rate		mms_entity_fx_bal_history.mbh_input_rate%TYPE,
	in_txn_id		mms_entity_fx_bal_history.mbh_txn_id%TYPE,
--	in_txn_dt_seq		mms_entity_fx_bal_history.mbh_txn_dt_seq%TYPE,
	in_user			mms_entity_fx_bal_history.mbh_create_user%TYPE
	)
  RETURN NUMBER IS

BEGIN
  INSERT INTO MMS_ENTITY_FX_BAL_HISTORY (
	mbh_history_id,
	mbh_entity_id,
	mbh_fx_bal_date,
	mbh_org_ccy,
	mbh_ccy,
	mbh_acr_ind,
	mbh_old_bal,
	mbh_new_bal,
	mbh_old_rate,
	mbh_new_rate,
	mbh_old_ratio,
	mbh_new_ratio,
	mbh_funds_direction,
	mbh_input_rate,
	mbh_txn_id,
--	mbh_txn_dt_seq,
     	mbh_create_timestamp,
     	mbh_create_user,
       	mbh_update_timestamp,
       	mbh_update_user
	)

  VALUES (
	in_history_id,
	in_entity_id,
	in_fx_bal_date,
	in_org_ccy,
	in_ccy,
	in_acr_ind,
	in_old_bal,
	in_new_bal,
	in_old_rate,
	in_new_rate,
	in_old_ratio,
	in_new_ratio,
	in_funds_direction,
	in_input_rate,
	in_txn_id,
	--in_txn_dt_seq,
       	SYSDATE,
       	in_user,
       	SYSDATE,
       	in_user
	);

  IF SQL%ROWCOUNT = 0
  THEN
     RETURN 1;
  ELSE
     RETURN 0;
  END IF;

EXCEPTION
   WHEN OTHERS
   THEN
      RETURN 9;

END sp_entity_fx_bal_hist_insert;
/

