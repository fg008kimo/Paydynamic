CREATE OR REPLACE FUNCTION sp_ol_merch_config_insert (
   in_merchant_id                   OL_MERCH_CONFIG.omc_merchant_id%TYPE,
   in_currency_id                   OL_MERCH_CONFIG.omc_currency_id%TYPE,
   in_country_id                    OL_MERCH_CONFIG.omc_country_id%TYPE,
   in_service_code                  OL_MERCH_CONFIG.omc_service_code%TYPE,
   in_match_fifo                    OL_MERCH_CONFIG.omc_match_fifo%TYPE,
   in_match_fifo_amt_limit          OL_MERCH_CONFIG.omc_match_fifo_amt_limit%TYPE,
   in_min_settle_amt                OL_MERCH_CONFIG.omc_min_settle_amt%TYPE,
   in_min_settle_amt_apply_admin    OL_MERCH_CONFIG.omc_min_settle_amt_apply_admin%TYPE,
   in_preferred_settle_ccy          OL_MERCH_CONFIG.omc_preferred_settle_ccy%TYPE,
   in_preferred_acr_pool	    OL_MERCH_CONFIG.omc_preferred_acr_pool%TYPE,
   in_disabled                      OL_MERCH_CONFIG.omc_disabled%TYPE,
   in_create_user                   OL_MERCH_CONFIG.omc_create_user%TYPE)
   RETURN NUMBER
IS
BEGIN
   INSERT INTO OL_MERCH_CONFIG (omc_merchant_id,
                                omc_currency_id,
                                omc_country_id,
                                omc_service_code,
                                omc_match_fifo,
                                omc_match_fifo_amt_limit,
                                omc_min_settle_amt,
                                omc_min_settle_amt_apply_admin,
                                omc_preferred_settle_ccy,
                                omc_preferred_acr_pool,
                                omc_disabled,
                                omc_create_user,
                                omc_create_timestamp,
                                omc_update_user,
                                omc_update_timestamp)
        VALUES (in_merchant_id,
                in_currency_id,
                in_country_id,
                in_service_code,
                in_match_fifo,
                in_match_fifo_amt_limit,
                in_min_settle_amt,
                in_min_settle_amt_apply_admin,
                in_preferred_settle_ccy,
                in_preferred_acr_pool,
                in_disabled,
                in_create_user,
                SYSDATE,
                in_create_user,
                SYSDATE);

   IF SQL%ROWCOUNT = 0
   THEN
      RETURN 1;
   ELSE
      RETURN 0;
   END IF;
EXCEPTION
   WHEN OTHERS
   THEN
      RETURN 9;
END sp_ol_merch_config_insert;
/

