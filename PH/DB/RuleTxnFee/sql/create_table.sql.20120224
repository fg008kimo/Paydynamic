  RF_PAY_METHOD        VARCHAR2(4 BYTE),
  RF_CUSTOM_TAG        VARCHAR2(20 BYTE),
  RF_CUSTOM_VALUE      VARCHAR2(50 BYTE),
  RF_MIN_VALUE         NUMBER(14,2)             DEFAULT 0,
  RF_MAX_VALUE         NUMBER(14,2)             DEFAULT 0,
  RF_EFFECT_DATE       DATE                     DEFAULT sysdate
)
