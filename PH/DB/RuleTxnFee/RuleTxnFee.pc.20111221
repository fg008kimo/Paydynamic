#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <oraca.h>
#include <sqlca.h>
#include "RuleTxnFee.h"
#include "common.h"
#include "utilitys.h"
#include "dbutility.h"

#define SQLCA_STORAGE_CLASS extern
#define SQLCODE sqlca.sqlcode

char    cDebug;
void RuleTxnFee(char    cdebug)
{
        cDebug = cdebug;
}

int perr01(const char * msg){
    ERRLOG("Error occured when %s\n", msg);
    ERRLOG("{\n");
    ERRLOG(" sqlcaid = %s\n",sqlca.sqlcaid);
    ERRLOG(" sqlabc = %d\n",sqlca.sqlabc);
    ERRLOG(" sqlcode = %d\n",sqlca.sqlcode);
    ERRLOG(" sqlerrm.sqlerrml = %d\n",sqlca.sqlerrm.sqlerrml);
    ERRLOG(" sqlerrm.sqlerrmc = %s\n",sqlca.sqlerrm.sqlerrmc);
    ERRLOG(" sqlerrp = %s\n",sqlca.sqlerrp);
    ERRLOG(" sqlerrd = %d\n",sqlca.sqlerrd);
    ERRLOG(" sqlwarn = %s\n",sqlca.sqlwarn);
    ERRLOG(" sqlext = %s\n",sqlca.sqlext);
    ERRLOG("}\n");
    return 1;
}
int Find(const char* csType,
		     const char* csCountryId,
		     const char* csChannelCode,
		     const char* csServiceCode,
                     const char* csTxnCode,
		     const char* csCcy,
                     const char* csMerchantId,
		     const char* csClientId,
		     recordset_t* myRec)

{
	hash_t *myHash;
	int	iCnt = 0;
        EXEC SQL WHENEVER SQLERROR GOTO find_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;
    
        EXEC SQL BEGIN DECLARE SECTION;
                short           hv_return_value;

		varchar		hv_rule_txn_type[PD_TXN_CHG_TYPE_LEN];
		varchar		hv_country_id[PD_COUNTRY_LEN];
		varchar		hv_channel_code[PD_CHANNEL_CODE_LEN];
		varchar		hv_service_code[PD_SERVICE_CODE_LEN];
		varchar		hv_txn_code[PD_TXN_CODE_LEN];
		varchar		hv_ccy[PD_CCY_ID_LEN];
                varchar         hv_merchant_id[PD_MERCHANT_ID_LEN];
                varchar         hv_client_id[PD_CLIENT_ID_LEN];

		int		v_rule_id;
		int		v_tier_id;
		varchar		v_custom_tag[PD_CUSTOM_TAG_LEN +1];
		varchar		v_custom_value[PD_CUSTOM_VALUE_LEN +1];


		short		ind_rule_txn_type = -1;
		short		ind_country_id = -1;
		short		ind_channel_code = -1;
		short		ind_service_code = -1;
		short		ind_txn_code = -1;
		short		ind_ccy = -1;
		short		ind_merchant_id = -1;
		short		ind_client_id = -1;

		short		ind_rule_id = -1;
		short		ind_tier_id = -1;
		short		ind_custom_tag = -1;
		short		ind_custom_value = -1;
	
		SQL_CURSOR      c_cursor_ruleid;

    
        EXEC SQL END DECLARE SECTION;

        hv_rule_txn_type.len = strlen(csType);
        memcpy(hv_rule_txn_type.arr,csType,hv_rule_txn_type.len);
DEBUGLOG(("Find rule_txn_type = [%.*s]\n",hv_rule_txn_type.len,hv_rule_txn_type.arr));
	ind_rule_txn_type = 0;

        hv_country_id.len = strlen(csCountryId);
        memcpy(hv_country_id.arr,csCountryId,hv_country_id.len);
DEBUGLOG(("Find country_id = [%.*s]\n",hv_country_id.len,hv_country_id.arr));
	ind_country_id = 0;

        hv_channel_code.len = strlen(csChannelCode);
        memcpy(hv_channel_code.arr,csChannelCode,hv_channel_code.len);
DEBUGLOG(("Find channel_code = [%.*s]\n",hv_channel_code.len,hv_channel_code.arr));
	ind_channel_code = 0;

        hv_service_code.len = strlen(csServiceCode);
        memcpy(hv_service_code.arr,csServiceCode,hv_service_code.len);
DEBUGLOG(("Find service_code = [%.*s]\n",hv_service_code.len,hv_service_code.arr));
	ind_service_code = 0;

        hv_txn_code.len = strlen(csTxnCode);
        memcpy(hv_txn_code.arr,csTxnCode,hv_txn_code.len);
DEBUGLOG(("Find txn_code = [%.*s]\n",hv_txn_code.len,hv_txn_code.arr));
	ind_txn_code = 0;

        hv_ccy.len = strlen(csCcy);
        memcpy(hv_ccy.arr,csCcy,hv_ccy.len);
DEBUGLOG(("Find ccy = [%.*s]\n",hv_ccy.len,hv_ccy.arr));
	ind_ccy = 0;

        hv_merchant_id.len = strlen(csMerchantId);
        memcpy(hv_merchant_id.arr,csMerchantId,hv_merchant_id.len);
DEBUGLOG(("Find merchant_id = [%.*s]\n",hv_merchant_id.len,hv_merchant_id.arr));
	ind_merchant_id = 0;
    
        hv_client_id.len = strlen(csClientId);
        memcpy(hv_client_id.arr,csClientId,hv_client_id.len);
DEBUGLOG(("Find client_id = [%.*s]\n",hv_client_id.len,hv_client_id.arr));
	ind_client_id = 0;

        EXEC SQL ALLOCATE       :c_cursor_ruleid;
        EXEC SQL EXECUTE
                BEGIN
                        :hv_return_value := sp_rule_txn_fee_find(:hv_rule_txn_type:ind_rule_txn_type,
									:hv_country_id:ind_country_id,
									:hv_channel_code:ind_channel_code,
									:hv_service_code:ind_service_code,
									:hv_txn_code:ind_txn_code,
									:hv_ccy:ind_ccy,
									:hv_merchant_id:ind_merchant_id,
									:hv_client_id:ind_client_id,
									:c_cursor_ruleid);
                END;
        END-EXEC;

        if (hv_return_value > 0)  {
DEBUGLOG(("Find Found\n"));
		for (;;) {
                        myHash = (hash_t*) malloc (sizeof(hash_t));
                        hash_init(myHash,0);

                        ind_rule_id = -1;
                        ind_tier_id = -1;
			ind_custom_tag = -1;
			ind_custom_value = -1;

                        EXEC SQL WHENEVER NOTFOUND DO break;
                        EXEC SQL FETCH :c_cursor_ruleid
                        INTO    :v_rule_id:ind_rule_id,
                                :v_tier_id:ind_tier_id,
				:v_custom_tag:ind_custom_tag,
				:v_custom_value:ind_custom_value;

			if (SQLCODE == SQL_NOT_FOUND) {
                        	break;
                	}

			iCnt++;

                        if (ind_rule_id >= 0 ) {
DEBUGLOG(("find[%d] rule_id = [%d]\n",iCnt,v_rule_id));
                                PutField_Int(myHash,"rule_id",v_rule_id);
                        }

                        if (ind_tier_id >= 0 ) {
DEBUGLOG(("find[%d] tier_id = [%d]\n",iCnt,v_tier_id));
                                PutField_Int(myHash,"tier_id",v_tier_id);
                        }
                        if (ind_custom_tag >= 0 ) {
				v_custom_tag.arr[v_custom_tag.len] = '\0';
DEBUGLOG(("find[%d] custom_tag = [%s]\n",iCnt,v_custom_tag.arr));
				PutField_CString(myHash,"custom_tag",(const char*)v_custom_tag.arr);
			}
                        if (ind_custom_value >= 0 ) {
				v_custom_value.arr[v_custom_value.len] = '\0';
DEBUGLOG(("find[%d] custom_value = [%s]\n",iCnt,v_custom_value.arr));
				PutField_CString(myHash,"custom_value",(const char*)v_custom_value.arr);
			}
                        RecordSet_Add(myRec,myHash);

                }
                return PD_OK;
        }
        else {
DEBUGLOG(("Find Not Found\n"));
                return PD_ERR;
        }

	EXEC SQL CLOSE :c_cursor_ruleid;

find_error:
ERRLOG("RuleTxnFee::find_error code %d\n", sqlca.sqlcode);
DEBUGLOG(("find_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
        EXEC SQL WHENEVER SQLERROR CONTINUE;
	EXEC SQL CLOSE :c_cursor_ruleid;
perr01("find_fee");
        return PD_ERR;
}
