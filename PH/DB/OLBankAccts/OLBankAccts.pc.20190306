        return iRet;

chkbankacctunique_error:
DEBUGLOG(("ChkBankAcctsUnique_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
	EXEC SQL WHENEVER SQLERROR CONTINUE;

	return PD_ERR;
}

