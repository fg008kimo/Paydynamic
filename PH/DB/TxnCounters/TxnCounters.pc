int GetMobAmtCount(const char* csMerchantId,
		   const char* csCcy,
		   const char* csGroup,
		   double *dAmt)
{
	int iRet = PD_OK;
	*dAmt = 0.0;

DEBUGLOG(("GetMobAmtCount()\n"));
        EXEC SQL WHENEVER SQLERROR GOTO getmobamt_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;
		varchar	hv_merchant_id[PD_MERCHANT_ID_LEN];
		varchar	hv_group[PD_CUSTOMER_GROUP_CODE_LEN];
		varchar	hv_ccy[PD_CCY_ID_LEN];

                double  v_counter;

                short   ind_counter = -1;
        EXEC SQL END DECLARE SECTION;



        hv_merchant_id.len = strlen(csMerchantId);
        memcpy(hv_merchant_id.arr,csMerchantId,hv_merchant_id.len);
DEBUGLOG(("GetMobAmtCount: merchant_id = [%.*s][%d]\n",hv_merchant_id.len,hv_merchant_id.arr,hv_merchant_id.len));

        hv_ccy.len = strlen(csCcy);
        memcpy(hv_ccy.arr,csCcy,hv_ccy.len);
DEBUGLOG(("GetMobAmtCount: ccy = [%.*s][%d]\n",hv_ccy.len,hv_ccy.arr,hv_ccy.len));

        hv_group.len = strlen(csGroup);
        memcpy(hv_group.arr,csGroup,hv_group.len);
DEBUGLOG(("GetMobAmtCount: group = [%.*s][%d]\n",hv_group.len,hv_group.arr,hv_group.len));

	EXEC SQL DECLARE c_cursor_getmobamt CURSOR FOR
		SELECT SUM (TC_TOTAL_COUNTER)
		FROM (SELECT psp_id, customer_segment
			FROM PSP_LIMIT_MERCHANT_VIEW
			WHERE merchant_id = :hv_merchant_id
			AND currency_id = :hv_ccy
			AND customer_segment = :hv_group
			group by psp_id, customer_segment)
		LEFT JOIN
		(  SELECT SUM (TC_TOTAL_COUNTER) AS TC_TOTAL_COUNTER,
		   TC_PARTY_ID AS PARTY_PSP_ID
		   FROM TXN_COUNTERS
		   WHERE TC_PARTY_TYPE = 'P' AND TC_CATEGORY = 'AMT' AND TC_TYPE = 'D'
		   GROUP BY TC_PARTY_ID)
		ON PSP_ID = PARTY_PSP_ID;
	
	
        EXEC SQL OPEN c_cursor_getmobamt;
        do {
                EXEC SQL FETCH c_cursor_getmobamt
		  INTO  
			:v_counter:ind_counter;

		
                if (SQLCODE == SQL_NOT_FOUND) {
                        break;
                }

DEBUGLOG(("GetMobAmtCount  found\n"));
	
		if (ind_counter < 0 ) {
			v_counter = 0.0;
		}

		*dAmt = v_counter;
	}
	while(PD_TRUE);

        EXEC SQL CLOSE c_cursor_getmobamt;

DEBUGLOG(("GetMobAmtCount Counter iRet = [%d]\n",iRet));
        return iRet;
getmobamt_error:
DEBUGLOG(("getmobamt_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
    EXEC SQL WHENEVER SQLERROR CONTINUE;
    EXEC SQL CLOSE c_cursor_getmobamt;
    return PD_ERR;
}
