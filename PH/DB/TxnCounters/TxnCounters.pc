DEBUGLOG(("TxnReqCountersCreate: SP_INTERNAL_ERR TxnAbort\n"));
	//TxnAbort();
        return PD_INTERNAL_ERR;
}

int ResetReqCounter(const hash_t *hRls)
{
        char *csTmp;
        int  iTmp;
        char cTmp;
        char *csBuf;

        EXEC SQL WHENEVER SQLERROR GOTO resetreqcounter_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;
    
        EXEC SQL BEGIN DECLARE SECTION;

        varchar hv_dynstmt[1024];
    
        EXEC SQL END DECLARE SECTION;

DEBUGLOG(("ResetReqCounter: Begin\n"));
        csBuf = (char*) malloc (128);
        strcpy((char*)hv_dynstmt.arr,"update txn_counters set tc_update_timestamp = sysdate");
        hv_dynstmt.len = strlen((const char*)hv_dynstmt.arr);

        GetField_Int(hRls, "counter", &iTmp);
DEBUGLOG(("ResetReqCounter:counter = [%d]\n",iTmp));
        sprintf(csBuf, "%d",iTmp);
        strcat((char*)hv_dynstmt.arr, ",tc_req_counter = ");
        strcat((char*)hv_dynstmt.arr, csBuf);
        strcat((char*)hv_dynstmt.arr, ",tc_total_req_counter = ");
        strcat((char*)hv_dynstmt.arr, csBuf);
        hv_dynstmt.len = strlen((const char*)hv_dynstmt.arr);

