DROP TABLE PHUSER.DEF_PRODUCT CASCADE CONSTRAINTS;

CREATE TABLE PHUSER.DEF_PRODUCT
(
  DP_ID                CHAR(3 BYTE),
  DP_DESC              VARCHAR2(50 BYTE),
  DP_CREATE_TIMESTAMP  DATE,
  DP_CREATE_USER       VARCHAR2(20 BYTE),
  DP_UPDATE_TIMESTAMP  DATE,
  DP_UPDATE_USER       VARCHAR2(20 BYTE),
  DP_DISABLED          INTEGER                  DEFAULT 0
)
TABLESPACE USER_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


CREATE UNIQUE INDEX PHUSER.DEF_PRODUCT_PK ON PHUSER.DEF_PRODUCT
(DP_ID)
LOGGING
TABLESPACE USER_IDX
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOPARALLEL;


ALTER TABLE PHUSER.DEF_PRODUCT ADD (
  CONSTRAINT DEF_PRODUCT_PK
  PRIMARY KEY
  (DP_ID)
