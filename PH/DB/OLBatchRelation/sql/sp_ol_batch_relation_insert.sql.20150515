CREATE OR REPLACE FUNCTION sp_ol_batch_relation_insert(
	in_org_batch_id		ol_batch_relation.obr_org_batch_id%type,
	in_batch_id		ol_batch_relation.obr_batch_id%type,
	in_create_user		ol_stmt_txn_relation.ostp_create_user%type)
  RETURN NUMBER IS
BEGIN
	INSERT INTO OL_BATCH_RELATION(
		obr_org_batch_id,
		obr_batch_id,
                obr_relation_timestamp,
		obr_create_timestamp,
		obr_create_user,
		obr_update_timestamp,
		obr_update_user
		)
	VALUES (
		in_org_batch_id,
		in_batch_id,
                systimestamp,
		sysdate,
		in_create_user,
		sysdate,
		in_create_user
		);

	IF SQL%ROWCOUNT = 0 THEN
		RETURN 1;
	ELSE
		RETURN 0;
	END IF;

EXCEPTION
	WHEN OTHERS THEN	
		RETURN 9;

END sp_ol_batch_relation_insert;
/
