        EXEC SQL EXECUTE
                BEGIN
                        :hv_return_value := sp_mms_txn_detail_insert(
                                                :hv_txn_id:ind_txn_id,
                                                :hv_sd_ind:ind_sd_ind,
						:hv_merchant_id:ind_merchant_id,
						:hv_psp_id:ind_psp_id,
