int Add(const hash_t *hRls)
{
	char  cTmp;
	char  *csTmp;
	/*int   iTmp;*/

	EXEC SQL WHENEVER SQLERROR GOTO add_error;
	EXEC SQL WHENEVER NOTFOUND CONTINUE;

	EXEC SQL BEGIN DECLARE SECTION;
		char		hv_party_type;
		char		hv_business_type;
		varchar		hv_party_id[PD_MMS_PARTY_ID_LEN];
		varchar		hv_party_name[PD_MMS_PARTY_NAME_LEN];
		varchar		hv_mmc_id[PD_MMS_MMC_ID_LEN];
		varchar         hv_create_user[PD_USER_LEN];

		short		ind_party_type = -1;
		short		ind_business_type = -1;
		short		ind_party_id = -1;
		short		ind_party_name = -1;
		short		ind_mmc_id = -1;
		short		ind_create_user = -1;

		short		hv_return_value;	
		
        EXEC SQL END DECLARE SECTION;

DEBUGLOG(("Add: Begin\n"));

	if (GetField_Char(hRls, "party_type", &cTmp)) {	
		hv_party_type = cTmp;
		ind_party_type = 0;
	}
DEBUGLOG(("Add:party_type = [%c]\n",hv_party_type));

	if (GetField_Char(hRls, "business_type", &cTmp)) {
		hv_business_type = cTmp;
		ind_business_type = 0;
	}	
DEBUGLOG(("Add:business_type = [%c]\n",hv_business_type));

        if(GetField_CString(hRls,"party_id",&csTmp)) {
                hv_party_id.len = strlen(csTmp);
                strncpy((char*)hv_party_id.arr, csTmp, hv_party_id.len);
                ind_party_id = 0;
        }
DEBUGLOG(("Add:party_id = [%.*s]\n",hv_party_id.len, hv_party_id.arr));

        if(GetField_CString(hRls,"party_name",&csTmp)) {
                hv_party_name.len = strlen(csTmp);
                strncpy((char*)hv_party_name.arr, csTmp, hv_party_name.len);
                ind_party_name = 0;
        }
DEBUGLOG(("Add:party_name = [%.*s]\n",hv_party_name.len, hv_party_name.arr));

        if(GetField_CString(hRls,"mmc_id",&csTmp)) {
                hv_mmc_id.len = strlen(csTmp);
                strncpy((char*)hv_mmc_id.arr, csTmp, hv_mmc_id.len);
                ind_mmc_id = 0;
        }
DEBUGLOG(("Add:mmc_id = [%.*s]\n",hv_mmc_id.len, hv_mmc_id.arr));


        if(GetField_CString(hRls,"create_user",&csTmp))
        {
                hv_create_user.len = strlen(csTmp);
                strncpy((char*)hv_create_user.arr, csTmp, hv_create_user.len);
                ind_create_user = 0;
        }
DEBUGLOG(("Add:create_user = [%.*s]\n",hv_create_user.len,hv_create_user.arr));

	EXEC SQL EXECUTE
		BEGIN
			:hv_return_value := sp_instant_pm_acct_dt_ins(
						:hv_party_type:ind_party_type,
						:hv_business_type:ind_business_type,
						:hv_party_id:ind_party_id,
						:hv_party_name:ind_party_name,
						:hv_mmc_id:ind_mmc_id,
						:hv_create_user:ind_create_user); 
		END;
	END-EXEC;

        DEBUGLOG(("Add:Ret = [%d]\n",hv_return_value));
        if (hv_return_value == SP_OK)
        {
                DEBUGLOG(("Add:Normal Exit\n"));
                return PD_OK;
        }

        if (hv_return_value == SP_OTHER_ERR)  {
                ERRLOG("MmsInstantPMAcctDt_Add: SP_OTHER_ERR TxnAbort\n");
                DEBUGLOG(("Add: SP_OTHER_ERR TxnAbort\n"));
                return PD_OTHER_ERR;
        }

        if (hv_return_value == SP_ERR)  {
                ERRLOG("MmsInstantPMAcctDt_Add: SP_ERR TxnAbort\n");
                DEBUGLOG(("Add: SP_ERR TxnAbort\n"));
                return PD_ERR;
        }


add_error:
DEBUGLOG(("add_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
ERRLOG("MmsInstantPMAcctDt_Add: SP_INTERNAL_ERR TxnAbort\n");
        EXEC SQL WHENEVER SQLERROR CONTINUE;
        return PD_ERR;


}


	
int	GetDetailRec(const char cPartyType, 
			const char cBusinessType,
			recordset_t *myRec)
{
	int 	iCnt = 0;
	hash_t *myHash;

        EXEC SQL WHENEVER SQLERROR GOTO getdetailrec_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;
		char		hv_party_type;
		char		hv_business_type;	

		varchar		v_party_id[PD_MMS_PARTY_ID_LEN + 1];
		varchar		v_party_name[PD_MMS_PARTY_NAME_LEN + 1];
		varchar		v_mmc_id[PD_MMS_MMC_ID_LEN + 1];

		short		ind_party_id = -1;
		short		ind_party_name = -1;
		short		ind_mmc_id = -1;

        EXEC SQL END DECLARE SECTION;

	hv_party_type = cPartyType;
DEBUGLOG(("GetDetailRec party_type = [%.c]\n", hv_party_type));

	hv_business_type = cBusinessType;
DEBUGLOG(("GetDetailRec business_type = [%.c]\n", hv_business_type));

        EXEC SQL DECLARE c_cursor_getdetailrec CURSOR FOR
		select mid_party_id,
		       mid_party_name,
	               mid_mmc_id
		  from mms_instant_pm_acct_dt
		 where mid_party_type = :hv_party_type
		   and mid_business_type = :hv_business_type
		order by mid_mmc_id, mid_party_id;

        EXEC SQL OPEN c_cursor_getdetailrec;
        do {
		EXEC SQL FETCH c_cursor_getdetailrec
		INTO	:v_party_id:ind_party_id,
			:v_party_name:ind_party_name,
			:v_mmc_id:ind_mmc_id;

		if (SQLCODE == SQL_NOT_FOUND) {
			break;
		}	

		iCnt++;

                myHash = (hash_t*) malloc (sizeof(hash_t));
                hash_init(myHash,0);

/* party_id */	
		if (ind_party_id >= 0) {
			v_party_id.arr[v_party_id.len] = '\0';
			PutField_CString(myHash, "party_id", (const char *)v_party_id.arr);
DEBUGLOG(("GetDetailRec party_id = [%s]\n",v_party_id.arr));
		}

/* party_name */	
		if (ind_party_name >= 0) {
			v_party_name.arr[v_party_name.len] = '\0';
			PutField_CString(myHash, "party_name", (const char *)v_party_name.arr);
DEBUGLOG(("GetDetailRec party_name = [%s]\n",v_party_name.arr));
		}

/* mmc_id */	
		if (ind_mmc_id >= 0) {
			v_mmc_id.arr[v_mmc_id.len] = '\0';
			PutField_CString(myHash, "mmc_id", (const char *)v_mmc_id.arr);
DEBUGLOG(("GetDetailRec mmc_id = [%s]\n",v_mmc_id.arr));
		}
	
		RecordSet_Add(myRec, myHash);
	}
	while (PD_TRUE);
	EXEC SQL CLOSE c_cursor_getdetailrec;


        if (iCnt > 0 ) {
DEBUGLOG(("GetDetailRec Normal Exit\n"));
                return  PD_OK;
        }
        else {
DEBUGLOG(("GetDetailRec Normal Exit, Not Found\n"));
                return PD_ERR;
        }


getdetailrec_error:
DEBUGLOG(("getdetailrec_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
        EXEC SQL WHENEVER SQLERROR CONTINUE;
        EXEC SQL CLOSE c_cursor_getdetailrec; 
        return PD_ERR;

}

