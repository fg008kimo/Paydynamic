DROP TABLE PAR_TXN_DATA CASCADE CONSTRAINTS;

CREATE TABLE PAR_TXN_DATA
(
  PTD_TXN_SEQ		NUMBER(10)     NOT NULL,
  PTD_TXN_DATE          DATE,
  PTD_TXN_MERCH_NMB     VARCHAR2(50 BYTE),
  PTD_TXN_NMB           VARCHAR2(16 BYTE),
  PTD_MERCH_REF         VARCHAR2(50 BYTE),
  PTD_TXN_TYPE          VARCHAR2(50 BYTE),
  PTD_TXN_STATUS        VARCHAR2(50 BYTE),
  PTD_TXN_STATUS_GROUP  VARCHAR2(50 BYTE),
  PTD_TO_CCY            VARCHAR2(3 BYTE),
  PTD_TO_AMOUNT         NUMBER(12,2),
  PTD_CCY               VARCHAR2(3 BYTE),
  PTD_AMOUNT            NUMBER(12,2),
  PTD_FEE               NUMBER(12,2),
  PTD_EX_RATE           NUMBER(14,5),
  PTD_MARKUP_AMT        NUMBER(12,2),
  PTD_BALANCE           NUMBER(12,2),
  PTD_PSP_TYPE_CODE     VARCHAR2(50 BYTE),
  PTD_ORDER_ID          VARCHAR2(50 BYTE),
  PTD_GATE_ID           VARCHAR2(50 BYTE),
  PTD_CLIENT_IP         VARCHAR2(15 BYTE),
  PTD_CREATE_TIMESTAMP  DATE                    DEFAULT sysdate,
  PTD_CREATE_USER       VARCHAR2(20 BYTE),
  PTD_UPDATE_TIMESTAMP  DATE                    DEFAULT sysdate,
  PTD_UPDATE_USER       VARCHAR2(20 BYTE),
  PTD_PROC_STATUS       VARCHAR2(10 BYTE)
)
TABLESPACE USER_DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


