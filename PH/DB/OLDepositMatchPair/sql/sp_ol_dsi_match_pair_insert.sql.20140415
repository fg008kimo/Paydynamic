CREATE OR REPLACE FUNCTION sp_ol_dsi_match_pair_insert (
   in_stmt_txn_id     ol_deposit_match_pair.odmp_stmt_txn_id%TYPE,
   in_merch_txn_id    ol_deposit_match_pair.odmp_merch_txn_id%TYPE,
   in_create_user     ol_deposit_match_pair.odmp_create_user%TYPE)
   RETURN NUMBER
IS
BEGIN
   INSERT INTO ol_deposit_match_pair (odmp_stmt_txn_id,
                                      odmp_merch_txn_id,
                                      odmp_create_timestamp,
                                      odmp_create_user,
                                      odmp_update_timestamp,
                                      odmp_update_user)
        VALUES (in_stmt_txn_id,
                in_merch_txn_id,
                SYSDATE,
                in_create_user,
                SYSDATE,
                in_create_user);

   IF SQL%ROWCOUNT = 0
   THEN
      RETURN 1;
   ELSE
      RETURN 0;
   END IF;
EXCEPTION
   WHEN OTHERS
   THEN
      RETURN 9;
END sp_ol_dsi_match_pair_insert;
/