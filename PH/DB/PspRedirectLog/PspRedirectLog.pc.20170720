#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
#include <sqlca.h>
#include "common.h"
#include "utilitys.h"
#include "dbutility.h"
#include "PspRedirectLog.h"

#define SQLCA_STORAGE_CLASS extern
#define SQLCODE sqlca.sqlcode


char	cDebug;

void PspRedirectLog(char	cdebug)
{
	cDebug = cdebug;
}


int Add(const hash_t *hPspRedirectLog)
{
	char    *csTmp;

	EXEC SQL WHENEVER SQLERROR GOTO add_error;
	EXEC SQL WHENEVER NOTFOUND CONTINUE;

	EXEC SQL BEGIN DECLARE SECTION;
		varchar		hv_txn_id[PD_TXN_SEQ_LEN];
		varchar		hv_url[PD_TLG_URL_LEN];
		varchar		hv_client_ip[PD_IP_LEN];
		varchar		hv_rcv_ts[PD_DATETIME_LEN];
		varchar		hv_create_user[PD_USER_LEN];

		short		ind_txn_id = -1;
		short		ind_url = -1;
		short		ind_client_ip = -1;
		short		ind_rcv_ts = -1;
		short		ind_create_user = -1;

                short           hv_return_value;
	EXEC SQL END DECLARE SECTION;

DEBUGLOG(("Add: Begin\n"));

        if(GetField_CString(hPspRedirectLog,"rtn_tid",&csTmp))
        {
                hv_txn_id.len = strlen(csTmp);
                strncpy((char *)hv_txn_id.arr, csTmp, hv_txn_id.len);
                ind_txn_id = 0;
DEBUGLOG(("Add:rcv_tid = [%.*s]\n",hv_txn_id.len,hv_txn_id.arr));
        }

        if(GetField_CString(hPspRedirectLog,"rd_domain",&csTmp))
        {
                hv_url.len = strlen(csTmp);
                strncpy((char *)hv_url.arr, csTmp, hv_url.len);
                ind_url = 0;
DEBUGLOG(("Add:rd_domain = [%.*s]\n",hv_url.len,hv_url.arr));
        }

        if(GetField_CString(hPspRedirectLog,"client_ipaddr",&csTmp))
        {
                hv_client_ip.len = strlen(csTmp);
                strncpy((char *)hv_client_ip.arr, csTmp, hv_client_ip.len);
                ind_client_ip = 0;
DEBUGLOG(("Add:client_ipaddr = [%.*s]\n",hv_client_ip.len,hv_client_ip.arr));
        }

        if(GetField_CString(hPspRedirectLog,"rcv_ts",&csTmp))
        {
                hv_rcv_ts.len = strlen(csTmp);
                strncpy((char *)hv_rcv_ts.arr, csTmp, hv_rcv_ts.len);
                ind_rcv_ts = 0;
DEBUGLOG(("Add:rcv_ts = [%.*s]\n",hv_rcv_ts.len,hv_rcv_ts.arr));
        }

        if(GetField_CString(hPspRedirectLog,"create_user",&csTmp))
        {
                hv_create_user.len = strlen(csTmp);
                strncpy((char *)hv_create_user.arr, csTmp, hv_create_user.len);
                ind_create_user = 0;
DEBUGLOG(("Add:create_user = [%.*s]\n",hv_create_user.len,hv_create_user.arr));
        }

	EXEC SQL EXECUTE
		BEGIN

		:hv_return_value := sp_psp_redirect_log_insert(
					:hv_txn_id:ind_txn_id,
					:hv_url:ind_url,
					:hv_client_ip:ind_client_ip,
					:hv_rcv_ts:ind_rcv_ts,
					:hv_create_user:ind_create_user);


		END;
	END-EXEC;

DEBUGLOG(("Add:Ret = [%d]\n",hv_return_value));
	if (hv_return_value == SP_OK)
	{
		DEBUGLOG(("Add:Normal Exit\n"));
		return PD_OK;
	}

	if (hv_return_value == SP_OTHER_ERR)  {
		ERRLOG("PspRedirectLog_Add: SP_OTHER_ERR TxnAbort\n");
		DEBUGLOG(("Add: SP_OTHER_ERR TxnAbort\n"));
		return PD_OTHER_ERR;
	}

	if (hv_return_value == SP_ERR)  {
		ERRLOG("PspRedirectLog_Add: SP_ERR TxnAbort\n");
		DEBUGLOG(("Add: SP_ERR TxnAbort\n"));
		return PD_ERR;
	}

add_error:
DEBUGLOG(("add_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
ERRLOG("PspRedirectLog_Add: SP_INTERNAL_ERR TxnAbort\n");
        EXEC SQL WHENEVER SQLERROR CONTINUE;
        return PD_ERR;
}

