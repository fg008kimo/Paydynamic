#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <oraca.h>
#include <sqlca.h>
#include "MerchantBucket.h"
#include "common.h"
#include "utilitys.h"
#include "dbutility.h"

#define SQLCA_STORAGE_CLASS extern
#define SQLCODE sqlca.sqlcode

char    cDebug;
void MerchantBucket(char    cdebug)
{
        cDebug = cdebug;
}

int Add(const hash_t *hRls)
{
        char		*csPtr;
	double		dTmp;


        EXEC SQL WHENEVER SQLERROR GOTO add_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;

        short 		hv_return_value;

	varchar		hv_posting_date[PD_DATE_LEN];	
	varchar		hv_merchant_id[PD_MERCHANT_ID_LEN];
	varchar		hv_currency_id[PD_CCY_ID_LEN];
	varchar		hv_psp_id[PD_PSP_ID_LEN];
	varchar		hv_bucket_type[PD_BUCKET_TYPE_LEN];
        double		hv_bal;
	varchar		hv_create_user[PD_USER_LEN];	



	short		ind_posting_date = -1;
	short		ind_merchant_id = -1;
	short		ind_currency_id = -1;
	short		ind_psp_id = -1;
	short		ind_bucket_type = -1;
	short		ind_bal = -1;
	short		ind_create_user = -1;

        EXEC SQL END DECLARE SECTION;

DEBUGLOG(("Add: Begin\n"));

	if (GetField_CString(hRls,"PHDATE",&csPtr)) {
                hv_posting_date.len = strlen(csPtr);
                memcpy(hv_posting_date.arr,csPtr,hv_posting_date.len);
                ind_posting_date = 0;
DEBUGLOG(("Add:host_posting_date = [%.*s]\n",hv_posting_date.len,hv_posting_date.arr));
        }

	if (GetField_CString(hRls,"merchant_id",&csPtr)) {
                hv_merchant_id.len = strlen(csPtr);
                memcpy(hv_merchant_id.arr,csPtr,hv_merchant_id.len);
                ind_merchant_id = 0;
DEBUGLOG(("Add:host_merchant_id = [%.*s]\n",hv_merchant_id.len,hv_merchant_id.arr));
        }

	if (GetField_CString(hRls,"bal_ccy",&csPtr)) {
                hv_currency_id.len = strlen(csPtr);
                memcpy(hv_currency_id.arr,csPtr,hv_currency_id.len);
                ind_currency_id = 0;
DEBUGLOG(("Add:currency_id = [%.*s]\n",hv_currency_id.len,hv_currency_id.arr));
        }

	if (GetField_CString(hRls,"psp_id",&csPtr)) {
                hv_psp_id.len = strlen(csPtr);
                memcpy(hv_psp_id.arr,csPtr,hv_psp_id.len);
                ind_psp_id = 0;
DEBUGLOG(("Add:psp_id = [%.*s]\n",hv_psp_id.len,hv_psp_id.arr));
        }

	if (GetField_CString(hRls,"bucket_type",&csPtr)) {
                hv_bucket_type.len = strlen(csPtr);
                memcpy(hv_bucket_type.arr,csPtr,hv_bucket_type.len);
                ind_bucket_type = 0;
DEBUGLOG(("Add:bucket_type = [%.*s]\n",hv_bucket_type.len,hv_bucket_type.arr));
        }

	/* bal  */
        if (GetField_Double(hRls,"bal",&dTmp)) {
                hv_bal = dTmp;
                ind_bal = 0;
DEBUGLOG(("Add:bal = [%f]\n",hv_bal));
        }

	if (GetField_CString(hRls,"add_user",&csPtr)) {
                hv_create_user.len = strlen(csPtr);
                memcpy(hv_create_user.arr,csPtr,hv_create_user.len);
                ind_create_user = 0;
DEBUGLOG(("Add:create_user = [%.*s]\n",hv_create_user.len,hv_create_user.arr));
        }

	EXEC SQL EXECUTE
                BEGIN
			:hv_return_value := sp_merchant_bucket_insert(
					:hv_posting_date:ind_posting_date,
					:hv_merchant_id:ind_merchant_id,
					:hv_currency_id:ind_currency_id,
					:hv_psp_id:ind_psp_id,
					:hv_bucket_type:ind_bucket_type,
					:hv_bal:ind_bal,
					:hv_create_user:ind_create_user);
	        END;
        END-EXEC;

DEBUGLOG(("Add:Ret = [%d]\n",hv_return_value));
        if (hv_return_value == SP_OK) {
DEBUGLOG(("Add:Normal Exit\n"));
                return PD_OK;
        }

        if (hv_return_value == SP_OTHER_ERR)  {
ERRLOG("MerchantBucket_Add: SP_OTHER_ERR TxnAbort\n");
DEBUGLOG(("Add: SP_OTHER_ERR TxnAbort\n"));
		TxnAbort();
                return PD_OTHER_ERR;
        }

        if (hv_return_value == SP_ERR)  {
ERRLOG("MerchantBucket_Add: SP_ERR TxnAbort\n");
DEBUGLOG(("Add: SP_ERR TxnAbort\n"));
		TxnAbort();
                return PD_ERR;
        }

add_error:
DEBUGLOG(("add_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
        EXEC SQL WHENEVER SQLERROR CONTINUE;
ERRLOG("MerchantBucket_Add: SP_INTERNAL_ERR TxnAbort\n");
DEBUGLOG(("Add: SP_INTERNAL_ERR TxnAbort\n"));
	TxnAbort();
        return PD_INTERNAL_ERR;
}
