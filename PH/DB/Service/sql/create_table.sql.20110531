ALTER TABLE PHUSER.SERVICE
 DROP PRIMARY KEY CASCADE;

DROP TABLE PHUSER.SERVICE CASCADE CONSTRAINTS;

CREATE TABLE PHUSER.SERVICE
(
  CODE              VARCHAR2(3 BYTE)            NOT NULL,
  URL               VARCHAR2(50 BYTE),
  EFFECT_DATE       DATE,
  DISABLED          VARCHAR2(1 BYTE),
  CREATE_TIMESTAMP  DATE                        DEFAULT sysdate,
  UPDATE_TIMESTAMP  DATE                        DEFAULT sysdate,
  CREATE_USER       VARCHAR2(20 BYTE),
  UPDATE_USER       VARCHAR2(20 BYTE)
)
TABLESPACE USER_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


CREATE UNIQUE INDEX PHUSER.SERVICE_PK ON PHUSER.SERVICE
(CODE, EFFECT_DATE)
LOGGING
TABLESPACE USER_IDX
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOPARALLEL;


ALTER TABLE PHUSER.SERVICE ADD (
  CONSTRAINT SERVICE_PK
  PRIMARY KEY
  (CODE, EFFECT_DATE)
  USING INDEX PHUSER.SERVICE_PK);

