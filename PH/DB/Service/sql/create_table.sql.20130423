ALTER TABLE SERVICE
 DROP PRIMARY KEY CASCADE;

DROP TABLE SERVICE CASCADE CONSTRAINTS;

CREATE TABLE SERVICE
(
  CODE              CHAR(3 BYTE)                NOT NULL,
  URL               VARCHAR2(50 BYTE),
  EFFECT_DATE       DATE,
  DISABLED          INTEGER,
  CREATE_TIMESTAMP  DATE                        DEFAULT sysdate,
  UPDATE_TIMESTAMP  DATE                        DEFAULT sysdate,
  CREATE_USER       VARCHAR2(20 BYTE),
  UPDATE_USER       VARCHAR2(20 BYTE),
  ROUTE_TYPE        CHAR(2 BYTE),
  PSP_ID            VARCHAR2(10 BYTE)
)
TABLESPACE USER_DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


CREATE INDEX SERVICE_PK ON SERVICE
(CODE, EFFECT_DATE, ROUTE_TYPE)
LOGGING
TABLESPACE USER_IDX
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
NOPARALLEL;


ALTER TABLE SERVICE ADD (
  CONSTRAINT SERVICE_PK
  PRIMARY KEY
  (CODE, EFFECT_DATE, ROUTE_TYPE)
  USING INDEX SERVICE_PK
  ENABLE VALIDATE);

ALTER TABLE SERVICE ADD (
  CONSTRAINT SERVICE_FK01 
  FOREIGN KEY (ROUTE_TYPE) 
  REFERENCES DEF_ROUTE_TYPE (DR_TYPE)
  ENABLE VALIDATE,
  CONSTRAINT SERVICE_FK02 
  FOREIGN KEY (CODE) 
  REFERENCES DEF_SERVICE_CODE (SC_CODE)
  ENABLE VALIDATE);

