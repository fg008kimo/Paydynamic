ALTER TABLE PSP_MASTER
 DROP PRIMARY KEY CASCADE;

DROP TABLE PSP_MASTER CASCADE CONSTRAINTS;

CREATE TABLE PSP_MASTER
(
  PM_CLIENT_ID         VARCHAR2(10 BYTE),
  PM_CLIENT_NAME       VARCHAR2(50 BYTE),
  PM_STATUS            VARCHAR2(2 BYTE),
  PM_CREATE_TIMESTAMP  DATE,
  PM_UPDATE_TIMESTAMP  DATE,
  PM_CREATE_USER       VARCHAR2(20 BYTE),
  PM_UPDATE_USER       VARCHAR2(20 BYTE)
)
TABLESPACE USER_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


CREATE UNIQUE INDEX PSP_MASTER_PK ON PSP_MASTER
(PM_CLIENT_ID)
LOGGING
TABLESPACE USER_IDX
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOPARALLEL;


ALTER TABLE PSP_MASTER ADD (
  CONSTRAINT PSP_MASTER_PK
  PRIMARY KEY
  (PM_CLIENT_ID)
  USING INDEX PSP_MASTER_PK);

ALTER TABLE PSP_MASTER ADD (
  CONSTRAINT PSP_MASTER_FK01 
  FOREIGN KEY (PM_STATUS) 
  REFERENCES DEF_STATUS (DS_TYPE)
  ENABLE VALIDATE);
