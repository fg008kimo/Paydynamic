	char	*csTmp;
	int	iTmp;

        EXEC SQL WHENEVER SQLERROR GOTO addkeywords_error;
        EXEC SQL WHENEVER NOTFOUND CONTINUE;

        EXEC SQL BEGIN DECLARE SECTION;
		varchar		hv_int_bank_code[PD_FORMAT_ID_LEN];
