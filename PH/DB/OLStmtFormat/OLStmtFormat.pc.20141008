int GetTxnCodeSplit(const hash_t* hContext, recordset_t* myRec)
{
	int iRet = PD_FOUND;
	int iCnt = 0;
	char *csTmp;

	hash_t *hRec;

	EXEC SQL WHENEVER SQLERROR GOTO getsplit_error;
	EXEC SQL WHENEVER NOTFOUND CONTINUE;

	EXEC SQL BEGIN DECLARE SECTION;
		varchar	hv_int_bank_code[PD_BANK_CODE_LEN];
		varchar	hv_ccy[PD_CCY_ID_LEN];

		varchar	v_code[PD_TXN_CODE_LEN + 1];
		varchar	v_replace_code[PD_TXN_CODE_LEN + 1];
		varchar	v_split_code[PD_TXN_CODE_LEN + 1];
		varchar	v_amt_type[PD_AMT_TYPE_LEN + 1];
		double	v_split_amount;

		short	ind_code = -1;
		short	ind_replace_code = -1;
		short	ind_split_code = -1;
		short	ind_amt_type = -1;
		short	ind_split_amount = -1;
	EXEC SQL END DECLARE SECTION;

	if (GetField_CString(hContext,"int_bank_code",&csTmp)) {
		hv_int_bank_code.len = strlen(csTmp);
		strncpy((char*)hv_int_bank_code.arr, csTmp, hv_int_bank_code.len);
DEBUGLOG(("GetTxnCodeSplit int_bank_code = [%.*s]\n", hv_int_bank_code.len, hv_int_bank_code.arr));
	}

	if (GetField_CString(hContext,"acct_ccy",&csTmp)) {
		hv_ccy.len = strlen(csTmp);
		strncpy((char*)hv_ccy.arr, csTmp, hv_ccy.len);
DEBUGLOG(("GetTxnCodeSplit ccy = [%.*s]\n", hv_ccy.len, hv_ccy.arr));
	}

	EXEC SQL DECLARE getsplitcursor CURSOR FOR
		SELECT	OBTS_CODE,
			OBTS_REPLACE_CODE,
			OBTS_SPLIT_CODE,
			OBTC_AMT_TYPE,
			OBTS_SPLIT_AMOUNT
		FROM	OL_BAID_TXN_CODE,
			OL_BAID_TXN_CODE_SPLIT
		WHERE	OBTS_BANK_CODE = :hv_int_bank_code
		AND	OBTS_CCY = :hv_ccy
		AND	OBTS_SPLIT_CODE = OBTC_CODE;

	EXEC SQL OPEN getsplitcursor;
	for (;;) {
		EXEC SQL FETCH getsplitcursor
		INTO	:v_code:ind_code,
			:v_replace_code:ind_replace_code,
			:v_split_code:ind_split_code,
			:v_amt_type:ind_amt_type,
			:v_split_amount:ind_split_amount;

		if (SQLCODE == SQL_NOT_FOUND) {
			break;
		}

		iCnt++;

		hRec = (hash_t*) malloc (sizeof(hash_t));
		hash_init(hRec, 0);

/* code */
		if (ind_code >= 0) {
			v_code.arr[v_code.len] = '\0';
			PutField_CString(hRec, "code", (char*)v_code.arr);
DEBUGLOG(("GetTxnCodeSplit code = [%s]\n",v_code.arr));
		}
/* replace_code */
		if (ind_replace_code >= 0) {
			v_replace_code.arr[v_replace_code.len] = '\0';
			PutField_CString(hRec, "replace_code", (char*)v_replace_code.arr);
DEBUGLOG(("GetTxnCodeSplit replace_code = [%s]\n",v_replace_code.arr));
		}
/* split_code */
		if (ind_split_code >= 0) {
			v_split_code.arr[v_split_code.len] = '\0';
			PutField_CString(hRec, "split_code", (char*)v_split_code.arr);
DEBUGLOG(("GetTxnCodeSplit split_code = [%s]\n",v_split_code.arr));
		}
/* amt_type */
		if (ind_amt_type >= 0) {
			v_amt_type.arr[v_amt_type.len] = '\0';
			PutField_CString(hRec, "split_type", (char*)v_amt_type.arr);
DEBUGLOG(("GetTxnCodeSplit amt_type = [%s]\n",v_amt_type.arr));
		}
/* split_amt */
		if (ind_split_amount >= 0) {
			PutField_Double(hRec, "split_amt", v_split_amount);
DEBUGLOG(("GetTxnCodeSplit split_amount = [%.2lf]\n",v_split_amount));
		}

		RecordSet_Add(myRec,hRec);
	}
	EXEC SQL CLOSE getsplitcursor;

	if (iCnt > 0) {
DEBUGLOG(("GetTxnCodeSplit FOUND\n"));
	} else {
		iRet = PD_NOT_FOUND;
DEBUGLOG(("GetTxnCodeSplit NOT FOUND!!!\n"));
	}

DEBUGLOG(("GetTxnCodeSplit Normal Exit! iRet = [%d]\n", iRet));
	return iRet;

getsplit_error:
DEBUGLOG(("getsplit_error code %d\n", sqlca.sqlcode));
DEBUGLOG(("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc));
ERRLOG("OLStmtFormat getsplit_error code %d\n", sqlca.sqlcode);
ERRLOG("\n%.*s\n", sqlca.sqlerrm.sqlerrml, sqlca.sqlerrm.sqlerrmc);
	EXEC SQL CLOSE getsplitcursor;
	EXEC SQL WHENEVER SQLERROR CONTINUE;
	return PD_ERR;
}


