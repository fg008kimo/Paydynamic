		varchar		hv_format_txn_code[PD_TXN_CODE_LEN];
		varchar		hv_format_template[PD_FORMAT_TEMPLATE_LEN];
		varchar		hv_format_col_name[PD_COL_NAME_LEN];
		int		hv_display_order;
		int		hv_disabled;
