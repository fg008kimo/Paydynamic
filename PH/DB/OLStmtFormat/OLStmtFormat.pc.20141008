DEBUGLOG(("GetByFormatId (%s) default = [%s]\n", "STATEMENT_DATE", PD_DEFAULT_DATE_FORMAT));
				} else if (!strcmp((char*)v_cont_desc.arr,"statement_time")) {
					PutField_CString(myHash, "STATEMENT_TIME", PD_DEFAULT_TIME_FORMAT); //
DEBUGLOG(("GetByFormatId (%s) default = [%s]\n", "STATEMENT_TIME", PD_DEFAULT_TIME_FORMAT));
