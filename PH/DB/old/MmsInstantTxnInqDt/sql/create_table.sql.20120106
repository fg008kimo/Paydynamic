  MID_BANK_CHARGE_REFUND  NUMBER(14,2),
  MID_FILING_NUMBER       VARCHAR2(50 BYTE),
  MID_CREATE_USER         VARCHAR2(20 BYTE),
  MID_CREATE_TIMESTAMP    DATE                  DEFAULT sysdate,
  MID_UPDATE_USER         VARCHAR2(20 BYTE),
  MID_UPDATE_TIMESTAMP    DATE                  DEFAULT sysdate
)
TABLESPACE USER_DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


