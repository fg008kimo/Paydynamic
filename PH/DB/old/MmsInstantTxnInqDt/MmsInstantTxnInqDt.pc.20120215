		short           ind_mb_id = -1;
		short           ind_bank_id = -1;
		short           ind_status = -1;
		short           ind_ar_ind = -1;
		short           ind_isd_ind = -1;
		short           ind_txn_date = -1;
		short           ind_txn_amt = -1;
