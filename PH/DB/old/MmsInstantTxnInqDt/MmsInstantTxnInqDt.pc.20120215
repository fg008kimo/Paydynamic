        EXEC SQL END DECLARE SECTION;

DEBUGLOG(("Add: Begin\n"));

	if (GetField_CString(hRls, "inq_seq", &csTmp)) {
                hv_inq_id.len = strlen(csTmp);
                memcpy(hv_inq_id.arr, csTmp, hv_inq_id.len);
