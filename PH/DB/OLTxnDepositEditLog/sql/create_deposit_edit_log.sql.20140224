DROP TABLE OL_TXN_DEPOSIT_EDIT_LOG CASCADE CONSTRAINTS;

CREATE TABLE OL_TXN_DEPOSIT_EDIT_LOG
(
  OTC_TXN_ID                VARCHAR2(16 BYTE),
  OTC_EXEC_SEQ              INTEGER,
  OTC_AMOUNT                NUMBER(12,2),
  OTC_DEPOSIT_DATETIME      VARCHAR2(14 BYTE),
  OTC_BANK_CODE             VARCHAR2(50 BYTE),
  OTC_BANK_ACCT_NUM         VARCHAR2(50 BYTE),
  OTC_DEPOSIT_BANK_CODE     VARCHAR2(10 BYTE),
  OTC_DEPOSIT_BANK          VARCHAR2(150 BYTE),
  OTC_DEPOSIT_REF           VARCHAR2(150 BYTE),
  OTC_MATCH                 INTEGER,
  OTC_CREATE_USER           VARCHAR2(20 BYTE),
  OTC_CREATE_TIMESTAMP      DATE                    DEFAULT sysdate
)
TABLESPACE USER_DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MAXSIZE          UNLIMITED
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

CREATE UNIQUE INDEX OL_TXN_DEPOSIT_EDIT_LOG_PK ON OL_TXN_DEPOSIT_EDIT_LOG
(OTC_TXN_ID,OTC_EXEC_SEQ)
LOGGING
TABLESPACE USER_IDX
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MAXSIZE          UNLIMITED
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
NOPARALLEL;


ALTER TABLE OL_TXN_DEPOSIT_EDIT_LOG ADD (
  CONSTRAINT OL_TXN_DEPOSIT_EDIT_LOG_PK
  PRIMARY KEY
  (OTC_TXN_ID,OTC_EXEC_SEQ)
  USING INDEX PHUSER.OL_TXN_DEPOSIT_EDIT_LOG_PK
  ENABLE VALIDATE);

ALTER TABLE OL_TXN_DEPOSIT_EDIT_LOG ADD (
  CONSTRAINT OL_TXN_DEPOSIT_EDIT_LOG_FK01 
  FOREIGN KEY (OTC_TXN_ID) 
  REFERENCES OL_TXN_HEADER (OTH_TXN_ID)
  ENABLE VALIDATE);

