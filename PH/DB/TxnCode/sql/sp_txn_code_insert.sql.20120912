CREATE OR REPLACE FUNCTION sp_txn_code_insert(
	in_txn_code		txn_code.tc_code%type,
	in_process_type		txn_code.tc_process_type%type,
	in_process_code		txn_code.tc_process_code%type,
	in_desc			txn_code.tc_desc%type,
	in_apply_fee		txn_code.tc_apply_fee%type,
	in_voidable		txn_code.tc_voidable%type,
	in_apply_limit		txn_code.tc_apply_limit%type,
	in_fe_display		txn_code.tc_fe_display%type,
	in_create_user		txn_code.tc_create_user%type)
  RETURN NUMBEr IS
BEGIN

	INSERT INTO txn_code(
		tc_code,
		tc_process_type,
		tc_process_code,
		tc_desc,
		tc_create_user,
		tc_create_timestamp,
		tc_update_user,
		tc_update_timestamp,
		tc_apply_fee,
		tc_voidable,
		tc_apply_limit,	
		tc_fe_display
		)
	VALUES (	
		in_txn_code,
		in_process_type,
		in_process_code,
		in_desc,
		in_create_user,
		sysdate,
		in_create_user,
		sysdate,
		in_apply_fee,
		in_voidable,
		in_apply_limit,
		in_fe_display
		);

	IF SQL%ROWCOUNT = 0 THEN
		RETURN 1;
	ELSE
		RETURN 0;
	END IF;

EXCEPTION
	WHEN OTHERS THEN	
		RETURN 9;

END sp_txn_code_insert;
/
