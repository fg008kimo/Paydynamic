ALTER TABLE PHUSER.TXN_CODE
 DROP PRIMARY KEY CASCADE;

DROP TABLE PHUSER.TXN_CODE CASCADE CONSTRAINTS;

CREATE TABLE PHUSER.TXN_CODE
(
  TC_CODE              CHAR(3 BYTE),
  TC_PROCESS_TYPE      CHAR(4 BYTE),
  TC_PROCESS_CODE      CHAR(6 BYTE),
  TC_DESC              VARCHAR2(50 BYTE),
  TC_CREATE_USER       VARCHAR2(20 BYTE),
  TC_CREATE_TIMESTAMP  DATE                     DEFAULT sysdate,
  TC_UPDATE_USER       VARCHAR2(20 BYTE),
  TC_UPDATE_TIMESTAMP  DATE                     DEFAULT sysdate,
  TC_APPLY_FEE         INTEGER                  DEFAULT 0,
  TC_VOIDABLE          INTEGER                  DEFAULT 0,
  TC_APPLY_LIMIT       INTEGER                  DEFAULT 0,
  TC_FE_DISPLAY        INTEGER                  DEFAULT 1
)
TABLESPACE USER_DATA
RESULT_CACHE (MODE DEFAULT)
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN TXN_CODE.TC_FE_DISPLAY IS 'Front End Only, 0 : Not allow to Display, 1: Allow to Display';


CREATE UNIQUE INDEX PHUSER.TXN_CODE_PK ON PHUSER.TXN_CODE
(TC_CODE)
LOGGING
TABLESPACE USER_DATA
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
            FLASH_CACHE      DEFAULT
            CELL_FLASH_CACHE DEFAULT
           )
NOPARALLEL;


ALTER TABLE PHUSER.TXN_CODE ADD (
  CONSTRAINT TXN_CODE_PK
  PRIMARY KEY
  (TC_CODE)
  USING INDEX PHUSER.TXN_CODE_PK
  ENABLE VALIDATE);

