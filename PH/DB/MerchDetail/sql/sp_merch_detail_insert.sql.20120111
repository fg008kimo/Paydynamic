DROP FUNCTION PHUSER.SP_MERCH_DETAIL_INSERT;

CREATE OR REPLACE FUNCTION PHUSER.sp_merch_detail_insert(
	in_merchant_id		merch_detail.merchant_id%type,
	in_client_id		merch_detail.client_id%type,
	in_name			merch_detail.name%type,
        in_disabled		merch_detail.disabled%type,
	in_merchant_type	merch_detail.merchant_type%type,
	in_create_user		merch_detail.create_user%type)
  RETURN NUMBER IS

BEGIN
  INSERT INTO merch_detail(
	merchant_id,
	client_id,
	name,
        disabled,
	merchant_type,
	create_timestamp,
	create_user
	)

  VALUES(
	in_merchant_id,
	in_client_id,
	in_name,
        in_disabled,
	in_merchant_type,
	sysdate,
	in_create_user
	);

  IF SQL%ROWCOUNT = 0 THEN
     RETURN 1;
  ELSE
     RETURN 0;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
     RETURN 9;

END sp_merch_detail_insert;
/

