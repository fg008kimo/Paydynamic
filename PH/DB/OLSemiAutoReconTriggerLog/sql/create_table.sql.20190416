


CREATE TABLE OL_SEMI_AUTO_RECON_TRIGGER_LOG
(
  OTL_TRIGGER_SEQ       INTEGER                 NOT NULL,
  OTL_TS_TYPE           VARCHAR2(10 BYTE),
  OTL_S_TS_FROM         VARCHAR2(20 BYTE),
  OTL_S_TS_TO           VARCHAR2(20 BYTE),
  OTL_S_PROVIDER_ID     VARCHAR2(10 BYTE),
  OTL_S_PID             VARCHAR2(10 BYTE),
  OTL_S_TXN_AMT         NUMBER(12,2),
  OTL_R_TS_FROM         VARCHAR2(20 BYTE),
  OTL_R_TS_TO           VARCHAR2(20 BYTE),
  OTL_R_PROVIDER_ID     VARCHAR2(10 BYTE),
  OTL_R_PID             VARCHAR2(10 BYTE),
  OTL_R_TXN_AMT         NUMBER(12,2),
  OTL_TRIGGER_STATUS    CHAR(1 BYTE),
  OTL_CREATE_TIMESTAMP  DATE                    DEFAULT sysdate,
  OTL_CREATE_USER       VARCHAR2(20 BYTE),
  OTL_UPDATE_TIMESTAMP  DATE                    DEFAULT sysdate,
  OTL_UPDATE_USER       VARCHAR2(20 BYTE)
)
TABLESPACE USER_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


