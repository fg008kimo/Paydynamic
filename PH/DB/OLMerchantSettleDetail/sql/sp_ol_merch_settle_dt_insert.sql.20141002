CREATE OR REPLACE FUNCTION sp_ol_merch_settle_dt_insert(
	in_txn_id		ol_merchant_settlement_detail.osd_txn_id%type,
	in_merchant_id		ol_merchant_settlement_detail.osd_merchant_id%type,
	in_country_id		ol_merchant_settlement_detail.osd_country_id%type,
	in_service_code		ol_merchant_settlement_detail.osd_service_code%type,
	in_status		ol_merchant_settlement_detail.osd_status%type,
	in_request_ccy		ol_merchant_settlement_detail.osd_request_ccy%type,
	in_inter_ccy		ol_merchant_settlement_detail.osd_inter_ccy%type,
	in_deliver_ccy		ol_merchant_settlement_detail.osd_deliver_ccy%type,
	in_request_amt		ol_merchant_settlement_detail.osd_request_amt%type,
	in_inter_amt		ol_merchant_settlement_detail.osd_inter_amt%type,
	in_deliver_amt		ol_merchant_settlement_detail.osd_deliver_amt%type,
	in_remark		ol_merchant_settlement_detail.osd_remark%type,
	in_type			ol_merchant_settlement_detail.osd_settlement_type%type,
	in_create_user		ol_merchant_settlement_detail.osd_create_user%type
)
return number is
begin
	insert into ol_merchant_settlement_detail
		(osd_txn_id,
		 osd_merchant_id,
		 osd_country_id,
		 osd_service_code,
		 osd_status,
		 osd_request_ccy,
		 osd_inter_ccy,
		 osd_deliver_ccy,
		 osd_request_amt,
		 osd_inter_amt,
		 osd_deliver_amt,
		 osd_remark,
		 osd_settlement_type,
		 osd_create_user,
		 osd_update_user,
		 osd_create_timestamp,
		 osd_update_timestamp)
	values(in_txn_id,
               in_merchant_id,
               in_country_id,
	       in_service_code,
	       in_status,
	       in_request_ccy,
	       in_inter_ccy,
	       in_deliver_ccy,
	       in_request_amt,
	       in_inter_amt,
	       in_deliver_amt,
	       in_remark,
	       in_type,
	       in_create_user,
	       in_create_user,
	       sysdate,
	       sysdate);
	if SQL%ROWCOUNT = 0 THEN
		return 1;

	else
		return 0;
	end if;

exception
  when others then
  return 9;
end sp_ol_merch_settle_dt_insert;
/
