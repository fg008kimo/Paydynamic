		sprintf((char*)csTmp,"%ld",double2long(dTmp));
DEBUGLOG(("BuildData:: AMOUNT = [%s]\n",csTmp));
       		strcat((char*)csDATA,(char*)csTmp);
       		strcat((char*)csDATA,MY_XPAY_DATA_TOKEN);	
