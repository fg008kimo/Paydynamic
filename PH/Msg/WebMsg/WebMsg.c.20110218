                strcat((char*)outMsg,MY_WEB_FIELD_TOKEN);
                strcat((char*)outMsg,csPtr);
                strcat((char*)outMsg,MY_WEB_TOKEN);

        }

        for (i =0;  i <  iTotal; i++) {
/* txn_seq */
                sprintf(csTag,"txn_seq_%d",i);
                if (GetField_CString(hIn,csTag,&csPtr)) {
DEBUGLOG(("FormatBatchMsg:: %s = [%s]\n",csTag,csPtr));
