/* process type */
		if (!strcmp(csTag,"process_type")) {
			strcpy(csTag,"process_type");
			PutField_CString(hOut,csTag,csValue);
DEBUGLOG(("BreakDownMsg:[%s] = [%s]\n",csTag,csValue));
		}
/* process code */
		else if (!strcmp(csTag,"process_code")) {
			strcpy(csTag,"process_code");
			PutField_CString(hOut,csTag,csValue);
DEBUGLOG(("BreakDownMsg:[%s] = [%s]\n",csTag,csValue));
