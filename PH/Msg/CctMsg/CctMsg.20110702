        char    *csBuf;
        char    *p;

        csBuf = (char*) malloc (1024 +1);

        memcpy(csBuf,inMsg,inLen);
        csBuf[inLen] = '\0';
DEBUGLOG(("BreakDownRespMsg()\n"));
DEBUGLOG(("DATA = [%s]\n",inMsg));

/* order_num */
        p = GetField(csBuf,"order_num",MY_CCT_TOKEN,MY_CCT_FIELD_TOKEN);
