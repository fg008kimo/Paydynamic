// orderNo
	if (GetField_CString(hIn, "txn_seq", &csPtr)) {
		strcat((char*)csBuf, "orderNo");
		strcat((char*)csBuf, MY_HDV_FIELD_TOKEN);
		strcat((char*)csBuf, csPtr);
