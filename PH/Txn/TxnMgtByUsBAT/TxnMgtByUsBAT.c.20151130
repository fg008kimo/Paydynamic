#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "common.h"
#include "utilitys.h"
#include "ObjPtr.h"
#include "internal.h"
#include "TxnMgtByUsBAT.h"
#include "myrecordset.h"
#include <curl/curl.h>
#include "queue_utility.h"
#include "mq_db.h"

char cDebug;
OBJPTR(Txn);

void TxnMgtByUsBAT(char    cdebug)
{
        cDebug = cdebug;
}

int     Authorize(hash_t* hContext,
		  const hash_t* hRequest,
		  hash_t* hResponse)
{
        int	iRet = PD_OK;

	char	*csTmp = NULL;
	char	*csTxnId = NULL;
	char	*csEntityId = NULL;
	char	*csProductCode = NULL;
	char	*csTxnCcy = NULL;
	char	*csToCcy = NULL;
	char	*csUpdateUser = NULL;  
	double	dAmt=0.0;
	int	iSkipTolCheck = PD_FALSE;


DEBUGLOG(("Authorize::Start\n"));
/* txn_seq */
        if(GetField_CString(hContext,"txn_seq",&csTxnId)){
		PutField_CString(hResponse,"txn_seq_tf",csTxnId);
DEBUGLOG(("Authorize::from_txn_seq = [%s]\n", csTxnId));
        }
        else{
DEBUGLOG(("Authorize::txnid not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::txnid not found!!\n");
                iRet=INT_INVALID_TXN;
                PutField_Int(hContext,"internal_error",iRet);
        }

/*txn_code*/
        if(GetField_CString(hContext,"txn_code",&csTmp)){
DEBUGLOG(("Authorize::txn_code = [%s]\n", csTmp));

		PutField_CString(hContext,"to_txn_code",PD_MI_TXN_CODE_OPBANK_INTRA_TRF_IN);
DEBUGLOG(("Authorize::to_txn_code = [%s]\n", PD_MI_TXN_CODE_OPBANK_INTRA_TRF_IN));

	}

/* add_user */
        if(GetField_CString(hRequest,"add_user",&csUpdateUser)){
                PutField_CString(hContext,"add_user",csUpdateUser);
                PutField_CString(hContext,"update_user",csUpdateUser);
DEBUGLOG(("Authorize::add_user= [%s]\n",csUpdateUser));
        }

/* entity_id */
        if(GetField_CString(hRequest,"entity_id",&csEntityId)){
                PutField_CString(hContext,"entity_id",csEntityId);
DEBUGLOG(("Authorize::entity_id = [%s]\n",csEntityId));
        }
	else {
DEBUGLOG(("Authorize::entity_id not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::entity_id not found!!\n");
		iRet = INT_MI_ENTITY_ID_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
	}

/* product_code */
        if(GetField_CString(hRequest,"product_code",&csProductCode)){
                PutField_CString(hContext,"product_code",csProductCode);
DEBUGLOG(("Authorize::product_code = [%s]\n",csProductCode));
        } 
	else {
DEBUGLOG(("Authorize::product_code not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::product_code not found!!\n");
		iRet=INT_MI_PRODUCT_CODE_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
	}

/* txn_ccy */
        if(GetField_CString(hRequest,"txn_ccy",&csTxnCcy)){
                PutField_CString(hContext,"fr_ccy",csTxnCcy);
DEBUGLOG(("Authorize::fr_ccy = [%s]\n",csTxnCcy));
        }
        else{
DEBUGLOG(("Authorize::ccy not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::ccy not found!!\n");
                iRet=INT_CURRENCY_CODE_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
        }

/* txn_amt */
        if(GetField_Double(hContext,"txn_amt",&dAmt)){
                PutField_Double(hContext, "fr_amt", dAmt);
DEBUGLOG(("Authorize::txn_amt= [%f]\n",dAmt));
        }
        else{
DEBUGLOG(("Authorize::txn_amt not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::txn_amt not found!!\n");
                iRet=INT_PAY_AMOUNT_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
        }

/* cost_amt */
	if(GetField_CString(hRequest,"cost_amt",&csTmp)){
DEBUGLOG(("Authorize: cost_amt = [%s]\n",csTmp));
	}

/* report_date*/
        if(GetField_CString(hRequest,"report_date",&csTmp)){
		PutField_CString(hContext, "report_date", csTmp);
DEBUGLOG(("Authorize::report_date = [%s]\n", csTmp)); 
        }

/* remark */
        if(GetField_CString(hRequest,"remark",&csTmp)){
                PutField_CString(hContext,"remark",csTmp);
DEBUGLOG(("Authorize::remark = [%s]\n",csTmp));
        }

/* to_entity_id */
        if(GetField_CString(hRequest,"to_entity_id",&csEntityId)){
                PutField_CString(hContext,"to_entity_id",csEntityId);
DEBUGLOG(("Authorize::to_entity_id = [%s]\n",csEntityId));
        }
	else {
DEBUGLOG(("Authorize::to_entity_id not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::to_entity_id not found!!\n");
		iRet = INT_MI_ENTITY_ID_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
	}

/* to_product_code */
        if(GetField_CString(hRequest,"to_product_code",&csProductCode)){
                PutField_CString(hContext,"to_product_code",csProductCode);
DEBUGLOG(("Authorize::to_product_code = [%s]\n",csProductCode));
        } 
	else {
DEBUGLOG(("Authorize::to_product_code not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::to_product_code not found!!\n");
		iRet=INT_MI_PRODUCT_CODE_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
	}

/* to_ccy */
        if(GetField_CString(hRequest,"dst_txn_ccy",&csToCcy)){
                PutField_CString(hContext,"to_ccy",csToCcy);
DEBUGLOG(("Authorize::to_ccy= [%s]\n",csToCcy));
        }
        else{
DEBUGLOG(("Authorize::to_ccy not found!!\n"));
ERRLOG("TxnMgtByUsBAT::Authorize::to_ccy not found!!\n");
                iRet=INT_CURRENCY_CODE_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
        }

/* to_txnamt */
	if(GetField_CString(hRequest,"to_txnamt",&csTmp)){
DEBUGLOG(("Authorize: to_txnamt = [%s]\n",csTmp));
	}

/* to_cost_amt */
	if(GetField_CString(hRequest,"to_cost_amt",&csTmp)){
DEBUGLOG(("Authorize: to_cost_amt = [%s]\n",csTmp));
	}

/* to_report_date*/
        if(GetField_CString(hRequest,"to_report_date",&csTmp)){
		PutField_CString(hContext, "to_report_date", csTmp);
DEBUGLOG(("Authorize::to_report_date = [%s]\n", csTmp)); 
        }

	if(GetField_CString(hContext,"PHDATE",&csTmp)){
		PutField_CString(hContext, "txn_date", csTmp);
DEBUGLOG(("Authorize::txn_date (PHDATE) = [%s]\n", csTmp)); 
	}

/* to_remark */
        if(GetField_CString(hRequest,"to_remark",&csTmp)){
                PutField_CString(hContext,"to_remark",csTmp);
DEBUGLOG(("Authorize::to_remark = [%s]\n",csTmp));
        }

/* skip_tol_check */
        if(GetField_CString(hRequest,"skip_tol_check",&csTmp)){
		iSkipTolCheck = atoi(csTmp);
	}
	PutField_Int(hContext,"skip_tol_check",iSkipTolCheck);
DEBUGLOG(("Authorize::skip_tol_check = [%d]\n", iSkipTolCheck)); 



        PutField_CString(hContext,"mini_txn_type",PD_MI_TXN_TYPE_BANK_BALTFR);


DEBUGLOG(("Authorize::Call TxnMgtByUs2MiniMMM:Authorize\n"));
	TxnObjPtr = CreateObj(TxnPtr,"TxnMgtByUs2MiniMMM","Authorize");
	iRet = (unsigned long)((*TxnObjPtr)(hContext,hRequest,hResponse));
	if (iRet != PD_OK) {
DEBUGLOG(("Authorize::Call TxnMgtByUs2MiniMMM:Authorize FAILED, ret [%d]\n",iRet));
ERRLOG("TxnMgtByUsBAT::Authorize::Call TxnMgtByUs2MiniMMM:Authorize FAILED, ret [%d]\n",iRet);
	}

DEBUGLOG(("TxnMgtByUsBAT Normal Exit() iRet = [%d]\n",iRet));
        return iRet;
}
