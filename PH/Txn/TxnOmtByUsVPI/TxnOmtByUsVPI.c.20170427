						PutField_Char(hBalance,"response_mode",PD_REVERSED);
                        
DEBUGLOG(("Authorize::Call BOOLBalance::UpdatePayoutAmount\n"));
						BOObjPtr = CreateObj(BOPtr,"BOOLBalance","UpdatePayoutAmount");
                        			if ((unsigned long)(*BOObjPtr)(hBalance) == PD_OK) {

/* merchant_open_bal */
