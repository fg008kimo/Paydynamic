                        		}

/* party_type */
					if(i>0) {
                                		if (iVersionNo != 1) {
                                        		PutField_Char(hBalance,"party_type",PD_TYPE_PSP);
DEBUGLOG(("Authorize:: party_type = [%d][%c]\n", i, PD_TYPE_PSP));
