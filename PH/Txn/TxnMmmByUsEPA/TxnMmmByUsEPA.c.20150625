#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "common.h"
#include "utilitys.h"
#include "ObjPtr.h"
#include "internal.h"
#include "TxnMmmByUsEPA.h"
#include "myrecordset.h"

char cDebug;

OBJPTR(DB);

void TxnMmmByUsEPA(char    cdebug)
{
        cDebug = cdebug;
}

int     Authorize(hash_t* hContext,
                  const hash_t* hRequest,
                  hash_t* hResponse)
{
        int     iRet = PD_OK;
        int     iDtlRet = FOUND;
		
	int	iNatCnt = 0;
	int	i = 0;

	char	cTmp;
	
	char*	csTmp = NULL;
	
	char	csEntityId[PD_MMS_ENTITY_ID_LEN + 1];
	char	csNatureGrp[PD_NATURE_GROUP_LEN + 1];
	char	csNatureGrpVal[PD_NATURE_GROUP_VAL_LEN + 1];
	char	csNatureId[PD_NATURE_ID_LEN + 1];
	char	csNewNatureId[PD_NATURE_ID_LEN + 1];
	char	csUser[PD_USER_LEN + 1];
	char	csTag[PD_TAG_LEN + 1];	

	hash_t  *hNatRec = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hNatRec,0);

	hash_t 	*hRec = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hRec,0);

        recordset_t	*rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));
       	recordset_init(rRecordSet,0);

	hash_t  *hNewRec = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hNewRec,0);

        recordset_t     *rNewRecordset = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rNewRecordset,0);

	cTmp = ' ';

DEBUGLOG(("Authorize()\n"));

/* eid (entity_id) */
	if (GetField_CString(hRequest,"entity_id",&csTmp)) {
		sprintf(csEntityId,"%s",csTmp);
DEBUGLOG(("Authorize: eid = [%s]\n",csEntityId));
	} else {
DEBUGLOG(("Authorize:: eid missing!!\n"));
ERRLOG("TxnMmsByUsEPA:: Authorize:: eid missing!!\n");
		iRet = INT_MMS_ENTITY_ID_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
	}

/* nat_cnt (nature_group_count) */
	if (GetField_CString(hRequest,"nat_cnt",&csTmp)) {
                iNatCnt = atoi(csTmp);
DEBUGLOG(("Authorize: nat_cnt = [%d]\n",iNatCnt));
        } else {
DEBUGLOG(("Authorize:: nat_cnt missing!!\n"));
ERRLOG("TxnMmsByUsEPA:: Authorize:: nat_cnt missing!!\n");
                iRet = INT_MMS_NATURE_CNT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* create_user */
	if (GetField_CString(hRequest,"user",&csTmp)) {
		sprintf(csUser,"%s",csTmp);
DEBUGLOG(("Authorize: create_user = [%s]\n",csUser));
	} else {
DEBUGLOG(("Authorize:: user missing!!\n"));
ERRLOG("TxnMmsByUsEPA:: Authorize:: user missing!!\n");
		iRet = INT_USER_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
	}

	if (iRet == PD_OK) {

		// Get Nature Group and Nature Group Value
		for (i=1; i<=iNatCnt; i++) 
		{
/* grp */
			sprintf(csTag,"nat.%d.grp",i);
			if (GetField_CString(hRequest,csTag,&csTmp)) {
				sprintf(csNatureGrp,"%s",csTmp);
DEBUGLOG(("Authorize: grp = [%s]\n",csNatureGrp));
			
				// Check Nature Group Exists
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrp Find\n"));
                        	DBObjPtr = CreateObj(DBPtr, "DBMmsEntityNatureGrp","Find");
                        	iDtlRet = (unsigned long)(*DBObjPtr)(csNatureGrp);
                        	if (iDtlRet == FOUND) {
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrp:: Found!!!\n"));
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrp:: csNatureGrp = [%s]\n", csNatureGrp));

					sprintf(csTag,"%s_%s_%d",PD_NATURE_TAG,"group",i);
                        		PutField_CString(hNatRec,csTag,csNatureGrp);
                        		PutField_CString(hRec,"group",csNatureGrp);
                        	} else if (iDtlRet == NOT_FOUND) {
					iRet = INT_MMS_NATURE_GRP_NOT_FOUND;
					PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrp:: grp Not Found!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityNatureGrp:: grp Not Found!!!\n");
                        	} else {
                        	        iRet = INT_ERR;
                        	        PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrp:: Error!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityNatureGrp:: Error!!!\n");
                        	}
			} else {
DEBUGLOG(("Authorize:: grp missing!!\n"));
ERRLOG("TxnMmsByUsEPA:: Authorize:: grp missing!!\n");
				iRet = INT_MMS_NATURE_GRP_NOT_FOUND;
				PutField_Int(hContext,"internal_error",iRet);
			}

/* val */
			sprintf(csTag,"nat.%d.val",i);
			if (GetField_CString(hRequest,csTag,&csTmp)) {
				sprintf(csNatureGrpVal,"%s",csTmp);
DEBUGLOG(("Authorize: val = [%s]\n",csNatureGrpVal));

				// Check Nature Group Val Exists
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrpVal Find\n"));
                        	DBObjPtr = CreateObj(DBPtr, "DBMmsEntityNatureGrpVal","Find");
                        	iDtlRet = (unsigned long)(*DBObjPtr)(csNatureGrp,csNatureGrpVal);
                        	if (iDtlRet == FOUND) {
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrpVal:: Found!!!\n"));
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrpVal:: csNatureGrpVal = [%s]\n", csNatureGrpVal));

					sprintf(csTag,"%s_%s_%d",PD_NATURE_TAG,"group_val",i);
                       	 		PutField_CString(hNatRec,csTag,csNatureGrpVal);
                        		PutField_CString(hRec,"group_val",csNatureGrpVal);
                        	} else if (iDtlRet == NOT_FOUND) {
                                	iRet = INT_MMS_NATURE_GRP_VAL_NOT_FOUND;
                                	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrpVal:: grp val Not Found!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityNatureGrpVal:: grp val Not Found!!!\n");
                        	} else {
                                	iRet = INT_ERR;
                                	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureGrpVal:: Error!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityNatureGrpVal:: Error!!!\n");
                        	}
			} else {
DEBUGLOG(("Authorize:: val missing!!\n"));
ERRLOG("TxnMmsByUsEPA:: Authorize:: val missing!!\n");
				iRet = INT_MMS_NATURE_GRP_VAL_NOT_FOUND;
				PutField_Int(hContext,"internal_error",iRet);
			}

			RecordSet_Add(rRecordSet, hRec);
		}

		// Check Nature Id Exists
		if (iRet == PD_OK) {	
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap Find\n"));
        		DBObjPtr = CreateObj(DBPtr, "DBMmsEntityNatureMap","Find");
        		iDtlRet = (unsigned long)(*DBObjPtr)(rRecordSet,csNatureId);
        		if (iDtlRet == FOUND) {
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap:: Found!!!\n"));
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap:: csNatureId = [%s]\n", csNatureId));
				strcpy(csNewNatureId,csNatureId);
        		} else if (iDtlRet == NOT_FOUND) {
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap:: Not Found!!!\n"));
	
				// Generate a New Nature Id
				PutField_CString(hNewRec,"desc","test");
                        	PutField_CString(hNewRec,"add_user",csUser);
DEBUGLOG(("Authorize:: Call DBMmsEntityNature NewNatureId\n"));
                        	DBObjPtr = CreateObj(DBPtr, "DBMmsEntityNature", "NewNatureId");
                        	strcpy(csNewNatureId, (*DBObjPtr)(hNewRec));
DEBUGLOG(("Authorize:: Call DBMmsEntityNature NewNatureId, csNewNatureId = [%s]\n",csNewNatureId));

				// Add a New Nature Id in Nature Map
/* nature_id */
                        	PutField_CString(hNewRec,"nature_id",csNewNatureId);
DEBUGLOG(("Authorize: nature_id = [%s]\n",csNewNatureId));

                        	for (i=1; i<=iNatCnt; i++) {
/* nature_grp */
                                	sprintf(csTag,"%s_%s_%d",PD_NATURE_TAG,"group",i);
                                	if (GetField_CString(hNatRec,csTag,&csTmp)) {
DEBUGLOG(("Authorize: nature_grp = [%s]\n",csTmp));
                                        	PutField_CString(hNewRec,"nature_grp",csTmp);
                                	}

/* nature_grp_val */
                                	sprintf(csTag,"%s_%s_%d",PD_NATURE_TAG,"group_val",i);
                                	if (GetField_CString(hNatRec,csTag,&csTmp)) {
DEBUGLOG(("Authorize: nature_grp_val = [%s]\n",csTmp));
                                        	PutField_CString(hNewRec,"nature_grp_val",csTmp);
                                	}

                                	RecordSet_Add(rNewRecordset, hNewRec);
                        	}

DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap Add\n"));
                       	 	DBObjPtr = CreateObj(DBPtr, "DBMmsEntityNatureMap","Add");
                        	iDtlRet = (unsigned long)(*DBObjPtr)(rNewRecordset);
                        	if (iDtlRet == PD_OK) {
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap Add Success!!!\n"));
                        	} else {
                                	iRet = INT_ERR;
                                	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap:: Error!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityNatureMap:: Error!!!\n");
                        	}
        		} else {
        			iRet = INT_ERR;
        			PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityNatureMap:: Error!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityNatureMap:: Error!!!\n");
        		}	
		}
	}

	// Update Default Nature Id
	if (iRet == PD_OK) {

DEBUGLOG(("Authorize:: Call DBMmsEntityPsp UpdateNatureIdByEntityId\n"));
            	DBObjPtr = CreateObj(DBPtr, "DBMmsEntityPsp","UpdateNatureIdByEntityId");
             	iDtlRet = (unsigned long)(*DBObjPtr)(csNewNatureId,csEntityId,csUser);
              	if (iDtlRet == PD_OK) {
DEBUGLOG(("Authorize:: Call DBMmsEntityPsp:: Success!!!\n"));
		} else {
			iRet = INT_ERR;
                       	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call DBMmsEntityPsp:: Fail!!!\n"));
ERRLOG("TxnOmtByUsEPA::Authorize::Call DBMmsEntityPsp:: Fail!!!\n");
		}
	}

	// Response
        PutField_Int(hResponse,"ret",iRet);

	hash_destroy(hNatRec);
	FREE_ME(hNatRec);

	hash_destroy(hRec);
	FREE_ME(hRec);

	//RecordSet_Destroy(rRecordSet);
	FREE_ME(rRecordSet);

	hash_destroy(hNewRec);
        FREE_ME(hNewRec);

        RecordSet_Destroy(rNewRecordset);
        FREE_ME(rNewRecordset);

DEBUGLOG(("TxnMmsByUsEPA Normal Exit() iRet = [%d]\n",iRet));
	return iRet;
}
