						PutField_CString(hPsp, "p1_txn_id", csTmp);
					}
		
					if (GetField_CString(hPsp, "org_txn_seq", &csTmp)) {
						PutField_CString(hPsp, "p2_txn_id", csTmp);
					}
					PutField_Char(hPsp, "txn_level", PD_TXN_LEVEL);
					PutField_Char(hPsp, "relation_type", PD_REL_VOID);
					
					DBObjPtr = CreateObj(DBPtr,"DBOLTxnRelation","Add");
					if((unsigned long) ((*DBObjPtr)(hPsp))!=PD_OK){
							iRet = INT_ERR;
DEBUGLOG(("DBOLTxnRelation:Add Failed\n"));
					}
				}

			}
DEBUGLOG(("TxnOmtByUsVPO: Authroize:: BOBalance:UpdatePayoutAmount() result = [%d]\n",iRet));
		}
	}

	RecordSet_Destroy(rRecordSet);
        FREE_ME(hBalance);
        FREE_ME(hOrgTxn);
        FREE_ME(hIn);
        FREE_ME(hPspDetail);
        FREE_ME(hPsp);
        FREE_ME(hElement);
        FREE_ME(hTxnDetail);
	FREE_ME(rRecordSet);
	FREE_ME(hCosts);

DEBUGLOG(("Normal exit iRet = [%d]\n",iRet));	
	return iRet;
}

int	isAllowProcess(hash_t *hContext)
{
	int iRet = PD_OK;

	int	i    = 0;
	int	iCnt = 0;
	int	iTmp = 0;

	int	iTotalReconCnt = 0;
	int	iTotalUnReconCnt = 0;

	int	iSystemAllowPartial = PD_FALSE;

	char	csTag[PD_TAG_LEN+1];
	char	*csOrgGenTxnSeq = NULL;

	int	iReconCnt = 0;
	int	iLevel = 0;
	int iVersionNo;

	hash_t *hPsp;
	hPsp= (hash_t*)  malloc (sizeof(hash_t));
        hash_init(hPsp,0);

	if(GetField_Int(hContext,"generate_txn_cnt",&iCnt)){
DEBUGLOG(("isAllowProcess:: generate_txn_cnt = [%d]\n",iCnt));
	}


	char *csValueTmp;
	csValueTmp = (char *)malloc(128);

	DBObjPtr = CreateObj(DBPtr,"DBSystemParameter","FindCode");
	if ((unsigned long)(DBObjPtr)("OFL_PO_RTN_ALLOW_DIFF_RECON_STATUS",csValueTmp) == FOUND) {
		if(strcmp(csValueTmp,"Y")==0) {
			iSystemAllowPartial = PD_TRUE;
		}
	}
	FREE_ME(csValueTmp);



	for(i=0; i<iCnt; i++){
		sprintf(csTag,"version_no_%d",i);
		if(GetField_Int(hContext,csTag,&iVersionNo)){
			if (iVersionNo == 1) {
				iRet = PD_OK;
				return iRet;
			}
		}



		sprintf(csTag,"gen_txn_id_%d",i);

		if(GetField_CString(hContext,csTag,&csOrgGenTxnSeq)){
DEBUGLOG(("[%d] Txn_id [%s]\n", i, csOrgGenTxnSeq));

DEBUGLOG(("CheckReconMulti::Call DBOLPayoutReturn:CheckPayoutReconMulti\n"));
			DBObjPtr = CreateObj(DBPtr,"DBOLPayoutReturn","CheckPayoutReconMulti");
			iRet = (unsigned long)(*DBObjPtr)(csOrgGenTxnSeq);
			if (iRet != PD_OK) {
DEBUGLOG(("CheckReconMulit:: Not Pass to continue....\n"));
				break;
			}


DEBUGLOG(("CheckAllow::Call DBOLPayoutReturn:CheckPayoutRtnAllow\n"));
			DBObjPtr = CreateObj(DBPtr,"DBOLPayoutReturn","CheckPayoutRtnAllow");
			iRet = (unsigned long)(*DBObjPtr)(csOrgGenTxnSeq, hPsp);

			if (iRet == PD_OK) {
DEBUGLOG(("CheckAllow::Call DBOLPayoutReturn:CheckPayoutRtnAllow Result OK\n"));

				if (GetField_Int(hPsp, "recon_cnt", &iReconCnt)) {
					iTotalReconCnt = iTotalReconCnt + iReconCnt;
DEBUGLOG(("CheckAllow::recon_cnt[%d] total[%d]\n", iTmp, iTotalReconCnt));

					sprintf(csTag, "gen_recon_cnt_%d", i);
					PutField_Int(hContext, csTag, iReconCnt);
				}

				if (GetField_Int(hPsp, "unrecon_cnt", &iTmp)) {
					iTotalUnReconCnt = iTotalUnReconCnt + iTmp;
DEBUGLOG(("CheckAllow::unrecon_cnt[%d] total[%d]\n", iTmp, iTotalUnReconCnt));
				}

				if (GetField_Int(hPsp, "level", &iLevel)) {
DEBUGLOG(("CheckAllow::level[%d]\n", iLevel));
					sprintf(csTag, "gen_txn_id_level_%d", i);
					PutField_Int(hContext, csTag, iLevel);
				}

				sprintf(csTag, "fail_at_provider_%d", i);
				if (iLevel == 1 && iReconCnt  == 0) {
					//sprintf(csTag, "fail_at_provider_%d", i);
DEBUGLOG(("CheckAllow::[%d] fail_at_provider TRUE [%d]\n", i, PD_TRUE));
					PutField_Int(hContext, csTag, PD_TRUE);
				} else {
DEBUGLOG(("CheckAllow::[%d] fail_at_provider FALSE [%d]\n", i, PD_FALSE));
					PutField_Int(hContext, csTag, PD_FALSE);
				}

			} else {
				break;
			}
		}
	}


	if (iRet == PD_OK) {
		if (iTotalReconCnt == 0 && iTotalUnReconCnt == 0) {
			iRet = INT_INVALID_TXN;
		} else {
			if (iSystemAllowPartial == PD_FALSE) {
				if (iTotalReconCnt != 0 && iTotalUnReconCnt != 0) {
					iRet = INT_INVALID_TXN;
				}
			} 
		}
	}

	hash_destroy(hPsp);

	FREE_ME(hPsp);


	return iRet;
}

int	UpdatePayoutGenerateTxn(hash_t *hUpdateTxn)
{
	int	iRet = PD_OK;
	
	char	*csTxnSeq = NULL;
	char	*csTmp = NULL;
	char	cTmp;

	hash_t	*hOrgTxn;
	hOrgTxn = (hash_t*)  malloc (sizeof(hash_t));
	

	hash_t	*hRec;
	recordset_t     *rRecordSet;
        rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));
        recordset_init(rRecordSet,0);

DEBUGLOG(("UpdatePayoutGenerateTxn:START!\n"));
	if (GetField_CString(hUpdateTxn, "txn_seq", &csTxnSeq)) {
DEBUGLOG(("UpdatePayoutGenerateTxn:txn_seq [%s]!\n", csTxnSeq));
	}


	DBObjPtr = CreateObj(DBPtr,"DBOLPayoutReturn","GetPayoutRtnList");
	if((unsigned long) ((*DBObjPtr)(csTxnSeq, rRecordSet))==PD_OK){
		hRec = RecordSet_GetFirst(rRecordSet);
		while (hRec) {

			hash_init(hOrgTxn, 0);

			if (GetField_CString(hRec, "txn_seq", &csTmp)) {
DEBUGLOG(("UpdatePayoutGenerateTxn: Update txn_seq [%s]!\n", csTmp));
				PutField_CString(hOrgTxn, "txn_seq", csTmp);
			}

			PutField_Char(hOrgTxn,"status",PD_REVERSED);
			PutField_CString(hOrgTxn,"sub_status",PD_VOID);

			if (GetField_CString(hUpdateTxn,"update_user",&csTmp)) {
				PutField_CString(hOrgTxn,"update_user",csTmp);
			}

			if (GetField_Char(hRec, "recon_status", &cTmp)) {
				if (cTmp == PD_UNRECONCILED) {
DEBUGLOG(("UpdatePayoutGenerateTxn: Update org recon_status [%c] to [%c]!\n", cTmp, PD_RECON_NOT_REQ));
					PutField_Char(hOrgTxn,"recon_status", PD_RECON_NOT_REQ);
				}
			}
			
			DBObjPtr = CreateObj(DBPtr,"DBOLTransaction","Update");
			if((unsigned long) ((*DBObjPtr)(hOrgTxn))!=PD_OK){

DEBUGLOG(("UpdatePayoutGenerateTxn: Update OLTransaction ERROR!\n"));
				iRet = INT_INVALID_TXN;
				break;
			}

			hash_destroy(hOrgTxn);
			hRec = RecordSet_GetNext(rRecordSet);
		}
	} else {
DEBUGLOG(("UpdatePayoutGenerateTxn: FAILED!\n"));
		iRet = INT_INVALID_TXN;
		
	}

DEBUGLOG(("UpdatePayoutGenerateTxn:END!\n"));
	
	
	RecordSet_Destroy(rRecordSet);
	FREE_ME(rRecordSet);
	

	return iRet;
}

