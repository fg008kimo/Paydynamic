int     Authorize(hash_t* hContext,
                        hash_t* hRequest,
                        hash_t* hResponse)
{
        int     iRet = PD_OK;

	char	cTmp;
	char	*csTmp = NULL;

        char    *csUser = NULL;
	char	*csTxnSeq = NULL;
	char	*csTxnCode = NULL;
	char	*csProcessType = NULL;
	char	*csProcessCode = NULL;
	
	char	cBatchType;
	char 	*csBatchId = NULL;

	int	iRecCnt = 0;

	hash_t	*hRec = NULL;
	hash_t	*hBatchRec = NULL;
	hash_t  *hTxnRec = NULL;

	recordset_t     *rRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
	recordset_init(rRecordSet,0);

	recordset_t     *rBatchRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
	recordset_init(rBatchRecordSet,0);

	recordset_t     *rTxnRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
	recordset_init(rTxnRecordSet,0);


DEBUGLOG(("Authorize::TxnOmtByUsRBT: START!!!!!!!!!\n"));

// user
	if(GetField_CString(hRequest,"add_user",&csUser)){
DEBUGLOG(("Authorize: add_user= [%s]\n",csUser));
		PutField_CString(hContext,"update_user",csUser);
	} else {
		iRet = INT_ERR;
DEBUGLOG(("Authorize: add_user missing\n"));
ERRLOG("TxnOmtByUsRBT::Authorize: add_user missing\n");
	}

// txn_seq
	if(GetField_CString(hContext, "txn_seq", &csTxnSeq)) {
DEBUGLOG(("Authorize::txn_seq = [%s]\n",csTxnSeq));
		PutField_CString(hContext,"txn_seq",csTxnSeq);
		PutField_CString(hResponse,"org_txn_seq",csTxnSeq);
	} else {
		iRet = INT_TXN_ID_MISSING;
DEBUGLOG(("Authorize: txn_id missing\n"));
ERRLOG("TxnOmtByUsRBT::Authorize: txn_id missing\n");
	}


// Check txn is valid
	if (iRet == PD_OK) {
		DBObjPtr = CreateObj(DBPtr,"DBOLTransaction","GetTxnHeader");
		if ((unsigned long)(*DBObjPtr)(csTxnSeq,hRec) == PD_OK) {
			hRec = RecordSet_GetFirst(rRecordSet);
			while (hRec) {
				if (GetField_Char(hRec,"status",&cTmp)) {
DEBUGLOG(("Authorize: txn_id [%s] status = [%c]\n",csTxnSeq,cTmp));
					if (cTmp != PD_COMPLETE) {
						iRet = INT_INVALID_TXN;
DEBUGLOG(("Authorize: txn_id [%s] status must be [%c]\n",csTxnSeq,PD_COMPLETE));
ERRLOG("TxnOmtByUsRBT::Authorize: txn_id [%s] status must be [%c]\n",csTxnSeq,PD_COMPLETE);
					}
				}
				if (GetField_Char(hRec,"ar_ind",&cTmp)) {
DEBUGLOG(("Authorize: txn_id [%s] ar_ind = [%c]\n",csTxnSeq,cTmp));
					if (cTmp != PD_ACCEPT) {
						iRet = INT_INVALID_TXN;
DEBUGLOG(("Authorize: txn_id [%s] ar_ind must be [%c]\n",csTxnSeq,PD_ACCEPT));
ERRLOG("TxnOmtByUsRBT::Authorize: txn_id [%s] ar_ind must be [%c]\n",csTxnSeq,PD_ACCEPT);
					}
				}

				if (GetField_CString(hRec,"txn_code",&csTxnCode)) {
DEBUGLOG(("Authorize: txn_id [%s] txn_code = [%s]\n",csTxnSeq,csTxnCode));
				}
				if (GetField_CString(hRec,"process_type",&csProcessType)) {
DEBUGLOG(("Authorize: txn_id [%s] process_type = [%s]\n",csTxnSeq,csProcessType));
				}
				if (GetField_CString(hRec,"process_code",&csProcessCode)) {
DEBUGLOG(("Authorize: txn_id [%s] process_code = [%s]\n",csTxnSeq,csProcessCode));
				}
				DBObjPtr = CreateObj(DBPtr,"DBTxnCode","IsVoidable");
				if ((unsigned long)(*DBObjPtr)(csTxnCode,csProcessType,csProcessCode) == PD_FALSE) {
					iRet = INT_ERR;
				}
				DBObjPtr = CreateObj(DBPtr,"DBTxnCode","IsOffset");
				if ((unsigned long)(*DBObjPtr)(csTxnCode,csProcessType,csProcessCode) == PD_TRUE) {
					iRet = INT_ERR;
				}

				hRec = RecordSet_GetNext(rRecordSet);
			}
		}
	}

// Get batch id
	if (iRet == PD_OK) {
		iRecCnt = 0;
	
		PutField_Char(hContext,"txn_level",PD_TXN_LEVEL);

DEBUGLOG(("Authorize:: Call DBOLBatchTxnRelation:GetBatchId\n"));
                DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","GetBatchId");
                iRet = (unsigned long)(*DBObjPtr)(hContext,rBatchRecordSet);
                if (iRet == PD_OK) {
			hBatchRec = RecordSet_GetFirst(rBatchRecordSet);
                        while (hBatchRec) {
/* batch_type */
                                if (GetField_Char(hBatchRec,"batch_type",&cBatchType)) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() batch_type = [%c]\n",cBatchType));
                                }

/* batch_id */
				if (GetField_CString(hBatchRec,"batch_id",&csBatchId)) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() batch_id = [%s]\n",csBatchId));
                                }

				iRecCnt++;	
				
				hBatchRec = RecordSet_GetNext(rBatchRecordSet);
			}
		} else {
			iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Invalid Txn Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetBatchId() Invalid Txn Id!!!\n");
		}

		// Check Number of Found Batch Id
		if (iRecCnt == 0) {
			iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Batch Id Not Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetBatchId() Batch Id Not Found!!!\n");
		} else if (iRecCnt > 1) {
			iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Multi-Batch Id Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetBatchId() Multi-Batch Id Found!!!\n");
		}
	}
	
// Get Txn Id
	if (iRet == PD_OK) {
		iRecCnt = 0;

		if (cBatchType != PD_COMBINE) {
	  		PutField_Char(hContext,"batch_type",cBatchType);
		} else {
			iRet = INT_ERR;
DEBUGLOG(("Authorize() batch_type invalid\n"));
ERRLOG("TxnOmtByUsRBT::Authorize: batch_type invalid\n");
		}
		PutField_CString(hContext,"batch_id",csBatchId);	

DEBUGLOG(("Authorize:: Call DBOLBatchTxnRelation:GetTxnId\n"));
                DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","GetTxnId");
                iRet = (unsigned long)(*DBObjPtr)(hContext,rTxnRecordSet);
		if (iRet == PD_OK) {
                        hTxnRec = RecordSet_GetFirst(rTxnRecordSet);
                        while (hTxnRec) {
/* txn_level */
                                if (GetField_Char(hTxnRec,"txn_level",&cTmp)) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() txn_level = [%c]\n",cTmp));
                                }

/* txn_id */
                                if (GetField_CString(hTxnRec,"txn_id",&csTmp)) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() txn_id = [%s]\n",csTmp));
                                }

                                iRecCnt++;

                                hTxnRec = RecordSet_GetNext(rTxnRecordSet);
                        }
                } else {
                        iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() Invalid Batch Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetTxnId() Invalid Batch Id!!!\n");
                }
	}

// Check Cancel/Void/Not Allow Change
	if (iRet == PD_OK) {
		
	}

DEBUGLOG(("Authorize::TxnOmtByUsRBT: END!!!!!!!!!\n"));
	
	RecordSet_Destroy(rRecordSet);
        FREE_ME(rRecordSet);

	RecordSet_Destroy(rBatchRecordSet);
        FREE_ME(rBatchRecordSet);
	
	RecordSet_Destroy(rTxnRecordSet);
        FREE_ME(rTxnRecordSet);

	return iRet;
}
