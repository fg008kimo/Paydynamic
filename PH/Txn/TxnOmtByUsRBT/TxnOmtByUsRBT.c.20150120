int     AddTxnRelationLinkage(const char* csOrgTxnSeq, hash_t *hContext)
{
        int iRet = PD_OK;
        int iDtlRet = PD_FOUND;

        int iRecCnt = 0;

        char *csTmpTxnId = NULL;
        char *csP1TxnId = NULL;
        char *csP2TxnId = NULL;

	char *csTmp = NULL;

        hash_t  *hRec = NULL;
        hRec = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hRec,0);
        recordset_t     *rRecordSet;
        rRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rRecordSet,0);

        hash_t  *hTmpRec = NULL;
        hTmpRec = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hTmpRec,0);
        recordset_t     *rTmpRS;
        rTmpRS = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rTmpRS,0);

DEBUGLOG(("AddTxnRelationLinkage: Get TxnRelation(Linkage)\n"));

	iRecCnt = 0;

// get new P1TxnId
        PutField_CString(hTmpRec,"p2_txn_id",csOrgTxnSeq);
        PutField_Char(hTmpRec,"party_type",PD_TYPE_PSP);
        PutField_Char(hTmpRec,"txn_level",PD_TXN_LEVEL);
        PutField_Char(hTmpRec,"relation_type",PD_REL_VOID);

        DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","GetP1TxnId");
        iDtlRet = (unsigned long)(*DBObjPtr)(hTmpRec,rTmpRS);
        if (iDtlRet == PD_FOUND) {
        	hTmpRec = RecordSet_GetFirst(rTmpRS);
                while (hTmpRec) {
                	if (GetField_CString(hTmpRec,"p1_txn_id",&csP1TxnId)) {
DEBUGLOG(("AddTxnRelationLinkage(Void) Call DBOLTxnRelation::GetP1TxnId() p1_txn_id = [%s]\n",csP1TxnId));
                        }

                        iRecCnt++;

                        hTmpRec = RecordSet_GetNext(rTmpRS);
                }
		if (iRecCnt > 1) {
			iRet = INT_ERR;
DEBUGLOG(("AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Multi-Txn Id Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Multi-Txn Id Found!!!\n");
		}
        } else {
        	iRet = INT_ERR;
DEBUGLOG(("AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Not Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::AddTxnRelationLinkage() Call DBOTxnRelation::GetP1TxnId() Not Found!!!\n");
        }

// get all old P1TxnID
	PutField_CString(hRec,"p2_txn_id",csOrgTxnSeq);
        PutField_Char(hRec,"party_type",PD_TYPE_PSP);
        PutField_Char(hRec,"txn_level",PD_TXN_LEVEL);
        PutField_Char(hRec,"relation_type",PD_REL_LINKAGE);

        DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","GetP1TxnId");
        iDtlRet = (unsigned long)(*DBObjPtr)(hRec,rRecordSet);
        if (iDtlRet == PD_FOUND) {
                hRec = RecordSet_GetFirst(rRecordSet);
                while ((hRec) && (iRet == PD_OK)) {
                        if (GetField_CString(hRec,"p1_txn_id",&csTmpTxnId)) {
DEBUGLOG(("AddTxnRelationLinkage(Linkage) Call DBOLTxnRelation::GetP1TxnId() p1_txn_id = [%s]\n",csTmpTxnId));
                        }

// get new P2TxnID
                       	if (iRet == PD_OK) {
                                iRecCnt = 0;

                                PutField_CString(hTmpRec,"p2_txn_id",csTmpTxnId);
                                PutField_Char(hTmpRec,"party_type",PD_TYPE_PSP);
                                PutField_Char(hTmpRec,"txn_level",PD_TXN_LEVEL);
                                PutField_Char(hTmpRec,"relation_type",PD_REL_VOID);

                                DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","GetP1TxnId");
                                iDtlRet = (unsigned long)(*DBObjPtr)(hTmpRec,rTmpRS);
                                if (iDtlRet == PD_FOUND) {
                                        hTmpRec = RecordSet_GetFirst(rTmpRS);
                                        while (hTmpRec) {
                                                if (GetField_CString(hTmpRec,"p1_txn_id",&csP2TxnId)) {
DEBUGLOG(("AddTxnRelationLinkage(Void) Call DBOLTxnRelation::GetP1TxnId() p1_txn_id = [%s]\n",csP2TxnId));
                                                }

                                                iRecCnt++;

                                                hTmpRec = RecordSet_GetNext(rTmpRS);
                                        }
					if (iRecCnt > 1) {
						iRet = INT_ERR;
DEBUGLOG(("AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Multi-Txn Id Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Multi-Txn Id Found!!!\n");
					}
                                } else {
                                        iRet = INT_ERR;
DEBUGLOG(("AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Not Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::AddTxnRelationLinkage() Call DBOTxnRelation::GetP1TxnId() Not Found!!!\n");
                                }
                        }

			if (iRet == PD_OK) {
DEBUGLOG(("AddTxnRelationLinkage: Add TxnRelation\n"));

                                PutField_Char(hTmpRec, "party", PD_TYPE_PSP);
                                PutField_CString(hTmpRec, "p1_txn_id", csP1TxnId);
                                PutField_CString(hTmpRec, "p2_txn_id", csP2TxnId);
                                PutField_Char(hTmpRec, "txn_level", PD_TXN_LEVEL);
                                PutField_Char(hTmpRec, "relation_type", PD_REL_LINKAGE);
				if (GetField_CString(hContext, "update_user", &csTmp)) {
                                        PutField_CString(hTmpRec, "update_user", csTmp);
                                        PutField_CString(hTmpRec, "add_user", csTmp);
                                        PutField_CString(hTmpRec, "create_user", csTmp);
                                }

				iRet = AddTxnRelation(hTmpRec);
                        }

			hRec = RecordSet_GetNext(rRecordSet);
                }
        } else {
                iRet = INT_ERR;
DEBUGLOG(("AddTxnRelationLinkage() Call DBOLTxnRelation::GetP1TxnId() Not Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::AddTxnRelationLinkage() Call DBOTxnRelation::GetP1TxnId() Not Found!!!\n");
        }

        hash_destroy(hRec);
        FREE_ME(hRec);

        RecordSet_Destroy(rRecordSet);
        FREE_ME(rRecordSet);

        hash_destroy(hTmpRec);
        FREE_ME(hTmpRec);

        RecordSet_Destroy(rTmpRS);
        FREE_ME(rTmpRS);

        return iRet;
}

int     ProcessLastOffset(const char* csTxnId, hash_t *hContext)
{
        int     iRet = PD_OK;
        int     iDtlRet = PD_FALSE;

        char    *csTxnCode = NULL;
        char    *csProcessType = NULL;
        char    *csProcessCode = NULL;

        hash_t  *hRec = NULL;

        recordset_t     *rRecordSet;
        rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));

	// Get Txn Header
	if (iRet == PD_OK) {
                recordset_init(rRecordSet,0);

DEBUGLOG(("ProcessLastOffset:: Call DBOLTransaction:GetTxnHeader [%s]\n", csTxnId));
                DBObjPtr = CreateObj(DBPtr, "DBOLTransaction", "GetTxnHeader");
                if ((*DBObjPtr)(csTxnId,rRecordSet) == PD_OK) {
                        hRec = RecordSet_GetFirst(rRecordSet);
                        while (hRec) {

/* txn_code */
                                if (GetField_CString(hRec, "txn_code", &csTxnCode)) {
DEBUGLOG(("ProcessLastOffset::call DBOLTransaction:: txn_code = [%s]\n", csTxnCode));
                                }

/* csProcessType */
                                if (GetField_CString(hRec, "process_type", &csProcessType)) {
DEBUGLOG(("ProcessLastOffset::call DBOLTransaction:: process_type = [%s]\n", csProcessType));
                                }

/* csProcessCode */
                                if (GetField_CString(hRec, "process_code", &csProcessCode)) {
DEBUGLOG(("ProcessLastOffset::call DBOLTransaction:: process_code = [%s]\n", csProcessCode));
                                }

                                hRec = RecordSet_GetNext(rRecordSet);
                        }
                } else {
                        iRet = INT_INVALID_TXN;
DEBUGLOG(("ProcessLastOffset::call DBOLBAIDTxn GetTxnHeader::not found record for [%s]\n", csTxnId));
ERRLOG("TxnOmtByUsRBT::ProcessLastOffset::call DBOLBAIDTxn GetTxnHeader::not found record for [%s]\n", csTxnId);
                }

                RecordSet_Destroy(rRecordSet);
        }

	// Get Fe Init
	if (iRet == PD_OK) {
                DBObjPtr = CreateObj(DBPtr,"DBTxnCode","AllowFeInit");
                iDtlRet = (unsigned long)(*DBObjPtr)(csTxnCode,csProcessType,csProcessCode);
                if (iDtlRet == PD_TRUE) {
DEBUGLOG(("ProcessLastOffset::call DBTxnCode AllowFeInit:: fe_init = PD_TRUE\n"));

			hash_t  *hOrgTxn;
			hOrgTxn = (hash_t*) malloc (sizeof(hash_t));
			hash_init(hOrgTxn, 0);

			hash_t  *hNewTxn;
			hNewTxn = (hash_t*) malloc (sizeof(hash_t));
			hash_init(hNewTxn, 0);

			// all same information of csOrgTxnSeq, re-create txn_header, txn_detail, txn_psp_detail...?
			// update balance, elements

			// (1) GetTxnInfo(const char *csOrgTxnSeq, hash_t *hContext, hash_t *hTxn);
			GetTxnInfo(csTxnId,hContext,hOrgTxn);
 
			// (2) FormOffsetTxn(hash_t *hOrgTxn, hash_t *hOfstTxn, hash_t *hContext, int iIsOffset);
			FormOffsetTxn(hOrgTxn,hNewTxn,hContext,PD_FALSE);

			// (3) ProcessOffsetTxn(hash_t *hOfstTxn, hash_t *hContext);
			ProcessOffsetTxn(hNewTxn,hContext);
		
			FREE_ME(hOrgTxn);
			FREE_ME(hNewTxn);

                } else if (iDtlRet == PD_FALSE) {
DEBUGLOG(("ProcessLastOffset::call DBTxnCode AllowFeInit:: fe_init = PD_FALSE\n"));
                } else {
                        iRet = INT_ERR;
DEBUGLOG(("ProcessLastOffset::call DBTxnCode AllowFeInit::not found!!\n"));
ERRLOG("TxnOmtByUsRBT::ProcessLastOffset::call DBTxnCode AllowFeInit::not found!!\n");
                }
        }

        FREE_ME(rRecordSet);

DEBUGLOG(("ProcessLastOffset() Ret = [%d]\n",iRet));
        return iRet;
}

