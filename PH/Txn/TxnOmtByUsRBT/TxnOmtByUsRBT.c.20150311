int 	CheckActionByTxnId(const char* csTxnId, hash_t* hContext)
{
	int 	iRet = PD_OK;

	int 	iOffsetTxn = PD_FALSE;
	
	char    *csTxnCode = NULL;	
        char    *csSubStatus = NULL;
        char    *csProcessType = NULL;
        char    *csProcessCode = NULL;
        char    cStatus;
        char    cArInd;
        char    cReconStatus;

        hash_t  *hRec = NULL;

        recordset_t     *rRecordSet;
        rRecordSet = (recordset_t*) malloc (sizeof(recordset_t));

	cStatus = ' ';
	cArInd = ' ';
	cReconStatus = ' ';	
	
	// Get Txn Header
	if (iRet == PD_OK) {
                recordset_init(rRecordSet,0);

DEBUGLOG(("CheckActionByTxnId::Call DBOLTransaction:GetTxnHeader [%s]\n", csTxnId));
                DBObjPtr = CreateObj(DBPtr, "DBOLTransaction", "GetTxnHeader");
                if ((*DBObjPtr)(csTxnId,rRecordSet) == PD_OK) {
                        hRec = RecordSet_GetFirst(rRecordSet);
                        while (hRec) {

/* txn_code */
                                if (GetField_CString(hRec, "txn_code", &csTxnCode)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: txn_code = [%s]\n", csTxnCode));
                                }

/* status */
				if (GetField_Char(hRec,"status",&cStatus)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: status = [%c]\n",cStatus));
                        	}

/* ar_ind */
                                if (GetField_Char(hRec, "ar_ind", &cArInd)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: ar_ind = [%c]\n", cArInd));
                                }

/* sub_status */
                                if (GetField_CString(hRec, "sub_status", &csSubStatus)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: sub_status = [%s]\n", csSubStatus));
                                }

/* recon_status */
                                if (GetField_Char(hRec, "recon_status", &cReconStatus)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: recon_status = [%c]\n", cReconStatus));
                                }

/* process_type */ 
				if (GetField_CString(hRec,"process_type",&csProcessType)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: process_type = [%s]\n",csProcessType));
                            	}

/* process_code */
                             	if (GetField_CString(hRec,"process_code",&csProcessCode)) {
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: process_code = [%s]\n",csProcessCode));
                              	}

				DBObjPtr = CreateObj(DBPtr,"DBTxnCode","IsOffset");
                                iOffsetTxn = (unsigned long)(*DBObjPtr)(csTxnCode);
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction:: isOffsetTxn = [%d]\n",iOffsetTxn));

                                hRec = RecordSet_GetNext(rRecordSet);
                        }
                } else {
                        iRet = INT_INVALID_TXN;
DEBUGLOG(("CheckActionByTxnId::call DBOLTransaction GetTxnHeader::not found record for [%s]\n", csTxnId));
ERRLOG("TxnOmtByUsRBT::CheckActionByTxnId::call DBOLTransaction GetTxnHeader::not found record for [%s]\n", csTxnId);
                }

                RecordSet_Destroy(rRecordSet);
        }

	if (iRet == PD_OK) {
DEBUGLOG(("CheckActionByTxnId: Default iAction = [%d]\n", iAction));
		// Check Status+ARInd
		if ((cStatus == PD_COMPLETE) && (cArInd == PD_ACCEPT)) {
			// check offset txn
			if (iOffsetTxn != PD_TRUE) {
				// check sub_status
				if ((!strcmp(csSubStatus, PD_UNDO)) || (!strcmp(csSubStatus, PD_CANCELLED))) { 
DEBUGLOG(("CheckActionByTxnId: sub_status = voided/cancelled, FAILED\n"));
					iAction = PD_FALSE;
				} else {
					// Check Cases for Cancel/Void/Not Allow
					// Step: Txn Code -> Sub Status -> Recon Status					
DEBUGLOG(("CheckActionByTxnId: Check Cancel/Void/False \n"));
DEBUGLOG(("CheckActionByTxnId: csTxnCode = [%s], csSubStatus = [%s], cReconStatus = [%c]\n", csTxnCode,csSubStatus,cReconStatus));
                			if (!strcmp(csTxnCode, PD_BANK_DEPOSIT_TXN_CODE)) {
                        			if (!strcmp(csSubStatus, PD_UNALLOCATED)) {
                                			iAction = PD_BATCH_ACTION_VOID;
                        			} else {
							iAction = PD_FALSE;
						}
                			} else if (!strcmp(csTxnCode, PD_PSP_SETTLEMENT_TXN_CODE)) {
                        			if ((!strcmp(csSubStatus, PD_DELIVERED)) || (!strcmp(csSubStatus, PD_DELIVERING))) {
                                			iAction = PD_FALSE;
                        			} else {
                                			if (cReconStatus == PD_RECONCILED) {
								iAction = PD_BATCH_ACTION_VOID;
							}
                        			}
                			} else {
                        			if (cReconStatus == PD_UNRECONCILED) {
                                			//Do nothing
                        			} else if (cReconStatus == PD_RECONCILED) {
                                			iAction = PD_BATCH_ACTION_VOID;
                        			} else {
							iAction = PD_FALSE;
						}
                			}
				}
			} else {
DEBUGLOG(("CheckActionByTxnId: Txn is offset, skip checking\n"));
			}
		} else {
DEBUGLOG(("CheckActionByTxnId: Txn Status not equal to [C][A], skip checking\n"));
		}
	}

	if (iRet == PD_OK) {
		if (iAction > PD_FALSE) {
                        switch (iAction) {
                                case PD_BATCH_ACTION_VOID:
                                        DBObjPtr = CreateObj(DBPtr,"DBTxnCode","OflIsVoidable");
                                        if ((unsigned long)(*DBObjPtr)(csTxnCode) != PD_TRUE) {
DEBUGLOG(("CheckActionByTxnId: TxnCode IsVoidable = FALSE\n"));
                                                iAction = PD_FALSE;
                                        }
DEBUGLOG(("CheckActionByTxnId: iAction [%d]\n",iAction));
                                        break;
                                case PD_BATCH_ACTION_CANCEL:
                                        DBObjPtr = CreateObj(DBPtr,"DBTxnCode","AllowCancel");
                                        if ((unsigned long)(*DBObjPtr)(csTxnCode) != PD_TRUE) {
DEBUGLOG(("CheckActionByTxnId: TxnCode AllowCancel = FALSE\n"));
                                                iAction = PD_FALSE;
                                        }
DEBUGLOG(("CheckActionByTxnId: iAction [%d]\n",iAction));
                                        break;
                                default:
DEBUGLOG(("CheckActionByTxnId: return error [%d]\n",iAction));
                                        break;
                        }
                } else {
DEBUGLOG(("CheckActionByTxnId: iAction [%d]\n",iAction));
		}
	}

	// result
	if (iRet == PD_OK) {
		if (iAction > PD_FALSE) {
			iRet = iAction;
            	} else {
                 	iRet = INT_INVALID_TXN;
                   	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("CheckActionByTxnId: not allow to void/cancel\n"));
ERRLOG("TxnOmtByUsRBT::CheckActionByTxnId: not allow to void/cancel\n");
          	}
	}

        RecordSet_Destroy(rRecordSet);
        FREE_ME(rRecordSet);

DEBUGLOG(("CheckActionByTxnId: iRet = [%d]\n",iRet));
	return iRet;
}

int	ProcessOffset(const char* csOrgTxnSeq, hash_t* hOrgTxn, hash_t* hNewTxn, hash_t* hContext)
{
	int	iRet = PD_OK;

DEBUGLOG(("===== ProcessOffset ===== START!!\n"));	

DEBUGLOG(("ProcessOffset:: txn_id [%s]\n", csOrgTxnSeq));

	// Lock Org Txn + Get Org Txn
	iRet = GetTxnInfo(csOrgTxnSeq, hContext, hOrgTxn);

	// Form Offset Txn (include reversal CR / DR sign)
	if (iRet == PD_OK) {
		iRet = FormNewTxn(hOrgTxn, hNewTxn, hContext, PD_TRUE);
	}

	// Create Txn + Update Balance (with add approval_date, approval_timestamp)
	if (iRet == PD_OK) {
		iRet = ProcessNewTxn(hNewTxn, hContext, PD_TRUE);
	}

	// Update Org Txn to (void)
	if (iRet == PD_OK) {
		iRet = ProcessOrgTxn(hOrgTxn, hContext);
	}

DEBUGLOG(("ProcessOffset:: iRet [%d]\n", iRet));
DEBUGLOG(("===== ProcessOffset ===== END !!\n"));	

	return iRet;
}

