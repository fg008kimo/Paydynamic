#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "common.h"
#include "utilitys.h"
#include "ObjPtr.h"
#include "internal.h"
#include "TxnOmtByUsRBT.h"
#include "myrecordset.h"
#include <curl/curl.h>
#include "queue_utility.h"
#include "mq_db.h"

#define PD_DEFAULT_DATE_FORMAT          "%Y%m%d"
#define PD_DEFAULT_DATETIME_FORMAT      "%Y%m%d%H%M%S"

char cDebug;
OBJPTR(DB);
OBJPTR(BO);
OBJPTR(Channel);

int 	iAction = PD_BATCH_ACTION_CANCEL;
int	iNoBatchId = PD_FALSE;	

int     AddTxnRelation(hash_t* hRls);
int     AddBatchTxnRelation(hash_t* hRls);
int     AddBatchRelation(hash_t* hRls);
int	CheckActionByTxnId(const char* csTxnId, hash_t *hContext);
int     ProcessOffset(const char* csOrgTxnSeq, hash_t* hOrgTxn, hash_t* hNewTxn, hash_t *hContext);
int  	GetTxnInfo(const char *csTxnSeq, hash_t *hContext, hash_t *hTxn);
int	FormNewTxn(hash_t *hOrgTxn, hash_t *hOfstTxn, hash_t *hContext, int iIsOffset);
int	ProcessNewTxn(hash_t *hOfstTxn, hash_t *hContext, int iIsOffset);
int	ProcessOrgTxn(hash_t *hOrgTxn, hash_t *hContext);
int	AddTxnRelationLinkage(const char *csOrgBatchId, hash_t *hContext, hash_t *hTxnRelation);
int	AddLastTxnRelationLinkage(hash_t *hRec);
int     ProcessLastTxn(const char* csTxnId, hash_t* hOrgTxn, hash_t* hNewTxn, hash_t *hContext);
int	ProcessOrgStmt(hash_t *hOrgStmt, hash_t *hContext);
int     CheckActionByStmtTxnId(const char* csStmtTxnId);
int     ProcessStmtOffset(const char* csOrgStmtTxnSeq, hash_t *hContext);
int     GetStmtTxnInfo(const char *csOrgStmtTxnSeq, hash_t *hContext, hash_t *hStmtTxn);
int     FormNewStmtTxn(hash_t *hOrgStmtTxn, hash_t *hOfstStmtTxn, hash_t *hContext);
int     ProcessNewStmtTxn(hash_t *hOfstStmtTxn, hash_t *hContext);

void 	TxnOmtByUsRBT(char    cdebug)
{
        cDebug = cdebug;
}

int     Authorize(hash_t* hContext,
                        hash_t* hRequest,
                        hash_t* hResponse)
{
        int     iRet = PD_OK;
	int     iDtlRet = PD_FALSE;

	char    *csTmp = NULL;
	char	csTag[PD_TMP_BUF_LEN+1];

        char    *csUser = NULL;
	
	char	*csOrgTxnSeq = NULL;
	char	*csNewTxnSeq = NULL;
	char	*csLastTxnSeq = NULL;
	char	*csRelatedTxnSeq = NULL;
	char	*csVoidTxnSeq = NULL;
	char	*csTmpTxnSeq = NULL;
	
	char    *csOrgStmtTxnSeq = NULL;
        char    *csTmpStmtTxnSeq = NULL;
	
	//char	cRelationType;

	char	csOrgBatchId[PD_TXN_SEQ_LEN + 1];
	char	csNewBatchId[PD_TXN_SEQ_LEN + 1];
	char	csIgnoreBatchId[PD_TXN_SEQ_LEN + 1];

	int     iRecCnt = 0;	

	hash_t  *hBatchRec = NULL;
        hash_t  *hTxnRec = NULL;
	hash_t  *hStmtTxnRec = NULL;

	recordset_t     *rBatchRecordSet; 
	rBatchRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rBatchRecordSet,0);

	hash_t  *hLastTxnRecGet = NULL;
	hash_t  *hLastTxnRec;
	hLastTxnRec = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hLastTxnRec,0);
        recordset_t     *rLastTxnRecordSet;
        rLastTxnRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rLastTxnRecordSet,0);

	hash_t  *hTmpTxnRec;
	hTmpTxnRec = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hTmpTxnRec,0);

	hash_t	*hTxnContext;
	hTxnContext = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hTxnContext,0);
        recordset_t     *rTxnRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rTxnRecordSet,0);

	hash_t  *hStmtTxnContext;
        hStmtTxnContext = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hStmtTxnContext,0);
        recordset_t     *rStmtTxnRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rStmtTxnRecordSet,0);

	hash_t	*hTxnRelation;
	hTxnRelation = (hash_t*) malloc (sizeof(hash_t));
	hash_init(hTxnRelation,0);

DEBUGLOG(("Authorize::TxnOmtByUsRBT: START!!!!!!!!!\n"));

	// init iAction
	iAction = PD_BATCH_ACTION_CANCEL;

/* txn_seq */
	if(GetField_CString(hRequest, "org_txn_seq", &csTmp)) {
DEBUGLOG(("Authorize::txn_seq = [%s]\n",csTmp));
		PutField_CString(hContext,"txn_seq",csTmp);
		PutField_CString(hContext,"org_txn_seq",csTmp);
		PutField_CString(hResponse,"org_txn_seq",csTmp);
        } else {
                iRet = INT_TXN_ID_MISSING;
		PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize: txn_id missing\n"));
ERRLOG("TxnOmtByUsRBT::Authorize: txn_id missing\n");
        }

/* user */
        if(GetField_CString(hRequest,"add_user",&csUser)){
DEBUGLOG(("Authorize::add_user= [%s]\n",csUser));
                PutField_CString(hContext,"update_user",csUser);
                PutField_CString(hTxnContext,"update_user",csUser);
                PutField_CString(hStmtTxnContext,"update_user",csUser);
        } else {
                iRet = INT_USER_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize: add_user missing\n"));
ERRLOG("TxnOmtByUsRBT::Authorize: add_user missing\n");
        }

/*
 * Step 1: Do Txn Level * * * * * * * * * * * * * * * * * * * *
 */	
	// Get Batch Id, check if multiple batch include
	if (iRet == PD_OK) {
                iRecCnt = 0;

                PutField_Char(hContext,"txn_level",PD_TXN_LEVEL);
		PutField_Char(hContext,"batch_type",PD_GENERAL);
		if (GetField_CString(hContext, "txn_seq", &csTmp)) {
                        PutField_CString(hContext,"txn_id",csTmp);
                }

DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation:GetBatchId\n"));
                DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","GetBatchId");
                iRet = (unsigned long)(*DBObjPtr)(hContext,rBatchRecordSet);
                if (iRet == PD_OK) {
                        hBatchRec = RecordSet_GetFirst(rBatchRecordSet);
                        while (hBatchRec) {

				// batch_id
				if (GetField_CString(hBatchRec,"batch_id",&csTmp)) {
					//do it once only, get the first batchId
					if (iRecCnt == 0) {
						strcpy(csOrgBatchId,csTmp);
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() batch_id = [%s]\n",csOrgBatchId));
                                	}
				}

                                iRecCnt++;

				//Special handling for intra-balance-transfer case
				if (iRecCnt > 1) {
					DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","CheckIsBalTransBatch");
					if ((unsigned long)(*DBObjPtr)(csTmp) == PD_TRUE) {
						strcpy(csIgnoreBatchId,csTmp);
DEBUGLOG(("Authorize() call DBOLBatchTxnRelation:: IgnoreBatchId = [%s]\n",csIgnoreBatchId));
						iRecCnt--;
					}
				}

                                hBatchRec = RecordSet_GetNext(rBatchRecordSet);
                        }

			// Check Number of Found Batch Id
			if (iRecCnt == 0) {
				iNoBatchId = PD_TRUE;	
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Batch Id Not Found!!!\n"));
                	} else if (iRecCnt == 1) {
				iNoBatchId = PD_FALSE;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Single Batch Id Found!!!\n"));
			} else if (iRecCnt > 1) {
				iNoBatchId = PD_FALSE;
/*
                        	iRet = INT_ERR;
				PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Multi-Batch Id Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetBatchId() Multi-Batch Id Found!!!\n");
*/  
	              	}
                } else {
                        iRet = INT_ERR;
			PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId() Invalid Txn Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetBatchId() Invalid Txn Id!!!\n");
                }
        }

	// Get Txn Id recordset, order by approval_ts DESC
	if ((iNoBatchId == PD_TRUE) && (iRet == PD_OK)) {
		hash_t  *hOrgTxn;
                hOrgTxn = (hash_t*) malloc (sizeof(hash_t));
		hash_init(hOrgTxn, 0);
                
		hash_t  *hNewTxn;
                hNewTxn = (hash_t*) malloc (sizeof(hash_t));
                hash_init(hNewTxn, 0);

DEBUGLOG(("Authorize: iNoBatchId = [%d]\n",iNoBatchId));		

		if (GetField_CString(hContext, "txn_seq", &csOrgTxnSeq)) {
DEBUGLOG(("Authorize: org_txn_id = [%s]\n",csOrgTxnSeq));		

			//check voided txn id
                       	PutField_CString(hTmpTxnRec,"txn_id",csOrgTxnSeq);
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation FindVoidTxnCount\n"));
                      	DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","FindVoidTxnCount");
                        iDtlRet = (unsigned long)(*DBObjPtr)(hTmpTxnRec);
                       	if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Voided Txn Not Found!!!\n"));
                    	} else {
                              	iRet = INT_ERR;
                           	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Voided Txn Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLTxnRelation:: Call DBOLTxnRelation:: Voided Txn Found!!!\n");
                      	}

			// Call function to check action
			if (iRet == PD_OK) {
				iDtlRet = CheckActionByTxnId(csOrgTxnSeq, hContext);
				if (iDtlRet == PD_FALSE) {
DEBUGLOG(("Authorize: CheckActionByTxnId txn_id not allow to void/cancel\n"));
					iRet = INT_INVALID_TXN;
					PutField_Int(hContext,"internal_error",iRet);
				} else if (iDtlRet != PD_BATCH_ACTION_VOID &&
					   iDtlRet != PD_BATCH_ACTION_CANCEL &&
					   iDtlRet != PD_FALSE) {
					iRet = iDtlRet;
					PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize: CheckActionByTxnId return error, iRet = [%d]\n",iRet));
				} else {
DEBUGLOG(("Authorize: CheckActionByTxnId iAction=[%d]\n",iDtlRet));
				}
			}

			if (iRet == PD_OK) {
				iRet = ProcessOffset(csOrgTxnSeq, hOrgTxn, hNewTxn, hContext);	
			}
	
			//remove fields - element_acct_bal and element_intransit
			RemoveField_Double(hContext, "element_acct_bal");
			RemoveField_Double(hContext, "element_intransit");                                              
		}

		hash_destroy(hOrgTxn);
                hash_destroy(hNewTxn);

		FREE_ME(hOrgTxn);
                FREE_ME(hNewTxn);
	}	

	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {

DEBUGLOG(("Authorize: iNoBatchId = [%d]\n",iNoBatchId));		

		PutField_Char(hTxnContext,"txn_level",PD_TXN_LEVEL);
		PutField_Char(hTxnContext,"batch_type",PD_GENERAL);
                PutField_CString(hTxnContext,"batch_id",csOrgBatchId);

		hash_t		*hTxnHeaderRec;
		recordset_t	*rsTxnHeader;
		rsTxnHeader = (recordset_t*) malloc (sizeof(recordset_t));
		char	cStatus;
		char	cARInd;

DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation:GetTxnId\n"));
                DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","GetTxnId");
                iRet = (unsigned long)(*DBObjPtr)(hTxnContext,rTxnRecordSet);
                if (iRet == PD_OK) {
                        hTxnRec = RecordSet_GetFirst(rTxnRecordSet);
			while ((hTxnRec) && (iRet == PD_OK)) {
/* txn_id */
                                if (GetField_CString(hTxnRec,"txn_id",&csTmpTxnSeq)) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() txn_id = [%s]\n",csTmpTxnSeq));
                                
					//get Txn Header Status ARInd info
					recordset_init(rsTxnHeader,0);
DEBUGLOG(("Authorize() Call DBOLTransaction::GetTxnHeader\n"));
					DBObjPtr = CreateObj(DBPtr, "DBOLTransaction","GetTxnHeader");
					if ((*DBObjPtr)(csTmpTxnSeq,rsTxnHeader) == PD_OK) {
						hTxnHeaderRec = RecordSet_GetFirst(rsTxnHeader);
						while (hTxnHeaderRec) {
							if (GetField_Char(hTxnHeaderRec, "status", &cStatus)) {
DEBUGLOG(("Authorize() status = [%c]\n",cStatus));
							}
							if (GetField_Char(hTxnHeaderRec, "ar_ind", &cARInd)) {
DEBUGLOG(("Authorize() status = [%c]\n",cARInd));
							}
							hTxnHeaderRec = RecordSet_GetNext(rsTxnHeader);
						}
					} else {
						iRet = INT_ERR;
						PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLTransaction::GetTxnHeader FAILED!!\n"));
ERRLOG("TxnOmtByUsRBT::Authorize():: Call DBOLTransaction::GetTxnHeader FAILED!!\n");
					}
					recordset_destroy(rsTxnHeader);	
				}

				if (iRet == PD_OK) {
					//check status and ar_ind
					if (cStatus == PD_COMPLETE && cARInd == PD_ACCEPT) {
						//check multiple batch id (if status==CA)
	 					PutField_CString(hTxnContext,"txn_id",csTmpTxnSeq);
						PutField_CString(hTxnContext,"txn_seq",csTmpTxnSeq);
						iRecCnt = 0;
						hBatchRec = NULL;
						recordset_init(rBatchRecordSet,0);
						DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","GetBatchId");
						iRet = (unsigned long)(*DBObjPtr)(hContext,rBatchRecordSet);
						if (iRet == PD_OK) {
							hBatchRec = RecordSet_GetFirst(rBatchRecordSet);
							while (hBatchRec) {
								// batch_id
								if (GetField_CString(hBatchRec,"batch_id",&csTmp)) {
									if (memcmp(csTmp,csIgnoreBatchId,strlen(csTmp)) != 0) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetBatchId batch_id=[%s]\n",csTmp));
										iRecCnt++;
									}
								}
								hBatchRec = RecordSet_GetNext(rBatchRecordSet);
							}
						}
						if (iRecCnt == 1) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation:: Call DBOLBatchTxnRelation:: Single Batch Id Found!!!\n"));
						} else {
							iRet = INT_ERR;
							PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation:: Call DBOLBatchTxnRelation:: Invalid Txn Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation:: Call DBOLBatchTxnRelation:: Invalid Txn Id!!!\n");
						} 
					}
				}

				//check voided txn id
				if (iRet == PD_OK) {
					PutField_CString(hTmpTxnRec,"txn_id",csTmpTxnSeq);								
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation FindVoidTxnCount\n"));
                                	DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","FindVoidTxnCount");
                                	iDtlRet = (unsigned long)(*DBObjPtr)(hTmpTxnRec);
                                	if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Voided Txn Not Found!!!\n"));
                                	} else {
                                        	iRet = INT_ERR;
                                        	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Voided Txn Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLTxnRelation:: Call DBOLTxnRelation:: Voided Txn Found!!!\n");
					}
				}
				

				// Call function to check action
				if (iRet == PD_OK) {
			 		iDtlRet = CheckActionByTxnId(csTmpTxnSeq, hContext);	
					if (iDtlRet == PD_FALSE) {
DEBUGLOG(("Authorize: CheckActionByTxnId txn_id not allow to void/cancel\n"));
                         	       		iRet = INT_INVALID_TXN;
						PutField_Int(hContext,"internal_error",iRet);
						break;
                        		} else if (iDtlRet != PD_BATCH_ACTION_VOID &&
                                		iDtlRet != PD_BATCH_ACTION_CANCEL &&
						iDtlRet != PD_FALSE) {
                                		iRet = iDtlRet;
						PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize: CheckActionByTxnId return error, iRet = [%d]\n",iRet));
						break;
                        		} else {
DEBUGLOG(("Authorize: CheckActionByTxnId iAction=[%d]\n",iDtlRet));
					}
				}

                                hTxnRec = RecordSet_GetNext(rTxnRecordSet);
                        }
                } else {
                        iRet = INT_ERR;
			PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() Invalid Batch Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetTxnId() Invalid Batch Id!!!\n");
                }
	
		FREE_ME(hTxnHeaderRec);
                FREE_ME(rsTxnHeader);
        }

	// handle Offset for batch txn_id
	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {
		DBObjPtr = CreateObj(DBPtr, "DBOLTxnSeq", "GetNextActionBatchSeq");
		strcpy(csNewBatchId, (*DBObjPtr)());
		PutField_CString(hContext, "batch_txn_seq", csNewBatchId);
DEBUGLOG(("Authorize: new_batch_id = [%s]\n",csNewBatchId));
	}

	//loop txnid from batch for void/cancel action
	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {
		hash_t  *hOrgTxn;
                hOrgTxn = (hash_t*) malloc (sizeof(hash_t));

                hash_t  *hNewTxn;
                hNewTxn = (hash_t*) malloc (sizeof(hash_t));

		hash_t  *hRelatedTxn;
                hRelatedTxn = (hash_t*) malloc (sizeof(hash_t));

		hash_t  *hRelatedTxnRec = NULL;	
		recordset_t     *rRelatedTxnRecordSet;
		rRelatedTxnRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
		
		hash_t  *hVoidTxnRec = NULL;
                recordset_t     *rVoidTxnRecordSet;
                rVoidTxnRecordSet = (recordset_t *)malloc (sizeof(recordset_t));

                hTxnRec = RecordSet_GetFirst(rTxnRecordSet);
                while (hTxnRec) {
                        hash_init(hOrgTxn, 0);
DEBUGLOG(("Authorize: Process Next Txn\n"));
                        if (GetField_CString(hTxnRec,"txn_id",&csOrgTxnSeq)) {
DEBUGLOG(("Authorize: org_txn_id = [%s]\n",csOrgTxnSeq));
		
				hash_init(hNewTxn, 0);

                                iRet = ProcessOffset(csOrgTxnSeq, hOrgTxn, hNewTxn, hContext);

				if (GetField_CString(hNewTxn, "txn_seq", &csNewTxnSeq)) {
DEBUGLOG(("Authorize: new_txn_id [%s]\n", csNewTxnSeq));
        			}

				hash_destroy(hNewTxn);
			}

                        hTxnRec = RecordSet_GetNext(rTxnRecordSet);

			//Store new linkage of txnRelation into Hash
			sprintf(csTag,"id_%s",(const char*)csOrgTxnSeq);
			PutField_CString(hTxnRelation,csTag,(const char*)csNewTxnSeq);
DEBUGLOG(("Authorize: org_txn_id=[%s], new_txn_id=[%s]\n",(char*)csOrgTxnSeq,(char*)csNewTxnSeq));

			if (!hTxnRec && iAction != PD_BATCH_ACTION_CANCEL) {

				//do last txn
				if (iRet == PD_OK) {
					hash_init(hNewTxn, 0);

                                	iRet = ProcessLastTxn(csNewTxnSeq, hOrgTxn, hNewTxn, hContext);	

					if (GetField_CString(hNewTxn, "txn_seq", &csLastTxnSeq)) {
DEBUGLOG(("Authorize() last_txn_seq = [%s]\n", csLastTxnSeq));
					}

					hash_destroy(hNewTxn);
				}

				//do related last txn (Balance Transfer - Intra)
				if (iRet == PD_OK) {
        				recordset_init(rRelatedTxnRecordSet,0);	
			
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation GetRelatedTxnId\n"));
                                        DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","GetRelatedTxnId");
                                        iDtlRet = (unsigned long)(*DBObjPtr)(hOrgTxn, rRelatedTxnRecordSet);

                                        if (iDtlRet == PD_FOUND) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Related Txn Found!!!\n"));

						hRelatedTxnRec = RecordSet_GetFirst(rRelatedTxnRecordSet);
                				while (hRelatedTxnRec) {

/*
							if (GetField_Char(hRelatedTxnRec, "relation_type", &cRelationType)) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: relation_type = [%c]\n", cRelationType));
							} 	
*/

							if (GetField_CString(hRelatedTxnRec, "related_txn_seq", &csRelatedTxnSeq)) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: related_txn_seq = [%s]\n", csRelatedTxnSeq));
								PutField_CString(hOrgTxn,"txn_seq",csRelatedTxnSeq);
						
								recordset_init(rVoidTxnRecordSet,0);
		
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation GetVoidTxnId\n"));
                                        			DBObjPtr = CreateObj(DBPtr, "DBOLTxnRelation","GetVoidTxnId");
                                        			iDtlRet = (unsigned long)(*DBObjPtr)(hOrgTxn, rVoidTxnRecordSet);							

								if (iDtlRet == PD_FOUND) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Void Txn Found!!!\n"));
								
									hVoidTxnRec = RecordSet_GetFirst(rVoidTxnRecordSet);
                                                			while (hVoidTxnRec) {

										if (GetField_CString(hVoidTxnRec, "void_txn_seq", &csVoidTxnSeq)) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: void_txn_seq = [%s]\n", csVoidTxnSeq));

											hash_init(hRelatedTxn, 0);

                                                                			iRet = GetTxnInfo(csRelatedTxnSeq, hContext, hRelatedTxn);

                                                                			if (iRet == PD_OK) {
                                                                        			hash_init(hNewTxn, 0);
	
                                                                        			iRet = ProcessLastTxn(csVoidTxnSeq, hRelatedTxn, hNewTxn, hContext);
	
												if (GetField_CString(hNewTxn, "txn_seq", &csTmp)) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: p2_last_txn_seq = [%s]\n", csTmp));
													PutField_CString(hLastTxnRec,"p2_last_txn_seq",csTmp);
                                        							}

												PutField_CString(hLastTxnRec,"p1_last_txn_seq",csLastTxnSeq);
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: p1_last_txn_seq = [%s]\n", csLastTxnSeq));

/*
												PutField_Char(hLastTxnRec,"relation_type",cRelationType);
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: relation_type = [%c]\n", cRelationType));
*/

												PutField_Char(hLastTxnRec,"relation_type",PD_REL_BAL_TRF);
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: relation_type = [%c]\n", PD_REL_BAL_TRF));

												PutField_CString(hLastTxnRec,"update_user",csUser);	
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: update_user = [%s]\n", csUser));

												RecordSet_Add(rLastTxnRecordSet, hLastTxnRec);

                                                                        			hash_destroy(hNewTxn);
                                                               	 			}

                                                                			hash_destroy(hRelatedTxn);
										}

										hVoidTxnRec = RecordSet_GetNext(rVoidTxnRecordSet);
									}	
								} else if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Void Txn Not Found!!!\n"));
                                        			} else {
                                                			iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Invalid Void Txn!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLTxnRelation:: Call DBOLTxnRelation:: Invalid Void Txn!!!\n");
                                        			}

                                        			RecordSet_Destroy(rVoidTxnRecordSet);
							}

							hRelatedTxnRec = RecordSet_GetNext(rRelatedTxnRecordSet);		
						}
                                        } else if (iDtlRet == PD_NOT_FOUND) {
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Related Txn Not Found!!!\n"));
                                        } else {
                                        	iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLTxnRelation:: Call DBOLTxnRelation:: Invalid Related Txn!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLTxnRelation:: Call DBOLTxnRelation:: Invalid Related Txn!!!\n");
                                	}

					RecordSet_Destroy(rRelatedTxnRecordSet);
				}
                        }

			//remove fields - element_acct_bal and element_intransit
			RemoveField_Double(hContext, "element_acct_bal");
                        RemoveField_Double(hContext, "element_intransit");

                        hash_destroy(hOrgTxn);
                }

                FREE_ME(hOrgTxn);
                FREE_ME(hNewTxn);
                FREE_ME(hRelatedTxn);

                FREE_ME(rRelatedTxnRecordSet);
                FREE_ME(rVoidTxnRecordSet);
	}

/*
 * Step 2: Do Txn Relation (Linkage) * * * * * * * * * * * * * * * * * * * *
 */
	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {
		iRet = AddTxnRelationLinkage(csOrgBatchId, hContext, hTxnRelation);

		if (iRet == PD_OK) {
			hLastTxnRecGet = RecordSet_GetFirst(rLastTxnRecordSet);
                        while (hLastTxnRecGet) {
				iRet = AddLastTxnRelationLinkage(hLastTxnRecGet);
				hLastTxnRecGet = RecordSet_GetNext(rLastTxnRecordSet);
			}
		}
	}

/*
 * Step 3: Do Stmt Txn Level * * * * * * * * * * * * * * * * * * * *
 */
	//Get Stmt Txn Id recordset, order by approval_ts DESC
	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {
		hash_t	*hBaidTxnRec;
		hBaidTxnRec = (hash_t*) malloc (sizeof(hash_t));
		hash_init(hBaidTxnRec,0);
                char    cStatus;
                char    cARInd;

                PutField_Char(hStmtTxnContext,"txn_level",PD_STMT_LEVEL);
                PutField_Char(hStmtTxnContext,"batch_type",PD_GENERAL);
                PutField_CString(hStmtTxnContext,"batch_id",csOrgBatchId);

DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation:GetTxnId\n"));
                DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","GetTxnId");
                iRet = (unsigned long)(*DBObjPtr)(hStmtTxnContext,rStmtTxnRecordSet);
                if (iRet == PD_OK) {
                        hStmtTxnRec = RecordSet_GetFirst(rStmtTxnRecordSet);
			while ((hStmtTxnRec) && (iRet == PD_OK)) {

/* txn_id */
                                if (GetField_CString(hStmtTxnRec,"txn_id",&csTmpStmtTxnSeq)) {
					PutField_CString(hStmtTxnContext,"txn_id",csTmpStmtTxnSeq);
					PutField_CString(hStmtTxnContext,"txn_seq",csTmpStmtTxnSeq);
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() txn_id = [%s]\n",csTmpStmtTxnSeq));
                                
					//get BAID Status ARInd info
DEBUGLOG(("Authorize() Call DBOLBAIDTxn:GetBaidTxn [%s]\n", csTmpStmtTxnSeq));
                			DBObjPtr = CreateObj(DBPtr, "DBOLBAIDTxn", "GetBaidTxn");
                			iRet = (unsigned long)(*DBObjPtr)(csTmpStmtTxnSeq,hBaidTxnRec);
                			if (iRet == PD_OK) {
                        			if (GetField_Char(hBaidTxnRec, "status", &cStatus)) {
DEBUGLOG(("Authorize() call DBOLBAIDTxn:: status [%c]\n", cStatus));
                        			}
                        			if (GetField_Char(hBaidTxnRec, "ar_ind", &cARInd)) {
DEBUGLOG(("Authorize() call DBOLBAIDTxn:: ar_ind [%c]\n", cARInd));
                        			}	
					} else {
						iRet = INT_ERR;
DEBUGLOG(("Authorize() call DBOLBAIDTxn GetBaidTxn::GetBaidTxn FAILED!!\n"));
ERRLOG("TxnOmtByUsRBT::call DBOLBAIDTxn GetBaidTxn::GetBaidTxn FAILED!!\n");
					}
				}			

				if (iRet == PD_OK) {
					//check status and ar_ind
					if (cStatus == PD_COMPLETE && cARInd == PD_ACCEPT) {
						//check multiple batch id	
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation:CheckSingleBatchId\n"));
                                		DBObjPtr = CreateObj(DBPtr, "DBOLBatchTxnRelation","CheckSingleBatchId");
                                		iDtlRet = (unsigned long)(*DBObjPtr)(hStmtTxnContext);
                                		if (iDtlRet == PD_TRUE) {
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::CheckSingleBatchId() Single Batch Id Found!!!\n"));
                                		} else if (iDtlRet == PD_FALSE) {
                                        		iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::CheckSingleBatchId() Multi-Batch Id Found!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::CheckSingleBatchId() Multi-Batch Id Found!!!\n");
                                		} else {
                                        		iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::CheckSingleBatchId() Invalid Txn Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::CheckSingleBatchId() Invalid Txn Id!!!\n");
                                		}
					}
				}

                                hStmtTxnRec = RecordSet_GetNext(rStmtTxnRecordSet);
                        }
                } else {
                        iRet = INT_ERR;
DEBUGLOG(("Authorize() Call DBOLBatchTxnRelation::GetTxnId() Invalid Batch Id!!!\n"));
ERRLOG("TxnOmtByUsRBT::Call DBOLBatchTxnRelation::GetTxnId() Invalid Batch Id!!!\n");
                }
	
		FREE_ME(hBaidTxnRec);
        }

	//loop stmt txnid from batch for action
	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {
                hStmtTxnRec = RecordSet_GetFirst(rStmtTxnRecordSet);
                while (hStmtTxnRec) {
                        if (GetField_CString(hStmtTxnRec,"txn_id",&csOrgStmtTxnSeq)) {
DEBUGLOG(("Authorize: stmt_org_txn_id = [%s]\n",csOrgStmtTxnSeq));
		
                                // Call function to check action
				iDtlRet = CheckActionByStmtTxnId(csOrgStmtTxnSeq);

                                if (iDtlRet == PD_TRUE) {
                                        iRet = ProcessStmtOffset(csOrgStmtTxnSeq, hContext);
                                } else if (iDtlRet == PD_FALSE) {
					// Do Nothing...
				} else {
                                        iRet = iDtlRet;
                                }
                        }

                        hStmtTxnRec = RecordSet_GetNext(rStmtTxnRecordSet);
                }
        }

/*
 * Step 4: Do Batch Relation * * * * * * * * * * * * * * * * * * * *
 */
	// Add Batch Relation	
	if ((iNoBatchId == PD_FALSE) && (iRet == PD_OK)) {

DEBUGLOG(("Authorize: org_batch_id = [%s], new_batch_id = [%s]\n",csOrgBatchId,csNewBatchId));
                PutField_CString(hContext, "org_batch_id", csOrgBatchId);
                PutField_CString(hContext, "batch_id", csNewBatchId);

                iRet = AddBatchRelation(hContext);
        }

	RecordSet_Destroy(rBatchRecordSet);
        FREE_ME(rBatchRecordSet);

	RecordSet_Destroy(rLastTxnRecordSet);
        FREE_ME(rLastTxnRecordSet);

        RecordSet_Destroy(rTxnRecordSet);
        FREE_ME(rTxnRecordSet);

	RecordSet_Destroy(rStmtTxnRecordSet);
        FREE_ME(rStmtTxnRecordSet);

	hash_destroy(hLastTxnRec);
	FREE_ME(hLastTxnRec);

	FREE_ME(hTmpTxnRec);
	FREE_ME(hTxnContext);
	FREE_ME(hStmtTxnContext);

DEBUGLOG(("Authorize::TxnOmtByUsRBT: END!!!!!!!!!\n"));

DEBUGLOG(("TxnOmtByUsRBT Normal Exit() iRet = [%d]\n",iRet));
	return iRet;
}

