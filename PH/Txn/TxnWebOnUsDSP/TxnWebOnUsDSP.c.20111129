/* precal psp fee if define */
	if (iRet == PD_OK) {
		/* Update Stat */
DEBUGLOG(("TxnWebOnUsDSP::PostTxn():: Call BOPsp CalPspFee\n"));
                BOObjPtr = CreateObj(BOPtr,"BOPsp","CalPspFee");
                iRet = (unsigned long)(*BOObjPtr)(hContext,hRequest);
DEBUGLOG(("TxnWebOnUsDSP::PostTxn():: BOStat::CalPspFee() result = [%d]\n",iRet));
	}

	if (iRet == PD_OK) {
		/* Update Balance */
