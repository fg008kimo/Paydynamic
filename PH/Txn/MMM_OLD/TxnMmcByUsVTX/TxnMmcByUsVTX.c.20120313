#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "common.h"
#include "utilitys.h"
#include "ObjPtr.h"
#include "internal.h"
#include "TxnMmcByUsVTX.h"
#include "myrecordset.h"
#include <curl/curl.h>
#include "queue_utility.h"
#include "mq_db.h"

char cDebug;
#define	PD_RETRY	'R'

OBJPTR(DB);
OBJPTR(BO);
OBJPTR(Txn);
void TxnMmcByUsVTX(char    cdebug)
{
        cDebug = cdebug;
}

int     Authorize(hash_t* hContext,
                        const hash_t* hRequest,
                        hash_t* hResponse)
{
        int     iRet = PD_OK;
	char*	csOrgTxnSeq;
	char*	csOrgDtlTxnSeq;
	char	cTmp;
	char	*csTmp = NULL;
	double	dAmt = 0.0;
	hash_t  *hRec;
	hash_t  *hTxn;

	hTxn = (hash_t*)  malloc (sizeof(hash_t));
        hash_init(hTxn,0);



	recordset_t     *rRecordSet;
        rRecordSet = (recordset_t *)malloc (sizeof(recordset_t));
        recordset_init(rRecordSet,0);


DEBUGLOG(("TxnMmcByUsVTX: Authorize()\n"));

	if(GetField_CString(hRequest,"txn_seq",&csOrgTxnSeq)){
DEBUGLOG(("TxnMmcByUsVTX: txn_seq = [%s]\n",csOrgTxnSeq));
	}

	if(GetField_CString(hRequest,"dtl_txn_seq",&csOrgDtlTxnSeq)){
DEBUGLOG(("TxnMmcByUsVTX: dtl_txn_seq = [%s]\n",csOrgDtlTxnSeq));
	}

	if(GetField_Char(hRequest,"auto_reverse",&cTmp)){
                PutField_Char(hTxn,"auto_reverse",cTmp);
DEBUGLOG(("TxnMmcByUsVTX: auto_reverse = [%c]\n",cTmp));
        }

	if(GetField_CString(hRequest,"update_user",&csTmp)){
		PutField_CString(hTxn,"update_user",csTmp);
DEBUGLOG(("TxnMmcByUsVTX: update_user = [%s]\n",csTmp));
	}


	if(iRet == PD_OK){
DEBUGLOG(("Authorize:: call DBMmsTransaction:GetMmsTxnDetail\n"));
		DBObjPtr = CreateObj(DBPtr,"DBMmsTransaction","GetMmsTxnDetail");
		if ((unsigned long)((*DBObjPtr)(csOrgTxnSeq,csOrgDtlTxnSeq,rRecordSet)) != PD_OK) {

DEBUGLOG(("Authorize:: call DBMmsTransaction:GetMmsTxnDetail fail\n"));
			iRet = INT_ERR;
			PutField_Int(hContext,"internal_error",iRet);
		}
		else{
			hRec = RecordSet_GetFirst(rRecordSet);
			while (hRec) {
				if(GetField_CString(hRec,"txn_code",&csTmp)){
DEBUGLOG(("GetMmsTxnDetail: txn_code[%s]\n",csTmp));
				}
				if(GetField_Char(hRec,"isd_ind",&cTmp)){
					PutField_Char(hTxn,"isd_ind",cTmp);
DEBUGLOG(("GetMmsTxnDetail: isd_ind[%c]\n",cTmp));
				}
				if(GetField_CString(hRec,"party_node_id",&csTmp)){
					PutField_CString(hTxn,"node_id",csTmp);
DEBUGLOG(("GetMmsTxnDetail: party_node_id[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"merchant_id",&csTmp)){
					PutField_CString(hTxn,"party_type",PD_MMS_PARTY_MERCH);
					PutField_CString(hTxn,"merchant_id",csTmp);
DEBUGLOG(("GetMmsTxnDetail: merchant_id[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"psp_id",&csTmp)){
					PutField_CString(hTxn,"party_type",PD_MMS_PARTY_PSP);
					PutField_CString(hTxn,"psp_id",csTmp);
DEBUGLOG(("GetMmsTxnDetail: psp_id[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"mb_id",&csTmp)){
					PutField_CString(hTxn,"party_type",PD_MMS_PARTY_MB);
					PutField_CString(hTxn,"mb_id",csTmp);
DEBUGLOG(("GetMmsTxnDetail: mb_id[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"bank_id",&csTmp)){
					PutField_CString(hTxn,"party_type",PD_MMS_PARTY_BANK);
					PutField_CString(hTxn,"bank_id",csTmp);
DEBUGLOG(("GetMmsTxnDetail: bank_id[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"stl_bank_id",&csTmp)){
					PutField_CString(hTxn,"party_type",PD_MMS_PARTY_STL_BK);
					PutField_CString(hTxn,"stl_bank_id",csTmp);
DEBUGLOG(("GetMmsTxnDetail: stl_bank_id[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"src_party_type",&csTmp)){
					PutField_CString(hTxn,"mms_from_type",csTmp);
DEBUGLOG(("GetMmsTxnDetail: src_party_type[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"dst_party_type",&csTmp)){
					PutField_CString(hTxn,"mms_to_type",csTmp);
DEBUGLOG(("GetMmsTxnDetail: dst_party_type[%s]\n",csTmp));
				}
				if(GetField_CString(hRec,"txn_ccy",&csTmp)){
					PutField_CString(hTxn,"txn_ccy",csTmp);
DEBUGLOG(("GetMmsTxnDetail: txn_ccy[%s]\n",csTmp));
				}
				if(GetField_Double(hRec,"txn_amt",&dAmt)){
					PutField_Double(hTxn,"txn_amt",dAmt);
DEBUGLOG(("GetMmsTxnDetail: txn_amt[%lf]\n",dAmt));
				}
				if(GetField_Double(hRec,"adjustment",&dAmt)){
					PutField_Double(hTxn,"adjustment",dAmt);
DEBUGLOG(("GetMmsTxnDetail: adjustment[%lf]\n",dAmt));
				}
				if(GetField_Double(hRec,"processing_cost",&dAmt)){
					PutField_Double(hTxn,"processing_cost",dAmt);
DEBUGLOG(("GetMmsTxnDetail: processing_cost[%lf]\n",dAmt));
				}
				if(GetField_Double(hRec,"bank_charge",&dAmt)){
					PutField_Double(hTxn,"bank_charge",dAmt);
DEBUGLOG(("GetMmsTxnDetail: bank_charge[%lf]\n",dAmt));
				}
				if(GetField_Double(hRec,"bank_charge_refund",&dAmt)){
					PutField_Double(hTxn,"bank_charge_refund",dAmt);
DEBUGLOG(("GetMmsTxnDetail: bank_charge_refund[%lf]\n",dAmt));
				}

				hRec = RecordSet_GetNext(rRecordSet);
			}
		}

	}

	if(iRet==PD_OK){
//reverse balance
		PutField_Int(hTxn, "void_flag", PD_TRUE);

		//BOObjPtr = CreateObj(BOPtr,"BOMmsBalance","TxnTypeHandler");
                //iRet = (unsigned long)((*BOObjPtr)(hTxn));
		BOObjPtr = CreateObj(BOPtr,"BOMmsTask","NewTask");
                iRet = (unsigned long)((*BOObjPtr)(hTxn));


	}


	FREE_ME(hTxn);
	RecordSet_Destroy(rRecordSet);
	FREE_ME(rRecordSet);

DEBUGLOG(("TxnMmcByUsVTX:  Normal Exit() iRet = [%d]\n",iRet));
	return iRet;
}
