#endif

#if 0
/* statement_date */
						PutField_CString(hContext, "statement_date",csStatementDate);
DEBUGLOG(("Authorize() AddDetail::statement_time = [%s]\n",csStatementDate));

/* statement_time */
						PutField_CString(hContext, "statement_time",csStatementTime);
DEBUGLOG(("Authorize() AddDetail::statement_time = [%s]\n", csStatementTime));

/* tfr_bank_name */
