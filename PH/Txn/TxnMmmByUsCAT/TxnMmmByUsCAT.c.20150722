#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "common.h"
#include "utilitys.h"
#include "ObjPtr.h"
#include "internal.h"
#include "TxnMmmByUsCAT.h"
#include "myrecordset.h"

char cDebug;

OBJPTR(BO);

void TxnMmmByUsCAT(char    cdebug)
{
        cDebug = cdebug;
}

int     Authorize(hash_t* hContext,
                  const hash_t* hRequest,
                  hash_t* hResponse)
{
        int     iRet = PD_OK;

	int	iBalCnt = 0;
	int	iFxCnt = 0;
	int	i = 0;

	double	dTxnAmt = 0.00;
	double	dTotalNatTxnAmt = 0.00;
	double	dTmp = 0.00;

	char	cTmp;

	char* 	csTxnSeq = NULL;
	char*   csTmp = NULL;

	char    csTag[PD_TAG_LEN + 1];

	hash_t  *hData = (hash_t*) malloc (sizeof(hash_t));
        hash_init(hData,0);

	cTmp = ' ';

DEBUGLOG(("Authorize()\n"));

/* txn_seq */
       	if (GetField_CString(hContext,"txn_seq",&csTxnSeq)) {
DEBUGLOG(("Authorize() txn_seq = [%s]\n",csTxnSeq));
		PutField_CString(hData, "txn_seq", csTxnSeq);
  	} else {
DEBUGLOG(("Authorize:: txn_id missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: txn_id missing!!\n");
                iRet = INT_TXN_ID_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);	
	}

/* txn_country */
        if (GetField_CString(hRequest,"txn_country",&csTmp)) {
DEBUGLOG(("Authorize: txn_country = [%s]\n",csTmp));
                PutField_CString(hData, "txn_country", csTmp);
        } else {
DEBUGLOG(("Authorize:: txn_country missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: txn_country missing!!\n");
                iRet = INT_MMS_COUNTRY_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* txn_ccy */
        if (GetField_CString(hRequest,"txn_ccy",&csTmp)) {
DEBUGLOG(("Authorize: txn_ccy = [%s]\n",csTmp));
                PutField_CString(hData, "txn_ccy", csTmp);
        } else {
DEBUGLOG(("Authorize:: txn_ccy missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: txn_ccy missing!!\n");
                iRet = INT_MMS_CCY_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* txn_amt */
        if (GetField_CString(hRequest,"txn_amt",&csTmp)) {
                dTxnAmt = atof(csTmp);
DEBUGLOG(("Authorize: txn_amt = [%.2lf]\n",dTxnAmt));
                PutField_Double(hData, "txn_amt", dTxnAmt);
        } else {
DEBUGLOG(("Authorize:: txn_amt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: txn_amt missing!!\n");
                iRet = INT_MMS_ADJ_AMT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* fee_amt */
	if (GetField_CString(hRequest,"fee_amt",&csTmp)) {
                dTmp = atof(csTmp);
DEBUGLOG(("Authorize: fee_amt = [%.2lf]\n",dTmp));
                PutField_Double(hData, "fee_amt", dTmp);
        }

/* remark */
        if (GetField_CString(hRequest,"remark",&csTmp)) {
DEBUGLOG(("Authorize: remark = [%s]\n",csTmp));
                PutField_CString(hData, "remark", csTmp);
		PutField_CString(hContext, "remark", csTmp);
        }

/* code */
        if (GetField_CString(hRequest,"code",&csTmp)) {
DEBUGLOG(("Authorize: code = [%s]\n",csTmp));
                PutField_CString(hData, "code", csTmp);
		PutField_CString(hContext, "txn_code", csTmp);
        } else {
DEBUGLOG(("Authorize:: code missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: code missing!!\n");
                iRet = INT_MMS_ADJ_CODE_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* eid (entity_id) */
        if (GetField_CString(hContext,"entity_id",&csTmp)) {
DEBUGLOG(("Authorize: eid = [%s]\n",csTmp));
		PutField_CString(hData, "entity_id", csTmp);
        } else {
DEBUGLOG(("Authorize:: eid missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: eid missing!!\n");
                iRet = INT_MMS_ENTITY_ID_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* bal_cnt */
	if (GetField_CString(hRequest,"txn_data.bal_cnt",&csTmp)) {
                iBalCnt = atoi(csTmp);
DEBUGLOG(("Authorize: bal_cnt = [%d]\n",iBalCnt));
		PutField_Int(hData, "bal_cnt", iBalCnt);
        } else {
DEBUGLOG(("Authorize:: bal_cnt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: bal_cnt missing!!\n");
                iRet = INT_MMS_BAL_CNT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

	if (iRet == PD_OK) {

                for (i=1; i<=iBalCnt; i++)
                {
/* amt */
			sprintf(csTag,"txn_data.bal.%d.amt",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
				dTmp = atof(csTmp);
DEBUGLOG(("Authorize: amt_%d = [%.2lf]\n",i,dTmp));
				dTotalNatTxnAmt += dTmp;
				sprintf(csTag,"bal.%d.amt",i);
				PutField_Int(hData, csTag, dTmp);
			} else {
DEBUGLOG(("Authorize:: amt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: amt missing!!\n");
                		iRet = INT_MMS_ADJ_AMT_NOT_FOUND;
                		PutField_Int(hContext,"internal_error",iRet);
			}
	
/* nature_id */
			sprintf(csTag,"bal.%d.nature_id",i);
                        if (GetField_CString(hContext,csTag,&csTmp)) {
DEBUGLOG(("Authorize: nature_id_%d = [%s]\n",i,csTmp));
				PutField_CString(hData, csTag, csTmp);
                        } else {
DEBUGLOG(("Authorize:: nature_id missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: nature_id missing!!\n");
                                iRet = INT_MMS_NATURE_ID_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }
		}

/* total_nat_txn_amt */
		if (iRet == PD_OK) {
DEBUGLOG(("Authorize: total_nat_txn_amt = [%.2lf]\n",dTotalNatTxnAmt));
        		if (dTotalNatTxnAmt != dTxnAmt) {
DEBUGLOG(("Authorize:: total_nat_txn_amt invalid!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: total_nat_txn_amt invalid!!\n");
        	        	iRet = INT_MMS_INVALID_AMT;
        	        	PutField_Int(hContext,"internal_error",iRet);
        		}
		}
	}

/* fx_cnt */
        if (GetField_CString(hRequest,"txn_data.fx_cnt",&csTmp)) {
                iFxCnt = atoi(csTmp);
DEBUGLOG(("Authorize: fx_cnt = [%d]\n",iFxCnt));
        } else {
DEBUGLOG(("Authorize:: fx_cnt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: fx_cnt missing!!\n");
                iRet = INT_MMS_FX_CNT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

        if (iRet == PD_OK) {

                for (i=1; i<=iFxCnt; i++)
                {
/* occy */
                        sprintf(csTag,"txn_data.fx.%d.occy",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
DEBUGLOG(("Authorize: occy_%d = [%s]\n",i,csTmp));
                        } else {
DEBUGLOG(("Authorize:: occy missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: occy missing!!\n");
                                iRet = INT_MMS_FX_ORG_CCY_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }

/* ccy */
                        sprintf(csTag,"txn_data.fx.%d.ccy",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
DEBUGLOG(("Authorize: ccy_%d = [%s]\n",i,csTmp));
                        } else {
DEBUGLOG(("Authorize:: ccy missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: ccy missing!!\n");
                                iRet = INT_MMS_FX_CCY_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }

/* amt */
                        sprintf(csTag,"txn_data.fx.%d.amt",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
				dTmp = atof(csTmp);
DEBUGLOG(("Authorize: amt_%d = [%.2lf]\n",i,dTmp));
                        } else {
DEBUGLOG(("Authorize:: amt missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: amt missing!!\n");
                                iRet = INT_MMS_FX_AMT_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }
		}
	}

/* create_user */
	if (GetField_CString(hRequest,"user",&csTmp)) {
DEBUGLOG(("Authorize: create_user = [%s]\n",csTmp));
		PutField_CString(hData, "user", csTmp);
		PutField_CString(hContext,"add_user",csTmp);
                PutField_CString(hContext,"update_user",csTmp);
	} else {
DEBUGLOG(("Authorize:: user missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: user missing!!\n");
		iRet = INT_USER_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
	}

	// Update Entity Nature Balance
	if (iRet == PD_OK) {

DEBUGLOG(("Authorize:: Call BOMMSAdjustment ProcessEntityNatureBalance\n"));
         	BOObjPtr = CreateObj(BOPtr, "BOMMSAdjustment","ProcessEntityNatureBalance");
              	iRet = (unsigned long)(*BOObjPtr)(hContext,hData);
              	if (iRet == PD_OK) {
DEBUGLOG(("Authorize:: Call BOMMSAdjustment:: Success!!!\n"));
            	} else {
                 	//iRet = INT_ERR;
                    	PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call BOMMSAdjustment:: Failure!!!\n"));
ERRLOG("TxnOmtByUsCAT::Authorize::Call BOMMSAdjustment:: Failure!!!\n");
             	}
	}

	// Add and Update Entity Transaction
	if (iRet == PD_OK) {

		// Add Txn Log
/*
DEBUGLOG(("Authorize:: Call BOMMSTransaction AddTxnLog\n"));
                BOObjPtr = CreateObj(BOPtr, "BOMMSTransaction","AddTxnLog");
                iRet = (unsigned long)(*BOObjPtr)(hContext,hRequest);
                if (iRet == PD_OK) {
DEBUGLOG(("Authorize:: Call BOMMSTransaction:: Success!!!\n"));
                } else {
                        iRet = INT_ERR;
                       	 PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call BOMMSTransaction:: Failure!!!\n"));
ERRLOG("TxnOmtByUsCAT::Authorize::Call BOMMSTransaction:: Failure!!!\n");
                }
*/
	
		// Update Txn Log
		
/* sub_status */
		PutField_CString(hContext, "sub_status", PD_APPROVED);		

/*
DEBUGLOG(("Authorize:: Call BOMMSTransaction UpdateTxnLog\n"));
                BOObjPtr = CreateObj(BOPtr, "BOMMSTransaction","UpdateTxnLog");
                iRet = (unsigned long)(*BOObjPtr)(hContext,hRequest,hResponse);
                if (iRet == PD_OK) {
DEBUGLOG(("Authorize:: Call BOMMSTransaction:: Success!!!\n"));
                } else {
                        iRet = INT_ERR;
                        PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call BOMMSTransaction:: Failure!!!\n"));
ERRLOG("TxnOmtByUsCAT::Authorize::Call BOMMSTransaction:: Failure!!!\n");
                }
*/
	}

	// Add Txn Elements
	if (iRet == PD_OK) {
	
/*
		PutField_CString(hData, "txn_element_type", PD_ELEMENT_TXN_AMT);
	
DEBUGLOG(("Authorize:: Call BOMMSTxnElement AddTxnElement\n"));
                BOObjPtr = CreateObj(BOPtr, "BOMMSTxnElement","AddTxnElement");
                iRet = (unsigned long)(*BOObjPtr)(hData);
                if (iRet == PD_OK) {
DEBUGLOG(("Authorize:: Call BOMMSTxnElement:: Success!!!\n"));
                } else {
                        iRet = INT_ERR;
                        PutField_Int(hContext,"internal_error",iRet);
DEBUGLOG(("Authorize:: Call BOMMSTxnElement:: Failure!!!\n"));
ERRLOG("TxnOmtByUsCAT::Authorize::Call BOMMSTxnElement:: Failure!!!\n");
                }
*/
	}

	// Response
	PutField_CString(hResponse,"reply_txn_code",PD_TXN_CODE_MMS_ADJUSTMENT_TXN);
	PutField_CString(hResponse,"txn_seq",csTxnSeq);
	PutField_Int(hResponse,"ret",iRet);

	hash_destroy(hData);
        FREE_ME(hData);

DEBUGLOG(("TxnMmsByUsCAT Normal Exit() reply_txn_code = [%s], txn_seq = [%s], iRet = [%d]\n",PD_TXN_CODE_MMS_ADJUSTMENT_TXN,csTxnSeq,iRet));
	return iRet;
}
