#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include "common.h"
#include "utilitys.h"
#include "ObjPtr.h"
#include "internal.h"
#include "TxnMmmByUsCAT.h"
#include "myrecordset.h"

char cDebug;

OBJPTR(DB);

void TxnMmmByUsCAT(char    cdebug)
{
        cDebug = cdebug;
}

int     Authorize(hash_t* hContext,
                  const hash_t* hRequest,
                  hash_t* hResponse)
{
        int     iRet = PD_OK;

	int	iBalCnt = 0;
	int	iNatCnt = 0;
	int	iFxCnt = 0;
	int	i = 0;
	int	j = 0;

	double	dTmp = 0.00;

	char	cTmp;

	char* 	csTxnSeq = NULL;
	char*   csTmp = NULL;

	char    csTag[PD_TAG_LEN + 1];

	cTmp = ' ';

DEBUGLOG(("Authorize()\n"));

/* txn seq */
       	if (GetField_CString(hContext,"txn_seq",&csTxnSeq)) {
DEBUGLOG(("Authorize() txn_seq = [%s]\n",csTxnSeq));
  	} else {
DEBUGLOG(("Authorize:: txn_id missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: txn_id missing!!\n");
                iRet = INT_TXN_ID_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);	
	}

/* eid (entity_id) */
        if (GetField_CString(hContext,"entity_id",&csTmp)) {
DEBUGLOG(("Authorize: eid = [%s]\n",csTmp));
        } else {
DEBUGLOG(("Authorize:: eid missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: eid missing!!\n");
                iRet = INT_MMS_ENTITY_ID_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* adj_code */
        if (GetField_CString(hRequest,"txn_data.adj_code",&csTmp)) {
DEBUGLOG(("Authorize: adj_code = [%s]\n",csTmp));
        } else {
DEBUGLOG(("Authorize:: adj_code missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: adj_code missing!!\n");
                iRet = INT_MMS_ADJ_CODE_NOT_FOUND;
               	PutField_Int(hContext,"internal_error",iRet);
        }

/* adj_cty */
	if (GetField_CString(hRequest,"txn_data.adj_cty",&csTmp)) {
DEBUGLOG(("Authorize: adj_cty = [%s]\n",csTmp));
        } else {
DEBUGLOG(("Authorize:: adj_cty missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: adj_cty missing!!\n");
               	iRet = INT_MMS_COUNTRY_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* adj_ccy */
        if (GetField_CString(hRequest,"txn_data.adj_ccy",&csTmp)) {
DEBUGLOG(("Authorize: adj_ccy = [%s]\n",csTmp));
        } else {
DEBUGLOG(("Authorize:: adj_ccy missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: adj_ccy missing!!\n");
                iRet = INT_MMS_CCY_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* adj_amt */
        if (GetField_CString(hRequest,"txn_data.adj_amt",&csTmp)) {
                dTmp = atof(csTmp);
DEBUGLOG(("Authorize: adj_amt = [%.2lf]\n",dTmp));
        } else {
DEBUGLOG(("Authorize:: adj_amt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: adj_amt missing!!\n");
                iRet = INT_MMS_ADJ_AMT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

/* amt_type */
        if (GetField_CString(hRequest,"txn_data.amt_type",&csTmp)) {
                cTmp = csTmp[0];
DEBUGLOG(("Authorize: amt_type = [%c]\n",cTmp));
        } else {
DEBUGLOG(("Authorize:: amt_type missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: amt_type missing!!\n");
           	iRet = INT_AMT_TYPE_NOT_FOUND;
              	PutField_Int(hContext,"internal_error",iRet);
        }

/* bal_cnt */
	if (GetField_CString(hRequest,"txn_data.bal_cnt",&csTmp)) {
                iBalCnt = atoi(csTmp);
DEBUGLOG(("Authorize: bal_cnt = [%d]\n",iBalCnt));
        } else {
DEBUGLOG(("Authorize:: bal_cnt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: bal_cnt missing!!\n");
                iRet = INT_MMS_BAL_CNT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

	if (iRet == PD_OK) {

                for (i=1; i<=iBalCnt; i++)
                {
/* adj_amt */
			sprintf(csTag,"txn_data.bal.%d.adj_amt",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
				dTmp = atof(csTmp);
DEBUGLOG(("Authorize: adj_amt = [%.2lf]\n",dTmp));
			} else {
DEBUGLOG(("Authorize:: adj_amt missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: adj_amt missing!!\n");
                		iRet = INT_MMS_ADJ_AMT_NOT_FOUND;
                		PutField_Int(hContext,"internal_error",iRet);
			}
		
/* nat_cnt */
			sprintf(csTag,"txn_data.bal.%d.nat_cnt",i);
        		if (GetField_CString(hRequest,csTag,&csTmp)) {
                		iNatCnt = atoi(csTmp);
DEBUGLOG(("Authorize: nat_cnt = [%d]\n",iNatCnt));
        		} else {
DEBUGLOG(("Authorize:: nat_cnt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: nat_cnt missing!!\n");
                		iRet = INT_MMS_NATURE_CNT_NOT_FOUND;
                		PutField_Int(hContext,"internal_error",iRet);
        		}

			if (iRet == PD_OK) {

				for (j=1; j<=iNatCnt; j++)
                		{	
/* grp */
                        		sprintf(csTag,"bal.%d.nat.%d.grp",i,j);
                        		if (GetField_CString(hContext,csTag,&csTmp)) {
DEBUGLOG(("Authorize: grp = [%s]\n",csTmp));
					} else {
DEBUGLOG(("Authorize:: grp missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: grp missing!!\n");
                                		iRet = INT_MMS_NATURE_GRP_NOT_FOUND;
                                		PutField_Int(hContext,"internal_error",iRet);
					}
/* val */
                                        sprintf(csTag,"bal.%d.nat.%d.val",i,j);
                                        if (GetField_CString(hContext,csTag,&csTmp)) {
DEBUGLOG(("Authorize: val = [%s]\n",csTmp));
                                        } else {
DEBUGLOG(("Authorize:: val missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: val missing!!\n");
                                                iRet = INT_MMS_NATURE_GRP_VAL_NOT_FOUND;
                                                PutField_Int(hContext,"internal_error",iRet);
                                        }	
				}
			}
		}
	}

/* fx_cnt */
        if (GetField_CString(hRequest,"txn_data.fx_cnt",&csTmp)) {
                iFxCnt = atoi(csTmp);
DEBUGLOG(("Authorize: fx_cnt = [%d]\n",iFxCnt));
        } else {
DEBUGLOG(("Authorize:: fx_cnt missing!!\n"));
ERRLOG("TxnMmsByUsCAT:: Authorize:: fx_cnt missing!!\n");
                iRet = INT_MMS_FX_CNT_NOT_FOUND;
                PutField_Int(hContext,"internal_error",iRet);
        }

        if (iRet == PD_OK) {

                for (i=1; i<=iFxCnt; i++)
                {
/* occy */
                        sprintf(csTag,"txn_data.fx.%d.occy",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
DEBUGLOG(("Authorize: occy = [%s]\n",csTmp));
                        } else {
DEBUGLOG(("Authorize:: occy missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: occy missing!!\n");
                                iRet = INT_MMS_FX_ORG_CCY_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }

/* ccy */
                        sprintf(csTag,"txn_data.fx.%d.ccy",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
DEBUGLOG(("Authorize: ccy = [%s]\n",csTmp));
                        } else {
DEBUGLOG(("Authorize:: ccy missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: ccy missing!!\n");
                                iRet = INT_MMS_FX_CCY_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }

/* amt */
                        sprintf(csTag,"txn_data.fx.%d.amt",i);
                        if (GetField_CString(hRequest,csTag,&csTmp)) {
				dTmp = atof(csTmp);
DEBUGLOG(("Authorize: amt = [%.2lf]\n",dTmp));
                        } else {
DEBUGLOG(("Authorize:: amt missing!!\n"));
ERRLOG("TxnMmsByUsCCAT:: Authorize:: amt missing!!\n");
                                iRet = INT_MMS_FX_AMT_NOT_FOUND;
                                PutField_Int(hContext,"internal_error",iRet);
                        }
		}
	}

/* create_user */
	if (GetField_CString(hRequest,"user",&csTmp)) {
DEBUGLOG(("Authorize: create_user = [%s]\n",csTmp));
	} else {
DEBUGLOG(("Authorize:: user missing!!\n"));
ERRLOG("TxnMmsByUsCEA:: Authorize:: user missing!!\n");
		iRet = INT_USER_NOT_FOUND;
		PutField_Int(hContext,"internal_error",iRet);
	}

	// Response
	PutField_CString(hResponse,"txn_seq",csTxnSeq);
	PutField_Int(hResponse,"ret",iRet);

DEBUGLOG(("TxnMmsByUsCAT Normal Exit() iRet = [%d]\n",iRet));
	return iRet;
}
